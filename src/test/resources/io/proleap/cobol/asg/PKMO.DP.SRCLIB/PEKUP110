124500                 ,FSCL_MN_NBR                                     12420000
124600                 ,TO_CHAR(SEA_BEG_DTE, 'YYYY-MM-DD')              12430000
124700                 ,TO_CHAR(SEA_END_DTE, 'YYYY-MM-DD')              12440000
