014400 PROCEDURE DIVISION.                                              01440000
014500***************************************************************** 01450000
014600** THIS MODULE CONTROLS THE OVERALL PROCESSING IN THE PROGRAM. ** 01460000
014700** THE SETUP AND PERFORM OF PARAGRAPH 0001-CALL-CICS-ARCH-API  ** 01470000
014800** MUST BE THE FIRST CODE EXECUTED IN THIS PROGRAM.            ** 01480000
014900**                                                             ** 01490000
015000** THE SECOND OR 'EXIT' PERFORM OF THIS PARAGRAPH MUST BE THE  ** 01500000
015100** LAST CODE EXECUTED ON EACH ITERATION OF THIS PROGRAM.       ** 01510000
015200***************************************************************** 01520000
015300                                                                  01530000
015400 0000-MAIN-MODULE.                                                01540000
015500                                                                  01550000
015600     INITIALIZE DP030-CICS-API-FIELDS.                            01560000
015700                                                                  01570000
015800     MOVE +1                       TO DP030-NUMBER-OF-MAPS.       01580000
015900     MOVE PC-CURRENT-MAPSET-NAME   TO DP030-MAPSET-NAME.          01590000
016000     MOVE PC-CURRENT-MAP-NAME      TO DP030-MAP-NAME (1).         01600000
016100     SET DP030-RECEIVE-APPL-MAP    TO TRUE.                       01610000
016200     MOVE LENGTH OF PC171AI        TO DP030-MAP-LENGTH (1).       01620000
016300     MOVE 'PROGRAM ENTRY CALL'     TO DP013-MESSAGE-TEXT (1).     01630000
016400     PERFORM 0001-CALL-CICS-ARCH-API.                             01640000
016500                                                                  01650000
016600     PERFORM 1000-CONTROL-PROCESSING                              01660000
016700                                                                  01670000
016800     MOVE 'PROGRAM EXIT CALL'      TO DP013-MESSAGE-TEXT (1).     01680000
016900     PERFORM 0001-CALL-CICS-ARCH-API.                             01690000
017000                                                                  01700000
017100 0001-CALL-CICS-ARCH-API.                                         01710000
017200                                                                  01720000
1122RM*    CALL 'DPKCS030' USING DFHEIBLK                               01730015
1122RM     CALL DP930-CICS-ARCH-API                                     01731015
1122RM                     USING DFHEIBLK                               01732015
017400                           DFHCOMMAREA                            01740000
017500                           DP030-CICS-API-FIELDS                  01750000
017600                           DP020-STANDARD-COMMAREA                01760000
017700                           PC171AI.                               01770000
017800                                                                  01780000
017900     IF  DP030-RC-CALL-SUCCESSFUL                                 01790000
018000         CONTINUE                                                 01800000
018100     ELSE                                                         01810000
018200         SET DP013-NO-ROLLBACK                                    01820000
018300             DP013-XCTL-DISPLAY-RESTART                           01830000
018400             DP013-CICS-ABEND      TO TRUE                        01840000
018500         MOVE 'BEFORE 0000-MAIN-MODULE'                           01850000
018600                                   TO DP013-PARAGRAPH             01860000
1122RM*        MOVE 'CALL TO DPKCS030 NOT SUCCESSFUL, RETURN-CODE ON NEX01870015
1122RM*             'T LINE'             TO DP013-MESSAGE-TEXT (2)      01880015
1122RM         MOVE 'CALL TO CICS ARCH API NOT SUCCESSFUL, RETURN-CODE O01881015
1122RM-             'N NEXT LINE'        TO DP013-MESSAGE-TEXT (2)      01882015
018900         MOVE DP030-RETURN-CODE                                   01890000
019000                                   TO DP013-MESSAGE-TEXT (3)      01900000
019100         PERFORM DP013-0000-PROCESS-ABEND                         01910000
019200     END-IF.                                                      01920000
019300 EJECT                                                            01930000
019400*----------------------------------------------------------------*01940000
019500*    PROCESS THE APPROPRIATE PARAGRAPHS BASED ON WHAT THE NEXT   *01950000
019600*    COURSE OF ACTION IS FOR THIS TRANSACTION.                   *01960000
019700*----------------------------------------------------------------*01970000
019800                                                                  01980000
019900 1000-CONTROL-PROCESSING.                                         01990000
020000                                                                  02000000
020100     EVALUATE TRUE                                                02010000
020200         WHEN DP020-NEXT-ACT-INITIAL                              02020000
020300              INITIALIZE ASC-SPECIFIC-COMMAREA                    02030000
020400              PERFORM 4000-BUILD-INITIAL-PANEL                    02040000
020500              PERFORM 4400-MOVE-COMMAREA-TO-SCREEN                02050000
020600                                                                  02060000
020700         WHEN DP020-NEXT-ACT-READ-MAP                             02070000
020800              PERFORM 2000-PROCESS-PANEL                          02080000
020900                                                                  02090000
021000         WHEN DP020-NEXT-ACT-RETURN                               02100000
021100              PERFORM 4400-MOVE-COMMAREA-TO-SCREEN                02110000
021200              PERFORM 3000-EDIT-DATA-IN-COMMAREA                  02120000
021300                                                                  02130000
021400         WHEN OTHER                                               02140000
021500             SET DP013-LOGIC-ABEND                                02150000
021600                 DP013-NO-ROLLBACK TO TRUE                        02160000
021700             MOVE '1000-CONTROL-PROCESSING'                       02170000
021800                                   TO  DP013-PARAGRAPH            02180000
021900             MOVE 'INVALID NEXT ACTIVITY RETURNED TO APPL PGM:'   02190000
022000                                   TO  DP013-MESSAGE-TEXT(1)      02200000
022100             MOVE DP020-NEXT-APPL-ACTIVITY                        02210000
022200                                   TO DP013-MESSAGE-TEXT(2)       02220000
022300             PERFORM DP013-0000-PROCESS-ABEND                     02230000
022400     END-EVALUATE.                                                02240000
022500                                                                  02250000
022600*----------------------------------------------------------------*02260000
022700* FURTHER DETERMINE PROCESSING PATH BASED ON FUNCTION KEY ACTIONS*02270000
022800* SPECIFIC FUNCTION KEYS LISTED HERE ARE ACTED UPON REGARDLESS OF*02280000
022900* EDITS.                                                         *02290000
023000*----------------------------------------------------------------*02300000
023100                                                                  02310000
023200 2000-PROCESS-PANEL.                                              02320000
023300                                                                  02330000
023400     SET DP030-SET-CURSOR-APPL-1   TO  TRUE.                      02340000
023500                                                                  02350000
023600     EVALUATE TRUE                                                02360000
023700                                                                  02370000
023800         WHEN DP020-SRC-AID = DP016-CLEAR                         02380000
023900              PERFORM 3000-EDIT-DATA-IN-COMMAREA                  02390000
024000              PERFORM 4400-MOVE-COMMAREA-TO-SCREEN                02400000
024100                                                                  02410000
024200         WHEN DP020-FK-REFRESH(DP020-SRC-AID)                     02420000
024300              MOVE PC-S            TO  ASC-MASTER-PO              02430011
024400              MOVE SPACES          TO                             02440011
024510                                       ASC-MERCH-LOC              02451001
024600              PERFORM 4000-BUILD-INITIAL-PANEL                    02460000
024700              PERFORM 4400-MOVE-COMMAREA-TO-SCREEN                02470000
024800              PERFORM 3200-RESET-ATTRIBUTES                       02480000
024900                                                                  02490000
025000         WHEN OTHER                                               02500000
025100              PERFORM 2200-MOVE-SCREEN-TO-COMMAREA                02510000
025200              PERFORM 3000-EDIT-DATA-IN-COMMAREA                  02520000
025300              IF  PS-NO-ERROR                                     02530000
025400                  SET DP030-CONTINUE-GO                           02540000
025500                                   TO  TRUE                       02550000
025600                  PERFORM 2100-CHECK-FUNCTION-KEY                 02560000
025700              ELSE                                                02570000
025800                  SET DP030-OVERRIDE-GO                           02580000
025900                                   TO  TRUE                       02590000
026000             END-IF                                               02600000
026100             PERFORM 4400-MOVE-COMMAREA-TO-SCREEN                 02610000
026200     END-EVALUATE.                                                02620000
026300*----------------------------------------------------------------*02630000
026400*  ACT ON ANY FUNCTION KEYS THAT REQUIRE EDITS TO BE PASSED      *02640000
026500*  FIRST.  NOTE THAT INVALID FUNCTION KEYS WILL NOT BE RETURNED  *02650000
026600*  FROM THE CICS ARCHITECTURE API.                               *02660000
026700*----------------------------------------------------------------*02670000
026800                                                                  02680000
026900 2100-CHECK-FUNCTION-KEY.                                         02690000
027000                                                                  02700000
027100     EVALUATE TRUE                                                02710000
027200         WHEN DP020-FK-LOCAL-GOTO (DP020-SRC-AID)                 02720000
027300              MOVE ASC-MASTER-PO   TO  PC000-MASTER-PO            02730000
027400              MOVE SPACES          TO  PC000-CHECKIN-DOC          02740004
027500              MOVE ASC-PO-NBR      TO  PC000-PO-NBR-N             02750000
027600                                                                  02760000
027700              MOVE ASC-REC-SEQ-NBR TO  PC000-REC-SEQ-NBR-N        02770000
027800              MOVE ASC-PO-LINE-NBR TO  PC000-LINE-NBR-N           02780000
027900              MOVE ASC-OPER-UNT-ID TO  PC000-OPERATING-UNIT-ID    02790000
028000                                                                  02800000
028100              MOVE ASC-FCLTY-ID    TO  PC000-FACILITY-ID          02810000
028200              MOVE ASC-FCLTY-NME   TO  PC000-OPERATION-DESC       02820000
028210              MOVE ASC-MERCH-LOC   TO  PC000-MERCH-LOCATION       02821002
028300              IF  PS-ERROR                                        02830000
028400                  SET DP030-OVERRIDE-GO                           02840000
028500                                   TO  TRUE                       02850000
028600              END-IF                                              02860000
028700                                                                  02870000
028800         WHEN DP020-SRC-AID = DP016-ENTER                         02880000
028900              CONTINUE                                            02890000
029000                                                                  02900000
029100         WHEN OTHER                                               02910000
029200              SET DP013-NO-ROLLBACK                               02920000
029300                  DP013-XCTL-DISPLAY-RESTART                      02930000
029400                  DP013-CICS-ABEND TO  TRUE                       02940000
029500              MOVE '2100-CHECK-FUNCTION-KEY'                      02950000
029600                                   TO  DP013-PARAGRAPH            02960000
029700              MOVE 'INVALID FUNCTION KEY NOT CAPTURED BY API'     02970000
029800                                   TO  DP013-MESSAGE-TEXT (1)     02980000
029900              PERFORM DP013-0000-PROCESS-ABEND                    02990000
030000     END-EVALUATE.                                                03000000
030100/                                                                 03010000
030200*----------------------------------------------------------------*03020000
030300* MOVE DATA ENTERED ON THE SCREEN INTO THEIR RESPECTIVE FIELDS IN*03030000
030400* THE APPLICATION-SPECIFIC COMMAREA.  ALL EDITS ARE DONE IN THE  *03040000
030500* APPLICATION-SPECIFIC COMMAREA, NOT ON THE SCREEN.              *03050000
030600*----------------------------------------------------------------*03060000
030700                                                                  03070000
030800 2200-MOVE-SCREEN-TO-COMMAREA.                                    03080000
030900                                                                  03090000
031000     IF  AMASTPOL > ZERO                                          03100000
031100         MOVE AMASTPOI             TO ASC-MASTER-PO               03110000
031200     ELSE                                                         03120000
031300         IF  AMASTPOF = DP015-ERASE-EOF                           03130000
031400             MOVE SPACES           TO ASC-MASTER-PO               03140000
031500         END-IF                                                   03150000
031600     END-IF.                                                      03160000
031700                                                                  03170000
033210     IF  AMLOCL > ZERO                                            03321001
033220         MOVE AMLOCI               TO  ASC-MERCH-LOC              03322001
033230     ELSE                                                         03323001
033240         IF  AMLOCF  = DP015-ERASE-EOF                            03324001
033250             MOVE SPACES           TO  ASC-MERCH-LOC              03325001
033260         END-IF                                                   03326001
033270     END-IF.                                                      03327001
033300                                                                  03330000
033400*----------------------------------------------------------------*03340000
033500* ENSURE THE DATA IS CORRECT.  NOTE THAT IN A CRITERIA SPECIFI-  *03350000
033600* CATION SCREEN ONLY MINIMAL EDITS ARE DONE.  EDITS RELATED TO   *03360000
033700* A SPECIFIC SCREEN THAT THE USER IS GOING TO (LOCAL-GO) ARE DONE*03370000
033800* IN THE DESTINATION PROGRAM, NOT HERE.                          *03380000
033900*----------------------------------------------------------------*03390000
034000                                                                  03400000
034100 3000-EDIT-DATA-IN-COMMAREA.                                      03410000
034200                                                                  03420000
034300     PERFORM 3200-RESET-ATTRIBUTES.                               03430000
034400     SET DP030-SET-CURSOR-APPL-1   TO  TRUE.                      03440000
034500                                                                  03450000
037200     IF  ASC-MASTER-PO EQUAL          PC-S                        03720006
037300         CONTINUE                                                 03730000
037400     ELSE                                                         03740000
037500         SET PS-ERROR                                             03750000
037600             DP020-MSG-FATAL       TO  TRUE                       03760000
037700         MOVE PC-TSYMSG-00005      TO  DP020-MSG-NUMBER           03770000
037800         MOVE DP015-UNP-ALP-BRT-OFF                               03780000
037900                                   TO  AMASTPOA                   03790000
038000         MOVE DP015-RED            TO  AMASTPOC                   03800000
038100         MOVE DP015-REVERSE        TO  AMASTPOH                   03810000
038200         MOVE -1                   TO  AMASTPOL                   03820000
038300     END-IF.                                                      03830000
039900                                                                  03990000
040000     IF  PS-NO-ERROR                                              04000000
040100*        MOVE -1                   TO AMASTPOL                    04010013
040110         MOVE -1                   TO AMLOCL                      04011013
040200     END-IF.                                                      04020000
040300                                                                  04030000
