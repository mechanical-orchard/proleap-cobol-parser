013500 0000-PROGRAM-MAINLINE.                                           01350000
013510     INITIALIZE  CA-COMMAREA                                      01351000
013511                 PC045-COMMAREA-REC.                              01351100
013520     MOVE DFHCOMMAREA                TO CA-COMMAREA.              01352000
013530     MOVE PC233-HOST-SAVE-AREA       TO PC045-COMMAREA-REC.       01353000
013700     MOVE LOW-VALUES                 TO PC895AI.                  01370000
013800                                                                  01380000
013900     EVALUATE TRUE                                                01390000
014000         WHEN EIBCALEN = ZERO                                     01400000
014100         WHEN EIBAID = DFHCLEAR                                   01410000
014200         WHEN PC233-INITIALIZE-SCREEN                             01420000
014210              SET PC233-PROCESS-SCREEN  TO TRUE                   01421000
014300              MOVE PC045-TRLR-SRL-NBR   TO ATRLRO                 01430000
014400              MOVE PC045-AUDIT-CNTL-NBR TO AAUDNBRO               01440000
014500              IF PC045-100-AUDIT-CHECK                            01450000
014510                 MOVE '100%'            TO ATYPEO                 01451000
014520              ELSE                                                01452000
014530                 MOVE 'SPOT'            TO ATYPEO                 01453000
014540              END-IF                                              01454000
014600              PERFORM 8000-SEND-MAP                               01460000
014700              PERFORM 8200-RETURN-TRANID                          01470000
014800         WHEN EIBAID = DFHENTER                                   01480000
014900              PERFORM 1000-RECEIVE-MAP                            01490000
015000              PERFORM 1100-EDIT-SELECTION                         01500000
015100                                                                  01510000
015200         WHEN EIBAID = DFHPF3                                     01520000
015210*             SHOULD XCTL TO PCKRF892                             01521001
015300              MOVE PC233-ROOT-PROGRAM                             01530000
015400                                     TO PV-TRANSFER-PROGRAM       01540000
015410              MOVE 'PCKRF895'                                     01541000
015420                                     TO PC233-LAST-PROGRAM        01542000
015500              PERFORM 2100-XCTL-TO-SELECTED-PROGRAM               01550000
015600                                                                  01560000
015700         WHEN OTHER                                               01570000
015800              MOVE PC-UNRECOGNIZED-PF-KEY-MSG                     01580000
015900                                     TO AMSGLNO                   01590000
016000              PERFORM 8100-SEND-DATA-ONLY                         01600000
016100              PERFORM 8200-RETURN-TRANID                          01610000
016200     END-EVALUATE.                                                01620000
016300                                                                  01630000
016400     GOBACK.                                                      01640000
016500*                                                                 01650000
016600*                                                                 01660000
016700 1000-RECEIVE-MAP.                                                01670000
016800     EXEC CICS                                                    01680000
016900         RECEIVE MAP    ('PC895A')                                01690000
017000                 MAPSET ('PCKM895')                               01700000
017100                 RESP   (PV-CICS-RESPONSE)                        01710000
017200     END-EXEC.                                                    01720000
017300                                                                  01730000
017400     EVALUATE TRUE                                                01740000
017500         WHEN PV-CICS-RESPONSE = DFHRESP (NORMAL)                 01750000
017600              PERFORM 1010-RECEIVE-SCREEN-FIELDS                  01760000
017700         WHEN PV-CICS-RESPONSE = DFHRESP (MAPFAIL)                01770000
017800              CONTINUE                                            01780000
017900         WHEN OTHER                                               01790000
018000              MOVE '1000-RECEIVE-MAP'                             01800000
018100                                     TO DP013-PARAGRAPH           01810000
018200              MOVE 'UNABLE TO RECEIVE MAP'                        01820000
018300                                     TO DP013-MESSAGE-TEXT (1)    01830000
018400              SET DP013-LOGIC-ABEND  TO TRUE                      01840000
018500              SET DP013-NO-ROLLBACK  TO TRUE                      01850000
018600              PERFORM 9999-WRITE-MESSAGE-TO-CICS-LOG              01860000
018700     END-EVALUATE.                                                01870000
018800*                                                                 01880000
018900*                                                                 01890000
019000 1010-RECEIVE-SCREEN-FIELDS.                                      01900000
019100     IF  ASELECTL > ZERO                                          01910000
019200         MOVE ASELECTI               TO CA-SELECTION-X            01920000
019300     END-IF.                                                      01930000
019400*                                                                 01940000
019500*                                                                 01950000
019600 1100-EDIT-SELECTION.                                             01960000
019700     MOVE CA-SELECTION-X             TO DP010I-UNEDITED-FIELD.    01970000
019800     MOVE 1                          TO DP010I-MAXIMUM-DIGITS.    01980000
019900     MOVE 0                          TO DP010I-MAXIMUM-DECIMALS.  01990000
020000     SET DP010I-NEGATIVE-NOT-ALLOWED TO TRUE.                     02000000
020100                                                                  02010000
020210     CALL DP010I-NUMERIC-EDIT-ROUTINE                             02021005
020220                         USING DP010I-NUMERIC-EDIT-AREA           02022005
020300                                                                  02030000
020400     IF  DP010I-ERROR-DETECTED                                    02040000
020500         MOVE PC-NON-NUMERIC-SELECTION-MSG                        02050000
020600                                     TO AMSGLNO                   02060000
020700         PERFORM 8100-SEND-DATA-ONLY                              02070000
020800         PERFORM 8200-RETURN-TRANID                               02080000
020900     ELSE                                                         02090000
021000         MOVE DP010I-NUMERIC-FIELD   TO CA-SELECTION              02100000
021100     END-IF.                                                      02110000
021110                                                                  02111000
021200     EVALUATE TRUE                                                02120000
021300     WHEN CA-VALID-SELECTION                                      02130000
021400         PERFORM 2200-PROCESS-SELECT-TYPE                         02140000
021410         PERFORM 2000-PROCESS-SELECTION                           02141000
021500     WHEN OTHER                                                   02150000
021600         MOVE DP015-UNP-NUM-BRT-MDT  TO ASELECTA                  02160000
021700         MOVE PC-INVALID-SELECTION-MSG                            02170000
021800                                     TO AMSGLNO                   02180000
021900         PERFORM 8100-SEND-DATA-ONLY                              02190000
022000         PERFORM 8200-RETURN-TRANID                               02200000
022100     END-EVALUATE.                                                02210000
022200*                                                                 02220000
022300*                                                                 02230000
022400 2000-PROCESS-SELECTION.                                          02240000
022500      IF CA-SELECTION-ONE                                         02250000
022600         MOVE PC045-OPER-UNT-ID TO PC233-OPER-UNT-ID              02260002
022700         MOVE PC045-USERID      TO PC233-USER-ID                  02270002
022900         MOVE PC-OPEN-TRAILER-SCREEN TO PV-TRANSFER-PROGRAM       02290000
023000         PERFORM 2100-XCTL-TO-SELECTED-PROGRAM                    02300000
023100      ELSE                                                        02310000
023110         MOVE PC-STARTED-TRLR-AUDIT  TO PV-TRANSFER-PROGRAM       02311000
023300         PERFORM 2100-XCTL-TO-SELECTED-PROGRAM                    02330000
023400      END-IF.                                                     02340000
024100*                                                                 02410000
024200*                                                                 02420000
024300 2100-XCTL-TO-SELECTED-PROGRAM.                                   02430000
024400     SET PC233-INITIALIZE-SCREEN     TO TRUE.                     02440000
024401     MOVE PC-CURRENT-PROGRAM-NAME    TO PC233-LAST-PROGRAM.       02440103
024402     MOVE PC045-COMMAREA-REC         TO PC233-HOST-SAVE-AREA.     02440203
024410     MOVE LENGTH OF CA-COMMAREA      TO PV-COMMAREA-LENGTH.       02441000
024600                                                                  02460000
024700     EXEC CICS                                                    02470000
024800         XCTL                                                     02480000
024900             PROGRAM   (PV-TRANSFER-PROGRAM)                      02490000
025000             COMMAREA  (CA-COMMAREA)                              02500000
025100             LENGTH    (PV-COMMAREA-LENGTH)                       02510000
025200             RESP      (PV-CICS-RESPONSE)                         02520000
025300     END-EXEC.                                                    02530000
025400                                                                  02540000
025500     EVALUATE TRUE                                                02550000
025600         WHEN PV-CICS-RESPONSE = DFHRESP (NORMAL)                 02560000
025700              CONTINUE                                            02570000
025800         WHEN OTHER                                               02580000
025900              MOVE '2100-XCTL-TO-SELECTED-PROGRAM'                02590000
026000                                     TO DP013-PARAGRAPH           02600000
