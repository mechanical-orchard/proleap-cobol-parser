       PROCEDURE DIVISION.                                              02450000
      ******************************************************************02460000
       0000-MAINLINE.                                                   02470000
                                                                        02480000
           PERFORM 1000-INITIALIZATION.                                 02490000
           PERFORM 2000-PROCESS-FILES                                   02500000
                   UNTIL PS-EOF-INPUT-FILE.                             02510000
           PERFORM 9999-WRAPUP.                                         02520000
           GOBACK.                                                      02530000
                                                                        02540000
       EJECT.                                                           02550000
      ******************************************************************02560000
      *INITIALIZATION ROUTINE                                           02570000
      ******************************************************************02580000
       1000-INITIALIZATION.                                             02590000
                                                                        02600000
           OPEN INPUT   INPUT-FILE                                      02610000
                OUTPUT  REPORT-FILE.                                    02620000
                                                                        02630000
           ACCEPT DF-ACCEPT-TIME FROM TIME.                             02640000
           ACCEPT DF-ACCEPT-DATE FROM DATE.                             02650000
                                                                        02660000
           PERFORM 1100-SETUP-HEADINGS.                                 02670000
                                                                        02680000
           PERFORM 9999-READ-INPUT-FILE.                                02690000
                                                                        02700000
       EJECT.                                                           02710000
      ******************************************************************02720000
      *SETS UP ALL HEADING STATIC VARIABLES FOR THE REPORT DURATION     02730000
      ******************************************************************02740000
       1100-SETUP-HEADINGS.                                             02750000
                                                                        02760000
           MOVE '1200-SETUP-HEADINGS' TO PV-PARAGRAPH-NAME.             02770000
                                                                        02780000
           MOVE DF-ACCEPT-YR          TO DP132O-RUN-YEAR.               02790000
           MOVE DF-ACCEPT-MO          TO DP132O-RUN-MONTH.              02800000
           MOVE DF-ACCEPT-DY          TO DP132O-RUN-DAY.                02810000
           MOVE DF-ACCEPT-HR          TO DP132O-RUN-HOUR.               02820000
           MOVE DF-ACCEPT-MIN         TO DP132O-RUN-MINUTE.             02830000
                                                                        02840000
           MOVE PC-PROGRAM-NAME  TO DP132O-PROGRAM-NAME.                02850000
           MOVE 1                TO DP132O-REPORT-NUMBER.               02860000
                                                                        02870000
           PERFORM 4000-PRINT-HEADINGS.                                 02880000
                                                                        02890000
       EJECT.                                                           02900000
      ******************************************************************02910000
      *PROCESS BOTH FILES                                               02920000
      ******************************************************************02930000
       2000-PROCESS-FILES.                                              02940000
                                                                        02950000
           MOVE '2000-PROCESS-FILES' TO PV-PARAGRAPH-NAME.              02960000
                                                                        02970000
           PERFORM 4100-PRINT-DETAIL.                                   02980000
                                                                        02990000
           IF  PC-LINE-COUNT > 60                                       03000000
               PERFORM 4000-PRINT-HEADINGS                              03010000
           END-IF.                                                      03020000
                                                                        03030000
           PERFORM 9999-READ-INPUT-FILE.                                03040000
                                                                        03050000
       EJECT.                                                           03060000
      ******************************************************************03070000
      *PRINTS REPORT HEADINGS AT TOP OF PAGE                            03080000
      ******************************************************************03090000
       4000-PRINT-HEADINGS.                                             03100000
                                                                        03110000
           MOVE '4000-PRINT-HEADINGS' TO PV-PARAGRAPH-NAME.             03120000
                                                                        03130000
           INITIALIZE PC-LINE-COUNT.                                    03140000
           ADD +1                     TO PC-PAGE-COUNT.                 03150000
           MOVE PC-PAGE-COUNT         TO DP132O-PAGE-NUMBER.            03160000
                                                                        03170000
           WRITE REPORT-RECORD FROM DP132O-STANDRD-HEADER-132-COLS      03180000
                                                  AFTER ADVANCING PAGE. 03190000
           WRITE REPORT-RECORD FROM REPORT-HEADING-1                    03200000
                                               AFTER ADVANCING 2 LINES. 03210000
           WRITE REPORT-RECORD FROM REPORT-HEADING-2                    03220000
                                               AFTER ADVANCING 1 LINES. 03230000
           WRITE REPORT-RECORD FROM REPORT-HEADING-3                    03240000
                                               AFTER ADVANCING 3 LINES. 03250000
           WRITE REPORT-RECORD FROM REPORT-HEADING-4                    03260000
                                               AFTER ADVANCING 1 LINES. 03270000
           WRITE REPORT-RECORD FROM REPORT-HEADING-5                    03280000
                                               AFTER ADVANCING 1 LINES. 03290000
           WRITE REPORT-RECORD FROM REPORT-HEADING-6                    03300000
                                               AFTER ADVANCING 1 LINES. 03310000
                                                                        03320000
           ADD +9                     TO PC-LINE-COUNT.                 03330000
                                                                        03340000
       EJECT.                                                           03350000
