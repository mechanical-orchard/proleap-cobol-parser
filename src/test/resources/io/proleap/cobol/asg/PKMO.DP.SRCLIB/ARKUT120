023800                                                                  02320005
023900     PERFORM 2000-PROCESS-CARD-MASTER                             02330005
024000       UNTIL PS-STOP-PROCESSING.                                  02340005
024100                                                                  02350005
024200*-------------------------------*                                 02360005
024300 2000-PROCESS-CARD-MASTER.                                        02370005
024400*-------------------------------*                                 02380005
024410     IF IN-MSTR-RECORD                                            02390005
024420           MOVE IN-MASTER-RECORD    TO AR69-MASTER-DATA           02402012
024430           PERFORM 3000-PROCESS-MSTR-REC                          02410005
MFRETR           PERFORM 4200-PROCESS-MSTR-REC                          02420005
MFRETR           PERFORM 4205-PROCESS-MSTR-REC                          02430005
MFRETR           PERFORM 4400-PROCESS-MSTR-REC                          02440005
MFRETR           PERFORM 4600-PROCESS-MSTR-REC                          02450005
MFRETR           PERFORM 4800-PROCESS-MSTR-REC                          02460005
024440     END-IF                                                       02470005
025400     PERFORM 4000-READ-CARD-MASTER.                               02480005
025500     MOVE SPACES                 TO CARD-MASTER-OUT-REC.          02490005
025600                                                                  02500005
025700*-------------------------------*                                 02510005
025800 3000-PROCESS-MSTR-REC.                                           02520005
025900*-------------------------------*                                 02530005
026000                                                                  02540005
