023100*----------------------------------------------------------------*02310000
023200 PROCEDURE DIVISION.                                              02320000
023300                                                                  02330000
023400     DISPLAY '*** START - RCKRP091 ***'.                          02340001
023500     DISPLAY ' '.                                                 02350000
023600                                                                  02360000
023700                                                                  02370000
023800*----------------------------------------------------------------*02380000
023900*                                                                 02390000
024000*----------------------------------------------------------------*02400000
024100 0000-MAINLINE.                                                   02410000
024200                                                                  02420000
024300     PERFORM 0010-INITIALIZATION.                                 02430000
024400                                                                  02440000
024500     PERFORM 1000-PROCESS-INPUT                                   02450000
024600       UNTIL PS-EOF-INPUT.                                        02460000
024700                                                                  02470000
024800     WRITE REPORT-RECORD                                          02480000
024900      FROM END-OF-REPORT-LINE                                     02490000
025000     AFTER ADVANCING 3 LINES.                                     02500000
025100                                                                  02510000
025200     CLOSE INPUT01-FILE                                           02520000
025300           REPORT-FILE.                                           02530000
025400                                                                  02540000
025500     GOBACK.                                                      02550000
025600                                                                  02560000
025700                                                                  02570000
025800*----------------------------------------------------------------*02580000
025900*                                                                 02590000
026000*----------------------------------------------------------------*02600000
026100 0010-INITIALIZATION.                                             02610000
026200                                                                  02620000
026300     OPEN INPUT  INPUT01-FILE                                     02630000
026400          OUTPUT REPORT-FILE.                                     02640000
026500                                                                  02650000
026600     ACCEPT PV-TODAYS-DATE   FROM DATE.                           02660000
026700     ACCEPT PV-TODAYS-TIME   FROM TIME.                           02670000
026800                                                                  02680000
026900     MOVE PV-TODAYS-DATE-MM      TO DP250O-RUN-MONTH.             02690000
027000     MOVE PV-TODAYS-DATE-DD      TO DP250O-RUN-DAY.               02700000
027100     MOVE PV-TODAYS-DATE-YY      TO DP250O-RUN-YEAR.              02710000
027200     MOVE PV-TODAYS-TIME-HH      TO DP250O-RUN-HOUR.              02720000
027300     MOVE PV-TODAYS-TIME-MM      TO DP250O-RUN-MINUTE.            02730000
027400     MOVE PC-PROGRAM-NAME        TO DP250O-PROGRAM-NAME.          02740000
027500     MOVE PC-REPORT-NBR          TO DP250O-REPORT-NUMBER.         02750000
027600                                                                  02760000
027700     PERFORM 7000-READ-INPUT.                                     02770000
027800                                                                  02780000
027900     IF  PS-EOF-INPUT                                             02790000
028000         PERFORM 9040-PAGE-HEADERS                                02800000
028100         WRITE REPORT-RECORD                                      02810000
028200          FROM NO-DATA-TO-REPORT-LINE                             02820000
028300         AFTER ADVANCING 2 LINES                                  02830000
028400     ELSE                                                         02840000
028500         MOVE RC109-DIST-CNTR   TO PV-OPER-UNT-ID                 02850000
028600                                   PV-PREV-OPER-UNT-ID            02860000
028700                                                                  02870000
028800         PERFORM 2000-GET-OPER-NAME                               02880000
028900                                                                  02890000
029000         PERFORM 9040-PAGE-HEADERS                                02900000
029100     END-IF.                                                      02910000
029200                                                                  02920000
029300                                                                  02930000
029400*----------------------------------------------------------------*02940000
029500*                                                                 02950000
029600*----------------------------------------------------------------*02960000
029700 1000-PROCESS-INPUT.                                              02970000
029800                                                                  02980000
029900     IF  RC109-DIST-CNTR   NOT = PV-PREV-OPER-UNT-ID              02990000
030000         MOVE RC109-DIST-CNTR   TO PV-OPER-UNT-ID                 03000000
030100                                   PV-PREV-OPER-UNT-ID            03010000
030200         PERFORM 2000-GET-OPER-NAME                               03020000
030300         PERFORM 9040-PAGE-HEADERS                                03030000
030400                                                                  03040000
030500     END-IF.                                                      03050000
030600                                                                  03060000
030700     MOVE RC109-DIST-CNTR           TO  DTL01-DC.                 03070000
030800     MOVE RC109-EDI-VEND-ID         TO  DTL01-EDI-VNDR.           03080000
030900     MOVE RC109-CARRIER-SCAC-CODE   TO  DTL01-CARR-SCAC.          03090000
031000     MOVE RC109-BILL-OF-LADING-NBR  TO  DTL01-BOL.                03100000
031100     MOVE RC109-PO-NBR              TO  DTL01-PO-NBR.             03110000
031200     MOVE RC109-STORE-NBR           TO  DTL01-STR-NBR.            03120000
031300     MOVE RC109-PO-LINE-NBR         TO  DTL01-PO-LINE.            03130000
031400     MOVE RC109-EDI-VEND-NAME       TO  DTL01-EDI-VEND-NAME.      03140000
031500     MOVE RC109-DEPT-NBR            TO  DTL01-DEPT.               03150000
031600     MOVE RC109-DEPT-DESC           TO  DTL01-DEPT-DESC.          03160000
031700     MOVE RC109-DATE-SHIPPED        TO  DTL01-SHIP-DATE.          03170000
031800     MOVE RC109-PRO-NBR             TO  DTL01-ORIG-PRO.           03180000
031900     MOVE RC109-SKU-NBR             TO  DTL01-SKU.                03190000
032000     MOVE RC109-UPC-NBR             TO  DTL01-UPC-NBR.            03200000
032100     MOVE RC109-CARTON-NBR          TO  DTL01-SSCC.               03210000
032200     MOVE RC109-CARTON-QTY          TO  DTL01-SSCC-UNIT-QTY.      03220000
032300     MOVE RC109-CTN-UPC             TO  DTL01-SSCC-UPC.           03230000
032400                                                                  03240000
032500     PERFORM 9000-REPORT-DETAIL.                                  03250000
032600                                                                  03260000
032700     PERFORM 7000-READ-INPUT.                                     03270000
032800                                                                  03280000
032900                                                                  03290000
033000*----------------------------------------------------*            03300000
033100* GET OPER UNIT NAME                                 *            03310000
033200*----------------------------------------------------*            03320000
033300 2000-GET-OPER-NAME.                                              03330000
033400                                                                  03340000
033500     MOVE RC109-DIST-CNTR    TO  XLT00010-LOC-NBR.                03350000
033600                                                                  03360000
033700     EXEC SQL                                                     03370000
033800         SELECT A.LOC_NM                                          03380000
033900           INTO :XLT00010-LOC-NM                                  03390000
034000           FROM XLT_LOC A                                         03400000
034100          WHERE A.LOC_NBR     = :XLT00010-LOC-NBR                 03410000
034200           WITH UR                                                03420000
034300     END-EXEC.                                                    03430000
034400                                                                  03440000
034500     EVALUATE TRUE                                                03450000
034600       WHEN SQLCODE = ZERO                                        03460000
034700           MOVE XLT00010-LOC-NBR TO HDR02-DC                      03470000
034800           MOVE XLT00010-LOC-NM  TO HDR02-DC-DESCRIPTION          03480000
034900                                                                  03490000
035000       WHEN OTHER                                                 03500000
035100           MOVE ZEROES           TO HDR02-DC                      03510000
035200           MOVE SPACES           TO HDR02-DC-DESCRIPTION          03520000
035300                                                                  03530000
035400     END-EVALUATE.                                                03540000
035500                                                                  03550000
035600                                                                  03560000
035700*----------------------------------------------------*            03570000
035800* READ INPUT FILE                                    *            03580000
035900*----------------------------------------------------*            03590000
036000 7000-READ-INPUT.                                                 03600000
036100                                                                  03610000
036200     READ INPUT01-FILE                                            03620000
036300          INTO RC109-ASN-TRANSCRIPTION-RECORD                     03630000
036400        AT END SET  PS-EOF-INPUT  TO TRUE.                        03640000
036500                                                                  03650000
036600                                                                  03660000
036700*----------------------------------------------------*            03670000
036800* WRITE DETAIL REPORT LINE                           *            03680000
036900*----------------------------------------------------*            03690000
037000 9000-REPORT-DETAIL.                                              03700000
037100                                                                  03710000
037200     WRITE REPORT-RECORD FROM DTL01-LINE                          03720000
037300            AFTER ADVANCING 1 LINE.                               03730000
037400                                                                  03740000
037500     ADD  +1 TO PV-LINE-CNT                                       03750000
037600                PV-NBR-OF-LINES.                                  03760000
037700                                                                  03770000
037800     IF  PV-LINE-CNT  >  PV-LINE-LIMIT                            03780000
037900         PERFORM 9040-PAGE-HEADERS                                03790000
038000     END-IF.                                                      03800000
038100                                                                  03810000
038200                                                                  03820000
038300*----------------------------------------------------*            03830000
038400* WRITE PAGE HEADERS                                 *            03840000
038500*----------------------------------------------------*            03850000
038600 9040-PAGE-HEADERS.                                               03860000
038700                                                                  03870000
038800     ADD +1           TO PV-PAGE-CNT.                             03880000
038900     MOVE PV-PAGE-CNT TO DP250O-PAGE-NUMBER.                      03890000
039000                                                                  03900000
039100     WRITE REPORT-RECORD FROM DP250O-STANDRD-HEADER-250-COLS      03910000
039200            AFTER ADVANCING PAGE.                                 03920000
039300     WRITE REPORT-RECORD FROM HDR01-LINE                          03930000
039400            AFTER ADVANCING 1 LINE.                               03940000
039500                                                                  03950000
039600     MOVE +2 TO PV-LINE-CNT.                                      03960000
039700                                                                  03970000
039800     WRITE REPORT-RECORD FROM HDR02-LINE                          03980000
039900            AFTER ADVANCING 2 LINES.                              03990000
040000     WRITE REPORT-RECORD FROM HDR03-LINE                          04000000
040100            AFTER ADVANCING 1 LINE.                               04010000
040200     WRITE REPORT-RECORD FROM HDR04-LINE                          04020000
040300            AFTER ADVANCING 1 LINE.                               04030000
040400     WRITE REPORT-RECORD FROM HDR05-LINE                          04040000
040500            AFTER ADVANCING 1 LINES.                              04050000
040600     WRITE REPORT-RECORD FROM HDR06-LINE                          04060000
040700            AFTER ADVANCING 1 LINES.                              04070000
040800                                                                  04080000
040900     ADD  +6 TO PV-LINE-CNT.                                      04090000
041000                                                                  04100000
