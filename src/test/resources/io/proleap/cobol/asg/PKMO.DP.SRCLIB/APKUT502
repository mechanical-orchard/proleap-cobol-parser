018500*----------------------------------------------------------------*01500000
018600 A100-PROGRAM-MAINLINE.                                           01510000
018700                                                                  01520000
018800     PERFORM B100-INITIALIZATION.                                 01530000
019000     PERFORM B200-PROCESS-RECYCLE-FILE                            01540000
019100       UNTIL PS-EOF-RECYCLE-YES.                                  01550000
019300     PERFORM B300-END-OF-JOB.                                     01560000
019500     GOBACK.                                                      01570000
019600                                                                  01580000
019600                                                                  01590000
019700*----------------------------------------------------------------*01600000
019800* INITIALIZATION PROCESSING                                      *01610000
020100*----------------------------------------------------------------*01620000
020300 B100-INITIALIZATION.                                             01630000
020400                                                                  01640000
020500     OPEN INPUT  RECYCLE-IN-FILE                                  01650000
020500                 INPUT-IN-FILE                                    01660000
020600          OUTPUT RECYCLE-OUT-FILE                                 01670000
020610                 DETAIL-REPORT.                                   01680000
021610                                                                  01681005
021610     MOVE 'INPUT RECEIVED TO DELETE ENTRIES FROM RECYCLE FILE'    01682005
021610                                    TO DETAIL-RECORD-LONG.        01683005
021610     PERFORM W200-WRITE-DETAIL-MESSAGES.                          01684005
021610     MOVE SPACES                    TO DETAIL-RECORD-OUT.         01685005
021610     PERFORM W200-WRITE-DETAIL-MESSAGES.                          01686005
020700                                                                  01690000
020800     INITIALIZE PV-PO-INVC-TABLE.                                 01700001
020810     SET IDX1 TO 1.                                               01710000
020900                                                                  01720000
020900* INPUT FILE IS PROVIDED BY ACCOUNTS PAYABLE.  IT CONTAINS THE    01721008
020900* PO/INVC TO BE REMOVED FROM THE RECYCLE FILE.  THE FIRST RECORD  01722008
020900* IS A HEADER RECORD AND IT IS BYPASSED.                          01723008
021300     PERFORM R100-READ-INPUT-FILE.                                01730000
021300     IF PS-EOF-INPUT-YES                                          01730109
021310         SET AC-NO-INPUT-FILE       TO TRUE                       01730209
021310         MOVE 'B100-INITIALIZATION' TO AA-PARAGRAPH-NAME          01730309
021310         MOVE 'NO INPUT FILE PROVIDED'                            01730409
021310                                    TO AA-MESSAGE-1               01730509
021310         PERFORM Z998-ABEND                                       01730609
021300     END-IF.                                                      01730709
021300     PERFORM R100-READ-INPUT-FILE.                                01731008
021300                                                                  01732008
020900* IF EOF AT THIS POINT, THEN THERE WAS ONLY A HEADER RECORD ON    01732108
020900* THE FILE AND NO DATA RECORDS                                    01732208
021300     IF PS-EOF-INPUT-YES                                          01733008
021610         MOVE 'NO PO/INVOICE ON FILE - JUST A HEADER RECORD'      01733108
021610                                    TO DETAIL-RECORD-LONG         01733208
021610         PERFORM W200-WRITE-DETAIL-MESSAGES                       01733308
021610         SET PS-EOF-RECYCLE-YES     TO TRUE                       01733408
021300     ELSE                                                         01739008
021300         PERFORM T100-LOAD-TABLE                                  01740008
021300             UNTIL PS-EOF-INPUT-YES                               01750008
021300                                                                  01760000
021300         PERFORM R200-READ-RECYCLE-FILE                           01770008
021300         IF PS-EOF-RECYCLE-YES                                    01780008
021310             SET AC-NO-RECYCLE-FILE     TO TRUE                   01790008
021310             MOVE 'B100-INITIALIZATION' TO AA-PARAGRAPH-NAME      01800008
021310             MOVE 'NO RECYCLE FILE PROVIDED'                      01810008
021310                                    TO AA-MESSAGE-1               01820000
021310             PERFORM Z998-ABEND                                   01830008
021300         END-IF                                                   01840008
021300     END-IF.                                                      01850008
021610                                                                  01855004
021610                                                                  01860000
021700*---------------------------------------------------------------* 01870000
021800* READ THE RECORDS FROM THE RECYCLE FILE                        * 01880000
021800* CHECK TO SEE IF THE PO/INVOICE IS IN THE TABLE                * 01890000
021800* IF PO/INVC FOUND IN TABLE, DO NOT WRITE TO THE OUTPUT FILE    * 01900000
022000*---------------------------------------------------------------* 01910000
022200 B200-PROCESS-RECYCLE-FILE.                                       01920000
022300                                                                  01930000
022360     PERFORM T200-CHECK-TABLE.                                    01940000
022370                                                                  01950000
022380     IF PS-PO-INVC-FND-YES                                        01960000
050000         MOVE AP056-KOHLS-PO-NBR        TO DTL-PO-NBR             01970000
050000         MOVE AP056-VENDOR-INVOICE-NBR  TO DTL-INVC-ID            01980000
050000         MOVE PC-INVC-REMOVED-MSG       TO DTL-MESSAGE            01990000
022820         PERFORM W200-WRITE-DETAIL-MESSAGES                       02000000
022810     ELSE                                                         02010000
022820         PERFORM W100-WRITE-RECYCLE-REC                           02020000
023000     END-IF.                                                      02030000
023000                                                                  02040000
023100     PERFORM R200-READ-RECYCLE-FILE.                              02050000
023200                                                                  02060000
023300                                                                  02070000
023500*---------------------------------------------------------------* 02080000
023600* END OF PROGRAM PROCESSING                                     * 02090000
023600* CHECK FOR ANY PO/INVC THAT WAS NOT REMOVED FROM RECYCLE FILE  * 02091000
023600* DISPLAY COUNTS                                                * 02092000
023800*---------------------------------------------------------------* 02100000
024000 B300-END-OF-JOB.                                                 02110000
024100                                                                  02120000
024200     PERFORM T300-VALIDATE-TABLE                                  02130000
022360         VARYING IDX1 FROM 1 BY 1                                 02140000
022360         UNTIL IDX1 > MAX-IDX1                                    02141000
022360         OR TBL-PO-NBR (IDX1) = SPACES.                           02142000
024390                                                                  02270000
024400     CLOSE RECYCLE-IN-FILE                                        02280000
024400           INPUT-IN-FILE                                          02290000
024500           RECYCLE-OUT-FILE                                       02300000
024510           DETAIL-REPORT.                                         02310000
024600                                                                  02320000
024700     DISPLAY '****  APKUT502'.                                    02330007
024700     DISPLAY '    '.                                              02330107
024700** SUBTRACT 1 FROM INPUT RECORD COUNT FOR HEADER RECORD           02330216
024700     SUBTRACT 1 FROM PC-REC-INPUT-CNT.                            02330316
024700     DISPLAY 'INPUT RECORDS PROCESSED = ' PC-REC-INPUT-CNT.       02330416
024700     DISPLAY 'INPUT RECORDS BYPASSED  = ' PC-REC-BYPASS-CNT.      02330516
024700     DISPLAY 'RECYCLE RECORDS INPUT   = ' PC-REC-RECYCLE-IN-CNT.  02331000
024700     DISPLAY 'RECYCLE RECORDS OUTPUT  = ' PC-REC-RECYCLE-OUT-CNT. 02332000
024700     COMPUTE PC-CALC-CNT = PC-REC-RECYCLE-IN-CNT -                02332106
024700                           PC-REC-RECYCLE-OUT-CNT.                02332206
024700     DISPLAY 'RECYCLE RECORDS DELETED = ' PC-CALC-CNT.            02332306
024700     DISPLAY 'DETAIL MESSAGES OUTPUT  = ' PC-DETAIL-OUT-CNT.      02333001
024900                                                                  02370000
024900                                                                  02380000
502396*---------------------------------------------------------------- 02390000
502398*   LOAD THE INPUT PO/INVOICE RECORDS INTO A TABLE                02400000
502399*   THE RECYCLE FILE WILL BE COMPARED TO THE RECORDS IN THIS      02410000
502400*   TABLE.  ANY RECORDS THAT MATCH WILL BE DELETED FROM THE FILE  02420000
502401*---------------------------------------------------------------- 02430000
502402 T100-LOAD-TABLE.                                                 02440000
502403                                                                  02450000
502409     IF IDX1 > MAX-IDX1                                           02460000
050000         MOVE AP160-PO-NBR              TO DTL-PO-NBR             02470000
050000         MOVE AP160-INVOICE-ID          TO DTL-INVC-ID            02480000
050000         MOVE PC-FILE-TOO-LARGE-MSG     TO DTL-MESSAGE            02490000
022820         PERFORM W200-WRITE-DETAIL-MESSAGES                       02500000
022820         ADD 1                          TO PC-REC-BYPASS-CNT      02501015
502414     ELSE                                                         02510000
502474           MOVE AP160-PO-NBR         TO TBL-PO-NBR (IDX1)         02520000
502474           MOVE AP160-INVOICE-ID     TO TBL-INVC-ID (IDX1)        02530000
502474           SET IDX1                  UP BY 1                      02540000
502414     END-IF.                                                      02550000
502474                                                                  02560000
502474     PERFORM R100-READ-INPUT-FILE.                                02570000
502474                                                                  02580000
502474                                                                  02590000
502396*---------------------------------------------------------------- 02600000
502398*   COMPARE THE PO/INVOICE FROM THE RECYCLE FILE TO THE VALUES    02610000
502399*   IN THE TABLE.  IF FOUND, SET A SWITCH                         02620000
502401*---------------------------------------------------------------- 02630000
502402 T200-CHECK-TABLE.                                                02640000
502403                                                                  02650000
502403     SET PS-PO-INVC-FND-NO         TO TRUE.                       02660000
502403                                                                  02670000
502360     SET IDX1                      TO 1.                          02680000
502403                                                                  02690000
502370     SEARCH PO-INVC-TBL                                           02700001
502380         VARYING IDX1                                             02710000
502392             WHEN AP056-KOHLS-PO-NBR = TBL-PO-NBR (IDX1)          02720000
502392              AND AP056-VENDOR-INVOICE-NBR = TBL-INVC-ID (IDX1)   02730000
502393            MOVE 'Y'                TO TBL-DELETE-CD (IDX1)       02740001
502393            SET PS-PO-INVC-FND-YES  TO TRUE                       02750000
502394     END-SEARCH.                                                  02760000
502395                                                                  02770000
502474                                                                  02780000
502396*---------------------------------------------------------------- 02783000
502398*   CHECK TO SEE IF THERE ARE ANY PO/INVC THAT WAS NOT FOUND ON   02784000
502399*   THE RECYCLE FILE                                              02785000
502401*---------------------------------------------------------------- 02786000
502402 T300-VALIDATE-TABLE.                                             02787000
502403                                                                  02788000
502403     IF TBL-DELETE-CD (IDX1) NOT = 'Y'                            02789000
050000         MOVE TBL-PO-NBR (IDX1)         TO DTL-PO-NBR             02789100
050000         MOVE TBL-INVC-ID (IDX1)        TO DTL-INVC-ID            02789200
050000         MOVE PC-NOT-FOUND-MSG          TO DTL-MESSAGE            02789300
022820         PERFORM W200-WRITE-DETAIL-MESSAGES                       02789400
502403     END-IF.                                                      02789500
502474                                                                  02790200
502474                                                                  02791000
029989*----------------------------------------------------------------*02800000
029990* READS THE FILE OF PO/INVC RECORDS TO BE DELETED FROM RECYCLE   *02810000
029991*----------------------------------------------------------------*02820000
029992 R100-READ-INPUT-FILE.                                            02830000
029993                                                                  02840000
029994     READ INPUT-IN-FILE INTO AP160-INPUT-RECORD                   02850000
029995       AT END                                                     02860000
029996          SET PS-EOF-INPUT-YES    TO TRUE                         02870000
030000       NOT AT END                                                 02880000
030200        ADD 1                     TO PC-REC-INPUT-CNT             02890000
030300     END-READ.                                                    02900000
030301                                                                  02910000
030301                                                                  02920000
029989*----------------------------------------------------------------*02930000
029990* READS THE RECYCLE FILE                                         *02940000
029991*----------------------------------------------------------------*02950000
029992 R200-READ-RECYCLE-FILE.                                          02960000
029993                                                                  02970000
029994     READ RECYCLE-IN-FILE INTO AP056-EDI-IN-FILE                  02980000
029995       AT END                                                     02990000
029996          SET PS-EOF-RECYCLE-YES  TO TRUE                         03000000
030000       NOT AT END                                                 03010000
030200        ADD 1                     TO PC-REC-RECYCLE-IN-CNT        03020000
030300     END-READ.                                                    03030000
030301                                                                  03040000
030400                                                                  03050000
040000*----------------------------------------------------------------*03060000
040100* WRITES THE REMAINING RECORDS TO THE RECYCLE FILE               *03070000
040200*----------------------------------------------------------------*03080000
040300 W100-WRITE-RECYCLE-REC.                                          03090000
040400                                                                  03100000
040500     WRITE RECYCLE-OUT-RECORD FROM AP056-EDI-IN-FILE.             03110000
040600     ADD 1                        TO PC-REC-RECYCLE-OUT-CNT.      03120000
040700                                                                  03130000
040700                                                                  03140000
049300*----------------------------------------------------------------*03150000
049500* WRITE DETAIL MESSAGES TO INDICATE IF THE PO/INVC WAS DELETED   *03160000
049500* OR BYPASSED BY THE PROCESS                                     *03170000
049600*----------------------------------------------------------------*03180000
049800 W200-WRITE-DETAIL-MESSAGES.                                      03190000
049900                                                                  03200000
050000     WRITE DETAIL-RECORD FROM DETAIL-RECORD-OUT.                  03210000
050200     ADD 1                          TO PC-DETAIL-OUT-CNT.         03220000
050300                                                                  03230000
050300                                                                  03240000
050500*----------------------------------------------------------------*03250000
050600* ABEND ROUTINE                                                  *03260000
050800*----------------------------------------------------------------*03270000
050900 Z998-ABEND.                                                      03280000
051000                                                                  03290000
051100     DISPLAY AA-ABEND-LIT.                                        03300000
051200     DISPLAY AA-PROGRAM-LIT.                                      03310000
051300     DISPLAY AA-PARAGRAPH-LIT.                                    03320000
