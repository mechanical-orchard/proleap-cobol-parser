028300******************************************************************02830002
028400 PROCEDURE DIVISION.                                              02840002
028500******************************************************************02850002
028600                                                                  02860002
028700                                                                  02870002
028800*-----------------------------------------------------------------02880002
028900* 0000-PROGRAM-MAINLINE - CONTROLS THE MAIN PROCESSING OF THE     02890002
029000*      PROGRAM.                                                   02900002
029100*-----------------------------------------------------------------02910002
029200 0000-PROGRAM-MAINLINE.                                           02920002
029300                                                                  02930002
029400     DISPLAY ' '.                                                 02940002
029500     DISPLAY ' RCKUT182 - START'                                  02950002
029600     DISPLAY ' '.                                                 02960002
029700                                                                  02970002
029800     SET PS-END-OF-FILE-NO TO TRUE.                               02980002
029900                                                                  02990002
030000     OPEN INPUT  INPUT-FILE                                       03000002
030040          OUTPUT ASN-SINGLE-SKU-SSCC-FILE                         03004002
030070          OUTPUT ASN-MULTI-SKU-SSCC-FILE.                         03007002
030100                                                                  03010002
030200     PERFORM 8000-READ-INPUT.                                     03020002
030300                                                                  03030002
030600     IF PS-END-OF-FILE                                            03060002
030700        DISPLAY '*                                            *'  03070002
030800        DISPLAY '* NO RDC-BULK ASN TO PROCESS - EMPTY FILE    *'  03080002
030900        DISPLAY '*                                            *'  03090002
031000     ELSE                                                         03100002
031001         SET  PS-SINGLE-SKU-SSCC                                  03100102
031002              PS-FIRST-RECORD     TO  TRUE                        03100202
031030         MOVE RC109-ASN-TRANSCRIPTION-RECORD                      03103002
031040           TO SV-ASN-TRANSCRIPTION-RECORD                         03104003
031100         PERFORM 2000-PROCESS-FILE                                03110002
031200             UNTIL PS-END-OF-FILE                                 03120002
031300                OR PS-ERROR                                       03130002
031400     END-IF.                                                      03140002
031500                                                                  03150002
031501     IF  PS-END-OF-FILE  AND                                      03150102
031502         PV-READ-COUNT   > 0  AND                                 03150222
031503         PS-FIRST-RECORD-NO                                       03150322
031504         IF  PS-SINGLE-SKU-SSCC                                   03150422
031505             PERFORM 8100-WRITE-OUT01                             03150503
031506             ADD  1  TO  PV-SINGLE-SKU-COUNT                      03150602
031508         ELSE                                                     03150802
031509             PERFORM 8200-WRITE-OUT02                             03150902
031510             ADD  1  TO  PV-MULTI-SKU-COUNT                       03151002
031514         END-IF                                                   03151402
031515     END-IF.                                                      03151502
031516                                                                  03151602
031517     CLOSE INPUT-FILE                                             03151702
031520           ASN-SINGLE-SKU-SSCC-FILE                               03152002
031530           ASN-MULTI-SKU-SSCC-FILE.                               03153002
031700                                                                  03170002
031800     DISPLAY ' '                                                  03180002
031900     MOVE PV-READ-COUNT           TO  PV-DISP-COUNT               03190002
032000     DISPLAY ' RECORDS READ:    '     PV-DISP-COUNT               03200002
032010     DISPLAY ' '                                                  03201002
032100                                                                  03210002
032510     MOVE PV-SINGLE-SKU-COUNT     TO  PV-DISP-COUNT               03251019
032520     DISPLAY ' SINGLE SKU COUNT:'     PV-DISP-COUNT               03252019
032530                                                                  03253019
032600     MOVE PV-MULTI-SKU-COUNT      TO PV-DISP-COUNT                03260002
032700     DISPLAY ' MULTI SKU COUNT: '    PV-DISP-COUNT                03270002
032800     DISPLAY ' '                                                  03280002
032900                                                                  03290002
032910     MOVE PV-OUT01-COUNT          TO PV-DISP-COUNT                03291002
032920     DISPLAY ' OUT01 COUNT:     '    PV-DISP-COUNT                03292002
032921     MOVE PV-BULK-ASN-NO-SSCC-CNT TO  PV-DISP-COUNT               03292120
032922     DISPLAY ' DC BULK-NO SSCC: '     PV-DISP-COUNT               03292220
032923                                                                  03292320
032930     DISPLAY ' '                                                  03293002
032940                                                                  03294002
032990     MOVE PV-OUT02-COUNT          TO PV-DISP-COUNT                03299002
032991     DISPLAY ' OUT02 COUNT:     '    PV-DISP-COUNT                03299102
032992     DISPLAY ' '                                                  03299202
032993                                                                  03299302
033000     DISPLAY ' RCKUT182 - END'.                                   03300002
033100     DISPLAY ' '.                                                 03310002
033200                                                                  03320002
033300     STOP RUN.                                                    03330002
033400                                                                  03340002
033500                                                                  03350002
033600*-----------------------------------------------------------------03360002
033700*  PROCESS ASN INPUT FILE                                         03370002
033800*-----------------------------------------------------------------03380002
033900 2000-PROCESS-FILE.                                               03390002
034000                                                                  03400002
034001     IF  PS-FIRST-RECORD                                          03400104
034002         SET PS-FIRST-RECORD-NO  TO  TRUE                         03400204
034003         MOVE RC109-ASN-TRANSCRIPTION-RECORD                      03400302
034004           TO SV-ASN-TRANSCRIPTION-RECORD                         03400403
034006     ELSE                                                         03400602
034007         IF  RC109-CARTON-NBR  =  SV-CARTON-NBR  AND              03400718
034008             RC109-CARTON-NBR NOT = 0                             03400818
034009                 SET  PS-MULTI-SKU-SSCC  TO  TRUE                 03400918
034010                 SET  SV-CRTN-UNIQUE-NO  TO  TRUE                 03401018
034011                 PERFORM 8200-WRITE-OUT02                         03401118
034012                 ADD  1  TO  PV-MULTI-SKU-COUNT                   03401218
034013                 MOVE RC109-ASN-TRANSCRIPTION-RECORD              03401318
034014                   TO SV-ASN-TRANSCRIPTION-RECORD                 03401418
034015         ELSE                                                     03401518
034025             IF  PS-SINGLE-SKU-SSCC                               03402518
034026                 PERFORM 8100-WRITE-OUT01                         03402603
034030                 ADD  1  TO  PV-SINGLE-SKU-COUNT                  03403020
034032                 MOVE RC109-ASN-TRANSCRIPTION-RECORD              03403202
034033                   TO SV-ASN-TRANSCRIPTION-RECORD                 03403303
034034             ELSE                                                 03403402
034035                 SET SV-CRTN-UNIQUE      TO  TRUE                 03403513
034036                 PERFORM 8200-WRITE-OUT02                         03403602
034037                 ADD  1  TO  PV-MULTI-SKU-COUNT                   03403702
034038                 MOVE RC109-ASN-TRANSCRIPTION-RECORD              03403802
034039                   TO SV-ASN-TRANSCRIPTION-RECORD                 03403903
034040                 SET PS-SINGLE-SKU-SSCC  TO  TRUE                 03404002
034041             END-IF                                               03404102
034042         END-IF                                                   03404202
034043     END-IF.                                                      03404302
034044                                                                  03404402
034050     PERFORM 8000-READ-INPUT.                                     03405002
035011                                                                  03501102
035020                                                                  03502002
052900*----------------------------------------------------------------*05290002
053000* READ INPUT ASN FILE                                            *05300002
053100*----------------------------------------------------------------*05310002
053200 8000-READ-INPUT.                                                 05320002
053300     READ  INPUT-FILE                                             05330002
053400           INTO  RC109-ASN-TRANSCRIPTION-RECORD                   05340002
053500           AT END                                                 05350002
053600                 SET PS-END-OF-FILE  TO  TRUE                     05360002
053700           NOT AT END                                             05370002
053800                 ADD 1  TO  PV-READ-COUNT                         05380002
053900     END-READ.                                                    05390002
055300                                                                  05530002
056400                                                                  05640002
056410*----------------------------------------------------------------*05641002
056420* OUTPUT FILE 1 - SINGLE SKU CARTONS                             *05642002
056430*----------------------------------------------------------------*05643002
056440 8100-WRITE-OUT01.                                                05644002
056450                                                                  05645002
056451     SET  SV-CRTN-UNIQUE  TO  TRUE.                               05645113
056452                                                                  05645206
056460     WRITE ASN-SINGLE-SKU-SSCC-RECORD                             05646002
056470           FROM  SV-ASN-TRANSCRIPTION-RECORD.                     05647003
056480                                                                  05648002
056490     ADD  1  TO  PV-OUT01-COUNT.                                  05649002
056491                                                                  05649120
056492     IF  SV-CARTON-NBR     = 0                                    05649220
056493         ADD  1  TO  PV-BULK-ASN-NO-SSCC-CNT                      05649320
056494     END-IF.                                                      05649420
056495                                                                  05649502
056496                                                                  05649602
056500*----------------------------------------------------------------*05650002
