030500*  INITIALIZATIONS                                               *02690000
030600******************************************************************02700000
030700 1000-INITIALIZE.                                                 02710000
030800                                                                  02720000
030900     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02730000
031000     DISPLAY DM-DISPLAY-MESSAGE.                                  02740000
031400                                                                  02750000
031500     OPEN INPUT  IN-DRIVER-FILE.                                  02760000
031500     OPEN OUTPUT OUT-ARCHIVE-FILE.                                02761000
031200     MOVE PC-BEGINNING            TO DP-STATUS-DESCRIPTION.       02850000
032500     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02860000
033100     INITIALIZE DCLTAUCNPO                                        02900001
033200                AH021-ARCHIVE-RECORD-LAYOUT.                      02910001
033800                                                                  02970000
031200 1500-READ-DRIVER-RECORD.                                         02972100
031400     READ IN-DRIVER-FILE INTO AH020-DRIVER-RECORD-LAYOUT          02972301
031600          AT END SET END-OF-DRIVER  TO TRUE.                      02972400
031700                                                                  02972500
031800     IF MORE-DRIVER                                               02972600
031900         ADD 1 TO PV-TAUCNTK-INPUT-COUNT                          02972701
029600                                                                  02973200
032600         SET PS-TABLE-LOOP-NOT-DONE TO TRUE                       02973300
032700         PERFORM 9100-OPEN-TABLE-CURSOR                           02973400
033000                                                                  02973500
029700         PERFORM 2000-GET-NEXT-FROM-CURSOR                        02973600
029800             UNTIL PS-TABLE-LOOP-DONE                             02973700
                                                                        02973800
032700         PERFORM 9130-CLOSE-TABLE-CURSOR                          02973900
032000     END-IF.                                                      02974000
                                                                        02975000
033900******************************************************************02980000
034000*  FETCH NEXT FROM CURSOR & WRITE OUTPUT DRIVER FILE RECORD.     *02990000
034100******************************************************************03000000
034200 2000-GET-NEXT-FROM-CURSOR.                                       03010000
034300                                                                  03020000
036900     PERFORM 9110-FETCH-TABLE-CURSOR.                             03021000
           IF SQLCODE = 0                                               03022000
034400         MOVE AUCNPO-CONTNR-ID  TO DISPLAY-CNTR-NBR               03030001
034410*        DISPLAY 'FETCHED CONTAINER = ' DISPLAY-CNTR-NBR          03040000
034610                                                                  03050000
036100         PERFORM 2010-POPULATE-ARCHIVE-RECORD                     03060000
036200         PERFORM 9650-WRITE-ARCHIVE-RECORD                        03070000
036300         PERFORM 2100-CHECK-PGM-PROGRESS                          03080000
036600     END-IF.                                                      03090000
037100******************************************************************03140000
