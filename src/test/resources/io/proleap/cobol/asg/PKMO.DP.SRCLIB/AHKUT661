024700*    DEFINE CURSOR FOR ACCESSING TAUCNPO                        * 01920001
024800*---------------------------------------------------------------* 01930000
024900     EXEC SQL                                                     01940000
025000         DECLARE TABLE-CURSOR CURSOR FOR                          01950000
025100           SELECT                                                 01960000
025200              CONTNR_ID                                           01970000
025300             ,CONTNR_SEQ_NBR                                      01980000
025400             ,PO_NBR                                              01990000
025500             ,CSTM_ENTR_ID                                        02000000
025600             ,MSTR_BOL_NBR                                        02010000
025700             ,HSE_BOL_NBR                                         02020000
025800             ,LTR_OF_CR_NBR                                       02030000
025900             ,PO_RCVD_DELY_TMST                                   02040000
026000             ,PO_C_O_PROD_CDE                                     02050000
026100             ,PO_CART_CNT                                         02060000
026200             ,PO_CBMT_QTY                                         02070000
026300             ,PO_WT_KGRM_QTY                                      02080000
026400             ,PO_UNT_QTY                                          02090000
026700             ,LAST_UPD_BY_USR_ID                                  02120000
026800             ,LAST_UPD_TMST                                       02130000
026500             ,AUD_FUNC_CDE                                        02131002
026600             ,AUD_TMST                                            02132002
026900             ,EXTL_REF_NBR                                        02140000
027000             ,BKR_FRGT_RLSE_CDE                                   02150000
027100             ,DO_RCVD_CDE                                         02160000
027200             ,CONTNR_PU_CDE                                       02170000
027300             ,ACTL_POE_DELY_CDE                                   02180000
027400             ,DVN_CDE                                             02190000
027500             ,DCONSLR_RET_CDE                                     02200000
027600             ,SPLT_XMIT_CDE                                       02210000
027700             ,DOC_RCVD_ORGN_TMST                                  02220000
027700             ,DCONSLR_DELY_CDE                                    02230000
027900             FROM TAUCNPO                                         02470001
028100            WHERE CONTNR_ID = :AH020-CONTNR-ID                    02480001
028100              AND CONTNR_SEQ_NBR = :AH020-CONTNR-SEQ-NBR          02481001
028500         FOR FETCH ONLY                                           02510000
028600         WITH UR                                                  02520000
028700         OPTIMIZE FOR 1 ROW                                       02530000
028800     END-EXEC.                                                    02540000
029000                                                                  02550000
029100 PROCEDURE DIVISION.                                              02560000
029200                                                                  02570000
029300 0000-PROGRAM-MAINLINE.                                           02580000
029400                                                                  02590000
029500     PERFORM 1000-INITIALIZE.                                     02600000
029600                                                                  02610000
029700     PERFORM 1500-READ-DRIVER-RECORD                              02620000
029800         UNTIL END-OF-DRIVER.                                     02630000
029900                                                                  02640000
030000     PERFORM 6000-EOJ-ROUTINE.                                    02650000
030100     STOP RUN.                                                    02660000
