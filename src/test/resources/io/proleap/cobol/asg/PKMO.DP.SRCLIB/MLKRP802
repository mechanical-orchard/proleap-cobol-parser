348700*   4170-SORT-DEPT-TOTALS.                                       *46020000
348800*      MANIPULATE THE RT-DEPT-TOTALS TABLE SO THAT IT WILL BE    *46030000
348900*      SORTED BY DEPT.                                           *46040000
349000*                                                                *46050000
349100*   PERFORM 4100-PRINT-REPORT-TOTAL                              *46060000
349200******************************************************************46070000
349300                                                                  46080000
349400     INITIALIZE PI-RT-SORT-IND.                                   46090000
349500                                                                  46100000
349600     PERFORM UNTIL PI-RT-IS-SORTED                                46110000
349700         INITIALIZE PA-SUB2                                       46120000
349800                    PA-SUB3                                       46130000
349900                    WS-RT-AREA1                                   46140000
350000                    WS-RT-AREA2                                   46150000
350100         INITIALIZE PI-SORTING-SW                                 46160000
350200                 REPLACING ALPHANUMERIC DATA BY 'N'               46170000
350300         PERFORM VARYING PA-SUB2 FROM 2 BY 1                      46180000
350400             UNTIL PA-SUB2 = PC-MAX-DEPTS                         46190000
350500               OR RT-DEPT-NMBR (PA-SUB3) = 0                      46200000
350600             COMPUTE PA-SUB3 = PA-SUB2 + 1                        46210000
350700             IF RT-DEPT-NMBR (PA-SUB3) = 0                        46220000
350800                 CONTINUE                                         46230000
350900             ELSE                                                 46240000
351000                 IF RT-DEPT-NMBR (PA-SUB2) > RT-DEPT-NMBR(PA-SUB3)46250000
351100                     MOVE RT-DATA (PA-SUB2) TO WS-RT-AREA1        46260000
351200                     MOVE RT-DATA (PA-SUB3) TO WS-RT-AREA2        46270000
351300                     MOVE WS-RT-AREA1       TO  RT-DATA (PA-SUB3) 46280000
351400                     MOVE WS-RT-AREA2       TO  RT-DATA (PA-SUB2) 46290000
351500                     SET PI-SORTING-MOVED TO TRUE                 46300000
351600                 END-IF                                           46310000
351700            END-IF                                                46320000
351800         END-PERFORM                                              46330000
351900         IF PI-SORTING-NOT-MOVED                                  46340000
352000             SET PI-RT-IS-SORTED TO TRUE                          46350000
352100         END-IF                                                   46360000
352200     END-PERFORM.                                                 46370000
