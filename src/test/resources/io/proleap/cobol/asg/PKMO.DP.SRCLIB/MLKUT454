      ******************************************************************01250000
      *  DB2 TMNMCLS CURSOR                                             01260000
      ******************************************************************01270000
           EXEC SQL                                                     01280000
W31475         DECLARE TMNMCLS_CURSOR CURSOR                            01290004
W31475         WITH ROWSET POSITIONING FOR                              01291004
               SELECT DISTINCT DEPT_NBR                                 01300000
                              ,MAJ_CL_NBR                               01310000
                              ,STR_NBR                                  01311000
               FROM TMNMCLS                                             01320000
                   ,TMLCNTL                                             01331000
               WHERE (FSCL_MN_END_DTE BETWEEN                           01340000
                                   OPN_FSCL_MN_DTE                      01350000
                               AND MXPSTG_FSCL_MN_DTE) OR               01360000
                    ((FSCL_MN_END_DTE BETWEEN                           01361000
                      :DTATTR-PFM-END-DTE AND                           01362000
                      MXPSTG_FSCL_MN_DTE) AND                           01363000
                       (END_INV_RTL_AMT  <> 0  OR                       01363100
                        END_INV_COST_AMT <> 0))                         01364000
           END-EXEC.                                                    01380000
                                                                        01390000
       EJECT                                                            01400000
                                                                        01410000
      ******************************************************************01420000
       PROCEDURE DIVISION.                                              01430000
