      ******************************************************************02220000
       Z210-OPEN-CURSOR-TMNMCLS.                                        02230000
      ******************************************************************02240000
      *                                                                 02250000
           EXEC SQL                                                     02260000
               OPEN TMNMCLS_CURSOR                                      02270000
           END-EXEC.                                                    02280000
                                                                        02290000
           EVALUATE TRUE                                                02300000
               WHEN SQLCODE < 0                                         02310000
                   MOVE 'Z210-OPEN-CURSOR-TMNMCLS'                      02320000
                                       TO AA-PARAGRAPH-NAME             02330000
                   MOVE 'TMNMCLS TABLE ERROR'                           02340000
                                       TO AA-MESSAGE                    02350000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02360000
                   PERFORM Z998-SQL-ERROR                               02370000
               WHEN SQLCODE > 0                                         02380000
               WHEN SQLWARN0 NOT = SPACES                               02390000
                   MOVE 'Z210-OPEN-CURSOR-TMNMCLS'                      02400000
                                       TO AA-PARAGRAPH-NAME             02410000
                   MOVE 'TMNMCLS TABLE WARNING'                         02420000
                                       TO AA-MESSAGE                    02430000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02440000
                   PERFORM Z998-SQL-ERROR                               02450000
           END-EVALUATE.                                                02460000
                                                                        02470000
       EJECT                                                            02480000
                                                                        02490000
      ******************************************************************02500000
       Z220-FETCH-TMNMCLS.                                              02510000
      ******************************************************************02520000
      *                                                                 02530000
W31475*    EXEC SQL                                                     02540004
  |   *        FETCH TMNMCLS_CURSOR                                     02550004
  |   *         INTO :MNMCLS-DEPT-NBR                                   02560004
  |   *             ,:MNMCLS-MAJ-CL-NBR                                 02570004
  |   *             ,:MNMCLS-STR-NBR                                    02580004
  |   *    END-EXEC.                                                    02590004
  |        EXEC SQL                                                     02591004
  |            FETCH NEXT ROWSET FROM TMNMCLS_CURSOR FOR 100 ROWS       02592004
  |             INTO :HV-MNMCLS-DEPT-NBR                                02593004
  |                 ,:HV-MNMCLS-MAJ-CL-NBR                              02594004
  |                 ,:HV-MNMCLS-STR-NBR                                 02595004
W31475     END-EXEC.                                                    02596004
                                                                        02600000
           EVALUATE TRUE                                                02610000
               WHEN SQLCODE = 0                                         02620000
W31475*            ADD +1              TO PA-TMNMCLS-ROWS-FETCHED       02630004
W31475             ADD +100            TO PA-TMNMCLS-ROWS-FETCHED       02631004
               WHEN SQLCODE = +100                                      02640000
                   MOVE 'Y'            TO PS-TMNMCLS-DONE               02640103
W31475             ADD SQLERRD (3)     TO PA-TMNMCLS-ROWS-FETCHED       02641004
  |                PERFORM VARYING ROW-ID FROM 1 BY 1                   02642004
  |                UNTIL ROW-ID > SQLERRD (3)                           02643004
  |                     PERFORM C100-WRITE-EXTRACT                      02644004
W31475             END-PERFORM                                          02645004
               WHEN SQLCODE < 0                                         02660000
                   MOVE 'Z220-FETCH-TMNMCLS'                            02670000
                                       TO AA-PARAGRAPH-NAME             02680000
                   MOVE 'TMNMCLS TABLE ERROR'                           02690000
                                       TO AA-MESSAGE                    02700000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02710000
                   PERFORM Z998-SQL-ERROR                               02720000
               WHEN ((SQLCODE > 0) AND (SQLCODE NOT = +100))            02730000
               WHEN SQLWARN0 NOT = SPACES                               02740000
                   MOVE 'Z220-FETCH-TMNMCLS'                            02750000
                                       TO AA-PARAGRAPH-NAME             02760000
                   MOVE 'TMNMCLS TABLE WARNING'                         02770000
                                       TO AA-MESSAGE                    02780000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02790000
                   PERFORM Z998-SQL-ERROR                               02800000
           END-EVALUATE.                                                02810000
                                                                        02820000
       EJECT                                                            02830000
                                                                        02840000
      ******************************************************************02850000
       Z230-CLOSE-CURSOR-TMNMCLS.                                       02860000
      ******************************************************************02870000
      *                                                                 02880000
           EXEC SQL                                                     02890000
               CLOSE TMNMCLS_CURSOR                                     02900000
           END-EXEC.                                                    02910000
                                                                        02920000
           EVALUATE TRUE                                                02930000
               WHEN SQLCODE < 0                                         02940000
                   MOVE 'Z230-CLOSE-CURSOR-TMNMCLS'                     02950000
                                       TO AA-PARAGRAPH-NAME             02960000
                   MOVE 'TMNMCLS TABLE ERROR'                           02970000
                                       TO AA-MESSAGE                    02980000
                   MOVE 'CLOSE CURSOR' TO AA-DB2-OPERATION              02990000
                   PERFORM Z998-SQL-ERROR                               03000000
               WHEN SQLCODE > 0                                         03010000
               WHEN SQLWARN0 NOT = SPACES                               03020000
                   MOVE 'Z230-CLOSE-CURSOR-TMNMCLS'                     03030000
                                       TO AA-PARAGRAPH-NAME             03040000
                   MOVE 'TMNMCLS TABLE WARNING'                         03050000
                                       TO AA-MESSAGE                    03060000
                   MOVE 'CLOSE CURSOR' TO AA-DB2-OPERATION              03070000
                   PERFORM Z998-SQL-ERROR                               03080000
           END-EVALUATE.                                                03090000
                                                                        03100000
       EJECT                                                            03110000
                                                                        03120000
       7100-SELECT-DB2-DATES.                                           03120100
      *                                                                 03120200
           EXEC SQL                                                     03120300
               SELECT  PFM_END_DTE                                      03120500
                INTO  :DTATTR-PFM-END-DTE                               03121100
                FROM TDTATTR                                            03121600
                    ,TMLCNTL                                            03121700
                WHERE KY_DTE  = OPN_FSCL_MN_DTE                         03122000
           END-EXEC.                                                    03122100
      *                                                                 03122200
           EVALUATE SQLCODE                                             03122300
               WHEN 0                                                   03122400
                   DISPLAY 'DTATTR-PFM-END-DTE: '                       03122500
                            DTATTR-PFM-END-DTE                          03122600
      *        WHEN +100                                                03123900
      *            DISPLAY 'NO ROWS QUALIFY FOR RECALCULATION'          03124000
               WHEN OTHER                                               03124100
                  MOVE '7100-SELECT-DB2-DATES'                          03124200
                                       TO AA-PARAGRAPH-NAME             03124300
                   PERFORM Z998-SQL-ERROR                               03124500
           END-EVALUATE.                                                03124600
      *                                                                 03124700
      *                                                                 03125000
      ******************************************************************03130000
       Z998-SQL-ERROR.                                                  03140000
      ******************************************************************03150000
      *                                                                 03160000
                                                                        03170000
           CLOSE MNMCLS-EXTR-FILE.                                      03180000
                                                                        03190000
           DISPLAY AA-ABEND-LIT.                                        03200000
           DISPLAY AA-DB2-ERROR-LIT.                                    03210000
           DISPLAY AA-PROGRAM-LIT.                                      03220000
           DISPLAY AA-PARAGRAPH-LIT.                                    03230000
           DISPLAY AA-MESSAGE-LINE.                                     03240000
           DISPLAY AA-DB2-OPERATION-LIT.                                03250000
           SET AC-DB2-ERROR TO TRUE.                                    03260000
           MOVE +4013                  TO ABEND-CODE.                   03270000
                                                                        03280000
           COPY DPPD004.                                                03290000
           CALL 'ILBOABN0' USING ABEND-CODE.                            03300000
