423600             IF (PURCHS-PKGNG-CDE NOT = 'B')                      42360099
423700               AND (XL023-LO-DC-ALLC-MULT-QTY-DIFF = '*')         42370099
423800                 INITIALIZE DETAIL-LINE-1                         42380099
423900                 SUBTRACT 1 FROM  PV-LINE-COUNT                   42390099
424000                 SET PS-DIDNT-EXCEED-LIMIT TO TRUE                42400099
424100             ELSE                                                 42410099
