020500                                                                  02050024
020600 PROCEDURE DIVISION.                                              02060024
020700 A100-MAIN.                                                       02070024
020800                                                                  02080024
020900     PERFORM B100-INITIALIZE.                                     02090024
021000     PERFORM B200-FORMAT-OUTPUT UNTIL IN-EOF.                     02100024
021100     PERFORM B300-END.                                            02110024
021200     GOBACK.                                                      02120024
021300 EJECT                                                            02130024
021400*----------------------------------------------------------------*02140024
021500*    INITIALIZATION PROCESSING                                   *02150024
021600*----------------------------------------------------------------*02160024
021700 B100-INITIALIZE.                                                 02170024
021800                                                                  02180024
021900     OPEN INPUT  AJ-EXTRACT-FILE                                  02190025
022000          OUTPUT REPORT-FILE.                                     02200024
022100                                                                  02210024
022200     ACCEPT PV-CURRENT-DATE FROM DATE.                            02220024
022300     MOVE PV-CURRENT-MONTH            TO DP132O-RUN-MONTH.        02230024
022400     MOVE PV-CURRENT-DAY              TO DP132O-RUN-DAY.          02240024
022500     MOVE PV-CURRENT-YEAR             TO DP132O-RUN-YEAR.         02250024
022600                                                                  02260024
022700     ACCEPT PV-CURRENT-TIME FROM TIME.                            02270024
022800     MOVE PV-CURRENT-HOUR             TO DP132O-RUN-HOUR.         02280024
022900     MOVE PV-CURRENT-MINUTE           TO DP132O-RUN-MINUTE.       02290024
023000     MOVE PV-PROGRAM-NAME             TO DP132O-PROGRAM-NAME.     02300024
023100     MOVE 1                           TO DP132O-REPORT-NUMBER.    02310024
023200                                                                  02320024
023300     PERFORM R100-READ-INPUT.                                     02330024
023400                                                                  02340024
023500     IF IN-EOF                                                    02350024
023600        PERFORM W175-WRITE-NO-DATA-LINE.                          02360024
023700                                                                  02370024
023800 B200-FORMAT-OUTPUT.                                              02380024
023900                                                                  02390024
023910     MOVE AP046-DOC-NBR             TO PV-AP046-DOCNUM-X.         02391025
023920     IF PV-AP046-DOCNUM NUMERIC     AND                           02392025
023930        PV-AP046-DOCNUM-X(1:1) NOT = '1'                          02393025
024200        PERFORM B250-CHECK-FOR-SEQUENCE-GAPS.                     02420024
024300                                                                  02430024
024400     MOVE AP046-DOC-NBR             TO DL-DOCUMENT-NBR.           02440025
024500     MOVE AP046-AP-TRN-CDE          TO DL-TRAN-CODE.              02450025
024600     MOVE AP046-ENT-NME-DESC        TO DL-VENDOR-NAME.            02460025
024700     MOVE AP046-DEPT-NBR            TO DL-DEPT-NBR.               02470025
024800     MOVE AP046-GRO-COST-AMT        TO DL-COST.                   02480025
024900     MOVE AP046-RETL-AMT            TO DL-RETAIL.                 02490025
025000                                                                  02500024
025100     IF AP046-ENTR-DTE = SPACES                                   02510025
025200         MOVE 0                     TO PV-GREG-DATE-MM            02520024
025300         MOVE 0                     TO PV-GREG-DATE-DD            02530024
025400         MOVE 0                     TO PV-GREG-DATE-YY            02540024
025500     ELSE                                                         02550024
025510         MOVE AP046-ENTR-MM         TO PV-GREG-DATE-MM            02551025
025520         MOVE AP046-ENTR-DD         TO PV-GREG-DATE-DD            02552025
025530         MOVE AP046-ENTR-YY         TO PV-GREG-DATE-YY            02553025
025800     END-IF.                                                      02580024
025900     MOVE PV-GREG-DATE              TO DL-ENTERED-DATE.           02590024
026000                                                                  02600024
026100     PERFORM W100-WRITE-OUTPUT.                                   02610024
026200                                                                  02620024
026300     PERFORM R100-READ-INPUT.                                     02630024
026400                                                                  02640024
026500 B250-CHECK-FOR-SEQUENCE-GAPS.                                    02650024
026600                                                                  02660024
026900     IF (PV-SAVE-DOCNUM = 0)                       OR             02690025
027000        (PV-AP046-DOCNUM = PV-SAVE-DOCNUM)         OR             02700025
027100        (PV-AP046-DOCNUM = (PV-SAVE-DOCNUM + 1))                  02710025
027200        CONTINUE                                                  02720024
027300     ELSE                                                         02730024
027400        COMPUTE PV-RANGE-START                                    02740024
027500           =    PV-SAVE-DOCNUM                                    02750024
027600           +    1                                                 02760024
027700        COMPUTE PV-RANGE-END                                      02770024
027800           =    PV-AP046-DOCNUM                                   02780025
027900           -    1                                                 02790024
028000        IF PV-RANGE-START = PV-RANGE-END                          02800024
028100           MOVE PV-RANGE-START(1:10) TO DL-MISS-1-DOCNUM          02810024
028200           PERFORM W125-WRITE-MISSING-1                           02820024
028300        ELSE                                                      02830024
028400           MOVE PV-RANGE-START(1:10) TO DL-MISS-RANGE-BEG         02840024
028500           MOVE PV-RANGE-END(1:10)   TO DL-MISS-RANGE-END         02850024
028600           PERFORM W150-WRITE-MISSING-RANGE                       02860024
028700        END-IF                                                    02870024
028800     END-IF.                                                      02880024
028900                                                                  02890024
029000     MOVE AP046-DOC-NBR         TO PV-SAVE-DOCNUM-X.              02900025
029100                                                                  02910024
029200 B300-END.                                                        02920024
029300                                                                  02930024
029400     CLOSE AJ-EXTRACT-FILE                                        02940025
029500           REPORT-FILE.                                           02950024
029600                                                                  02960024
032200 EJECT                                                            03220024
032300 R100-READ-INPUT.                                                 03230024
032400                                                                  03240024
032500     READ AJ-EXTRACT-FILE INTO AP046-EXTRACT-FILE                 03250025
032600         AT END SET IN-EOF TO TRUE.                               03260024
032700                                                                  03270024
032800 EJECT                                                            03280024
032900 W100-WRITE-OUTPUT.                                               03290024
033000                                                                  03300024
033100     IF PV-LINE-COUNT > PV-MAX-LINES                              03310024
033200         PERFORM W200-PRINT-HEADERS                               03320024
033300     END-IF.                                                      03330024
033400                                                                  03340024
033500     WRITE REPORT-RECORD FROM DL-DETAIL-LINE AFTER 1.             03350024
033600     INITIALIZE DL-DETAIL-LINE.                                   03360024
033700                                                                  03370024
033800     ADD 1  TO PV-LINE-COUNT.                                     03380024
033900                                                                  03390024
034000 W125-WRITE-MISSING-1.                                            03400024
034100                                                                  03410024
034200     IF (PV-LINE-COUNT + 3) > PV-MAX-LINES                        03420024
034300         PERFORM W200-PRINT-HEADERS                               03430024
034400         WRITE REPORT-RECORD FROM DL-MISSING-1-LINE AFTER 1       03440024
034500         MOVE SPACES TO REPORT-RECORD                             03450024
034600         WRITE REPORT-RECORD                        AFTER 1       03460024
034700         ADD 2 TO PV-LINE-COUNT                                   03470024
034800     ELSE                                                         03480024
034900         MOVE SPACES TO REPORT-RECORD                             03490024
035000         WRITE REPORT-RECORD                        AFTER 1       03500024
035100         WRITE REPORT-RECORD FROM DL-MISSING-1-LINE AFTER 1       03510024
035200         MOVE SPACES TO REPORT-RECORD                             03520024
035300         WRITE REPORT-RECORD                        AFTER 1       03530024
035400         ADD 3 TO PV-LINE-COUNT                                   03540024
035500     END-IF.                                                      03550024
035600                                                                  03560024
035700     INITIALIZE DL-MISSING-1-LINE.                                03570024
035800                                                                  03580024
035900 W150-WRITE-MISSING-RANGE.                                        03590024
036000                                                                  03600024
036100     IF (PV-LINE-COUNT + 3) > PV-MAX-LINES                        03610024
036200         PERFORM W200-PRINT-HEADERS                               03620024
036300         WRITE REPORT-RECORD FROM DL-MISSING-RANGE-LINE AFTER 1   03630024
036400         MOVE SPACES TO REPORT-RECORD                             03640024
036500         WRITE REPORT-RECORD                            AFTER 1   03650024
036600         ADD 2 TO PV-LINE-COUNT                                   03660024
036700     ELSE                                                         03670024
036800         MOVE SPACES TO REPORT-RECORD                             03680024
036900         WRITE REPORT-RECORD                            AFTER 1   03690024
037000         WRITE REPORT-RECORD FROM DL-MISSING-RANGE-LINE AFTER 1   03700024
037100         MOVE SPACES TO REPORT-RECORD                             03710024
037200         WRITE REPORT-RECORD                            AFTER 1   03720024
037300         ADD 3 TO PV-LINE-COUNT                                   03730024
037400     END-IF.                                                      03740024
037500                                                                  03750024
037600     INITIALIZE DL-MISSING-RANGE-LINE.                            03760024
037700                                                                  03770024
037800 W175-WRITE-NO-DATA-LINE.                                         03780024
037900                                                                  03790024
038000     PERFORM W200-PRINT-HEADERS.                                  03800024
038100                                                                  03810024
038200     WRITE REPORT-RECORD FROM DL-NO-DATA-LINE AFTER 1.            03820024
038300                                                                  03830024
038400 W200-PRINT-HEADERS.                                              03840024
038500                                                                  03850024
038600     ADD 1                            TO PV-PAGE-COUNT.           03860024
038700     MOVE PV-PAGE-COUNT               TO DP132O-PAGE-NUMBER.      03870024
038800                                                                  03880024
038900     WRITE REPORT-RECORD FROM                                     03890024
039000                   DP132O-STANDRD-HEADER-132-COLS AFTER PAGE.     03900024
039100     WRITE REPORT-RECORD FROM H2-HEADER2 AFTER 1.                 03910024
039200                                                                  03920024
039300     WRITE REPORT-RECORD FROM DH-DETAIL-HEADER1 AFTER 2.          03930024
039400     WRITE REPORT-RECORD FROM DH-DETAIL-HEADER2 AFTER 1.          03940024
039500                                                                  03950024
039600     MOVE SPACES TO REPORT-RECORD.                                03960024
039700     WRITE REPORT-RECORD AFTER 1.                                 03970024
039800     MOVE 6  TO PV-LINE-COUNT.                                    03980024
039900                                                                  03990024
040000 EJECT                                                            04000024
040100*----------------------------------------------------------------*04010024
040200*    ABEND ROUTINE                                               *04020024
040300*----------------------------------------------------------------*04030024
040400*Z999-ABEND.                                                      04040027
