RONNA ******************************************************************        
RONNA *  TPPCPRC TABLE LOOKUP.                                                  
RONNA *****************************************************************         
RONNA      EXEC SQL                                                             
RONNA           DECLARE TPPCPRC-CSR CURSOR FOR                                  
RONNA              SELECT RTL_AMT                                               
RONNA                    ,MEITGP_NBR                                            
RONNA                    ,STATUS_CDE                                            
RONNA              FROM TPPCPRC                                                 
RONNA              WHERE TSK_ITM_NBR = :PPCPRC-TSK-ITM-NBR                      
RONNA                AND STR_NBR = :PPCPRC-STR-NBR                              
RONNA                AND (:PPCPRC-STRT-TMST >= STRT_TMST AND                    
RONNA                     :PPCPRC-END-TMST <= END_TMST)                         
RONNA              ORDER BY END_TMST DESC                                       
RONNA              FOR FETCH ONLY                                               
RONNA              WITH UR                                                      
RONNA      END-EXEC.                                                            
RONNA                                                                           
RONNA ******************************************************************        
RONNA *  INT_SKU_STYL_DLT TABLE LOOKUP - SKU                                    
RONNA *****************************************************************         
RONNA      EXEC SQL                                                             
RONNA         DECLARE SKUSTYL-SKU-CSR CURSOR FOR                                
RONNA            SELECT UPC_ID                                                  
RONNA                  ,LOC_NBR                                                 
RONNA                  ,EFF_BEG_TMST                                            
RONNA                  ,UPC_NBR                                                 
RONNA                  ,EFF_END_TMST                                            
RONNA                  ,UPC_STAT_CDE                                            
RONNA                  ,UNT_COST_AMT                                            
RONNA                  ,UNT_RTL_AMT                                             
RONNA                  ,MEITGP_NBR                                              
RONNA                  ,ITM_NBR                                                 
RONNA                  ,INTR_UPC_ID                                             
RONNA                  ,UPC_STAT_CHG_DTE                                        
RONNA                  ,UNT_RTL_CHG_DTE                                         
RONNA                  ,ORIG_CLR_DTE                                            
RONNA                  ,MKDN_CDE                                                
RONNA                  ,SUB_CL_MDSEAR_ID                                        
RONNA                  ,VS_ID                                                   
RONNA                  ,DEPT_NBR                                                
RONNA                  ,MAJ_CL_NBR                                              
RONNA                  ,SUB_CL_NBR                                              
RONNA                  ,SKU_NBR                                                 
RONNA                  ,ENT_ID                                                  
RONNA                  ,PLU_DWNLD_IND                                           
RONNA             FROM INT_SKU_STYL_DLT                                         
RONNA             WHERE SKU_NBR = :INT00003-SKU-NBR                             
RONNA               AND LOC_NBR = :INT00003-LOC-NBR                             
RONNA               AND EFF_BEG_TMST <= :INT00003-EFF-BEG-TMST                  
RONNA               AND (EFF_END_TMST >= :INT00003-EFF-END-TMST                 
RONNA                OR  EFF_END_TMST IS NULL)                                  
RONNA             ORDER BY EFF_END_TMST DESC                                    
RONNA             WITH UR                                                       
RONNA      END-EXEC.                                                            
RONNA                                                                           
RONNA ******************************************************************        
RONNA *  INT_SKU_STYL_DLT TABLE LOOKUP - UPC_NBR                                
RONNA *****************************************************************         
RONNA      EXEC SQL                                                             
RONNA         DECLARE SKUSTYL-UPC-CSR CURSOR FOR                                
RONNA            SELECT UPC_ID                                                  
RONNA                  ,LOC_NBR                                                 
RONNA                  ,EFF_BEG_TMST                                            
RONNA                  ,UPC_NBR                                                 
RONNA                  ,EFF_END_TMST                                            
RONNA                  ,UPC_STAT_CDE                                            
RONNA                  ,UNT_COST_AMT                                            
RONNA                  ,UNT_RTL_AMT                                             
RONNA                  ,MEITGP_NBR                                              
RONNA                  ,ITM_NBR                                                 
RONNA                  ,INTR_UPC_ID                                             
RONNA                  ,UPC_STAT_CHG_DTE                                        
RONNA                  ,UNT_RTL_CHG_DTE                                         
RONNA                  ,ORIG_CLR_DTE                                            
RONNA                  ,MKDN_CDE                                                
RONNA                  ,SUB_CL_MDSEAR_ID                                        
RONNA                  ,VS_ID                                                   
RONNA                  ,DEPT_NBR                                                
RONNA                  ,MAJ_CL_NBR                                              
RONNA                  ,SUB_CL_NBR                                              
RONNA                  ,SKU_NBR                                                 
RONNA                  ,ENT_ID                                                  
RONNA                  ,PLU_DWNLD_IND                                           
RONNA             FROM INT_SKU_STYL_DLT                                         
RONNA             WHERE UPC_NBR = :INT00003-UPC-NBR                             
RONNA               AND LOC_NBR = :INT00003-LOC-NBR                             
RONNA               AND EFF_BEG_TMST <= :INT00003-EFF-BEG-TMST                  
RONNA               AND (EFF_END_TMST >= :INT00003-EFF-END-TMST                 
RONNA                OR  EFF_END_TMST IS NULL)                                  
RONNA             ORDER BY EFF_END_TMST DESC                                    
RONNA             WITH UR                                                       
RONNA      END-EXEC.                                                            
RONNA                                                                           
RONNA ******************************************************************        
RONNA *  INT_SKU_STYL_DLT TABLE LOOKUP - INTR_UPC_ID                            
RONNA *****************************************************************         
RONNA      EXEC SQL                                                             
RONNA         DECLARE SKUSTYL-IUPC-CSR CURSOR FOR                               
RONNA            SELECT UPC_ID                                                  
RONNA                  ,LOC_NBR                                                 
RONNA                  ,EFF_BEG_TMST                                            
RONNA                  ,UPC_NBR                                                 
RONNA                  ,EFF_END_TMST                                            
RONNA                  ,UPC_STAT_CDE                                            
RONNA                  ,UNT_COST_AMT                                            
RONNA                  ,UNT_RTL_AMT                                             
RONNA                  ,MEITGP_NBR                                              
RONNA                  ,ITM_NBR                                                 
RONNA                  ,INTR_UPC_ID                                             
RONNA                  ,UPC_STAT_CHG_DTE                                        
RONNA                  ,UNT_RTL_CHG_DTE                                         
RONNA                  ,ORIG_CLR_DTE                                            
RONNA                  ,MKDN_CDE                                                
RONNA                  ,SUB_CL_MDSEAR_ID                                        
RONNA                  ,VS_ID                                                   
RONNA                  ,DEPT_NBR                                                
RONNA                  ,MAJ_CL_NBR                                              
RONNA                  ,SUB_CL_NBR                                              
RONNA                  ,SKU_NBR                                                 
RONNA                  ,ENT_ID                                                  
RONNA                  ,PLU_DWNLD_IND                                           
RONNA             FROM INT_SKU_STYL_DLT                                         
RONNA             WHERE INTR_UPC_ID = :INT00003-INTR-UPC-ID                     
RONNA               AND LOC_NBR = :INT00003-LOC-NBR                             
RONNA               AND EFF_BEG_TMST <= :INT00003-EFF-BEG-TMST                  
RONNA               AND (EFF_END_TMST >= :INT00003-EFF-END-TMST                 
RONNA                OR  EFF_END_TMST IS NULL)                                  
RONNA             ORDER BY EFF_END_TMST DESC                                    
RONNA             WITH UR                                                       
RONNA      END-EXEC.                                                            
