RONNA            SELECT  DEPT_BRND_VND_ID                                       
RONNA                   ,ENT_ID                                                 
RONNA            FROM IMT_DEPT_BRND_VND                                         
RONNA            WHERE LBL_SEQ_NBR   = :BV003-PV-WORK-BRAND                     
RONNA              AND EFF_BEG_DTE  <= :BV003-PV-WORK-DATE                      
RONNA              AND (EFF_END_DTE IS NULL OR                                  
RONNA                   EFF_END_DTE >= :BV003-PV-WORK-DATE)                     
RONNA              AND DEPT_NBR      = :BV003-PV-WORK-DEPT                      
RONNA            WITH UR                                                        
RONNA            FOR FETCH ONLY                                                 
RONNA      END-EXEC.                                                            
