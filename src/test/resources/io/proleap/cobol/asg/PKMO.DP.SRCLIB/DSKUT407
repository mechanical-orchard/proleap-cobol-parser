007000***************************************************************** 00700009
007100*     FOR EACH TABLE, INSERT DATA INTO THE TABLES                 00710009
007200***************************************************************** 00720009
007300*                                                                 00730009
007400     READ TBLE-FILE                                               00740009
007500         AT END SET TABLE-EOF TO TRUE.                            00750009
007600     IF NOT-TABLE-EOF                                             00760009
007700        MOVE TBLE-DATA-SIZE TO DS009-TBLE-DATA-SIZE               00770009
007800        MOVE TBLE-NAME  TO DS009-PROCESS-TABLE                    00780009
007900        MOVE TBLE-KEYS  TO DS009-PKYS                             00790009
008000        PERFORM DS002-DO-PROCESS                                  00800009
008100        IF DS009-DO-PROCESS                                       00810009
008200*       DISPLAY '**********************************'              00820009
008300*       DISPLAY DS009-PROCESS-TABLE                               00830009
008400        EVALUATE TRUE                                             00840009
008500         WHEN DS009-PROCESS-TABLE = 'TSHPTPO'                     00850009
008600             MOVE TBLE-DATA-ITEM TO WS-SHPTPO                     00860009
008700             MOVE WS-SHPTPO-PO-NBR TO HOLD-PO-NBR                 00870009
008800*            DISPLAY HOLD-PO-NBR                                  00880009
008900*            DISPLAY WS-SHPTPO                                    00890009
009000             PERFORM R100-INSERT                                  00900009
009100         WHEN DS009-PROCESS-TABLE = 'TPKSLIP'                     00910009
009200             MOVE TBLE-DATA-ITEM TO WS-PKSLIP                     00920009
009300             MOVE WS-PKSLIP-PO-NBR TO HOLD-PO-NBR                 00930009
009400*            DISPLAY HOLD-PO-NBR                                  00940009
009500*            DISPLAY WS-PKSLIP                                    00950009
009600             PERFORM R100-INSERT                                  00960009
009700         WHEN DS009-PROCESS-TABLE = 'TPKITEM'                     00970009
009800             MOVE TBLE-DATA-ITEM TO WS-PKITEM                     00980009
009900*            MOVE WS-PKITEM-PO-NBR TO HOLD-PO-NBR                 00990009
010000*            DISPLAY HOLD-PO-NBR                                  01000009
010100*            DISPLAY WS-PKITEM                                    01010009
010200             PERFORM R100-INSERT                                  01020009
010300         WHEN DS009-PROCESS-TABLE = 'TRECEIV'                     01030009
010400             MOVE TBLE-DATA-ITEM TO WS-RECEIV                     01040009
010500*            DISPLAY 'TBLE DATA ITEM' TBLE-DATA-ITEM              01050009
010600             MOVE WS-RECEIV-ORD-NBR TO HOLD-PO-NBR                01060009
010700*            DISPLAY HOLD-PO-NBR                                  01070009
010800*            DISPLAY WS-RECEIV                                    01080009
010900             PERFORM R100-INSERT                                  01090009
011000         WHEN DS009-PROCESS-TABLE = 'TRECITM'                     01100009
011100             MOVE TBLE-DATA-ITEM TO WS-RECITM                     01110009
011200             MOVE WS-RECITM-ORD-NBR TO HOLD-PO-NBR                01120009
011300*            DISPLAY HOLD-PO-NBR                                  01130009
