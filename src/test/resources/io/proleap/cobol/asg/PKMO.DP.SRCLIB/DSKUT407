022600*            DISPLAY HOLD-TRIP-ID                                 02260009
022700*            DISPLAY WS-LBPTDA                                    02270009
022800             PERFORM R100-INSERT                                  02280009
022900         END-EVALUATE                                             02290009
023000                                                                  02300009
023100         IF SQLCODE = ZERO                                        02310009
023200             SET DS009-SRCH-IDX TO +1                             02320009
023300             SEARCH DS009-CNTL-RECORDS                            02330009
023400                 VARYING DS009-SRCH-IDX                           02340009
023500                 AT END                                           02350009
023600                     CONTINUE                                     02360009
023700                 WHEN CNTL-TABLE (DS009-SRCH-IDX) =               02370009
023800                          DS009-PROCESS-TABLE                     02380009
023900*                    ADD SQLERRD(3)                               02390009
024000                     ADD +1                                       02400009
024100                        TO DS009-READ-COUNT (DS009-SRCH-IDX)      02410009
024200             END-SEARCH                                           02420009
024300         END-IF                                                   02430009
024400                                                                  02440009
024500                                                                  02450009
024600*        DISPLAY 'RETURN SQLCODE ON INSERT:' SQLCODE              02460009
024700*        DISPLAY 'NUMBER OF ROWS INSERTED:' SQLERRD (3)           02470009
024800         IF DS009-UPDATE-COUNT > DS009-COMMIT-COUNT               02480009
024900            EXEC SQL                                              02490009
025000                COMMIT                                            02500009
025100            END-EXEC                                              02510009
025200            IF SQLCODE NOT = ZERO                                 02520009
025300               PERFORM DS002-SQL-ABEND-COMMIT                     02530009
025400            END-IF                                                02540009
025500            MOVE +0  TO DS009-UPDATE-COUNT                        02550009
025600*           DISPLAY 'COMMIT TAKEN AFTER RECORD: '                 02560009
025700*                                     DS009-TUPDATE-COUNT         02570009
025800         END-IF                                                   02580009
025900         ADD SQLERRD(3) TO DS009-UPDATE-COUNT                     02590009
026000         ADD SQLERRD(3) TO DS009-TUPDATE-COUNT                    02600009
026100        END-IF                                                    02610009
026200     END-IF.                                                      02620009
026300                                                                  02630009
026400 EJECT                                                            02640009
