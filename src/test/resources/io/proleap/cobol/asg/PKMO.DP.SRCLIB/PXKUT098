           COPY PXRD124.                                                01830004
                                                                        01840004
      ****YELLOW TICKET EVENT EXTRACT OUTPUT RECORD LAYOUT              01850004
           COPY PXRD050.                                                01860004
                                                                        01870004
      ****TRACKING TABLE UPDATE OUTPUT RECORD LAYOUT                    01880004
           COPY PXRD126.                                                01890004
                                                                        01900004
      ****COUNT FILE OUTPUT RECORD LAYOUT                               01910004
           COPY PXRD045.                                                01920004
