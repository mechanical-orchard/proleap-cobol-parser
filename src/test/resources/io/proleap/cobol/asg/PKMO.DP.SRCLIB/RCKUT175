012300     COPY RCRD001.                                                01230000
012400                                                                  01240000
012500*----------------------------------------------------------------*01250000
012600*    SSCC CARTON INPUT FILE LAYOUT                                01260000
012700*----------------------------------------------------------------*01270000
012800     COPY RCRD102.                                                01280000
012900                                                                  01290000
013000                                                                  01300000
013100 LINKAGE SECTION.                                                 01310000
013200                                                                  01320000
013300***************************************************************** 01330000
013400 PROCEDURE DIVISION.                                              01340000
013500***************************************************************** 01350000
013600                                                                  01360000
013700 0000-MAINLINE-PROCESSING.                                        01370000
013800                                                                  01380000
013900     DISPLAY 'START OF PROGRAM RCKUT175'.                         01390000
014000                                                                  01400000
014010     MOVE PC-PROGRESSIVE-READ-LIMIT TO PV-DISP-NBR.               01401002
014020     DISPLAY 'PROGRESSIVE READ DISPLAY TRIGGER IS:  '             01402002
014030             PV-DISP-NBR.                                         01403002
014040                                                                  01404002
014100     PERFORM 1000-INITIALIZATION.                                 01410000
014200                                                                  01420000
014300     PERFORM 2000-PROCESS-PKSL-INPUT-FILE                         01430000
014400       UNTIL END-OF-PKSL-FILE.                                    01440000
014500                                                                  01450000
014600     IF  PV-READ-COUNT = 0                                        01460000
014700         DISPLAY ' '                                              01470000
014800         DISPLAY '*** EMPTY INPUT FILE ***'                       01480000
014900         DISPLAY ' '                                              01490000
015000     END-IF.                                                      01500000
015100                                                                  01510000
015200     PERFORM 8500-SHOW-COUNTERS.                                  01520000
015300                                                                  01530000
015400     CLOSE PACKSLIP-CARTON-FILE-IN                                01540000
015500           SSCC-CARTON-FILE-IN                                    01550000
015600           PACKSLIP-CARTON-FILE-OUT.                              01560000
015700                                                                  01570000
015800     STOP RUN.                                                    01580000
015900                                                                  01590000
016000                                                                  01600000
016100******************************************************************01610000
016200*  OPEN FILES AND READ THE FIRST INPUT RECORD                    *01620000
016300******************************************************************01630000
016400 1000-INITIALIZATION.                                             01640000
016500                                                                  01650000
016600     OPEN INPUT  PACKSLIP-CARTON-FILE-IN                          01660000
016700                 SSCC-CARTON-FILE-IN.                             01670000
016800     OPEN OUTPUT PACKSLIP-CARTON-FILE-OUT.                        01680000
016900                                                                  01690000
017000     PERFORM 7000-READ-PKSL-INPUT.                                01700000
017100     PERFORM 7200-READ-SSCC-INPUT.                                01710000
017200                                                                  01720000
017300     IF  END-OF-PKSL-FILE                                         01730000
017400         CONTINUE                                                 01740000
017500     ELSE                                                         01750000
017600         IF  RC001-CARTON-IND = SPACE                             01760000
017700         AND RC001-SEQUENCE-NBR = ZERO                            01770000
017800             CONTINUE                                             01780000
017900         ELSE                                                     01790000
018000               DISPLAY '**************************************'   01800000
018100               DISPLAY '** INVALID FIRST RECORD ON INP01 FILE '   01810000
018200               DISPLAY '** THIS IS THE RCRD001 PACKSLIP INPUT '   01820000
018300               DISPLAY '** FILE - FIRST RECORD IS NOT A HEADER'   01830000
018400               DISPLAY '** TYPE RECORD                        '   01840000
018500               DISPLAY '**************************************'   01850000
018600               MOVE '1000-INITIALIZATION' TO PV-PARAGRAPH-NAME    01860000
018700             PERFORM 9999-ABEND                                   01870000
018800         END-IF                                                   01880000
018900     END-IF.                                                      01890000
019000                                                                  01900000
019100                                                                  01910000
019510******************************************************************01951001
019520* THIS PARAGRAPH WILL MANAGE THE FLOW OF THE PROGRAM.  WHEN THE  *01952001
019530* RECORD ON THE RCRD001 INPUT FILE IS A HEADER RECORD, THEN THIS *01953001
019540* PARAGRAPH WILL MANAGE READING THE RCRD102 FILE TO COUNT THE    *01954001
019550* NUMBER OF CARTONS FOR THE DC/BOL AND THEN WRITING OUT THE      *01955001
019560* HEADER RECORD TO THE RCRD001 OUTPUT FILE.                      *01956001
019570* IN THIS PROGRAM, EVERY RECORD ON THE RCRD001 INPUT FILE WILL   *01957001
019580* BE WRITTEN TO THE RCRD001 OUTPUT FILE AND THE ONLY RECORD TYPE *01958001
019590* THAT WOULD BE CHANGED ON THE OUTPUT FILE IS THE HEADER TYPE    *01959001
019591* RECORD HAVING THE UPDATED CARTON COUNTS.                       *01959101
019592******************************************************************01959201
019600 2000-PROCESS-PKSL-INPUT-FILE.                                    01960000
019700                                                                  01970000
019800     IF  RC001-CARTON-IND = SPACE                                 01980000
019900     AND RC001-SEQUENCE-NBR = ZERO                                01990000
020000         SET PS-PO-COUNT-NOT-DONE TO TRUE                         02000000
020100         MOVE ZERO TO PV-PO-CARTON-QTY                            02010000
020200         MOVE RC001-DIST-CNTR          TO                         02020000
020300              PV-PKSLIP-DIST-CNTR                                 02030000
020400         MOVE RC001-BILL-OF-LADING-NBR TO                         02040000
020500              PV-PKSLIP-BILL-OF-LADING-NBR                        02050000
020800         PERFORM 3000-GET-CARTON-COUNT                            02080000
020900           UNTIL PS-PO-COUNT-DONE                                 02090000
021000              OR END-OF-SSCC-FILE                                 02100000
021100         MOVE PV-PO-CARTON-QTY TO RC001-CARTON-CNT-SHPMT          02110000
021300     END-IF.                                                      02130000
021400                                                                  02140000
021500     PERFORM 8000-WRITE-PKSL-OUTPUT.                              02150000
021600                                                                  02160000
021700     PERFORM 7000-READ-PKSL-INPUT.                                02170000
021800                                                                  02180000
022210******************************************************************02221001
022220* THIS PARAGRAPH WILL CONTROL THE READING OF THE RCRD102 FILE TO *02222001
022230* GET THE NEEDED CARTON COUNTS.                                  *02223001
022240******************************************************************02224001
022300 3000-GET-CARTON-COUNT.                                           02230000
022400                                                                  02240000
022500     IF  END-OF-SSCC-FILE                                         02250000
022600         SET PS-PO-COUNT-DONE TO TRUE                             02260000
022700     ELSE                                                         02270000
022800         IF  PV-SSCC-FILE-KEY    = PV-PKSLIP-FILE-KEY             02280000
022900             ADD +1 TO PV-PO-CARTON-QTY                           02290000
023000             PERFORM 7200-READ-SSCC-INPUT                         02300000
023100         ELSE                                                     02310000
023200             IF  PV-SSCC-FILE-KEY < PV-PKSLIP-FILE-KEY            02320000
023300                 PERFORM 7200-READ-SSCC-INPUT                     02330000
023400             ELSE                                                 02340000
023500                 SET PS-PO-COUNT-DONE TO TRUE                     02350000
023600             END-IF                                               02360000
023700         END-IF                                                   02370000
023800     END-IF.                                                      02380000
023900                                                                  02390000
024000                                                                  02400000
024100******************************************************************02410000
024200* READ A SHIPPED MERHANDISE INPUT FILE RECORD                    *02420001
024300******************************************************************02430000
024400 7000-READ-PKSL-INPUT.                                            02440000
024500                                                                  02450000
024600     READ PACKSLIP-CARTON-FILE-IN                                 02460000
024700       INTO RC001-ASN-TRANSCRIPTION-RECORD                        02470000
024800         AT END                                                   02480000
024900           SET END-OF-PKSL-FILE TO TRUE                           02490000
025000         NOT AT END                                               02500000
025100           ADD +1                     TO PV-READ-COUNT            02510000
025200                                         PV-PROGRESSIVE-READ-COUNT02520000
025300           IF  PV-PROGRESSIVE-READ-COUNT =                        02530000
025400               PC-PROGRESSIVE-READ-LIMIT                          02540000
025500               MOVE PV-READ-COUNT TO PV-DISP-NBR                  02550000
025600               DISPLAY 'PROGRESSIVE PACKSLIP READ COUNT IS: '     02560000
025700                       PV-DISP-NBR                                02570000
025800               MOVE ZEROS TO PV-PROGRESSIVE-READ-COUNT            02580000
025900           END-IF                                                 02590000
026000           IF  RC001-CARTON-IND = SPACE                           02600000
026100           AND RC001-SEQUENCE-NBR = ZERO                          02610000
026200               ADD +1 TO PV-READ-HEADER-COUNT                     02620000
026300           ELSE                                                   02630000
026400               IF  RC001-DETAIL-RECORD                            02640000
026500                   ADD +1 TO PV-READ-DETAIL-COUNT                 02650000
026600               ELSE                                               02660000
026700                   IF  RC001-CARTON-RECORD                        02670000
026800                       ADD +1 TO PV-READ-CARTON-COUNT             02680000
026900                   ELSE                                           02690000
027000                       ADD +1 TO PV-READ-OTHER-COUNT              02700000
027100                   END-IF                                         02710000
027200               END-IF                                             02720000
027300           END-IF                                                 02730000
027400     END-READ.                                                    02740000
027500                                                                  02750000
027600                                                                  02760000
027700******************************************************************02770000
027800* READ A SSCC CARTON INPUT FILE RECORD                           *02780001
027900******************************************************************02790000
028000 7200-READ-SSCC-INPUT.                                            02800000
028100                                                                  02810000
028200     READ SSCC-CARTON-FILE-IN                                     02820000
028300       INTO RC102-CARTON-REC                                      02830000
028400         AT END                                                   02840000
028500           SET END-OF-SSCC-FILE TO TRUE                           02850000
028600           SET PS-PO-COUNT-DONE TO TRUE                           02860000
028700         NOT AT END                                               02870000
028800           ADD +1                     TO PV-SSCC-READ-COUNT       02880000
028900         MOVE RC102-DC-NBR             TO                         02890000
029000              PV-SSCC-DIST-CNTR                                   02900000
029100         MOVE RC102-BOL-NBR            TO                         02910000
029200              PV-SSCC-BILL-OF-LADING-NBR                          02920000
029500     END-READ.                                                    02950000
029600                                                                  02960000
029700                                                                  02970000
029800******************************************************************02980000
029900* MOVE ALL OF THE DATA TO THE HEADER RECORD AND WRITE OUT THE    *02990001
030000* HEADER RECORD TO THE OUTPUT FILE                               *03000001
030100******************************************************************03010000
030200 8000-WRITE-PKSL-OUTPUT.                                          03020000
030300                                                                  03030000
030400     IF  RC001-CARTON-IND = SPACE                                 03040000
030500     AND RC001-SEQUENCE-NBR = ZERO                                03050000
030600         ADD +1 TO PV-WRITE-HEADER-COUNT                          03060000
030700     ELSE                                                         03070000
030800         IF  RC001-DETAIL-RECORD                                  03080000
030900             ADD +1 TO PV-WRITE-DETAIL-COUNT                      03090000
031000         ELSE                                                     03100000
031100             IF  RC001-CARTON-RECORD                              03110000
031200                 ADD +1 TO PV-WRITE-CARTON-COUNT                  03120000
031300             ELSE                                                 03130000
031400                 ADD +1 TO PV-WRITE-OTHER-COUNT                   03140000
031500             END-IF                                               03150000
031600         END-IF                                                   03160000
031700     END-IF.                                                      03170000
031800                                                                  03180000
031900     WRITE PACKSLIP-CARTON-RECORD-OUT                             03190000
032000      FROM RC001-ASN-TRANSCRIPTION-RECORD.                        03200000
032100                                                                  03210000
032200     ADD +1 TO PV-WRITE-TOTAL-COUNT.                              03220000
032300                                                                  03230000
032400                                                                  03240000
032410******************************************************************03241001
032420* DISPLAY RECORD COUNTS AT THE END OF THE RUN OF THE PROGRAM     *03242001
032430******************************************************************03243001
032800 8500-SHOW-COUNTERS.                                              03280000
032900     DISPLAY ' *********  RCKUT175 STATISTICS *********'          03290000
033000     MOVE PV-READ-HEADER-COUNT    TO PV-DISP-NBR-C                03300000
033100     DISPLAY 'HEADER PKSLIP RECORDS READ= ' PV-DISP-NBR-C.        03310000
033200     MOVE PV-READ-DETAIL-COUNT    TO PV-DISP-NBR-C                03320000
033300     DISPLAY 'DETAIL PKSLIP RECORDS READ= ' PV-DISP-NBR-C.        03330000
033400     MOVE PV-READ-CARTON-COUNT    TO PV-DISP-NBR-C                03340000
033500     DISPLAY 'CARTON PKSLIP RECORDS READ= ' PV-DISP-NBR-C.        03350000
033600     MOVE PV-READ-OTHER-COUNT     TO PV-DISP-NBR-C                03360000
033700     DISPLAY 'OTHER PKSLIP RECORDS READ = ' PV-DISP-NBR-C.        03370000
033800     MOVE PV-READ-COUNT           TO PV-DISP-NBR-C                03380000
033900     DISPLAY 'TOTAL PKSLIP RECORDS READ = ' PV-DISP-NBR-C.        03390000
034000     MOVE PV-SSCC-READ-COUNT      TO PV-DISP-NBR-C                03400000
034100     DISPLAY 'SSCC CARTON RECORDS READ  = ' PV-DISP-NBR-C.        03410000
034200     MOVE PV-WRITE-HEADER-COUNT   TO PV-DISP-NBR-C                03420000
034300     DISPLAY 'HEADER RECORDS WRITTEN    = ' PV-DISP-NBR-C.        03430000
034400     MOVE PV-WRITE-DETAIL-COUNT   TO PV-DISP-NBR-C                03440000
034500     DISPLAY 'DETAIL RECORDS WRITTEN    = ' PV-DISP-NBR-C.        03450000
034600     MOVE PV-WRITE-CARTON-COUNT   TO PV-DISP-NBR-C                03460000
034700     DISPLAY 'CARTON RECORDS WRITTEN    = ' PV-DISP-NBR-C.        03470000
034800     MOVE PV-WRITE-OTHER-COUNT    TO PV-DISP-NBR-C                03480000
034900     DISPLAY 'OTHER RECORDS WRITTEN     = ' PV-DISP-NBR-C.        03490000
035000     MOVE PV-WRITE-TOTAL-COUNT    TO PV-DISP-NBR-C.               03500000
035100     DISPLAY 'TOTAL NBR RECORDS WRITTEN = ' PV-DISP-NBR-C.        03510000
035200                                                                  03520000
035300******************************************************************03530000
035400* 9999-ABEND                                                      03540000
035500*     ABEND PROGRAM WHEN AN ERROR OCCURS.                         03550000
035600******************************************************************03560000
035700 9999-ABEND.                                                      03570000
035800                                                                  03580000
035900     DISPLAY '** ABEND           **'.                             03590000
036000     DISPLAY '** PROGRAM         ** = ' PC-CURRENT-PROGRAM-NAME.  03600000
036100     DISPLAY '** PARAGRAPH       ** = ' PV-PARAGRAPH-NAME.        03610000
036200     CALL 'ILBOABN0' USING AC-ABEND-CODE.                         03620000
