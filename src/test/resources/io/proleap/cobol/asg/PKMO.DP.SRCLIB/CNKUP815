046500******************************************************************04650004
046600*  CONTROLS THE MAIN PROCESSING OF THE PROGRAM.                   04660004
046700******************************************************************04670004
046800 0000-PROGRAM-MAINLINE.                                           04680004
046810     PERFORM 1000-INITIALIZE.                                     04681091
047000*                                                                 04700004
047010     PERFORM 5000-READ-INPUT.                                     04701066
047020     IF PS-END-OF-INPUT-NO                                        04702099
047029         PERFORM 2000-PROCESS-ATTR-ELEM                           04702999
047030             UNTIL PS-END-OF-INPUT-YES                            04703099
047050         PERFORM 8000-COMMIT                                      04705099
047060     END-IF.                                                      04706099
047480     PERFORM 9000-DISPLAY-COUNTS                                  04748043
047500*                                                                 04750004
047510     CLOSE ATTR-ELEM-FILE-IN.                                     04751099
047520*                                                                 04752064
047600     STOP RUN.                                                    04760004
047700*                                                                 04770004
048400******************************************************************04840004
048800*                                                                 04880004
048801 1000-INITIALIZE.                                                 04880191
048802                                                                  04880299
048803     DISPLAY '**'.                                                04880399
048804     DISPLAY '** PROGRAM:  CNKUP815'.                             04880499
048805     DISPLAY '**'.                                                04880599
048806                                                                  04880699
048810     OPEN INPUT ATTR-ELEM-FILE-IN.                                04881099
049100                                                                  04910094
049200     EXEC SQL                                                     04920091
049400          SELECT FUNC_QTY                                         04940092
049500            INTO :PV-COMMIT-CNT                                   04950092
049593            FROM TKRPRPM                                          04959392
049594           WHERE LOC_ID           = 90                            04959494
049595             AND INTFC_SYS_CDE    = 'KHL'                         04959594
049596             AND SYS_PRC_FUNC_CDE = 'DCCOMT'                      04959699
049597         WITH UR                                                  04959791
049598     END-EXEC                                                     04959891
049599     EVALUATE TRUE                                                04959995
049600         WHEN SQLCODE = ZERO                                      04960095
049601              CONTINUE                                            04960199
049602         WHEN OTHER                                               04960295
049605              DISPLAY 'SYS_PRC_FUNC_CDE = DCCOMT'                 04960599
049608              MOVE '1000-INITIALIZE'                              04960895
049609                                 TO PV-ABEND-PARAGRAPH            04960995
049610              MOVE 'SELECT'      TO PV-DB2-OPERATION-ATTEMPTED    04961095
049611              PERFORM 9200-SQL-ABEND                              04961195
049612     END-EVALUATE.                                                04961295
049613*                                                                 04961395
049620******************************************************************04962004
049700 2000-PROCESS-ATTR-ELEM.                                          04970099
058600*                                                                 05860004
059200     PERFORM 7000-INSERT-ATTR-ELEM                                05920099
059800*                                                                 05980004
061910     PERFORM 5000-READ-INPUT.                                     06191067
062000*                                                                 06200004
062001 5000-READ-INPUT.                                                 06200166
062010     READ ATTR-ELEM-FILE-IN                                       06201099
062021          INTO WS-WST00010                                        06202199
062030          AT END                                                  06203065
062040              SET PS-END-OF-INPUT-YES TO TRUE                     06204073
062050          NOT AT END                                              06205067
062060              ADD +1 TO PV-INPUT-CNT.                             06206067
111900*                                                                 11190004
112000******************************************************************11200004
112100*  INSERT ATTRIBUTE ELEMENTS                                      11210099
112200******************************************************************11220004
112300 7000-INSERT-ATTR-ELEM.                                           11230099
112301*                                                                 11230168
112302     EXEC SQL                                                     11230269
112310     INSERT INTO WST_ATTR_ELEM                                    11231099
112320           ( INTGRTN_TYP_CDE                                      11232099
112330           ,ATTR_DEFN_SEQ_NBR                                     11233099
112340           ,ATTR_ELEM_SEQ_NBR                                     11234099
112350           ,ATTR_ELEM_TXT                                         11235099
112360           ,ATTR_ELEM_TMST                                        11236099
112370           ,ATTR_ELEM_NBR)                                        11237099
112399   VALUES (:WS-INTGRTN-TYP-CDE                                    11239999
112500          ,:WS-ATTR-DEFN-SEQ-NBR                                  11250099
112600          ,:WS-ATTR-ELEM-SEQ-NBR                                  11260099
112610          ,:WS-ATTR-ELEM-TXT                                      11261099
112650               :WS-ATTR-ELEM-TXT-NULL                             11265099
112660          ,:WS-ATTR-ELEM-TMST                                     11266099
112670               :WS-ATTR-ELEM-TMST-NULL                            11267099
112671          ,:WS-ATTR-ELEM-NBR                                      11267199
112672               :WS-ATTR-ELEM-NBR-NULL)                            11267299
112680     END-EXEC.                                                    11268069
112700*                                                                 11270004
112800     EVALUATE TRUE                                                11280004
112900         WHEN SQLCODE = ZERO                                      11290004
112910              ADD +1 TO PV-ATTR-ELEM-CNT                          11291099
112911                        PV-INSERT-CNT                             11291197
112912              IF PV-INSERT-CNT > PV-COMMIT-CNT                    11291299
112920                   PERFORM 8000-COMMIT                            11292098
112930                   MOVE +1    TO PV-INSERT-CNT                    11293098
112933              END-IF                                              11293398
112934         WHEN SQLCODE = -803                                      11293498
112935              MOVE WS-ATTR-DEFN-SEQ-NBR TO PV-DISPLAY-ATTR-DEFN   11293599
112936              MOVE WS-ATTR-ELEM-SEQ-NBR TO PV-DISPLAY-ATTR-ELEM   11293699
112937              DISPLAY '*** DUPLICATE INSERT FOR: '                11293799
112940              DISPLAY '    ' WS-INTGRTN-TYP-CDE ' '               11294099
112950                             PV-DISPLAY-ATTR-DEFN ' '             11295099
112960                             PV-DISPLAY-ATTR-ELEM                 11296099
113100         WHEN OTHER                                               11310004
113101              PERFORM 9000-DISPLAY-COUNTS                         11310199
113102              MOVE WS-ATTR-DEFN-SEQ-NBR TO PV-DISPLAY-ATTR-DEFN   11310299
113103              MOVE WS-ATTR-ELEM-SEQ-NBR TO PV-DISPLAY-ATTR-ELEM   11310399
113105              DISPLAY WS-INTGRTN-TYP-CDE ' '                      11310599
113106                      PV-DISPLAY-ATTR-DEFN ' '                    11310699
113107                      PV-DISPLAY-ATTR-ELEM                        11310799
113200              MOVE '7000-INSERT-ATTR-ELEM'                        11320099
113300                                 TO PV-ABEND-PARAGRAPH            11330022
113400              MOVE 'INSERT'      TO PV-DB2-OPERATION-ATTEMPTED    11340068
113500              PERFORM 9200-SQL-ABEND                              11350004
113600     END-EVALUATE.                                                11360004
143400*                                                                 14340004
143500 8000-COMMIT.                                                     14350098
143510     EXEC SQL                                                     14351098
143520          COMMIT                                                  14352098
143530     END-EXEC                                                     14353098
143531     MOVE WS-ATTR-DEFN-SEQ-NBR TO PV-DISPLAY-ATTR-DEFN            14353199
143532     MOVE WS-ATTR-ELEM-SEQ-NBR TO PV-DISPLAY-ATTR-ELEM            14353299
143540     IF SQLCODE = ZERO                                            14354099
143552         DISPLAY '*** COMMIT TAKEN AT: '                          14355299
143553                  WS-INTGRTN-TYP-CDE ' '                          14355399
143554                  PV-DISPLAY-ATTR-DEFN ' '                        14355499
143555                  PV-DISPLAY-ATTR-ELEM                            14355599
143560     ELSE                                                         14356098
143810         DISPLAY WS-INTGRTN-TYP-CDE ' '                           14381099
143820                 PV-DISPLAY-ATTR-DEFN ' '                         14382099
143830                 PV-DISPLAY-ATTR-ELEM                             14383099
144100         PERFORM 9000-DISPLAY-COUNTS                              14410098
144300         MOVE '8000-COMMIT' TO PV-ABEND-PARAGRAPH                 14430099
144500         MOVE 'COMMIT'      TO PV-DB2-OPERATION-ATTEMPTED         14450098
144600         PERFORM 9200-SQL-ABEND                                   14460098
144610     END-IF.                                                      14461099
