000800****************************************************************  00080000
000900*   THIS PROGRAM CREATES TIMESTAMP FILES FOR THE BEGINNING OF     00090062
001000*   FLOWBACK WEEK.  INITIALLY EACH WEEK, THE TIMESTAMPS WILL BE   00100062
001100*   THE SAME.                                                     00110062
001200****************************************************************  00120000
001300                                                                  00130000
001400****************************************************************  00140000
001500 ENVIRONMENT DIVISION.                                            00150000
001600****************************************************************  00160000
001700                                                                  00170000
001800****************************************************************  00180000
001900 INPUT-OUTPUT SECTION.                                            00190000
002000****************************************************************  00200000
002100                                                                  00210000
002200****************************************************************  00220000
002300 FILE-CONTROL.                                                    00230000
002400****************************************************************  00240000
002601                                                                  00260159
002610     SELECT TIMESTAMP-FILE1                                       00261064
002620         ASSIGN TO INP01.                                         00262059
002630                                                                  00263062
002640     SELECT TIMESTAMP-FILE2                                       00264064
002650         ASSIGN TO INP02.                                         00265062
002700                                                                  00270000
002800****************************************************************  00280000
002900 DATA DIVISION.                                                   00290000
003000****************************************************************  00300000
003100                                                                  00310000
003200****************************************************************  00320000
003300 FILE SECTION.                                                    00330000
003400****************************************************************  00340000
003500                                                                  00350000
004220 FD  TIMESTAMP-FILE1                                              00422062
004230     RECORDING MODE IS F                                          00423059
004240     LABEL RECORDS ARE STANDARD                                   00424059
004250     RECORD CONTAINS 26                                           00425059
004260     BLOCK CONTAINS 0 RECORDS                                     00426059
004270     DATA RECORD IS EXTRACT-RECORD.                               00427059
004280 01  TIMESTAMP-RECORD1                   PIC  X(26).              00428062
004290                                                                  00429062
004291 FD  TIMESTAMP-FILE2                                              00429162
004292     RECORDING MODE IS F                                          00429262
004293     LABEL RECORDS ARE STANDARD                                   00429362
004294     RECORD CONTAINS 26                                           00429462
004295     BLOCK CONTAINS 0 RECORDS                                     00429562
004296     DATA RECORD IS EXTRACT-RECORD.                               00429662
004297 01  TIMESTAMP-RECORD2                   PIC  X(26).              00429762
004300                                                                  00430000
004400/                                                                 00440000
004500 WORKING-STORAGE SECTION.                                         00450000
004600                                                                  00460000
004700 01  TIMESTAMPS.                                                  00470063
004800     05  PV-TIMESTAMP-OUT                PIC X(26) VALUE SPACES.  00480063
016500*                                                                 01650000
016600*  COMMUNICATION AREAS FOR DB2                                    01660000
016700*                                                                 01670000
016800     EXEC SQL                                                     01680000
016900         INCLUDE SQLCA                                            01690000
017000     END-EXEC.                                                    01700000
017100                                                                  01710000
020930                                                                  02093005
021000******************************************************************02100000
021100 PROCEDURE DIVISION.                                              02110000
021200******************************************************************02120000
021300                                                                  02130000
021400 0000-MAINLINE.                                                   02140000
021500                                                                  02150001
021600     PERFORM 1000-INITIALIZATION.                                 02160001
021700                                                                  02170001
022500     STOP RUN.                                                    02250001
022600                                                                  02260001
022700******************************************************************02270002
022800** INITIALIZE                                                     02280062
023000******************************************************************02300002
023100 1000-INITIALIZATION.                                             02310001
023200                                                                  02320001
023310     OPEN OUTPUT TIMESTAMP-FILE1.                                 02331063
023320     OPEN OUTPUT TIMESTAMP-FILE2.                                 02332063
023700                                                                  02370061
023800     EXEC SQL                                                     02380063
023900         SET :PV-TIMESTAMP-OUT = CURRENT TIMESTAMP                02390063
024000     END-EXEC.                                                    02400063
024010                                                                  02401065
024100     MOVE PV-TIMESTAMP-OUT TO TIMESTAMP-RECORD1.                  02410065
024200                                                                  02420065
024300     WRITE TIMESTAMP-RECORD1.                                     02430065
024400                                                                  02440001
024410     DISPLAY 'TIMESTAMP RECORD 1: ' PV-TIMESTAMP-OUT.             02441063
024420                                                                  02442063
024421     MOVE PV-TIMESTAMP-OUT TO TIMESTAMP-RECORD2.                  02442165
024422                                                                  02442265
024430     WRITE TIMESTAMP-RECORD2.                                     02443065
024431                                                                  02443163
024440     DISPLAY 'TIMESTAMP RECORD 2: ' PV-TIMESTAMP-OUT.             02444063
024600                                                                  02460001
024700     CLOSE TIMESTAMP-FILE1.                                       02470063
024800     CLOSE TIMESTAMP-FILE2.                                       02480063
