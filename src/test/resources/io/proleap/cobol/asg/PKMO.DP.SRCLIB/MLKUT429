014000                                                                          
014100******************************************************************        
014200*  DB2 TWKMCLC TABLE LAYOUT                                               
014300******************************************************************        
014400     EXEC SQL                                                             
014500          INCLUDE TWKMCLC                                                 
014600     END-EXEC.                                                            
014700                                                                          
014800******************************************************************        
014900*  DATE ATTRIBUTE TABLE LAYOUT                                            
015000******************************************************************        
015100     EXEC SQL                                                             
015200          INCLUDE TDTATTR                                                 
015300     END-EXEC.                                                            
015400                                                                          
014800******************************************************************        
014900*  MERCHANDISE LEDGER CONTROL TABLE LAYOUT                                
015000******************************************************************        
015100     EXEC SQL                                                             
015200          INCLUDE TMLCNTL                                                 
015300     END-EXEC.                                                            
015400                                                                          
015500******************************************************************        
015600*  DB2 TWKMCLC CURSOR                                                     
015700******************************************************************        
015800     EXEC SQL                                                             
015900         DECLARE TWKMCLC_CURSOR CURSOR FOR                                
016200          SELECT A.FSCL_WK_END_DTE                                        
016400                ,DEPT_NBR                                                 
016400                ,MAJ_CL_NBR                                               
016500                ,SUM(SLS_RTL_AMT)                                         
016600                ,SUM(MKDN_PLU_RTL_AMT)                                    
016700                ,SUM(MKDN_PROMO_RTL_AMT)                                  
016700                ,SUM(MKDN_RGST_RTL_AMT)                                   
016700                ,SUM(MKDN_GLDST_RTL_AMT)                                  
016700                ,SUM(MKDN_UMTCH_RTL_AMT)                                  
016700                ,SUM(MKDN_BYR_RTL_AMT)                                    
016700                ,SUM(MKDN_STR_RTL_AMT)                                    
016700                ,SUM(MKDN_PRPT_RTL_AMT)                                   
016700                ,SUM(MKDN_OTH_RTL_AMT                                     
                         + MKDN_EMP_RTL_AMT)                                    
016700                ,SUM(MKUP_RTL_AMT)                                        
016800                ,SUM(XFER_IN_RTL_AMT)                                     
016900                ,SUM(XFER_OUT_RTL_AMT)                                    
017000                ,SUM(RCPT_RTL_AMT)                                        
017100                ,SUM(RCPT_NET_COST_AMT)                                   
017200                ,SUM(PADJ_RTL_AMT)                                        
017200                ,SUM(PADJ_FRGT_COST_AMT)                                  
017200                ,SUM(PADJ_RTV_COST_AMT)                                   
017200                ,SUM(PADJ_RTV_RTL_AMT)                                    
017200                ,SUM(PADJ_DA_COST_AMT)                                    
017200                ,SUM(PADJ_DA_RTL_AMT)                                     
017300                ,SUM(PADJ_NET_COST_AMT)                                   
016500                ,SUM(PADJ_OTH_COST_AMT                                    
016500                   + PADJ_BROP_COST_AMT                                   
016500                   + LIC_FEE_COST_AMT)                                    
016500                ,SUM(PADJ_OTH_RTL_AMT)                                    
017400                ,SUM(INV_ADJ_RTL_AMT)                                     
017500                ,SUM(SHNK_RTL_AMT)                                        
017600                ,SUM(END_INV_RTL_AMT)                                     
017700            FROM TWKMCLC A                                                
017800                ,TDTATTR B                                                
017900           WHERE (A.FSCL_WK_END_DTE                                       
018000                      BETWEEN :PV-STRT-DTE AND :PV-END-DTE)               
018200             AND A.FSCL_WK_END_DTE = B.KY_DTE                             
018200             AND A.CHNL_CDE IN ('A')                                      
018400        GROUP BY  A.FSCL_WK_END_DTE                                       
018300                 ,DEPT_NBR                                                
018300                 ,MAJ_CL_NBR                                              
W26603           WITH UR                                                        
018500     END-EXEC.                                                            
018600                                                                          
018700 EJECT                                                                    
018800                                                                          
