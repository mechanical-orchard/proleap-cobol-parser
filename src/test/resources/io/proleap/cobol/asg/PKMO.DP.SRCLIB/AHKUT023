021300     EXEC SQL                                                     02130000
021400          INCLUDE SQLCA                                           02140000
021500     END-EXEC.                                                    02150000
021600                                                                  02160000
021700*---------------------------------------------------------------* 02170000
021800*    DEFINE CURSOR FOR TALLPLN :                                * 02180000
021900*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02190000
022000*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02200000
022100*---------------------------------------------------------------* 02210000
022200     EXEC SQL                                                     02220000
022300          DECLARE TABLE-CURSOR CURSOR FOR                         02230000
022400           SELECT PO_NBR                                          02240000
022500                 ,PO_LNE_NBR                                      02250000
022600                 ,PLAN_NBR                                        02260000
022700                 ,CURR_SPLIT_QTY                                  02270000
022800                 ,DEST_NBR                                        02280000
022900                 ,STATUS_CDE                                      02290000
023000                 ,DC_SPLIT_QTY                                    02300000
023100                 ,SPLIT_REQ_ID                                    02310000
023200                 ,SPLIT_CRE_DATE                                  02320000
023300                 ,SPLIT_CRE_ID                                    02330000
023400                 ,SPLIT_UPD_DATE                                  02340000
023500                 ,SPLIT_UPD_ID                                    02350000
023600                 ,DISTRO_REQ_ID                                   02360000
023700                 ,DISTRO_CRE_DATE                                 02370000
023800                 ,DISTRO_CRE_ID                                   02380000
023900                 ,DISTRO_KEY_SEQ_NBR                              02390000
024000                 ,SPLIT_KEY_SEQ_NBR                               02400000
024100                 ,ALOC_PACK_QTY                                   02410000
024200                 ,PK_SIZE_RND_CDE                                 02420000
024300                 ,ALLOC_OUTR_PK_QTY                               02430000
                       ,SHTG_PRRT_OPT_CDE                               02440008
                       ,OVRG_PRRT_OPT_CDE                               02441008
                       ,EXST_SHTG_THRS_IND                              02442008
                       ,DC_SPLT_CNCLD_QTY                               02443010
024500           FROM   TALLPLN                                         02450000
024600          WHERE  (PO_NBR = :PV-PO-NBR-COMP)                       02460000
024800            FOR  FETCH ONLY                                       02480000
024900           WITH  UR                                               02490000
025000     END-EXEC.                                                    02500000
025100                                                                  02510000
025200                                                                  02520000
025300 PROCEDURE DIVISION.                                              02530000
025400********************                                              02540000
025500                                                                  02550000
025600 0000-PROGRAM-MAINLINE.                                           02560000
025700                                                                  02570000
025800     PERFORM 1000-INITIALIZE.                                     02580000
025900                                                                  02590000
026000     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02600000
026100     PERFORM 2000-PROCESS-DRIVER-FILE                             02610000
026200         UNTIL PS-IN-DRIVER-EOF.                                  02620000
026300                                                                  02630000
026400     PERFORM 6000-EOJ-ROUTINE.                                    02640000
026500     STOP RUN.                                                    02650000
026600                                                                  02660000
026700                                                                  02670000
026800* ************************************************************** *02680000
026900* * INITIALIZATIONS                                            * *02690000
027000* ************************************************************** *02700000
027100 1000-INITIALIZE.                                                 02710000
027200                                                                  02720000
027300     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02730000
027400     DISPLAY DM-DISPLAY-MESSAGE.                                  02740000
027500                                                                  02750000
027600     MOVE PC-BEGINNING              TO DP-STATUS-DESCRIPTION.     02760000
027700     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02770000
027800                                                                  02780000
027900     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02790000
028000     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02800000
028100                                                                  02810000
028200                                                                  02820000
028300* ************************************************************** *02830000
028400* * 1) GET NEXT RECORD FROM DRIVER FILE, AND                   * *02840000
028500* *    1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR        * *02850000
028600* *    1B- OPEN TABLE CURSOR                                   * *02860000
028700* *    1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL               * *02870000
028800* *        ROWS FOR THIS INPUT DRIVER RECORD                   * *02880000
028900* *    2) FOR EACH ROW,                                        * *02890000
029000* *       2A- INCREMENT COUNTS                                 * *02900000
029100* *       2B- WRITE EXTRACT RECORD                             * *02910000
029200* *    1D- CLOSE TABLE CURSOR                                  * *02920000
029300* ************************************************************** *02930000
029400 2000-PROCESS-DRIVER-FILE.                                        02940000
029500                                                                  02950000
029600     PERFORM 9050-READ-IN-DRIVER-FILE.                            02960000
029700     IF PS-IN-DRIVER-EOF                                          02970000
029800         CONTINUE                                                 02980000
029900     ELSE                                                         02990000
030000         MOVE AH002-PO-NBR TO         PV-PO-NBR-COMP              03000006
030100                                      PV-PO-NBR-NUM               03010000
030200         PERFORM 2100-CHECK-PGM-PROGRESS                          03020000
030300         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     03030000
030400         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     03040000
030500         PERFORM 9100-OPEN-TABLE-CURSOR                           03050000
030600         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     03060000
030700             UNTIL PS-TABLE-LOOP-DONE                             03070000
030800         PERFORM 9190-CLOSE-TABLE-CURSOR                          03080000
030900     END-IF.                                                      03090000
031000                                                                  03100000
031100                                                                  03110000
031200* ************************************************************** *03120000
031300* *    EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT     * *03130000
031400* *    RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST   * *03140000
031500* *    WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.   * *03150000
031600* ************************************************************** *03160000
031700 2100-CHECK-PGM-PROGRESS.                                         03170000
031800     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03180000
031900         BY        PC-HOW-OFTEN-TO-DISPLAY                        03190000
032000         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03200000
032100         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03210000
032200                                                                  03220000
032300     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03230000
032400         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03240000
032500         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03250000
032600     END-IF.                                                      03260000
032700                                                                  03270000
032800                                                                  03280000
032900* ************************************************************** *03290000
033000* * EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD     * *03300000
033100* ************************************************************** *03310000
033200 3000-EXTRACT-ROWS-FROM-TABLE.                                    03320000
033300     INITIALIZE DCLTALLPLN.                                       03330000
033400     PERFORM 9150-FETCH-TABLE-CURSOR.                             03340000
033500     IF PS-TABLE-LOOP-DONE                                        03350000
033600         CONTINUE                                                 03360000
033700     ELSE                                                         03370000
033800         PERFORM 9750-WRITE-EXTRACT-RECORD                        03380000
033900     END-IF.                                                      03390000
034000                                                                  03400000
034100                                                                  03410000
034200* ************************************************************** *03420000
034300* * DISPLAY END OF RUN MESSAGE, CLOSE THE OUTPUT FILES         * *03430002
034400* ************************************************************** *03440000
034500 6000-EOJ-ROUTINE.                                                03450000
034600                                                                  03460000
034700     PERFORM 9090-CLOSE-IN-DRIVER-FILE.                           03470000
034800     PERFORM 9790-CLOSE-OUT-EXTRACT-FILE.                         03480000
034900                                                                  03490000
035000     MOVE PC-ENDING                 TO DP-STATUS-DESCRIPTION.     03500000
035100     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           03510000
035200                                                                  03520000
035300                                                                  03530000
035400* ************************************************************** *03540000
035500* * DISPLAY PROGRAM PROGRESS.                                  * *03550000
035600* ************************************************************** *03560000
035700 6200-DISPLAY-PGM-PROGRESS.                                       03570000
035800                                                                  03580000
035900     EXEC SQL                                                     03590000
036000          SET :PV-TMST = CURRENT TIMESTAMP                        03600000
036100     END-EXEC.                                                    03610000
036200                                                                  03620000
036300     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                03630000
036400     MOVE PV-TIMESTAMP-19           TO DP-TIMESTAMP-19.           03640000
036500     MOVE PV-IN-DRIVER-RECS-READ-CNT                              03650000
036600                                    TO DP-IN-CNT.                 03660000
036700*    MOVE PV-RECS-WITH-MATCHES-CNT  TO DP-MATCHES-FOUND-CNT.      03670000
036800     MOVE PV-TABLE-ROW-SELECT-CNT   TO DP-ROWS-SELECTED-CNT.      03680000
036900     MOVE PV-EXTRACT-WRITTEN-CNT    TO DP-OUT-CNT.                03690000
037000                                                                  03700000
037100     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             03710000
037200                                                                  03720000
037300                                                                  03730000
037400* ************************************************************** *03740000
037500* * FILE AND DB2 TABLE ACCESS PARAGRAPHS.                      * *03750000
037600* * 90XX- INPUT:  DRIVER FILE                                  * *03760000
037700* * 91XX- INPUT:  TABLE CURSOR                                 * *03770000
037800* * 97XX- OUTPUT: TABLE EXTRACT                                * *03780000
037900* ************************************************************** *03790000
038000                                                                  03800000
038100* ************************************************************** *03810000
038200* * OPEN  INPUT DRIVER FILE                                    * *03820000
038300* ************************************************************** *03830000
038400 9000-OPEN-IN-DRIVER-FILE.                                        03840000
038500     OPEN  INPUT  IN-DRIVER-FILE.                                 03850000
038600                                                                  03860000
038700                                                                  03870000
038800* ************************************************************** *03880000
038900* * READ  INPUT DRIVER FILE OF ELIGIBLE DATA TO EXTRACT        * *03890000
039000* ************************************************************** *03900000
039100 9050-READ-IN-DRIVER-FILE.                                        03910000
039200     READ IN-DRIVER-FILE                                          03920000
039300          INTO AH002-DRIVER-RECORD-LAYOUT                         03930006
039400          AT END                                                  03940000
039500              SET PS-IN-DRIVER-EOF TO TRUE.                       03950000
039600                                                                  03960000
039700     IF PS-IN-DRIVER-EOF                                          03970000
039800         CONTINUE                                                 03980000
039900     ELSE                                                         03990000
040000         ADD +1 TO PV-IN-DRIVER-RECS-READ-CNT                     04000000
040100     END-IF.                                                      04010000
040200                                                                  04020000
040300                                                                  04030000
040400* ************************************************************** *04040000
040500* * CLOSE INPUT DRIVER FILE                                    * *04050000
040600* ************************************************************** *04060000
