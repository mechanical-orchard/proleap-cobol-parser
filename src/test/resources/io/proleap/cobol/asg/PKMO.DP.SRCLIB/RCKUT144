012400******************************************************************01240000
012500*   STORE EXTRACT RECORD LAYOUT                                   01250000
012600******************************************************************01260000
012700     COPY STOREEXT.                                               01270000
012800*                                                                 01280000
012900******************************************************************01290000
013000*   STORE EXTRACT BALANCE RECORD LAYOUT                           01300000
013100******************************************************************01310000
013200     COPY RCRD077.                                                01320000
013300*                                                                 01330000
013400/                                                                 01340000
013500*                                                                 01350000
013600******************************************************************01360000
013700 PROCEDURE DIVISION.                                              01370000
013800******************************************************************01380000
013900 0000-MAIN-DRIVER.                                                01390000
014000******************************************************************01400000
014100*  THIS PARAGRAPH CONTROLS THE OVERALL FLOW OF THE PROGRAM.       01410000
014200******************************************************************01420000
014300     PERFORM 1000-INITIAL-PROCESS.                                01430000
014400*                                                                 01440000
014500     PERFORM 2000-MAIN-PROCESS.                                   01450000
014600*                                                                 01460000
014700     PERFORM 9000-END-PROCESS.                                    01470000
014800*                                                                 01480000
014900     GOBACK.                                                      01490000
015000*                                                                 01500000
015100*                                                                 01510000
015200 1000-INITIAL-PROCESS.                                            01520000
015300******************************************************************01530000
015400*  IS THEN PERFORMED UNTIL ITS END.  THE FILES ARE THEN CLOSED    01540000
015500*  AND THE PROGRAM IS TERMINATED.                                 01550000
015600******************************************************************01560000
015700*                                                                 01570000
015800     OPEN INPUT  STORE-EXTRACT-FILE                               01580000
015900                 STR-EXT-BALANCE-FILE.                            01590000
016000*INITIAL READ                                                     01600000
016100     PERFORM 7000-READ-STORE-EXTRACT-FILE.                        01610000
016200*                                                                 01620000
016300     IF  PS-STORE-EXTRACT-EOF                                     01630000
016400         CONTINUE                                                 01640000
016500     END-IF.                                                      01650000
016600*                                                                 01660000
016700*                                                                 01670000
016800 2000-MAIN-PROCESS.                                               01680000
016900******************************************************************01690000
017000*                                                                 01700000
017100     PERFORM 7000-READ-STORE-EXTRACT-FILE                         01710000
017200       UNTIL PS-STORE-EXTRACT-EOF.                                01720000
017300                                                                  01730000
017400     IF PS-STORE-EXTRACT-EOF                                      01740000
017500        PERFORM 7100-READ-BALANCE-FILE                            01750000
017600                                                                  01760000
017700        IF PV-RECORD-COUNT              = RC077-RECORD-COUNT      01770000
017800           IF PV-SE-RETAIL-UNITS-RECVD  = RC077-TOTAL-UNIT-QTY    01780000
017900              CONTINUE                                            01790000
018000           ELSE                                                   01800000
018100              MOVE '2000-MAIN-PROCESS '  TO PV-ABEND-PARAGRAPH    01810000
018200              MOVE 'DATASET MISMATCH  '  TO PV-ERR-MSG-1          01820000
018300              PERFORM 9999-ABEND-PROGRAM                          01830000
018400           END-IF                                                 01840000
018500        ELSE                                                      01850000
018600           MOVE '2000-MAIN-PROCESS '  TO PV-ABEND-PARAGRAPH       01860000
018700           MOVE 'DATASET MISMATCH  '  TO PV-ERR-MSG-1             01870000
018800           PERFORM 9999-ABEND-PROGRAM                             01880000
018900        END-IF                                                    01890000
019000     END-IF.                                                      01900000
019100*                                                                 01910000
019200*                                                                 01920000
019300 7000-READ-STORE-EXTRACT-FILE.                                    01930000
019400******************************************************************01940000
019500*  READ THE INPUT FILE STORE-EXTRACT-FILE                         01950000
019600******************************************************************01960000
019700     READ STORE-EXTRACT-FILE INTO STORE-EXTRACT-LINE              01970000
019800       AT END                                                     01980000
019900          SET PS-STORE-EXTRACT-EOF       TO TRUE                  01990000
020000       NOT AT END                                                 02000000
020100          ADD +1 TO PV-RECORD-COUNT                               02010000
020200          ADD SE-RETAIL-UNITS-RECVD TO PV-SE-RETAIL-UNITS-RECVD   02020000
020300          ADD SE-RETAIL-EXTENSION   TO PV-SE-RETAIL-EXTENSION     02030000
020400     END-READ.                                                    02040000
020500*                                                                 02050000
020600*                                                                 02060000
020700 7100-READ-BALANCE-FILE.                                          02070000
020800******************************************************************02080000
020900*  READ THE INPUT FILE STR-EXT-BALANCE-FILE                       02090000
021000******************************************************************02100000
021100     READ STR-EXT-BALANCE-FILE INTO RC077-RECORD                  02110000
021200       AT END                                                     02120000
021300          SET PS-STR-EXT-BALANCE-EOF     TO TRUE                  02130000
021400       NOT AT END                                                 02140000
021500          CONTINUE                                                02150000
021600     END-READ.                                                    02160000
021700*                                                                 02170000
021800*                                                                 02180000
021900 9000-END-PROCESS.                                                02190000
022000******************************************************************02200000
022100*  THIS PARAGRAPH PERFORMS THE FOLLOWING AT THE END OF THE        02210000
022200*  PROGRAM:                                                       02220000
022300*    -- DISPLAY READ/WRITE TOTALS.                                02230000
022400*    -- CLOSE THE FILES.                                          02240000
022500******************************************************************02250000
022600*                                                                 02260000
022700     PERFORM 9100-DISPLAY-TOTALS.                                 02270000
022800                                                                  02280000
022900     CLOSE STORE-EXTRACT-FILE                                     02290000
023000           STR-EXT-BALANCE-FILE.                                  02300000
023100*                                                                 02310000
023200*                                                                 02320000
023300 9100-DISPLAY-TOTALS.                                             02330000
023400******************************************************************02340000
023500*  THIS PARAGRAPH FORMATS AND DISPLAYS THE RECORD COUNTS TO       02350000
023600*  APPEAR IN THE SYSLOG.                                          02360000
023700******************************************************************02370000
023800     DISPLAY 'RCKUT144'.                                          02380000
023900     DISPLAY ' '.                                                 02390000
024000     DISPLAY 'INP01 REC COUNT: '    PV-RECORD-COUNT.              02400000
024100     DISPLAY 'INP02 REC COUNT: '    RC077-RECORD-COUNT.           02410000
024200*                                                                 02420000
024300     DISPLAY 'INP01 UNIT QTY : '    PV-SE-RETAIL-UNITS-RECVD.     02430000
024400     DISPLAY 'INP02 UNIT QTY : '    RC077-TOTAL-UNIT-QTY.         02440000
024500*                                                                 02450000
024600*                                                                 02460000
024700 9999-ABEND-PROGRAM.                                              02470000
024800******************************************************************02480000
024900*  PERFORMS THE ABEND FOR THE PROGRAM WHEN NO DB2 IS INVLOVED.    02490000
025000******************************************************************02500000
025100     DISPLAY '***************************'.                       02510000
025200     DISPLAY '**       ABEND           **'.                       02520000
025300     DISPLAY '**  IN PROGRAM RCKUT144  **'.                       02530000
025400     DISPLAY '***************************'.                       02540000
025500     DISPLAY '** PARAGRAPH ** = ' PV-ABEND-PARAGRAPH.             02550000
025600     DISPLAY '** ERROR     ** = ' PV-ERR-MSG-1.                   02560000
025700     DISPLAY '** MORE      ** = ' PV-ERR-MSG-2.                   02570000
025800     DISPLAY ' '.                                                 02580000
025900     DISPLAY 'INP01 REC COUNT: '  PV-RECORD-COUNT                 02590000
026000     DISPLAY 'INP02 REC COUNT: '  RC077-RECORD-COUNT.             02600000
026100*                                                                 02610000
026200     DISPLAY 'INP01 UNIT QTY : '    PV-SE-RETAIL-UNITS-RECVD.     02620000
026300     DISPLAY 'INP02 UNIT QTY : '    RC077-TOTAL-UNIT-QTY.         02630000
026400                                                                  02640000
026500     MOVE +4014                      TO PV-ABEND-CODE.            02650000
026600     CALL 'ILBOABN0' USING PV-ABEND-CODE.                         02660000
