005800*                                                                 00580000
005900*                                                                 00590000
006000 01  PS-PROGRAM-SWITCHES.                                         00600000
006100     05  PS-STORE-EXTRACT-EOF-SW     PIC  X(03)    VALUE 'BOF'.   00610000
006200         88  PS-STORE-EXTRACT-EOF                  VALUE 'EOF'.   00620000
006300     05  PS-STR-EXT-BALANCE-EOF-SW   PIC  X(03)    VALUE 'BOF'.   00630000
006400         88  PS-STR-EXT-BALANCE-EOF                VALUE 'EOF'.   00640000
006500     05  PS-NOT-INTIAL-READ-SW       PIC  X(01)    VALUE 'N'.     00650000
006600         88  PS-INTIAL-READ                        VALUE 'Y'.     00660000
006700         88  PS-NOT-INTIAL-READ                    VALUE 'N'.     00670000
006800*                                                                 00680000
