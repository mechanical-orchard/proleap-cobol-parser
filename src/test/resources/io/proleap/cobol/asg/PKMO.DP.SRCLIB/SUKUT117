040300******************************************************************04030001
026200 2100-CHECK-MNFSTED-UNITS.                                        04031093
040500******************************************************************04050001
           MOVE ZERO TO SUT00028-SHPD-UNT-QTY                           04051093
040600     EXEC SQL                                                     04060001
040700         SELECT SUM(SHPD_UNT_QTY)                                 04070098
040800           INTO :SUT00028-SHPD-UNT-QTY                            04080099
                 FROM SUT_OTBND_UPC                                     04090063
                WHERE SHIPT_UNT_ID = :SU067-SHIPT-UNT-ID                04100064
041500     END-EXEC.                                                    04150001
041600                                                                  04160001
041700     EVALUATE TRUE                                                04170001
041800         WHEN SQLCODE = ZERO                                      04180001
                   IF SUT00028-SHPD-UNT-QTY > ZERO                      04181092
041900                 SET PS-CARTON-UNITS-MNFSTED-YES    TO TRUE       04190093
                   ELSE                                                 04191085
041900                 SET PS-CARTON-UNITS-MNFSTED-NO     TO TRUE       04192093
                   END-IF                                               04200083
042100         WHEN SQLCODE = +100                                      04210068
042100         WHEN SQLCODE = -305                                      04210194
041900             SET PS-CARTON-UNITS-MNFSTED-NO         TO TRUE       04211093
042300         WHEN SQLWARN0 NOT = SPACES                               04230001
042400         WHEN SQLCODE < +0                                        04240001
042500         WHEN SQLCODE > ZERO                                      04250001
042600             MOVE '2100-CHECK-MNFSTED-UNITS'                      04260093
042700                                      TO PV-ABEND-PARAGRAPH       04270068
042900             MOVE 'SELECT'            TO PV-ABEND-OPERATION1      04290068
043000             MOVE 'SUT_OTBND_UPC'     TO PV-DB2-TABLE-NAME        04300068
043100             MOVE SU067-SHIPT-UNT-ID  TO PV-DISP-FIELD-1          04310095
043200             STRING 'SHIPT-UNT-QTY: '  DELIMITED BY SIZE          04320055
043300                 PV-DISP-FIELD-1       DELIMITED BY SIZE          04330054
043400                           INTO PV-ABEND-OPERATION2               04340008
044500             PERFORM 9100-SQL-ABEND                               04450001
044600     END-EVALUATE.                                                04460001
044700                                                                  04470001
026200 2200-CHECK-CARTON-DIVERTED.                                      04480056
040600     EXEC SQL                                                     04490056
040700         SELECT DATE(A.DIVERT_TMST)                               04500060
040700               ,TIME(A.DIVERT_TMST)                               04501060
                     ,A.DOOR_ID                                         04510060
040800         INTO  :PV-DIVERT-DATE                                    04520078
040900              ,:PV-DIVERT-TIME                                    04530080
                    ,:SUCRDV-DOOR-ID                                    04531059
               FROM TSUCRDV A                                           04532060
              WHERE A.SHIPT_UNT_ID = :SU067-SHIPT-UNT-ID                04533060
                AND A.DIVERT_TMST IN                                    04534060
                   (SELECT MAX(B.DIVERT_TMST)                           04535061
                      FROM TSUCRDV B                                    04536062
                     WHERE A.SHIPT_UNT_ID = B.SHIPT_UNT_ID)             04537062
041500     END-EXEC.                                                    04540056
041600                                                                  04550056
041700     EVALUATE TRUE                                                04560056
041800         WHEN SQLCODE = ZERO                                      04570056
                   SET PS-CARTON-DIVERTED-YES TO TRUE                   04570170
042100         WHEN SQLCODE = +100                                      04571063
                   SET PS-CARTON-DIVERTED-NO  TO TRUE                   04572070
042300         WHEN SQLWARN0 NOT = SPACES                               04610056
042400         WHEN SQLCODE < +0                                        04620056
042500         WHEN SQLCODE > ZERO                                      04630056
042600             MOVE '2200-CHECK-CARTON-DIVERTED'                    04640063
042700                                  TO PV-ABEND-PARAGRAPH           04650056
042900             MOVE 'SELECT'        TO PV-ABEND-OPERATION1          04660056
043000             MOVE 'TSUCRDV'       TO PV-DB2-TABLE-NAME            04670063
043100             MOVE SU067-SHIPT-UNT-ID TO PV-DISP-FIELD-1           04680096
043200             STRING 'SHIPT-UNT-QTY: '  DELIMITED BY SIZE          04690056
043300                 PV-DISP-FIELD-1       DELIMITED BY SIZE          04700056
043400                           INTO PV-ABEND-OPERATION2               04710056
044500             PERFORM 9100-SQL-ABEND                               04720056
