019800 0000-MAIN.                                                       01980043
019900     PERFORM 1000-INITIALIZE.                                     01990043
020000     PERFORM 1100-PRINT-HEADING-LINES.                            02000043
020100     PERFORM 1200-CREATE-SUMMARY-FILE                             02010043
020200         UNTIL PS-END-OF-FILE.                                    02020043
020300     PERFORM 1300-WRITE-SUMMARY-REPORT.                           02030043
020400     STOP RUN.                                                    02040043
020500                                                                  02050043
020600 1000-INITIALIZE.                                                 02060043
020700     OPEN INPUT  POSITIVE-PAY-FILE.                               02070043
020800     OPEN OUTPUT POSITIVE-PAY-SUMMARY.                            02080043
020810     OPEN OUTPUT POSITIVE-PAY-CHCKNBR.                            02081044
020900                                                                  02090043
021000     MOVE FUNCTION CURRENT-DATE TO PV-CURRENT-DATE-TIME.          02100043
021100                                                                  02110043
021200     MOVE PV-CURRENT-DD      TO PPS-DAY                           02120043
021300                                DP075O-RUN-DAY.                   02130043
021400     MOVE PV-CURRENT-MM      TO PPS-MONTH                         02140043
021500                                DP075O-RUN-MONTH.                 02150043
021600     MOVE PV-CURRENT-YY      TO PPS-YEAR                          02160043
021700                                DP075O-RUN-YEAR.                  02170043
021800                                                                  02180043
021900     MOVE PV-CURRENT-HR      TO DP075O-RUN-HOUR                   02190043
022000                                PPS-HOUR.                         02200043
022100     MOVE PV-CURRENT-MIN     TO DP075O-RUN-MINUTE                 02210043
022200                                PPS-MIN.                          02220043
022300     MOVE PV-CURRENT-SEC     TO PPS-SEC.                          02230043
022400                                                                  02240043
022500     PERFORM 1250-READ-POSITIVE-PAY-INPUT.                        02251045
022600                                                                  02260043
022700     MOVE ARP-BANK-ONE-ACCT-NBR(11:8) TO PPS-ACCT-NBR.            02270043
022800     MOVE ARP-CHECK-NBR(12:7)         TO PPS-FIRST-CHECK-NBR.     02280043
022900     MOVE ARP-CHECK-DATE              TO PV-FORMAT-CHECK-DATE.    02290043
023000     MOVE PV-FORMAT-CHECK-MM          TO PV-DISPLAY-CHECK-MM.     02300043
023100     MOVE PV-FORMAT-CHECK-DD          TO PV-DISPLAY-CHECK-DD.     02310043
023200     MOVE PV-FORMAT-CHECK-CCYY        TO PV-DISPLAY-CHECK-CCYY.   02320043
023300     MOVE PV-DISPLAY-CHECK-DATE       TO PPS-FIRST-CHECK-DATE.    02330043
023400                                                                  02340043
023500 1100-PRINT-HEADING-LINES.                                        02350043
023600     MOVE 1               TO DP075O-PAGE-NUMBER.                  02360043
023700     MOVE 1               TO DP075O-REPORT-NUMBER.                02370043
023800     MOVE PC-PROGRAM-NAME TO DP075O-PROGRAM-NAME.                 02380043
023900                                                                  02390043
024000     WRITE POSITIVE-PAY-SUMMARY-REC                               02400043
024100        FROM DP075O-STANDARD-HEADER-75-COLS.                      02410043
024200                                                                  02420043
024300     INITIALIZE DP075O-LASER-NEW-PAGE-INDICATR.                   02430043
024400                                                                  02440043
024500     WRITE POSITIVE-PAY-SUMMARY-REC                               02450043
024600        FROM POSITIVE-PAY-HEADER-RPT-LINE                         02460043
024700        AFTER ADVANCING 2 LINES.                                  02470043
024800                                                                  02480043
024900 1200-CREATE-SUMMARY-FILE.                                        02490043
025000     IF ARP-RECORD-TYPE = 'O'                                     02500043
025100        ADD 1             TO PV-ARP-NON-VOIDS                     02510043
025200        ADD ARP-CHECK-AMT TO PV-ARP-NON-VOID-AMT                  02520043
025300     ELSE                                                         02530043
025400        ADD 1             TO PV-ARP-VOIDS                         02540043
025500        ADD ARP-CHECK-AMT TO PV-ARP-VOID-AMT                      02550043
025600     END-IF.                                                      02560043
025700                                                                  02570043
025800     ADD 1                TO PV-ARP-TOTAL-CHECKS.                 02580043
025900                                                                  02590043
026000     PERFORM 1250-READ-POSITIVE-PAY-INPUT.                        02600043
026100                                                                  02610043
026200 1250-READ-POSITIVE-PAY-INPUT.                                    02620043
026300     READ POSITIVE-PAY-FILE INTO ARP-STANDARD-RECORD-LAYOUT       02630043
026400         AT END                                                   02640043
026500            SET PS-END-OF-FILE TO TRUE                            02650043
026600     END-READ.                                                    02660043
026700                                                                  02670043
026800 1300-WRITE-SUMMARY-REPORT.                                       02680043
026900     MOVE ARP-CHECK-NBR(12:7)    TO PPS-LAST-CHECK-NBR.           02690043
027000                                                                  02700043
027100     MOVE ARP-CHECK-DATE         TO PV-FORMAT-CHECK-DATE.         02710043
027200     MOVE PV-FORMAT-CHECK-MM     TO PV-DISPLAY-CHECK-MM.          02720043
027300     MOVE PV-FORMAT-CHECK-DD     TO PV-DISPLAY-CHECK-DD.          02730043
027400     MOVE PV-FORMAT-CHECK-CCYY   TO PV-DISPLAY-CHECK-CCYY.        02740043
027500     MOVE PV-DISPLAY-CHECK-DATE  TO PPS-LAST-CHECK-DATE.          02750043
027600                                                                  02760043
027700     IF PPS-FIRST-CHECK-NBR = '0000000'                           02770043
027800        MOVE ZEROES               TO PPS-TOTAL-NON-VOIDS          02780043
027900     ELSE                                                         02790043
028000        MOVE PV-ARP-NON-VOIDS TO PPS-TOTAL-NON-VOIDS              02800043
028100        MOVE PV-ARP-VOIDS     TO PPS-TOTAL-VOIDS                  02810043
028200     END-IF.                                                      02820043
028300                                                                  02830043
028400     MOVE PV-ARP-NON-VOID-AMT     TO PPS-TOTAL-NON-VOID-AMT.      02840043
028500     MOVE PV-ARP-VOID-AMT         TO PPS-TOTAL-VOID-AMT.          02850043
028600                                                                  02860043
028700     MOVE PV-ARP-TOTAL-CHECKS     TO PPS-TOTAL-CHECKS.            02870043
028800                                                                  02880043
028900     MOVE SPACES TO POSITIVE-PAY-SUMMARY-REC.                     02890043
