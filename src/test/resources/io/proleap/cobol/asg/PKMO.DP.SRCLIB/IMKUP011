               END-IF                                                   02770024
               DISPLAY 'OPERCO NBR         ' IM007-TUPCPLS-OPERCO-NBR   08130024
               DISPLAY 'STORE NBR          ' IM007-TUPCPLS-STR-NBR      08140024
               DISPLAY 'BEGIN TIME STAMP   ' IM007-TUPCPLS-EFF-BEG-TMST 08150024
