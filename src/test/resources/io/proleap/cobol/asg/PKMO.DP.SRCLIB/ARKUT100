022000*--------------*                                                  00312099
022100 0000-MAINLINE.                                                   00313099
022200*--------------*                                                  00314099
022300                                                                  00315099
022400     PERFORM Z050-INITIALIZE.                                     00316099
022500                                                                  00317099
022600     PERFORM 1000-PROCESS-CARD-MASTER-FILE.                       00318099
022700                                                                  00319099
022800     PERFORM Z100-END-PROC.                                       00320099
022900                                                                  00321099
023000     STOP RUN.                                                    00322099
023100                                                                  00323099
023200*-------------------------------*                                 00324099
023300 1000-PROCESS-CARD-MASTER-FILE.                                   00325099
023400*-------------------------------*                                 00326099
023500     SET PS-CONTINUE-PROCESSING TO TRUE.                          00327099
023600                                                                  00328099
023700     PERFORM 4000-READ-CARD-MASTER.                               00329099
023800                                                                  00330099
023900     PERFORM 2000-PROCESS-CARD-MASTER                             00331099
024000       UNTIL PS-STOP-PROCESSING.                                  00332099
024100                                                                  00333099
024200*-------------------------------*                                 00334099
024300 2000-PROCESS-CARD-MASTER.                                        00335099
024400*-------------------------------*                                 00336099
024500     EVALUATE TRUE                                                00337099
024600        WHEN IN-MSTR-RECORD                                       00338099
C25106           MOVE IN-MASTER-RECORD(1:PV-IN-REC-LENGTH)              00340299
C25106                                    TO AR69-MASTER-DATA           00340399
024800           PERFORM 3000-PROCESS-MSTR-REC                          00340499
MFRETR           PERFORM 4200-PROCESS-MSTR-REC                          00341099
MFRETR           PERFORM 4205-PROCESS-MSTR-REC                          00342099
MFRETR           PERFORM 4400-PROCESS-MSTR-REC                          00343099
MFRETR           PERFORM 4600-PROCESS-MSTR-REC                          00344099
MFRETR           PERFORM 4800-PROCESS-MSTR-REC                          00345099
MFRETR           PERFORM 4805-PROCESS-MSTR-REC                          00346099
MFRETR           PERFORM 5000-PROCESS-MSTR-REC                          00347099
MFRETR           PERFORM 5025-PROCESS-MSTR-REC                          00348099
MFRETR           PERFORM 5050-PROCESS-MSTR-REC                          00349099
MFRETR           PERFORM 5100-PROCESS-MSTR-REC                          00350099
MFRETR           PERFORM 5200-PROCESS-MSTR-REC                          00351099
MFRETR           PERFORM 5250-PROCESS-MSTR-REC                          00352099
MFRETR           PERFORM 5300-PROCESS-MSTR-REC                          00353099
MFRETR           PERFORM 5400-PROCESS-MSTR-REC                          00354099
024900        WHEN IN-MTHO-RECORD                                       00355099
C25106           MOVE IN-MASTER-RECORD(1:PV-IN-REC-LENGTH)              00356299
C25106                                    TO AR71-OVRR-RECORD           00356399
025100           PERFORM 3300-PROCESS-MTHO-REC                          00357099
C90176        WHEN IN-FLAP-RECORD                                       00358099
C25106           MOVE IN-MASTER-RECORD(1:PV-IN-REC-LENGTH)              00359399
C25106                                    TO AR93-RECORD                00359499
C90176           PERFORM 3400-PROCESS-FLAP-REC                          00360099
025200     END-EVALUATE.                                                00361099
025300                                                                  00362099
025400     PERFORM 4000-READ-CARD-MASTER.                               00363099
025500     MOVE SPACES                 TO CARD-MASTER-OUT-REC.          00364099
025600                                                                  00365099
025700*-------------------------------*                                 00366099
025800 3000-PROCESS-MSTR-REC.                                           00367099
025900*-------------------------------*                                 00368099
026000                                                                  00369099
026100     MOVE AR69-RECORD-IDENTIFIER TO AR70-RECORD-IDENTIFIER.       00370099
026200                                                                  00371099
026300*************************************************************     00372099
026400* BASE AND CUST SEGS ARE REQUIRED ON ALL MSTR RECORDS.            00373099
026500*************************************************************     00374099
026600     MOVE AR69-BASE-SEGMENT      TO AR70-BASE-DATA-SEGMENT.       00375099
026700                                                                  00376099
026800     MOVE AR69-RECORD-ID          TO AR70-RECORD-ID               00377099
026900     MOVE AR69-NEW-FILE-TYPE      TO AR70-NEW-FILE-TYPE           00378099
027000     MOVE AR69-RECORD-FORMAT      TO WS-EDIT-N1                   00379099
027100     MOVE WS-EDIT-N1              TO AR70-RECORD-FORMAT           00380099
027200     MOVE AR69-BRANCH-NO          TO AR70-BRANCH-NO               00381099
027300     MOVE AR69-CUST-NO-BS         TO AR70-CUST-NO-BS              00382099
027400     MOVE AR69-CUST-NO-AL         TO AR70-CUST-NO-AL              00383099
027500     MOVE AR69-CLIENT-NUMBER      TO AR70-CLIENT-NUMBER           00384099
027600     MOVE AR69-FULL-ACCT-NO       TO AR70-FULL-ACCT-NO            00385099
027700     MOVE AR69-ACCOUNT-NO-EXPN    TO AR70-ACCOUNT-NO-EXPN         00386099
027800     IF AR69-CIT-TOTL-CT           > 0                            00387099
027900        MOVE AR69-CIT-TOTL-CT     TO WS-EDIT-S4                   00388099
028000        MOVE WS-EDIT-S4           TO WS-EDIT-N4                   00389099
028100        MOVE WS-EDIT-N4           TO AR70-CIT-TOTL-CT             00390099
028200     ELSE                                                         00391099
028300        MOVE ZEROES               TO AR70-CIT-TOTL-CT             00392099
028400     END-IF                                                       00393099
028500     IF AR69-NO-DEL-ITEMS          > 0                            00394099
