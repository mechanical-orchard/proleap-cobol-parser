154600* CARTON CURSOR FOR CALCULATING AUDIT DURATION.                   15460000
154700                                                                  15470000
154800     EXEC SQL                                                     15480000
154900         DECLARE CARTON-CSR CURSOR FOR                            15490000
155000             SELECT                                               15500000
155100                ( (( DAYS(AUD_STP_TMST) -                         15510000
155200                     DAYS(AUD_STRT_TMST) )                        15520000
155300                     * 1440)                                      15530000
155400             +                                                    15540000
155500                  (( HOUR(AUD_STP_TMST) -                         15550000
155600                     HOUR(AUD_STRT_TMST) )                        15560000
155700                     * 60 )                                       15570000
155800             +                                                    15580000
155900                  ( MINUTE(AUD_STP_TMST) -                        15590000
156000                    MINUTE(AUD_STRT_TMST) )                       15600000
156100                                                                  15610000
156200             -      AUD_PSPND_MNIT_QTY                            15620000
156300             -      AUD_CLO_MNIT_QTY )                            15630000
156400             FROM                                                 15640000
156500                    TSUNAUD                                       15650000
156600             WHERE                                                15660000
156700                    AUD_CNTL_NBR  = :DK-AUD-CNTL-NBR              15670000
156800               AND  AUD_STP_TMST <> :PC-DEFAULT-TMST              15680000
156810           WITH UR                                                15681018
156900     END-EXEC                                                     15690000
157000                                                                  15700000
157100     EXEC SQL                                                     15710000
157200          DECLARE MFST-CTNS-CSR CURSOR FOR                        15720000
157300             SELECT CART_QTY                                      15730000
157400               FROM SUT_TRIP_SUM                                  15740000
157500              WHERE OTBND_TRIP_ID = :DK-ORIG-OTBND-TRIP-ID        15750000
157600                AND CRE_TMST     <= :SUNAUD-AUD-STRT-TMST         15760000
157700           ORDER BY CRE_TMST DESC                                 15770000
157710           WITH UR                                                15771018
157800     END-EXEC.                                                    15780000
