205010                 ,  COUNT(*)                                      20501031
205100              INTO :PV-AUD-CRTN-ERR-CNT                           20510000
205110                 , :PV-AUD-EXC-ERR-CNT                            20511031
205200              FROM  SUT_AUD_TRLR_EXC                              20520000
205300             WHERE  AUD_CNTL_NBR  = :DK-AUD-CNTL-NBR              20530034
205400               AND  ERR_CDE <> 'G'                                20540000
205410            WITH UR                                               20541018
205500         END-EXEC                                                 20550000
205600                                                                  20560000
