      ******************************************************************02700000
       Z220-FETCH-TWKMCLC.                                              02710021
      ******************************************************************02720000
W31475*                                                                 02731024
  |   *    EXEC SQL                                                     02740024
  |   *        FETCH TWKMCLC_CURSOR                                     02750024
  |   *         INTO :WKMCLC-DEPT-NBR                                   02760024
  |   *             ,:WKMCLC-MAJ-CL-NBR                                 02770024
  |   *             ,:WKMCLC-CHNL-CDE                                   02780024
  |   *    END-EXEC.                                                    02790024
  |        INITIALIZE HOST-VARIABLES.                                   02790124
  |                                                                     02790224
  |        EXEC SQL                                                     02791024
  |            FETCH NEXT ROWSET FROM TWKMCLC_CURSOR FOR 100 ROWS       02792024
  |             INTO  :HV-WKMCLC-DEPT-NBR                               02793024
  |                  ,:HV-WKMCLC-MAJ-CL-NBR                             02794024
  |                  ,:HV-WKMCLC-CHNL-CDE                               02795024
W31475     END-EXEC.                                                    02796024
                                                                        02800000
           EVALUATE TRUE                                                02810000
               WHEN SQLCODE = 0                                         02820000
W31475*            ADD +1              TO PA-TWKMCLC-ROWS-FETCHED       02830026
W31475             ADD +100            TO PA-TWKMCLC-ROWS-FETCHED       02831026
               WHEN SQLCODE = +100                                      02840000
                   MOVE 'Y'            TO PS-TWKMCLC-DONE               02850021
W31475             ADD SQLERRD (3)     TO PA-TWKMCLC-ROWS-FETCHED       02850124
  |                PERFORM VARYING ROW-ID FROM 1 BY 1                   02851024
  |                UNTIL ROW-ID > SQLERRD (3)                           02852024
  |                     PERFORM C100-WRITE-EXTRACT                      02853024
W31475             END-PERFORM                                          02854024
               WHEN SQLCODE < 0                                         02860000
                   MOVE 'Z220-FETCH-TWKMCLC'                            02870021
                                       TO AA-PARAGRAPH-NAME             02880000
                   MOVE 'TWKMCLC TABLE ERROR'                           02890021
                                       TO AA-MESSAGE                    02900000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02910000
                   PERFORM Z998-SQL-ERROR                               02920000
               WHEN ((SQLCODE > 0) AND (SQLCODE NOT = +100))            02930000
               WHEN SQLWARN0 NOT = SPACES                               02940000
                   MOVE 'Z220-FETCH-TWKMCLC'                            02950021
                                       TO AA-PARAGRAPH-NAME             02960000
                   MOVE 'TWKMCLC TABLE WARNING'                         02970021
                                       TO AA-MESSAGE                    02980000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02990000
                   PERFORM Z998-SQL-ERROR                               03000000
           END-EVALUATE.                                                03010000
                                                                        03020000
       EJECT                                                            03030000
