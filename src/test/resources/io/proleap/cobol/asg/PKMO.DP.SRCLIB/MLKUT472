      ******************************************************************01370000
      *  DB2 TWKMCLC CURSOR                                             01380021
      ******************************************************************01390000
           EXEC SQL                                                     01400000
W31475         DECLARE TWKMCLC_CURSOR CURSOR                            01410024
W31475         WITH ROWSET POSITIONING FOR                              01411024
               SELECT DISTINCT DEPT_NBR                                 01420000
                              ,MAJ_CL_NBR                               01430000
                              ,CHNL_CDE                                 01440021
               FROM TWKMCLC                                             01450021
                   ,TMLCNTL                                             01460012
               WHERE (FSCL_WK_END_DTE BETWEEN                           01470001
WEEK                               OPN_FSCL_WK_DTE                      01480012
WEEK                           AND MXPSTG_FSCL_WK_DTE) OR               01490012
                    ((FSCL_WK_END_DTE BETWEEN                           01500008
                      :DTATTR-PFM-END-DTE AND                           01510008
                      MXPSTG_FSCL_MN_DTE) AND                           01520008
                       (END_INV_RTL_AMT  <> 0  OR                       01530008
                        END_INV_COST_AMT <> 0))                         01540008
               ORDER BY DEPT_NBR, MAJ_CL_NBR, CHNL_CDE                  01550021
           END-EXEC.                                                    01560000
                                                                        01570000
       EJECT                                                            01580000
                                                                        01590000
