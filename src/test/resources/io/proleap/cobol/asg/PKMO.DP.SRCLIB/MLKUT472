                                                                        02490000
           EVALUATE TRUE                                                02500000
               WHEN SQLCODE < 0                                         02510000
                   MOVE 'Z210-OPEN-CURSOR-TWKMCLC'                      02520021
                                       TO AA-PARAGRAPH-NAME             02530000
                   MOVE 'TWKMCLC TABLE ERROR'                           02540021
                                       TO AA-MESSAGE                    02550000
