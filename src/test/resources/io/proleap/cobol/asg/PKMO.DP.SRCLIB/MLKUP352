      ******************************************************************02120000
      * OPEN INPUT FILE , INITIALIZE COUNTERS , READ FIRST INPUT RECORD 02130000
      * PREPARE CHECKPOINT RESTART TABLES FOR PROCESSING / RESTART      02140000
      ******************************************************************02150000
      *                                                                 02160000
       1000-INITIALIZATION.                                             02170000
      *                                                                 02180000
           OPEN INPUT CONDENSED-FILE.                                   02190000
                                                                        02200000
           PERFORM 7400-INIT-CHECKPOINT-SELECT.                         02210000
                                                                        02220000
      * CKPT-STAT-CODE = '9' INDICATES A RESTART CONDITION              02230000
           IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                           02240000
               PERFORM 7500-SELECT-RESTART-INFO                         02250000
               MOVE TCKRSTINF-WORK-STRG-DESC-TEXT TO                    02260000
                                            CR-CHECKPOINT-RESTART-VAR   02270000
               MOVE CR-PREV-DTL-KEY          TO CK-SAVE-PREV-KEY        02280000
               DISPLAY 'CHECKPOINT RESTART INFORMATION:'                02290000
               DISPLAY 'DEPARTMENT NBR  ' CR-DEPT-NBR                   02320000
               DISPLAY 'MAJOR CLASS NBR ' CR-MAJ-CL-NBR                 02330000
               DISPLAY 'SUB CLASS NBR   ' CR-SUB-CL-NBR                 02340081
               DISPLAY 'STORE NBR       ' CR-LOC-NBR                    02340181
W28582         DISPLAY 'ML LWST ID      ' CR-ML-LWST-ID                 02340299
W28582***      DISPLAY 'ENT-ID          ' CR-ENT-ID                     02340399
               DISPLAY 'INPUT RECS READ ' CR-INPUT-READ-COUNT           02350000
           ELSE                                                         02360000
               SET PS-FIRST-COMMIT TO TRUE                              02370000
               INITIALIZE CR-CHECKPOINT-RESTART-VAR                     02380000
           END-IF.                                                      02390000
      *                                                                 02400000
           PERFORM 4000-READ-CONDENSED-FILE UNTIL                       02410000
               PA-INPUT-COUNT > CR-INPUT-READ-COUNT                     02420000
            OR PS-EOF-CONDENSED-FILE.                                   02430000
      *                                                                 02440000
           IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                           02450000
               DISPLAY 'INPUT FILE RESTARTED ON RECORD '                02460000
                        PA-INPUT-COUNT                                  02470000
               DISPLAY 'PARTITION NUMBER      ' ML155-WK-PARTN-NBR      02480099
               DISPLAY 'DEPARTMENT NBR        ' ML155-DEPT-NBR          02500087
               DISPLAY 'MAJOR CLASS NBR       ' ML155-MAJ-CL-NBR        02510087
               DISPLAY 'SUB CLASS NBR         ' ML155-SUB-CL-NBR        02511087
               DISPLAY 'LOCATION NBR          ' ML155-LOC-NBR           02511187
W28582         MOVE ML155-ML-LWST-ID TO PV-ML-LWST-ID                   02511299
W28582         DISPLAY 'ML LWST ID            ' PV-ML-LWST-ID           02511399
W28582***      MOVE ML155-ENT-ID   TO PV-ENT-ID                         02511499
W28582***      DISPLAY 'ENT-ID                ' PV-ENT-ID               02511699
           END-IF.                                                      02540000
                                                                        02550000
           IF PS-EOF-CONDENSED-FILE                                     02560000
               IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                       02570000
                   CONTINUE                                             02580000
               ELSE                                                     02590000
                   DISPLAY '** NO RECORDS ON INPUT FILE **'             02600000
           ELSE                                                         02610000
               PERFORM 7300-GET-COMMIT-TIMESTAMP                        02620000
               DISPLAY 'COMMITS WILL BE TAKEN EVERY ',                  02630000
                    TCKRSTCNTL-CKPT-FRQNCY-QTY, ' SECONDS.'             02640000
           END-IF.                                                      02650000
                                                                        02660000
      ******************************************************************02670000
      * PROCESS THE SORTED/CONDENSED FILE - UPDATE OR INSERT AS NEEDED  02680000
      ******************************************************************02690000
       2000-PROCESS-INPUT.                                              02700000
      *                                                                 02710000
           MOVE 'N' TO PS-DEADLOCK-RESTART-SW.                          02720000
      *                                                                 02730000
           PERFORM 7000-SELECT-DB-ROW.                                  02740000
      *                                                                 02750000
           IF PS-PROGRAM-DEADLOCKED                                     02760000
               CONTINUE                                                 02770000
           ELSE                                                         02780000
               EVALUATE TRUE                                            02790000
                   WHEN PI-UPDATE-MLT00005-ROW                          02800062
                       PERFORM 7100-UPDATE-MLT00005-ROW                 02810062
                   WHEN PI-INSERT-MLT00005-ROW                          02820062
                       PERFORM 7200-INSERT-MLT00005-ROW                 02830062
                   WHEN OTHER                                           02840000
                       MOVE '7000-SELECT-DB-ROW' TO PV-PARAGRAPH-NAME   02850000
                       MOVE PE-MSG-01 TO PV-OPERATION-ATTEMPTED         02860000
                       PERFORM 9999-SQL-ABEND                           02870000
               END-EVALUATE                                             02880000
           END-IF.                                                      02890000
      *                                                                 02900000
           IF PS-PROGRAM-DEADLOCKED                                     02910000
               CONTINUE                                                 02920000
           ELSE                                                         02930000
               PERFORM 7700-COMMIT-PROCESSING                           02940000
               PERFORM 4000-READ-CONDENSED-FILE                         02950000
           END-IF.                                                      02960000
      *                                                                 02970000
      *                                                                 02980000
      ******************************************************************02990000
      * CALCULATE FIELDS FOR UPDATE OF DATABASE                         03000000
      ******************************************************************03010000
       2100-CALCULATE-UPDATES.                                          03020000
      * ADD DB2 TABLE ROW VALUES TO CURRENT INPUT RECORD ROW VALUES     03030000
      *                                                                 03040000
           COMPUTE SV-SLS-REG-RTL-AMT =                                 03050000
                   MLT00005-SLS-REG-RTL-AMT + ML155-SLS-REG-RTL-AMT.    03060087
           COMPUTE SV-SLS-PROMO-RTL-AMT =                               03070000
                   MLT00005-SLS-PROMO-RTL-AMT + ML155-SLS-PROMO-RTL-AMT.03080087
           COMPUTE SV-SLS-PROCLR-RTL-AMT =                              03090000
                 MLT00005-SLS-PROCLR-RTL-AMT + ML155-SLS-PROCLR-RTL-AMT.03100087
           COMPUTE SV-SLS-CLR-RTL-AMT =                                 03110000
                   MLT00005-SLS-CLR-RTL-AMT + ML155-SLS-CLR-RTL-AMT.    03120087
           COMPUTE SV-SLS-OTH-RTL-AMT =                                 03130000
                   MLT00005-SLS-OTH-RTL-AMT + ML155-SLS-OTH-RTL-AMT.    03140087
           COMPUTE SV-MKDN-PLU-RTL-AMT  =                               03160000
                   MLT00005-MKDN-PLU-RTL-AMT + ML155-MKDN-PLU-RTL-AMT.  03170087
           COMPUTE SV-MKDN-RGST-RTL-AMT =                               03191031
                   MLT00005-MKDN-RGST-RTL-AMT + ML155-MKDN-RGST-RTL-AMT.03192087
           COMPUTE SV-MKDN-UMTCH-RTL-AMT =                              03200000
                 MLT00005-MKDN-UMTCH-RTL-AMT + ML155-MKDN-UMTCH-RTL-AMT.03210087
           COMPUTE SV-MKDN-BYR-RTL-AMT  =                               03220000
                   MLT00005-MKDN-BYR-RTL-AMT + ML155-MKDN-BYR-RTL-AMT.  03230087
           COMPUTE SV-MKDN-STR-RTL-AMT  =                               03240000
