      /*****************************************************************02220008
       Z210-OPEN-CURSOR-TMNDPTC.                                        02230010
      ******************************************************************02240000
      *                                                                 02250000
           EXEC SQL                                                     02260000
               OPEN TMNDPTC_CURSOR                                      02270010
           END-EXEC.                                                    02280000
                                                                        02290000
           EVALUATE TRUE                                                02300000
               WHEN SQLCODE < 0                                         02310000
                   MOVE 'Z210-OPEN-CURSOR-TMNDPTC'                      02320010
                                       TO AA-PARAGRAPH-NAME             02330000
                   MOVE 'TMNDPTC TABLE ERROR'                           02340010
                                       TO AA-MESSAGE                    02350000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02360000
                   PERFORM Z998-SQL-ERROR                               02370000
               WHEN SQLCODE > 0                                         02380000
               WHEN SQLWARN0 NOT = SPACES                               02390000
                   MOVE 'Z210-OPEN-CURSOR-TMNDPTC'                      02400010
                                       TO AA-PARAGRAPH-NAME             02410000
                   MOVE 'TMNDPTC TABLE WARNING'                         02420010
                                       TO AA-MESSAGE                    02430000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02440000
                   PERFORM Z998-SQL-ERROR                               02450000
           END-EVALUATE.                                                02460000
                                                                        02470000
      /*****************************************************************02500019
       Z220-FETCH-TMNDPTC.                                              02510010
      ******************************************************************02520000
W31475*                                                                 02531019
  |   *    EXEC SQL                                                     02540019
  |   *        FETCH TMNDPTC_CURSOR                                     02550019
  |   *         INTO :MNDPTC-DEPT-NBR                                   02560019
  |   *             ,:MNDPTC-CHNL-CDE                                   02580019
  |   *    END-EXEC.                                                    02590019
  |        INITIALIZE HOST-VARIABLES.                                   02590119
  |                                                                     02590219
  |        EXEC SQL                                                     03180319
  |            FETCH NEXT ROWSET FROM TMNDPTC_CURSOR FOR 100 ROWS       03180419
  |             INTO :HV-MNDPTC-DEPT-NBR                                03180519
  |                 ,:HV-MNDPTC-CHNL-CDE                                03180619
W31475     END-EXEC.                                                    03180719
                                                                        03180816
           EVALUATE TRUE                                                03180916
               WHEN SQLCODE = 0                                         03181016
W31475*            ADD +1              TO PA-TMNDPTC-ROWS-FETCHED       03181121
W31475             ADD +100            TO PA-TMNDPTC-ROWS-FETCHED       03181221
               WHEN SQLCODE = +100                                      03181321
                   MOVE 'Y'            TO PS-TMNDPTC-DONE               03181418
W31475             ADD SQLERRD (3)     TO PA-TMNDPTC-ROWS-FETCHED       03181519
  |                PERFORM VARYING ROW-ID FROM 1 BY 1                   03181619
  |                UNTIL ROW-ID > SQLERRD (3)                           03181719
  |                     PERFORM C100-WRITE-EXTRACT                      03181819
W31475             END-PERFORM                                          03181919
               WHEN SQLCODE < 0                                         03182116
                   MOVE 'Z220-FETCH-TMNDPTC'                            03182216
                                       TO AA-PARAGRAPH-NAME             03182316
                   MOVE 'TMNDPTC TABLE ERROR'                           03182416
                                       TO AA-MESSAGE                    03182516
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              03182616
                   PERFORM Z998-SQL-ERROR                               03182716
               WHEN ((SQLCODE > 0) AND (SQLCODE NOT = +100))            03182816
               WHEN SQLWARN0 NOT = SPACES                               03182916
                   MOVE 'Z220-FETCH-TMNDPTC'                            03183016
                                       TO AA-PARAGRAPH-NAME             03183116
                   MOVE 'TMNDPTC TABLE WARNING'                         03183216
                                       TO AA-MESSAGE                    03183316
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              03183416
                   PERFORM Z998-SQL-ERROR                               03183516
           END-EVALUATE.                                                03183616
                                                                        03183716
      /*****************************************************************03183816
       Z230-CLOSE-CURSOR-TMNDPTC.                                       03183916
      ******************************************************************03184016
      *                                                                 03184116
           EXEC SQL                                                     03184216
               CLOSE TMNDPTC_CURSOR                                     03184316
