011700*--------------------------------------------------------------*  01170000
011800     EXEC SQL                                                     01180000
011900          INCLUDE TENTNME                                         01190000
012000     END-EXEC.                                                    01200000
012100                                                                  01210000
012200*--------------------------------------------------------------*  01220000
012300*  COMMUNICATIONS AREA2 FOR DB2                                *  01230000
012400*--------------------------------------------------------------*  01240000
012500     COPY SQLCA2.                                                 01250000
012600*--------------------------------------------------------------*  01260000
012700*     SQL COMMUNICATIONS AREA DCLGEN                           *  01270000
012800*--------------------------------------------------------------*  01280000
012900     EXEC SQL                                                     01290000
013000         INCLUDE SQLCA                                            01300000
013100     END-EXEC.                                                    01310000
013200                                                                  01320000
013300*----------------------------------------------------------------*01330000
013400*    OPEN A CURSOR TO RETRIEVE ALL ACTIVE MERCHANDISE AND EXPENSE*01340000
013500*    VENDORS THAT HAVE A CURRENT OR PREVIOUS FISCAL YEAR SALES   *01350000
013600*    RANK GREATER THAN ZERO.                                     *01360000
013700*----------------------------------------------------------------*01370000
013800     EXEC SQL                                                     01380000
013900          DECLARE VENDOR_CSR CURSOR FOR                           01390000
014000           SELECT   A.ENT_ID                                      01400000
014100                   ,A.DUN_NBR                                     01410000
014200                   ,B.ENT_NAME_DESC                               01420000
014300                   ,A.PRIM_TYP_CDE                                01430000
014400                   ,A.CFY_SLSRNK_NBR                              01440000
014500                   ,A.PFY_SLSRNK_NBR                              01450000
014600                   ,'  '                                          01460000
014700                   ,' '                                           01470000
014800             FROM   TEXTENT A                                     01480000
014900                   ,TENTNME B                                     01490000
015000            WHERE   A.ENT_ID         = B.ENT_ID                   01500000
015100              AND   B.TYPE_CDE       = '01'                       01510000
015200              AND   A.PRIM_TYP_CDE   = 'M'                        01520000
015300              AND   A.STATUS_CDE     = 'A'                        01530000
015400              AND ((A.CFY_SLSRNK_NBR > 0) OR                      01540000
015500                   (A.PFY_SLSRNK_NBR > 0))                        01550000
015600                                                                  01560000
015700             UNION                                                01570000
015800                                                                  01580000
015900           SELECT   A.ENT_ID                                      01590000
016000                   ,A.DUN_NBR                                     01600000
016100                   ,B.ENT_NAME_DESC                               01610000
016200                   ,A.PRIM_TYP_CDE                                01620000
016300                   ,0                                             01630000
016400                   ,0                                             01640000
016500                   ,A.PRTNR_TYP_CDE                               01650000
016600                   ,A.CAP_EXP_CDE                                 01660000
016700             FROM   TEXTENT A                                     01670000
016800                   ,TENTNME B                                     01680000
016900            WHERE   A.ENT_ID         = B.ENT_ID                   01690000
017000              AND   B.TYPE_CDE       = '01'                       01700000
017100              AND   A.PRIM_TYP_CDE   = 'E'                        01710000
017200              AND   A.STATUS_CDE     = 'A'                        01720000
017300              AND   A.VND_CLSN_CDE NOT IN ('E')                   01730000
017301*PRB0059571 START                                                 01730110
017310              AND VND_PRSST_CDE <> 'O'                            01731010
017320              AND PAYT_HNDLG_CDE NOT IN ('AS','AT','KC','PR',     01732011
017321                                          'RT','TX','EM','NP')    01732110
017330              AND PRTNR_TYP_CDE NOT IN ('FA','CL','CR','TM')      01733010
017340              AND NEED_1099_IND = 'N'                             01734010
017350*PRB0059571 END                                                   01735010
017400     END-EXEC.                                                    01740000
017500                                                                  01750000
017600 LINKAGE SECTION.                                                 01760000
017700************************                                          01770000
017800 PROCEDURE DIVISION.                                              01780000
017900************************                                          01790000
018000 A100-MAINLINE-ROUTINE.                                           01800000
018100                                                                  01810000
018200     PERFORM B100-INITIALIZATION.                                 01820000
018300                                                                  01830000
018400     PERFORM B200-PROCESS-VENDOR-CSR                              01840000
018500       UNTIL END-OF-VENDOR-CSR.                                   01850000
018600                                                                  01860000
018700     PERFORM B300-TERMINATION.                                    01870000
018800     GOBACK.                                                      01880000
018900                                                                  01890000
019000*--------------------------------------------------------------*  01900000
019100*  INITIALIZATION PROCESSING                                   *  01910000
019200*--------------------------------------------------------------*  01920000
019300 B100-INITIALIZATION.                                             01930000
019400                                                                  01940000
019500     DISPLAY ' '.                                                 01950000
019600     DISPLAY '**--------------- VNKUT019 --------------------**'. 01960003
019700     DISPLAY 'DATE ' FUNCTION CURRENT-DATE(1:8)                   01970000
019800           '  TIME ' FUNCTION CURRENT-DATE(9:6)                   01980000
019900                                                                  01990000
020000     OPEN OUTPUT VENDOR-EXTRACT-FILE.                             02000004
020100                                                                  02010000
020200                                                                  02020000
020300     INITIALIZE DCLTEXTENT                                        02030000
020400                DCLTENTNME                                        02040000
020500                AA-DB2-TABLE-1                                    02050000
020600                AA-DB2-TABLE-2                                    02060000
020700                AA-DB2-TABLE-3.                                   02070000
020800                                                                  02080000
020900     PERFORM X100-OPEN-VENDOR-CSR.                                02090000
021000                                                                  02100000
021100     PERFORM F100-FETCH-VENDOR-CSR.                               02110000
021200     IF END-OF-VENDOR-CSR                                         02120000
021300         DISPLAY '***  VNKUT019  ***'                             02130003
021400         DISPLAY 'FIRST FETCH OF VENDOR CURSOR     '              02140000
021500         DISPLAY 'NO VENDOR DATA TO PROCESS  '                    02150000
021600         DISPLAY ' '                                              02160000
021700     END-IF.                                                      02170000
021800                                                                  02180000
021900*--------------------------------------------------------------*  02190000
022000* PROCESS THE VENDOR CURSOR                                    *  02200000
022100*--------------------------------------------------------------*  02210000
022200 B200-PROCESS-VENDOR-CSR.                                         02220000
022300                                                                  02230000
022400     INITIALIZE VN010-VENDOR-EXTRACT-REC.                         02240003
022500                                                                  02250003
022600     MOVE EXTENT-ENT-ID         TO VN010-ENT-ID.                  02260003
022700     MOVE EXTENT-DUN-NBR        TO VN010-DUN-NBR.                 02270003
022800     MOVE ENTNME-ENT-NAME-DESC  TO VN010-ENT-NAME-DESC.           02280003
022900     MOVE EXTENT-PRIM-TYP-CDE   TO VN010-PRIM-TYP-CDE.            02290003
023000     IF EXTENT-PRIM-TYP-CDE = 'M'                                 02300006
023100        SET VN010-MDSE-VENDOR      TO TRUE                        02310006
023200        SET VN010-MERCH-ACTIVE     TO TRUE                        02320006
023300     ELSE                                                         02330006
023400        SET VN010-EXP-VENDOR       TO TRUE                        02340006
023500        SET VN010-EXPEN-INACTIVE   TO TRUE                        02350006
023600     END-IF.                                                      02360006
023700     MOVE EXTENT-CFY-SLSRNK-NBR TO VN010-CFY-SLSRNK-NBR.          02370003
023800     MOVE EXTENT-PFY-SLSRNK-NBR TO VN010-PFY-SLSRNK-NBR.          02380003
023900     MOVE EXTENT-PRTNR-TYP-CDE  TO VN010-PRTNR-TYP-CDE.           02390003
024000     MOVE EXTENT-CAP-EXP-CDE    TO VN010-CAP-EXP-CDE.             02400003
024100*PRB0059571 START                                                 02410017
024110     IF EXTENT-PRIM-TYP-CDE NOT = 'M'                             02411015
024200        PERFORM W100-WRITE-VENDOR-EXTRACT                         02420017
024300     END-IF.                                                      02430015
024301*PRB0059571 END                                                   02430116
024310*WE ARE SKIPPING WRITING MERCHANDISE VENDORS INTO THE OUTPUT FILE 02431016
024320*AT THIS POINT, SINCE IT WOULD BE EASIEST TO REVERT THESE CHANGES 02432016
024330*IN THE FUTURE, IF NEEDED.                                        02433016
024400     PERFORM F100-FETCH-VENDOR-CSR.                               02440000
024500                                                                  02450000
024600                                                                  02460000
024700*--------------------------------------------------------------*  02470000
024800*  END OF PROGRAM PROCESSING                                   *  02480000
024900*--------------------------------------------------------------*  02490000
025000 B300-TERMINATION.                                                02500000
025100                                                                  02510000
025200     DISPLAY '**********************************************'.    02520000
025300     DISPLAY '**         VNKUT019 RECORD COUNTS           **'.    02530003
025400     DISPLAY '**********************************************'.    02540000
025500     DISPLAY ' VENDOR RECS FETCHED:' PV-COUNT-VENDOR-READ.        02550000
025600     DISPLAY ' MERCH VEND WRITTEN :' PV-COUNT-MERCH-RECORD.       02560006
025700     DISPLAY ' EXPEN VEND WRITTEN :' PV-COUNT-EXPEN-RECORD.       02570006
025800     DISPLAY '**********************************************'.    02580000
025900                                                                  02590000
026000     PERFORM X110-CLOSE-VENDOR-CSR.                               02600000
026100                                                                  02610000
026200     CLOSE VENDOR-EXTRACT-FILE.                                   02620003
026300                                                                  02630000
026400*----------------------------------------------------------------*02640000
026500*  RETRIEVES THE VENDOR CURSOR DATA                              *02650000
026600*----------------------------------------------------------------*02660000
026700 F100-FETCH-VENDOR-CSR.                                           02670000
026800                                                                  02680000
026900     EXEC SQL                                                     02690000
027000         FETCH VENDOR_CSR                                         02700000
027100          INTO  :EXTENT-ENT-ID                                    02710000
027200               ,:EXTENT-DUN-NBR                                   02720000
027300               ,:ENTNME-ENT-NAME-DESC                             02730000
027400               ,:EXTENT-PRIM-TYP-CDE                              02740000
027500               ,:EXTENT-CFY-SLSRNK-NBR                            02750000
027600               ,:EXTENT-PFY-SLSRNK-NBR                            02760000
027700               ,:EXTENT-PRTNR-TYP-CDE                             02770000
027800               ,:EXTENT-CAP-EXP-CDE                               02780000
027900     END-EXEC.                                                    02790000
028000                                                                  02800000
028100     EVALUATE TRUE                                                02810000
028200         WHEN SQLCODE = ZERO                                      02820000
028300             ADD 1 TO PV-COUNT-VENDOR-READ                        02830000
028400         WHEN SQLCODE = +100                                      02840000
028500             SET END-OF-VENDOR-CSR TO TRUE                        02850000
028600         WHEN SQLWARN0 NOT = SPACES                               02860000
028700         WHEN SQLCODE  NOT = ZERO                                 02870000
028800             MOVE 'F100-FETCH-VENDOR-CSR' TO AA-PARAGRAPH-NAME    02880000
028900             MOVE 'UNSUCCESSFUL FETCH WITH VENDOR_CSR'            02890000
029000                                        TO AA-DB2-OPERATION       02900000
029100             MOVE 'TEXTENT'             TO AA-DB2-TABLE-1         02910000
029200             MOVE 'TENTNME'             TO AA-DB2-TABLE-2         02920000
029300             MOVE SPACE                 TO AA-DB2-TABLE-3         02930000
029400             PERFORM Z998-DB2-ABEND                               02940000
029500     END-EVALUATE.                                                02950000
029600                                                                  02960000
029700*--------------------------------------------------------------*  02970000
029800*  WRITE OUT A RECORD TO THE VENDOR EXTRACT FILE               *  02980000
029900*--------------------------------------------------------------*  02990000
030000 W100-WRITE-VENDOR-EXTRACT.                                       03000000
030100                                                                  03010000
030200     WRITE VENDOR-EXTRACT-REC FROM VN010-VENDOR-EXTRACT-REC.      03020005
030300                                                                  03030006
030400     IF VN010-PRIM-TYP-CDE = 'M'                                  03040006
030500        ADD 1 TO PV-COUNT-MERCH-RECORD                            03050006
030600     ELSE                                                         03060006
030700        ADD 1 TO PV-COUNT-EXPEN-RECORD                            03070006
030800     END-IF.                                                      03080006
030900                                                                  03090000
031000 X100-OPEN-VENDOR-CSR.                                            03100000
031100*--------------------------------------------------------------*  03110000
031200*    OPEN VENDOR CURSOR.                                       *  03120000
031300*--------------------------------------------------------------*  03130000
031400                                                                  03140000
031500     EXEC SQL                                                     03150000
031600         OPEN VENDOR_CSR                                          03160000
031700     END-EXEC.                                                    03170000
031800                                                                  03180000
031900     EVALUATE TRUE                                                03190000
032000         WHEN SQLCODE = ZERO                                      03200000
032100             CONTINUE                                             03210000
032200         WHEN SQLWARN0 NOT = SPACES                               03220000
032300         WHEN SQLCODE  NOT = ZERO                                 03230000
032400             MOVE 'X100-OPEN-VENDOR-CSR'                          03240000
032500                                 TO AA-PARAGRAPH-NAME             03250000
032600             MOVE 'UNSUCCESSFUL OPEN ON VENDOR_CSR'               03260000
032700                                 TO AA-DB2-OPERATION              03270000
032800             MOVE 'TEXTENT'      TO AA-DB2-TABLE-1                03280000
032900             MOVE 'TENTNME'      TO AA-DB2-TABLE-2                03290000
033000             MOVE SPACE          TO AA-DB2-TABLE-3                03300000
033100             PERFORM Z998-DB2-ABEND                               03310000
033200     END-EVALUATE.                                                03320000
033300                                                                  03330000
033400 X110-CLOSE-VENDOR-CSR.                                           03340000
033500*--------------------------------------------------------------*  03350000
033600*    CLOSE THE VENDOR_CSR CURSOR                               *  03360000
033700*--------------------------------------------------------------*  03370000
033800                                                                  03380000
033900     EXEC SQL                                                     03390000
034000         CLOSE VENDOR_CSR                                         03400000
034100     END-EXEC.                                                    03410000
034200                                                                  03420000
034300       EVALUATE TRUE                                              03430000
034400           WHEN SQLCODE = ZERO                                    03440000
034500                CONTINUE                                          03450000
034600                                                                  03460000
034700           WHEN SQLWARN0 NOT = SPACES                             03470000
034800           WHEN SQLCODE  NOT = ZERO                               03480000
034900               MOVE 'X110-CLOSE-VENDOR-CSR'                       03490000
035000                                   TO AA-PARAGRAPH-NAME           03500000
035100               MOVE 'UNSUCCESSFUL CLOSE ON VENDOR_CSR'            03510000
035200                                   TO AA-DB2-OPERATION            03520000
035300               MOVE 'TEXTENT'      TO AA-DB2-TABLE-1              03530000
035400               MOVE 'TENTNME'      TO AA-DB2-TABLE-2              03540000
035500               MOVE SPACE          TO AA-DB2-TABLE-3              03550000
035600               PERFORM Z998-DB2-ABEND                             03560000
035700       END-EVALUATE.                                              03570000
035800                                                                  03580000
035900*--------------------------------------------------------------*  03590000
036000*  DB2 ABEND ROUTINE                                           *  03600000
036100*--------------------------------------------------------------*  03610000
036200 Z998-DB2-ABEND.                                                  03620000
036300                                                                  03630000
036400     DISPLAY '**********************************************'.    03640000
036500     DISPLAY '**        VNKUT019 ABENDING !!!!!!!         **'.    03650003
036600     DISPLAY '**********************************************'.    03660000
036700                                                                  03670000
036800     DISPLAY AA-ABEND-LIT.                                        03680000
036900     DISPLAY AA-DB2-ERROR-LIT.                                    03690000
037000     DISPLAY AA-PROGRAM-LIT.                                      03700000
037100     DISPLAY AA-PARAGRAPH-LIT.                                    03710000
037200     DISPLAY AA-DB2-OPERATION-LIT.                                03720000
037300     DISPLAY AA-DB2-TABLE-1.                                      03730000
037400     DISPLAY AA-DB2-TABLE-2.                                      03740000
037500     DISPLAY AA-DB2-TABLE-3.                                      03750000
037600                                                                  03760000
