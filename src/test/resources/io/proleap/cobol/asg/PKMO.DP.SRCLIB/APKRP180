021000     COPY APRD042.                                                02100019
021100                                                                  02110019
021200 PROCEDURE DIVISION.                                              02120019
021300 A100-MAINLINE.                                                   02130019
021400     PERFORM B100-INITIALIZATION.                                 02140019
021500                                                                  02150019
021600     PERFORM B200-PROCESS-VENDOR-DATA                             02160019
021700             UNTIL END-OF-INTRANSIT-DATA.                         02170019
021800                                                                  02180019
021900     PERFORM W400-WRITE-GRAND-TOTALS.                             02190019
022000                                                                  02200019
022100     PERFORM B300-TERMINATION.                                    02210019
022200                                                                  02220019
022300     GOBACK.                                                      02230019
022400                                                                  02240019
022500******************************************************************02250019
022600** OPEN INPUT FILE, INITIALIZE VARIABLES, AND DO A PRIMING READ **02260019
022700** ON THE INTRANSIT DATA FILE.                                  **02270019
022800******************************************************************02280019
022900 B100-INITIALIZATION.                                             02290019
023000     OPEN INPUT  INTRANSIT-FILE                                   02300019
023100          OUTPUT REPORT-FILE.                                     02310019
023200                                                                  02320019
023300     INITIALIZE AP042-EXTRACT-FILE                                02330019
023400                PV-PREV-VRF-DT.                                   02340019
023500                                                                  02350019
023600     SET NOT-STORE-FLAG TO TRUE.                                  02360019
023700     MOVE 'N' TO SW-END-OF-INTRANSIT-DATA.                        02370019
023800                                                                  02380019
023900     ACCEPT PV-CURRENT-DATE FROM DATE.                            02390019
024000     MOVE PV-CURRENT-MONTH            TO DP132O-RUN-MONTH.        02400019
024100     MOVE PV-CURRENT-DAY              TO DP132O-RUN-DAY.          02410019
024200     MOVE PV-CURRENT-YEAR             TO DP132O-RUN-YEAR.         02420019
024300                                                                  02430019
024400     ACCEPT PV-CURRENT-TIME FROM TIME.                            02440019
024500     MOVE PV-CURRENT-HOUR             TO DP132O-RUN-HOUR.         02450019
024600     MOVE PV-CURRENT-MINUTE           TO DP132O-RUN-MINUTE.       02460019
024700     MOVE PV-PROGRAM-NAME             TO DP132O-PROGRAM-NAME.     02470019
024800     MOVE 1                           TO DP132O-REPORT-NUMBER.    02480019
024900                                                                  02490019
025000     PERFORM W100-WRITE-REPORT-HEADINGS.                          02500019
025100                                                                  02510019
025200     PERFORM R100-READ-INTRANSIT-FILE.                            02520019
025300                                                                  02530019
025400     IF NOT END-OF-INTRANSIT-DATA                                 02540019
025500       PERFORM C200-MOVE-TO-HOLD-FIELDS                           02550019
025600       PERFORM C300-ADD-TO-TOTALS                                 02560019
025700       IF PV-PREV-VRF-DT = SPACES                                 02570019
025800          MOVE PV-HOLD-VRFD-DTE   TO  PV-PREV-VRF-DT.             02580019
025900                                                                  02590019
026000 B200-PROCESS-VENDOR-DATA.                                        02600019
026100                                                                  02610019
026200     PERFORM R100-READ-INTRANSIT-FILE.                            02620019
026300                                                                  02630019
026400     IF NOT END-OF-INTRANSIT-DATA                                 02640019
026500        MOVE AP042-VRFD-DTE TO PV-VRF-DTE.                        02650019
026600                                                                  02660019
026700     IF END-OF-INTRANSIT-DATA                                     02670019
026800        ADD MTH-TOT-RETAIL   TO GR-TOT-RETAIL                     02680019
026900        ADD MTH-TOT-COST     TO GR-TOT-COST                       02690019
027000        MOVE MTH-TOT-RETAIL  TO RPT1-RETAIL-TOTAL                 02700019
027100        MOVE MTH-TOT-COST    TO RPT1-COST-TOTAL                   02710019
027200        PERFORM C100-MOVES-TO-DETAIL                              02720019
027300        PERFORM W200-WRITE-REPORT-DETAIL                          02730019
027400        PERFORM W300-WRITE-MONTHLY-TOTALS                         02740019
027500     ELSE                                                         02750019
027600     IF PV-PREV-VRF-CYM NOT = PV-VRF-DT2-CYM                      02760019
027700        ADD MTH-TOT-RETAIL   TO GR-TOT-RETAIL                     02770019
027800        ADD MTH-TOT-COST     TO GR-TOT-COST                       02780019
027900        MOVE MTH-TOT-RETAIL  TO RPT1-RETAIL-TOTAL                 02790019
028000        MOVE MTH-TOT-COST    TO RPT1-COST-TOTAL                   02800019
028100        PERFORM C100-MOVES-TO-DETAIL                              02810019
028200        PERFORM W200-WRITE-REPORT-DETAIL                          02820019
028300        PERFORM W300-WRITE-MONTHLY-TOTALS                         02830019
028400        INITIALIZE MTH-TOTALS                                     02840019
028500                   PV-HOLD-RTL-AMT                                02850019
028600                   PV-HOLD-COST-AMT                               02860019
028700        SET NOT-STORE-FLAG            TO TRUE                     02870019
028800        PERFORM C200-MOVE-TO-HOLD-FIELDS                          02880019
028900        PERFORM C300-ADD-TO-TOTALS                                02890019
029000        MOVE PV-HOLD-VRFD-DTE TO PV-PREV-VRF-DT                   02900019
029100      ELSE                                                        02910019
029200       IF AP042-PO-NBR  NOT = PV-HOLD-PO-NBR OR                   02920019
029300          AP042-REC-SEQ-NBR NOT = PV-HOLD-REC-SEQ-NBR OR          02930019
029400          AP042-VRFD-DTE NOT = PV-HOLD-VRFD-DTE                   02940019
029500                                                                  02950019
029600* MOVE AND PRINT PREVIOUS DETAIL                                  02960019
029700          PERFORM C100-MOVES-TO-DETAIL                            02970019
029800          PERFORM W200-WRITE-REPORT-DETAIL                        02980019
029900          INITIALIZE PV-HOLD-RTL-AMT                              02990019
030000                     PV-HOLD-COST-AMT                             03000019
030100          SET NOT-STORE-FLAG       TO TRUE                        03010019
030200                                                                  03020019
030300* MOVE AND ADD FOR CURRENT RECORD                                 03030019
030400          PERFORM C200-MOVE-TO-HOLD-FIELDS                        03040019
030500          PERFORM C300-ADD-TO-TOTALS                              03050019
030600       ELSE                                                       03060019
030700          PERFORM C300-ADD-TO-TOTALS                              03070019
030800          SET STORE-FLAG                TO TRUE.                  03080019
030900                                                                  03090019
031000******************************************************************03100019
031100**  CLOSE THE OUTPUT FILES.                                     **03110019
031200******************************************************************03120019
031300 B300-TERMINATION.                                                03130019
031400                                                                  03140019
