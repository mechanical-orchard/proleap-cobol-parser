031200***************************************************************** 03120040
031300*  MOVE THE INPUT RECORD FIELDS TO THE PRINT DETAIL LINE.  WRITE  03130040
031400*  THE DETAIL RECORD TO THE PRINT FILE.                           03140040
031500***************************************************************** 03150040
031600 C100-PROCESS-DETAIL.                                             03160040
031700                                                                  03170040
031800     MOVE AP071-ENT-NM-DESC      TO  DL-VENDOR-NAME.              03180040
031900     IF AP071-ENT-NM-DESC = SPACES                                03190040
032000        MOVE 'UNKNOWN VENDOR'    TO   DL-VENDOR-NAME              03200040
032100     ELSE                                                         03210040
032200        MOVE AP071-ENT-NM-DESC   TO   DL-VENDOR-NAME              03220040
032300     END-IF.                                                      03230040
032400     MOVE AP071-VND-NBR          TO  DL-VENDOR-NBR.               03240040
032500     MOVE AP071-DEPT-NBR         TO  DL-DEPARTMENT.               03250040
032600     MOVE AP071-PO-NBR           TO  DL-PO-NBR.                   03260040
032700     MOVE AP071-OTB-PER-NBR      TO  DL-OTB-PER-NBR.              03270040
032800     MOVE AP071-TOTAL-COST       TO  DL-COST-TOTAL.               03280040
032900     MOVE AP071-TOTAL-RETAIL     TO  DL-RETAIL-TOTAL.             03290040
033000     MOVE AP071-COST-181         TO  DL-COST-181.                 03300040
033100     MOVE AP071-RETAIL-181       TO  DL-RETAIL-181.               03310040
033200     MOVE AP071-COST-121         TO  DL-COST-121.                 03320040
033300     MOVE AP071-RETAIL-121       TO  DL-RETAIL-121.               03330040
033400     MOVE AP071-COST-91          TO  DL-COST-91.                  03340040
033500     MOVE AP071-RETAIL-91        TO  DL-RETAIL-91.                03350040
033600                                                                  03360040
033700     IF PV-LINE-COUNT > PC-MAX-LINES                              03370040
033800        PERFORM H100-WRITE-HEADING                                03380040
033900     END-IF.                                                      03390040
034000                                                                  03400040
034100     WRITE REPT-RECORD FROM DETAIL-LINE                           03410040
034200        AFTER ADVANCING 1 LINES.                                  03420040
034300     ADD +1            TO PV-LINE-COUNT                           03430040
034400                          OUTPUT-COUNT.                           03440040
034500                                                                  03450040
034600     INITIALIZE DETAIL-LINE.                                      03460040
034700                                                                  03470040
034800***************************************************************** 03480040
034900*  WRITE THE HEADINGS TO THE REPORT.                              03490040
035000***************************************************************** 03500040
035100 H100-WRITE-HEADING.                                              03510040
035200                                                                  03520040
