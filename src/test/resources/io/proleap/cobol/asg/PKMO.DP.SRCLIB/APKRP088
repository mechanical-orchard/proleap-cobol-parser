025000*  PROGRAM INITIALIZATION.                                        02500040
025100***************************************************************** 02510028
025200 B100-BEGIN-PROGRAM.                                              02520039
025300                                                                  02530028
025400     MOVE 'B100-BEGIN-PROGRAM'   TO   PV-CURRENT-PARAGRAPH.       02540039
025500                                                                  02550028
025600     OPEN INPUT  AGED-NIR-FILE                                    02560039
025700          OUTPUT REPT-FILE.                                       02570039
025800                                                                  02580028
025900     ACCEPT PV-CURRENT-JULIAN    FROM DAY.                        02590028
026000     MOVE PV-CURRENT-JULIAN    TO PV-CURRENT-JUL-YYDDD.           02600028
026100     IF PV-CURRENT-JUL-YY > 80                                    02610028
026200        MOVE 19 TO PV-CURRENT-JUL-CC                              02620028
026300     ELSE                                                         02630028
026400        MOVE 20 TO PV-CURRENT-JUL-CC.                             02640028
026500                                                                  02650028
026600     ACCEPT PV-CURRENT-DATE      FROM DATE.                       02660028
026700     MOVE PV-CURRENT-MONTH       TO   DP200O-RUN-MONTH.           02670033
026800     MOVE PV-CURRENT-DAY         TO   DP200O-RUN-DAY.             02680033
026900     MOVE PV-CURRENT-YEAR        TO   DP200O-RUN-YEAR.            02690033
027000                                                                  02700028
027100     ACCEPT PV-CURRENT-TIME      FROM TIME.                       02710028
027200     MOVE PV-CURRENT-HOUR        TO   DP200O-RUN-HOUR.            02720033
027300     MOVE PV-CURRENT-MINUTE      TO   DP200O-RUN-MINUTE.          02730033
027400                                                                  02740028
027500     MOVE PV-PROGRAM-NAME        TO   DP200O-PROGRAM-NAME.        02750033
027600     MOVE 1                      TO   DP200O-REPORT-NUMBER.       02760033
027700                                                                  02770028
027800     MOVE 99                     TO   PV-LINE-COUNT.              02780028
027900                                                                  02790028
028000     PERFORM R100-READ-AGED-NIR-FILE.                             02800039
028100                                                                  02810028
028200***************************************************************** 02820028
028300*  PROCESS EACH RECORD READ.                                      02830040
028400***************************************************************** 02840028
028500 B200-PROCESS-DATA.                                               02850039
028600                                                                  02860028
028700     MOVE 'B200-PROCESS-DATA'    TO   PV-CURRENT-PARAGRAPH.       02870039
028800                                                                  02880028
028900     PERFORM C100-PROCESS-DETAIL.                                 02890039
029000                                                                  02900028
029100     PERFORM R100-READ-AGED-NIR-FILE.                             02910039
029200                                                                  02920028
029300***************************************************************** 02930039
029400*  WRITE OUT THE END OF REPORT LINE.  CLOSE THE FILES.  DISPLAY   02940040
029500*  THE COUNTS.                                                    02950040
029600***************************************************************** 02960040
029700 B300-END-PROGRAM.                                                02970040
029800                                                                  02980040
029900     MOVE 'B300-END-PROGRAM'     TO   PV-CURRENT-PARAGRAPH.       02990040
030000                                                                  03000040
