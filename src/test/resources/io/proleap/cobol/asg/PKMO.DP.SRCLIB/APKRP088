039000         AT END                                                   03900040
039100             SET EOF TO TRUE.                                     03910040
039200                                                                  03920040
039300     IF PV-EOF-SW = 'N'                                           03930040
039400        ADD 1 TO INPUT-COUNT                                      03940040
039500     END-IF.                                                      03950040
