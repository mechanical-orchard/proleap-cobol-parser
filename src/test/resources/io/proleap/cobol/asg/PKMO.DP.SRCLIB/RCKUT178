013600 PROCEDURE DIVISION.                                              01360000
013700******************************************************************01370000
013800*                                                                 01380000
013900******************************************************************01390000
014000*    MAIN PROCESSING.                                             01400000
014100******************************************************************01410000
014200 0000-MAIN-MODULE.                                                01420000
014300                                                                  01430000
014400     INITIALIZE  PV-PROGRAM-VARIABLES.                            01440000
014500                                                                  01450000
014600     MOVE '0000-MAIN-MODULE'           TO PV-PARAGRAPH.           01460000
014700                                                                  01470000
014800     PERFORM 1000-INITIALIZATION.                                 01480000
014900     PERFORM 1500-DETERMINE-REC-LENGTH.                           01490000
015000     PERFORM 2000-PROCESS-XML                                     01500000
015200              UNTIL PS-END-OF-CARTN-REC-YES                       01520008
015300     PERFORM 9300-EOJ.                                            01530000
015400                                                                  01540000
015500     GOBACK.                                                      01550000
015600                                                                  01560000
015700******************************************************************01570000
015800* INITIALIZATION                                                  01580000
015900******************************************************************01590000
016000 1000-INITIALIZATION.                                             01600000
016100                                                                  01610000
016200     MOVE '1000-INITIALIZATION'        TO PV-PARAGRAPH.           01620000
016300                                                                  01630000
016400     DISPLAY ' '.                                                 01640000
016500     DISPLAY '**********************************'.                01650000
016600     DISPLAY '* START OF RCKUT178               '.                01660000
016700     DISPLAY '**********************************'.                01670000
016800                                                                  01680000
016900     INITIALIZE PV-MSG-LENGTH.                                    01690000
017000                                                                  01700000
017100     OPEN INPUT  CARTON-IN-FILE.                                  01710000
017200     OPEN OUTPUT OUTPUT-FILE.                                     01720000
017300     MOVE +30000                 TO PV-MSG-LENGTH                 01730023
017400                                    PV-DATA-LENGTH.               01740000
017500     PERFORM 5000-READ-CARTON-IN-FILE.                            01750003
017600*----------------------------------------------------------------*01760000
017700*   THIS PARAGRAPH USED TO GET THE LENGTH OF THE RECORD          *01770000
017800*   EXCLUDING THE TRAILING SPACES.                               *01780000
017900*----------------------------------------------------------------*01790000
018000 1500-DETERMINE-REC-LENGTH.                                       01800000
018100                                                                  01810000
018200     MOVE +0                               TO PV-REC-LEN.         01820000
018300     MOVE LENGTH OF PV-MSG                 TO PV-REC-LEN.         01830000
018400                                                                  01840000
018500     PERFORM                                                      01850000
018600       UNTIL PV-MSG(PV-REC-LEN:1) > SPACES OR                     01860000
018700         PV-REC-LEN = +0                                          01870000
018800       COMPUTE PV-REC-LEN    = PV-REC-LEN - 1                     01880000
018900     END-PERFORM.                                                 01890000
018910     MOVE  PV-REC-LEN        TO PV-DATA-LENGTH.                   01891007
019000*                                                                 01900000
019800******************************************************************01980000
019900* PROCESS/PARSE THE XML DATA                                      01990000
020000******************************************************************02000000
020100 2000-PROCESS-XML.                                                02010000
020200*    DISPLAY '2000-PROCESS-XML'.                                  02020000
020300                                                                  02030000
020400     MOVE '2000-PROCESS-XML'           TO PV-PARAGRAPH.           02040000
020500                                                                  02050000
020600     XML PARSE PV-MSG (1:PV-DATA-LENGTH)                          02060009
020700         PROCESSING PROCEDURE                                     02070000
020800             2100-HANDLE-XML                                      02080000
020900                 ON EXCEPTION                                     02090000
021000                      PERFORM 3000-XML-ERROR                      02100000
021100                 NOT ON EXCEPTION                                 02110000
021200                      PERFORM 3100-XML-SUCCESS                    02120000
021300     END-XML.                                                     02130000
021400                                                                  02140000
021500                                                                  02150000
021600     INITIALIZE PV-PROGRAM-VARIABLES.                             02160000
021700                                                                  02170000
021800                                                                  02180000
021900******************************************************************02190000
022000* PREP THE OUTPUT FIELDS WITH THE XML DATA                        02200000
022100******************************************************************02210000
022200 2100-HANDLE-XML.                                                 02220000
022300                                                                  02230000
022500     COMPUTE XML-LENGTH = FUNCTION LENGTH(XML-TEXT).              02250000
022600                                                                  02260000
022700     EVALUATE XML-EVENT                                           02270000
022800                                                                  02280000
022900       WHEN 'END-OF-ELEMENT'                                      02290000
023000         EVALUATE XML-TEXT(1:XML-LENGTH)                          02300000
023100           WHEN 'Shipment'                                        02310017
023200               CONTINUE                                           02320017
023300                                                                  02330000
023400           WHEN OTHER                                             02340000
023500               CONTINUE                                           02350000
023600                                                                  02360000
023700         END-EVALUATE                                             02370000
023800                                                                  02380000
023900       WHEN 'START-OF-ELEMENT'                                    02390000
024000         MOVE XML-TEXT(1:XML-LENGTH) TO PV-CURRENT-ELEMENT        02400000
024100                                                                  02410000
024200       WHEN 'ATTRIBUTE-NAME'                                      02420000
024300         MOVE XML-TEXT(1:XML-LENGTH) TO PV-CURRENT-ATTRIBUTE      02430000
024400*          display 'ATTRIB-NAME=' XML-EVENT '-'                   02440018
024500*                  PV-CURRENT-ATTRIBUTE '-'                       02450018
024600*                  XML-TEXT(1:XML-LENGTH)                         02460018
024700*                 ' - ' xml-length                                02470018
024800                                                                  02480000
024900       WHEN 'ATTRIBUTE-CHARACTERS'                                02490000
025000       WHEN 'ATTRIBUTE-CHARACTER'                                 02500000
025100       WHEN 'CONTENT-CHARACTERS'                                  02510000
025200       WHEN 'CONTENT-CHARACTER'                                   02520000
025300                                                                  02530000
025400        EVALUATE PV-CURRENT-ATTRIBUTE                             02540000
025500         WHEN 'HEADER_CREATION_TIME'                              02550012
025600           MOVE XML-TEXT(1:XML-LENGTH) TO PV-FILE-NAME            02560014
025610           display 'HEADER_CREATION_TIME ' PV-FILE-NAME           02561020
025700           ADD +1                      TO PV-HOLD-NAME-SEQ-NBR    02570014
025710         WHEN 'PoNumber'                                          02571012
025730           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-KOHLS-ORD-NBR   02573012
025740           display 'PO Number  ' PV-HOLD-KOHLS-ORD-NBR            02574022
025800         WHEN 'VendorPONbr'                                       02580000
025900           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-VENDR-ORD-NBR   02590003
026000                                                                  02600000
026400         WHEN 'DcNumber'                                          02640012
026500           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-DC-NBR          02650003
026600           Display'DC Number  '   PV-HOLD-DC-NBR                  02660022
026700         WHEN 'DeliveryNbr'                                       02670000
026800           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-SEQ-NBR         02680003
026900                                                                  02690000
027000         WHEN 'CartonId'                                          02700000
027100           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-CARTON-ID       02710003
027200                                                                  02720000
027300         WHEN 'SKUNumber'                                         02730000
027400           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-SKU-NBR         02740003
027500                                                                  02750000
027600         WHEN 'UnitQuantity'                                      02760000
027700           MOVE XML-TEXT(1:XML-LENGTH) TO PV-HOLD-SHIP-QTY        02770003
027710           PERFORM 7900-LOAD-RECORD                               02771003
027800                                                                  02780000
027900         WHEN OTHER                                               02790000
028000             CONTINUE                                             02800000
028100                                                                  02810000
028200        END-EVALUATE                                              02820000
028300                                                                  02830000
028400       WHEN 'EXCEPTION'                                           02840000
028500            DISPLAY 'RCKUT178-2100 - UNKNOWN XML EVENT - '        02850000
028600                   XML-EVENT                                      02860000
028700            DISPLAY 'Exception Caught at ' XML-TEXT(1:XML-LENGTH) 02870000
028800            SET ABEND-4020-MQ-ERROR   TO TRUE                     02880000
028900            PERFORM 3000-XML-ERROR                                02890000
029000     END-EVALUATE.                                                02900000
029100                                                                  02910000
030200******************************************************************03020000
030300* XML ERROR                                                       03030000
030400******************************************************************03040000
030500 3000-XML-ERROR.                                                  03050000
030600                                                                  03060000
030700     MOVE '3000-XML-ERROR'             TO PV-PARAGRAPH.           03070000
030800                                                                  03080000
030900     DISPLAY 'RCKUT178 - 3000-XML-ERROR.               '.         03090000
031000                                                                  03100000
031100     PERFORM 9000-EXPLAIN-XML-RESPONSE                            03110000
031200     PERFORM VARYING XML-LINE-IDX FROM 1 BY 1                     03120000
031300       UNTIL XML-LINE-IDX > XML-RESPONSE-LINES                    03130000
031400          DISPLAY XML-MSG(XML-LINE-IDX)                           03140000
031500     END-PERFORM.                                                 03150000
031600                                                                  03160000
031700     MOVE +4018              TO PV-ABEND-CODE.                    03170000
031800***  PERFORM 9500-ABEND.                                          03180000
031900                                                                  03190000
032000******************************************************************03200000
032100* SUCCESSFUL XML PARSE                                            03210000
032200******************************************************************03220000
032300 3100-XML-SUCCESS.                                                03230000
032400                                                                  03240000
032500     MOVE '3100-XML-SUCCESS'           TO PV-PARAGRAPH.           03250000
032600                                                                  03260000
032700*    DISPLAY 'RCKUT178 - 3100-XML-SUCCESS.             '.         03270000
032800                                                                  03280000
032900     ADD +1                            TO PA-XML-PARSED.          03290000
033000                                                                  03300000
033010     PERFORM 5000-READ-CARTON-IN-FILE.                            03301013
033100******************************************************************03310000
033200 5000-READ-CARTON-IN-FILE.                                        03320003
033300******************************************************************03330000
033400     READ CARTON-IN-FILE INTO PV-MSG                              03340000
033500       AT END                                                     03350000
033600        SET PS-END-OF-CARTN-REC-YES TO TRUE                       03360000
033700     END-READ.                                                    03370000
033800                                                                  03380000
033801 7900-LOAD-RECORD.                                                03380106
033802      MOVE PV-FILE-NAME-YEAR      TO RC108-FILE-NAME-YEAR         03380214
033803      MOVE PV-FILE-NAME-MONTH     TO RC108-FILE-NAME-MONTH        03380314
033804      MOVE PV-FILE-NAME-DAY       TO RC108-FILE-NAME-DAY          03380414
033805      MOVE PV-FILE-NAME-HOUR      TO RC108-FILE-NAME-HOUR         03380514
033806      MOVE PV-FILE-NAME-MINUTE    TO RC108-FILE-NAME-MINUTE       03380614
033807      MOVE PV-FILE-NAME-SECOND    TO RC108-FILE-NAME-SECOND       03380714
033808      MOVE PV-HOLD-NAME-SEQ-NBR   TO RC108-FILE-NAME-SEQ-NBR      03380814
033809                                                                  03380914
033810      MOVE PV-HOLD-KOHLS-ORD-NBR  TO RC108-KOHLS-ORD-NBR.         03381014
033811      MOVE PV-HOLD-VENDR-ORD-NBR  TO RC108-VENDR-ORD-NBR.         03381114
033812      MOVE PV-HOLD-DC-NBR         TO RC108-DC-NBR.                03381214
033813      MOVE PV-HOLD-SEQ-NBR        TO RC108-SEQ-NBR.               03381314
033814      MOVE PV-HOLD-CARTON-ID      TO RC108-CARTON-ID.             03381414
033815      MOVE PV-HOLD-SKU-NBR        TO RC108-SKU-NBR.               03381514
033816      MOVE PV-HOLD-SHIP-QTY       TO RC108-SHIP-QTY.              03381614
033817      PERFORM 8000-WRITE-RECORD.                                  03381714
033818******************************************************************03381814
033820* WRITE MESSAGES TO A OUTPUT FILE                                 03382003
033830******************************************************************03383003
033840 8000-WRITE-RECORD.                                               03384003
033850                                                                  03385003
033860     WRITE OUTPUT-RECORD                                          03386003
033870       FROM RC108-SKU-SRT-CARTON-REC.                             03387003
033880                                                                  03388003
033890     ADD +1                            TO PA-TOTAL-OUTPUT-RECS.   03389003
033891                                                                  03389103
033900******************************************************************03390000
034000* CONVERTS THE DATA FROM EBCDIC TO ASCII FORMAT                   03400000
034100******************************************************************03410000
034200 8140-ASCII-EBCDIC.                                               03420000
034300                                                                  03430000
034400     INITIALIZE DP053-EBCDIC-ASCII-PARMS.                         03440000
034500                                                                  03450000
034600     SET  DP053-CT-EBCDIC-TO-ASCII TO TRUE.                       03460000
034700     MOVE PV-MSG(1:3000) TO DP053-INPUT-FIELD.                    03470000
034800     MOVE 3000                    TO DP053-FIELD-LENGTH.          03480000
034900                                                                  03490000
035000     CALL DP053-EBCDIC-ASCII-ROUTINE USING DP053-CONVERSION-TYPE  03500000
035100                                           DP053-FIELD-LENGTH     03510000
035200                                           DP053-INPUT-FIELD      03520000
035300                                           DP053-OUTPUT-FIELD     03530000
035400                                           DP053-RETURN-CODE.     03540000
035500     IF DP053-RC-SUCCESSFUL                                       03550000
035600        MOVE DP053-OUTPUT-FIELD  TO PV-MSG-ASCII                  03560000
035700     ELSE                                                         03570000
035800        DISPLAY 'ERROR WHILE CONVERTING EBCDIC to ASCII'          03580000
035900        DISPLAY 'RETURN CODE     - ' DP053-RETURN-CODE            03590000
036000**      SET AC-CONVERSION-ERROR TO TRUE                           03600000
036100        CALL 'ILBOABN0' USING PV-ABEND-CODE                       03610000
036200     END-IF.                                                      03620000
036300                                                                  03630000
036400                                                                  03640000
