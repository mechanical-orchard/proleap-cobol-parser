                                                                        76782050
769700******************************************************************76790000
769800* GET THIS YEAR'S AND LAST YEAR'S DATES USED IN THIS PROGRAM.     76800000
769900*                                                                 76810000
770000* THIS QUERY HANDLES GETTING THE MONTH END DATE WHEN THE CURRENT  76820000
770100* MONTH HAS THE 53RD WEEK IN IT.                                  76830000
770200*                                                                 76840000
770300* IN THE FIRST SELECT:                                            76850000
770400*                                                                 76860000
770500*    TABLE TDTATTR A - THIS FISCAL YEAR'S DATES                   76870000
770600*    TABLE TDTATTR B - LAST FISCAL YEAR'S DATES WHEN THIS YEAR    76880000
770700*                      ONLY HAS 52 WEEKS                          76890000
770800*    TABLE TDTATTR C - LAST FISCAL YEAR'S DATES WHEN THIS YEAR    76900000
770900*                      HAS 53 WEEKS                               76910000
771000*    TABLE TDTATTR D - TO GET TOTAL NUMBER OF WEEKS IN MONTH FOR  76920000
771100*                      THIS YEAR.                                 76930000
771200*    TABLE TDTATTR E - TO GET TOTAL NUMBER OF WEEKS IN MONTH FOR  76940000
771300*                      LAST YEAR.                                 76950000
771400******************************************************************76960000
771500 8000-GET-DATE-ATTRIBUTES.                                        76970000
771600                                                                  76980000
771700     EXEC SQL                                                     76990000
771800          SELECT                                                  77000000
771900                   A.FSCL_YR_NBR                                  77010000
772000                  ,A.FSCL_MN_NM                                   77020000
772100                  ,A.FSCL_MN_NBR                                  77030000
772200                  ,A.WK_IN_MN_NBR                                 77040000
772300                  ,A.FSCL_WK_NBR                                  77050000
772400                  ,A.FSCL_YR_BEG_DTE                              77060000
772500                  ,A.FSCL_YR_END_DTE                              77070000
772600                  ,D.WK_IN_MN_NBR                                 77080000
772700                  ,E.WK_IN_MN_NBR                                 77090000
772800          INTO     :DTATTR-FSCL-YR-NBR                            77100000
772900                  ,:DTATTR-FSCL-MN-NM                             77110000
773000                  ,:DTATTR-FSCL-MN-NBR                            77120000
773100                  ,:DTATTR-WK-IN-MN-NBR                           77130000
773200                  ,:DTATTR-FSCL-WK-NBR                            77140000
773300                  ,:DTATTR-FSCL-YR-BEG-DTE                        77150000
773400                  ,:DTATTR-FSCL-YR-END-DTE                        77160000
773500                  ,:TY-LAST-WK-IN-MN                              77170000
773600                  ,:LY-LAST-WK-IN-MN                              77180000
773700          FROM                                                    77190000
773800               TDTATTR A                                          77200000
773900              ,TDTATTR B                                          77210000
774000              ,TDTATTR C                                          77220000
774100              ,TDTATTR D                                          77230000
774200              ,TDTATTR E                                          77240000
774300          WHERE A.KY_DTE             =:ML114-EXTRACT-DTE          77250000
774400           AND E.KY_DTE              = C.FSCL_MN_END_DTE          77260000
774500           AND D.KY_DTE              = A.FSCL_MN_END_DTE          77270000
774600           AND C.KY_DTE              = B.LY_THIS_DAY_KY_DTE       77280000
774700           AND B.KY_DTE              = A.FSCL_MN_END_DTE - 7 DAYS 77290000
774800           AND A.FSCL_MN_END_DTE     = B.FSCL_MN_END_DTE          77300000
774900           AND A.LY_THIS_DAY_KY_DTE  = '9999-09-09'               77310000
775000                                                                  77320000
775100          UNION                                                   77330000
775200                                                                  77340000
775300          SELECT                                                  77350000
775400                   A.FSCL_YR_NBR                                  77360000
775500                  ,A.FSCL_MN_NM                                   77370000
775600                  ,A.FSCL_MN_NBR                                  77380000
775700                  ,A.WK_IN_MN_NBR                                 77390000
775800                  ,A.FSCL_WK_NBR                                  77400000
775900                  ,A.FSCL_YR_BEG_DTE                              77410000
776000                  ,A.FSCL_YR_END_DTE                              77420000
776100                  ,D.WK_IN_MN_NBR                                 77430000
776200                  ,E.WK_IN_MN_NBR                                 77440000
776300          INTO     :DTATTR-FSCL-YR-NBR                            77450000
776400                  ,:DTATTR-FSCL-MN-NM                             77460000
776500                  ,:DTATTR-FSCL-MN-NBR                            77470000
776600                  ,:DTATTR-WK-IN-MN-NBR                           77480000
776700                  ,:DTATTR-FSCL-WK-NBR                            77490000
776800                  ,:DTATTR-FSCL-YR-BEG-DTE                        77500000
776900                  ,:DTATTR-FSCL-YR-END-DTE                        77510000
777000                  ,:TY-LAST-WK-IN-MN                              77520000
777100                  ,:LY-LAST-WK-IN-MN                              77530000
777200          FROM                                                    77540000
777300               TDTATTR A                                          77550000
777400              ,TDTATTR B                                          77560000
777500              ,TDTATTR C                                          77570000
777600              ,TDTATTR D                                          77580000
777700              ,TDTATTR E                                          77590000
777800          WHERE A.KY_DTE             =:ML114-EXTRACT-DTE          77600000
777900           AND E.KY_DTE              = B.FSCL_MN_END_DTE          77610000
778000           AND D.KY_DTE              = A.FSCL_MN_END_DTE          77620000
778100           AND C.KY_DTE              = B.LY_THIS_DAY_KY_DTE       77630000
778200           AND B.KY_DTE              = A.LY_THIS_DAY_KY_DTE       77640000
778300           AND A.FSCL_MN_END_DTE    ^= B.FSCL_MN_END_DTE          77650000
W26603          WITH UR                                                         
778400     END-EXEC.                                                    77660000
778500                                                                  77670000
778600     EVALUATE TRUE                                                77680000
778700         WHEN SQLCODE = ZERO                                      77690000
778800             CONTINUE                                             77700000
778900         WHEN OTHER                                               77710000
779000             DISPLAY 'DTATTR-KY-DTE: ' DTATTR-KY-DTE              77720000
779100             MOVE    '8000-GET-DATE-ATTRIBUTES'                   77730000
779200                                          TO PV-PARAGRAPH-NAME    77740000
779300             MOVE    'SELECT ROW FROM TDTATTR'                    77750000
779400                                          TO PV-DB2-OPER-ATTEMPTED77760000
779500             PERFORM 9998-SQL-ABEND                               77770000
779600     END-EVALUATE.                                                77780000
779700                                                                  77790000
779800******************************************************************77800000
779900*                                                                 77810000
780000******************************************************************77820000
780100 8100-GET-GMM-NAME.                                               77830000
780200                                                                  77840000
780300     EXEC SQL                                                     77850000
780400          SELECT      B.WORKER_LAST_NAME                          77860000
780500               ,      B.WORKER_FIR_NAME                           77870000
780600            INTO     :RSPBAR-WORKER-LAST-NAME                     77880000
780700               ,     :RSPBAR-WORKER-FIR-NAME                      77890000
780800            FROM      TMDSEAR A                                   77900000
780900               ,      TRSPBAR B                                   77910000
781000               ,      TAPLAVL C                                   77920000
781100           WHERE      A.GMA_NBR     = :PV-GMA-NBR                 77930000
781200             AND      B.RSPLVL_CDE  = :PC-RSPLVL-CDE-GMM          77940000
781300             AND      A.OPERCO_NBR  = :PC-OPERCO-NBR              77950000
781400             AND      C.APLSYS_CDE  = :PC-APLSYS-CDE              77960000
781500             AND      A.MDSEAR_DKEY =  B.MDSEAR_DKEY              77970000
781600             AND      A.MDSEAR_DKEY =  C.MDSEAR_DKEY              77980000
781700             AND     (:ML114-EXTRACT-DTE                          77990000
781800                      BETWEEN B.WORKER_STRT_DTE                   78000000
781900                      AND     B.WORKER_END_DTE)                   78010000
782000             AND     (:ML114-EXTRACT-DTE                          78020000
782100                      BETWEEN C.MDSEAR_STRT_DTE                   78030000
782200                      AND     C.MDSEAR_END_DTE)                   78040000
W26603           WITH UR                                                        
782300     END-EXEC.                                                    78050000
782400                                                                  78060000
782500     EVALUATE TRUE                                                78070000
782600         WHEN SQLCODE = ZERO                                      78080000
782700             CONTINUE                                             78090000
782800         WHEN SQLCODE = +100                                      78100000
782900             MOVE 'OPEN' TO  RSPBAR-WORKER-FIR-NAME               78110000
783000             MOVE SPACE  TO  RSPBAR-WORKER-LAST-NAME              78120000
783100         WHEN OTHER                                               78130000
783200             MOVE '8100-GET-GMM-NAME'     TO PV-PARAGRAPH-NAME    78140000
783300             MOVE 'SELECT ROW FROM TRSPBAR'                       78150000
783400                                          TO PV-DB2-OPER-ATTEMPTED78160000
783500             PERFORM 9998-SQL-ABEND                               78170000
783600     END-EVALUATE.                                                78180000
783700                                                                  78190000
783800******************************************************************78200000
783900*                                                                 78210000
784000******************************************************************78220000
784100 8200-GET-DMM-NAME.                                               78230000
784200                                                                  78240000
