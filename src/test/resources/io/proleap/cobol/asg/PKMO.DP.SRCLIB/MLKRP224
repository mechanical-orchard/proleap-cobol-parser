746500                  AT-CMPY-LY-EOM-YTD) /                           74590000
746600                  AT-CMPY-LY-EOM-YTD) * 100                       74600000
746700        COMPUTE DYL3-INV-EOM-YTD      ROUNDED = PV-PCT            74610000
746800     END-IF.                                                      74620000
746900                                                                  74630000
747000*    YTD INVENTORY AVG                                            74640000
747100     IF AT-CMPY-LY-INV-YTD = ZEROS                                74650000
747200        MOVE ZEROS                     TO DYL3-INV-AVG-YTD        74660000
747300     ELSE                                                         74670000
747400        COMPUTE PV-PCT =                                          74680000
747500               ((AT-CMPY-TY-INV-YTD -                             74690000
747600                  AT-CMPY-LY-INV-YTD) /                           74700000
747700                  AT-CMPY-LY-INV-YTD) * 100                       74710000
747800        COMPUTE DYL3-INV-AVG-YTD      ROUNDED = PV-PCT            74720000
747900     END-IF.                                                      74730000
748000                                                                  74740000
748100*    YTD INVENTORY TURN LAST YEAR                                 74750000
748200     IF AT-CMPY-TY-INV-YTD = ZEROS                                74760000
               OR AT-CMPY-LY-INV-YTD = ZEROS                            74760000
748300        MOVE ZEROS                TO DYL3-INV-TURN-YTD            74770000
748400     ELSE                                                         74780000
748500        COMPUTE PV-PCT =                                          74790000
748600            ((AT-CMPY-TY-SLS-YTD /                                74800048
748700             (AT-CMPY-TY-INV-YTD /                                74810048
748800             (PV-INV-MONTHS)))) -                                 74820064
748600            ((AT-CMPY-LY-SLS-YTD /                                74821048
748700             (AT-CMPY-LY-INV-YTD /                                74822048
748800             (PV-INV-MONTHS))))                                   74823064
748900        COMPUTE DYL3-INV-TURN-YTD      ROUNDED = PV-PCT           74830000
749000     END-IF.                                                      74840000
749100                                                                  74850000
