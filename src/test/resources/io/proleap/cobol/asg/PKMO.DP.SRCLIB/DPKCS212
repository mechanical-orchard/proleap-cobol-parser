002900 PROCEDURE DIVISION.                                              00290001
003000******************************************************************00300001
003100 0000-MAIN-LINE.                                                  00310001
003200*    EXEC CICS START                                              00320006
003300*              TRANSID('Z300')                                    00330006
003400*              FROM(WI-PORT)                                      00340006
003500*    END-EXEC.                                                    00350006
003510     EXEC CICS START                                              00351002
003520               TRANSID('S611')                                    00352002
003540     END-EXEC.                                                    00354002
003550     EXEC CICS START                                              00355005
003560               TRANSID('L201')                                    00356005
003570     END-EXEC.                                                    00357005
003580     EXEC CICS START                                              00358007
003590               TRANSID('D408')                                    00359007
003591     END-EXEC.                                                    00359107
003592     EXEC CICS START                                              00359208
003593               TRANSID('E291')                                    00359308
003594     END-EXEC.                                                    00359408
003595     EXEC CICS START                                              00359512
003596               TRANSID('W303')                                    00359610
003597     END-EXEC.                                                    00359710
003598     EXEC CICS START                                              00359812
003599               TRANSID('W307')                                    00359912
003600     END-EXEC.                                                    00360012
003601*    EXEC CICS START                                              00360114
003602*              TRANSID('W314')                                    00360214
003603*    END-EXEC.                                                    00360314
003610     EXEC CICS RETURN                                             00361011
003700     END-EXEC.                                                    00370001
