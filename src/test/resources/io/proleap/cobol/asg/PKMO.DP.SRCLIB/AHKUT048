021800                                                                  02180001
021900*---------------------------------------------------------------* 02190001
022000*    DEFINE CURSOR FOR TPKSLIP :                                * 02200001
022100*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02210001
022200*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02220001
022300*---------------------------------------------------------------* 02230001
022400     EXEC SQL                                                     02240001
022500          DECLARE TABLE-CURSOR CURSOR FOR                         02250001
022600           SELECT PO_NBR                                          02260002
022700                 ,SEQ_NBR                                         02270002
022800                 ,CRE_TMST                                        02280002
022900                 ,VER_STAT_CDE                                    02290002
023000                 ,OPER_UNT_ID                                     02300002
023100                 ,FCLTY_ID                                        02310002
023200                 ,PKSL_NBR                                        02320003
023300                 ,STR_DEST_NBR                                    02330003
023400                 ,REC_SEQ_NBR                                     02340003
023410                 ,SHIPT_ID                                        02341003
023420                 ,BOL_NBR                                         02342003
023430                 ,PRO_NBR                                         02343003
023440                 ,PKSL_STAT_CDE                                   02344003
023450                 ,CRE_UID                                         02345003
023460                 ,PO_VER_NBR                                      02346003
023470                 ,CRE_SRC_CDE                                     02347003
023480                 ,EXPTED_PKG_QTY                                  02348003
023490                 ,OWNER_SCAC_ID                                   02349003
023491                 ,ASN_NBR                                         02349103
023492                 ,VEND_SHIP_DTE                                   02349203
023493                 ,PKSL_PG_CNT                                     02349303
023494                 ,CONTNR_ID                                       02349403
023500           FROM   TPKSLIP                                         02350003
023600          WHERE  (PO_NBR = :PV-PO-NBR-COMP)                       02360003
023700*      ORDER BY                                                   02370001
023800            FOR  FETCH ONLY                                       02380001
023900           WITH  UR                                               02390001
024000     END-EXEC.                                                    02400001
024100                                                                  02410001
024200                                                                  02420001
024300 PROCEDURE DIVISION.                                              02430001
024400********************                                              02440001
024500                                                                  02450001
024600 0000-PROGRAM-MAINLINE.                                           02460001
024700                                                                  02470001
024800     PERFORM 1000-INITIALIZE.                                     02480001
024900                                                                  02490001
025000     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02500001
025100     PERFORM 2000-PROCESS-DRIVER-FILE                             02510001
025200         UNTIL PS-IN-DRIVER-EOF.                                  02520001
025300                                                                  02530001
025400     PERFORM 6000-EOJ-ROUTINE.                                    02540001
025500     STOP RUN.                                                    02550001
025600                                                                  02560001
025700                                                                  02570001
025800* ************************************************************** *02580001
025900* * INITIALIZATIONS                                            * *02590001
026000* ************************************************************** *02600001
026100 1000-INITIALIZE.                                                 02610001
026200                                                                  02620001
026300     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02630001
026400     DISPLAY DM-DISPLAY-MESSAGE.                                  02640001
026500                                                                  02650001
026600     MOVE PC-BEGINNING              TO DP-STATUS-DESCRIPTION.     02660001
026700     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02670001
026800                                                                  02680001
026900     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02690001
027000     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02700001
027100                                                                  02710001
027200                                                                  02720001
027300* ************************************************************** *02730001
027400* * 1) GET NEXT RECORD FROM DRIVER FILE, AND                   * *02740001
027500* *    1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR        * *02750001
027600* *    1B- OPEN TABLE CURSOR                                   * *02760001
027700* *    1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL               * *02770001
027800* *        ROWS FOR THIS INPUT DRIVER RECORD                   * *02780001
027900* *    2) FOR EACH ROW,                                        * *02790001
028000* *       2A- INCREMENT COUNTS                                 * *02800001
028100* *       2B- WRITE EXTRACT RECORD                             * *02810001
028200* *    1D- CLOSE TABLE CURSOR                                  * *02820001
028300* ************************************************************** *02830001
028400 2000-PROCESS-DRIVER-FILE.                                        02840001
028500                                                                  02850001
028600     PERFORM 9050-READ-IN-DRIVER-FILE.                            02860001
028700     IF PS-IN-DRIVER-EOF                                          02870001
028800         CONTINUE                                                 02880001
028900     ELSE                                                         02890001
029000         MOVE AH002-PO-NBR TO         PV-PO-NBR-COMP              02900001
029100                                      PV-PO-NBR-NUM               02910001
029200         PERFORM 2100-CHECK-PGM-PROGRESS                          02920001
029300         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     02930001
029400         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     02940001
029500         PERFORM 9100-OPEN-TABLE-CURSOR                           02950001
029600         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     02960001
029700             UNTIL PS-TABLE-LOOP-DONE                             02970001
029800         PERFORM 9190-CLOSE-TABLE-CURSOR                          02980001
029900     END-IF.                                                      02990001
030000                                                                  03000001
030100                                                                  03010001
030200* ************************************************************** *03020001
030300* *    EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT     * *03030001
030400* *    RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST   * *03040001
030500* *    WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.   * *03050001
030600* ************************************************************** *03060001
030700 2100-CHECK-PGM-PROGRESS.                                         03070001
030800     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03080001
030900         BY        PC-HOW-OFTEN-TO-DISPLAY                        03090001
031000         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03100001
031100         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03110001
031200                                                                  03120001
031300     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03130001
031400         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03140001
031500         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03150001
031600     END-IF.                                                      03160001
031700                                                                  03170001
031800                                                                  03180001
031900* ************************************************************** *03190001
032000* * EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD     * *03200001
032100* ************************************************************** *03210001
032200 3000-EXTRACT-ROWS-FROM-TABLE.                                    03220001
032300     INITIALIZE DCLTPKSLIP.                                       03230003
032400     PERFORM 9150-FETCH-TABLE-CURSOR.                             03240001
032500     IF PS-TABLE-LOOP-DONE                                        03250001
032600         CONTINUE                                                 03260001
032700     ELSE                                                         03270001
032800         PERFORM 9750-WRITE-EXTRACT-RECORD                        03280001
032900     END-IF.                                                      03290001
033000                                                                  03300001
033100                                                                  03310001
033200* ************************************************************** *03320001
033300* * DISPLAY END OF RUN MESSAGE, CLOSE THE OUTPUT FILES         * *03330001
033400* ************************************************************** *03340001
033500 6000-EOJ-ROUTINE.                                                03350001
033600                                                                  03360001
033700     PERFORM 9090-CLOSE-IN-DRIVER-FILE.                           03370001
033800     PERFORM 9790-CLOSE-OUT-EXTRACT-FILE.                         03380001
033900                                                                  03390001
034000     MOVE PC-ENDING                 TO DP-STATUS-DESCRIPTION.     03400001
034100     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           03410001
034200                                                                  03420001
034300                                                                  03430001
034400* ************************************************************** *03440001
034500* * DISPLAY PROGRAM PROGRESS.                                  * *03450001
034600* ************************************************************** *03460001
034700 6200-DISPLAY-PGM-PROGRESS.                                       03470001
034800                                                                  03480001
034900     EXEC SQL                                                     03490001
035000          SET :PV-TMST = CURRENT TIMESTAMP                        03500001
035100     END-EXEC.                                                    03510001
035200                                                                  03520001
035300     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                03530001
035400     MOVE PV-TIMESTAMP-19           TO DP-TIMESTAMP-19.           03540001
035500     MOVE PV-IN-DRIVER-RECS-READ-CNT                              03550001
035600                                    TO DP-IN-CNT.                 03560001
035700*    MOVE PV-RECS-WITH-MATCHES-CNT  TO DP-MATCHES-FOUND-CNT.      03570001
035800     MOVE PV-TABLE-ROW-SELECT-CNT   TO DP-ROWS-SELECTED-CNT.      03580001
035900     MOVE PV-EXTRACT-WRITTEN-CNT    TO DP-OUT-CNT.                03590001
036000                                                                  03600001
036100     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             03610001
036200                                                                  03620001
036300                                                                  03630001
036400* ************************************************************** *03640001
036500* * FILE AND DB2 TABLE ACCESS PARAGRAPHS.                      * *03650001
036600* * 90XX- INPUT:  DRIVER FILE                                  * *03660001
036700* * 91XX- INPUT:  TABLE CURSOR                                 * *03670001
036800* * 97XX- OUTPUT: TABLE EXTRACT                                * *03680001
036900* ************************************************************** *03690001
037000                                                                  03700001
037100* ************************************************************** *03710001
037200* * OPEN  INPUT DRIVER FILE                                    * *03720001
037300* ************************************************************** *03730001
037400 9000-OPEN-IN-DRIVER-FILE.                                        03740001
037500     OPEN  INPUT  IN-DRIVER-FILE.                                 03750001
037600                                                                  03760001
037700                                                                  03770001
037800* ************************************************************** *03780001
037900* * READ  INPUT DRIVER FILE OF ELIGIBLE DATA TO EXTRACT        * *03790001
038000* ************************************************************** *03800001
038100 9050-READ-IN-DRIVER-FILE.                                        03810001
038200     READ IN-DRIVER-FILE                                          03820001
038300          INTO AH002-DRIVER-RECORD-LAYOUT                         03830001
038400          AT END                                                  03840001
038500              SET PS-IN-DRIVER-EOF TO TRUE.                       03850001
038600                                                                  03860001
038700     IF PS-IN-DRIVER-EOF                                          03870001
038800         CONTINUE                                                 03880001
038900     ELSE                                                         03890001
039000         ADD +1 TO PV-IN-DRIVER-RECS-READ-CNT                     03900001
039100     END-IF.                                                      03910001
039200                                                                  03920001
039300                                                                  03930001
039400* ************************************************************** *03940001
039500* * CLOSE INPUT DRIVER FILE                                    * *03950001
039600* ************************************************************** *03960001
039700 9090-CLOSE-IN-DRIVER-FILE.                                       03970001
039800     CLOSE IN-DRIVER-FILE.                                        03980001
039900                                                                  03990001
040000                                                                  04000001
040100* ************************************************************** *04010001
040200* * OPEN  TABLE CURSOR                                         * *04020001
040300* ************************************************************** *04030001
040400 9100-OPEN-TABLE-CURSOR.                                          04040001
040500     EXEC SQL                                                     04050001
040600          OPEN TABLE-CURSOR                                       04060001
