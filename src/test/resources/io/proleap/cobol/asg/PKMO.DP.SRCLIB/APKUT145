015300                                                                  01530000
015400     GOBACK.                                                      01540000
015500 EJECT                                                            01550000
015600*----------------------------------------------------------------*01560000
015700*    INITIALIZATION PROCESSING                                   *01570000
015800*----------------------------------------------------------------*01580000
015900 B100-INITIALIZE.                                                 01590000
016000                                                                  01600000
016100     OPEN INPUT  AP-TRANS-FILE                                    01610000
016200          OUTPUT KEYED-DOC-EXTRACT-FILE.                          01620000
016300                                                                  01630000
016400     INITIALIZE DCLTAPCNTL                                        01640000
016500                AP040-TRANSACTION-FILE                            01650000
016600                AP070-EXTRACT-FILE.                               01660000
016700                                                                  01670000
016800     PERFORM R200-GET-PROCESS-DATE.                               01680000
016900     DISPLAY AA-PROGRAM-LIT.                                      01690000
017000     DISPLAY 'PROCESS DATE = ' APCNTL-BTCH-PRC-DTE.               01700000
017100                                                                  01710000
017200     PERFORM R100-READ-TRANS.                                     01720000
017300                                                                  01730000
017400     IF OUT-OF-TRANS                                              01740000
017500         MOVE 'B100-INITIALIZE'    TO AA-PARAGRAPH-NAME           01750000
017600         DISPLAY   'AP TRANSACTION FILE IS EMPTY'                 01760000
017700                                                                  01770000
017800         SET AC-BAD-DATA           TO TRUE                        01780000
017900     END-IF.                                                      01790000
018000 EJECT                                                            01800000
018100*----------------------------------------------------------------*01810000
018200*    LOOPS THROUGH INPUT FILE AND WRITES OUTPUT FILE WHEN        *01820000
018300*    THE ENTERED DATE ON THE INPUT FILE MATCHES TAPCNTL DATE.    *01830000
018400*----------------------------------------------------------------*01840000
018500 B200-PROCESS-TRANS.                                              01850000
018600                                                                  01860000
018700     IF AP040-AUTO-OTP-PSTG-IND  = 'Y' AND                        01870000
018800        AP040-ENTR-DTE = APCNTL-BTCH-PRC-DTE                      01880000
018900         PERFORM C100-FILL-OUTPUT-FIELDS                          01890000
019000         PERFORM W100-WRITE-KEYED-DOCUMENT                        01900000
019100     ELSE                                                         01910000
019200         ADD +1 TO PV-SKIP-CNT                                    01920000
019300     END-IF.                                                      01930000
019400                                                                  01940000
019500     PERFORM R100-READ-TRANS.                                     01950000
019600 EJECT                                                            01960000
019700*----------------------------------------------------------------*01970000
019800*    CLOSES FILES, DISPLAYS COUNTS                               *01980000
019900*----------------------------------------------------------------*01990000
020000 B300-END-PROGRAM.                                                02000000
020100                                                                  02010000
020200     CLOSE AP-TRANS-FILE                                          02020000
020300           KEYED-DOC-EXTRACT-FILE.                                02030000
020400                                                                  02040000
020500     DISPLAY AA-PROGRAM-LIT.                                      02050000
020600     DISPLAY 'TRANS RECORDS READ      = ' PV-READ-CNT.            02060000
020700     DISPLAY 'TRANS RECORDS SKIPPED   = ' PV-SKIP-CNT.            02070000
020800     DISPLAY 'EXTRACT RECORDS WRITTEN = ' PV-WRITTEN-CNT.         02080000
020900 EJECT                                                            02090000
021000*----------------------------------------------------------------*02100000
021100*    PREPARES THE FIELDS ON THE INVOICE EXTRACT FILE.            *02110000
021200*----------------------------------------------------------------*02120000
021300 C100-FILL-OUTPUT-FIELDS.                                         02130000
021400                                                                  02140000
021500     INITIALIZE AP070-EXTRACT-FILE.                               02150000
021600     MOVE SPACES TO AP070-PS-PO-ID.                               02160000
021700                                                                  02170000
021800     MOVE AP040-ENTR-ID           TO AP070-ENTR-ID.               02180000
021900     MOVE AP040-ORGN-CDE          TO AP070-ORGN-CDE.              02190000
022000     MOVE AP040-AP-TRN-CDE        TO AP070-AP-TRN-CDE.            02200000
022100     MOVE AP040-ENTR-DTE          TO PV-DATE-VALUE.               02210000
022200     MOVE AP040-ENTR-TME          TO PV-TMST-VALUE.               02220000
022300     MOVE PV-DATE-TIME            TO AP070-ENTR-TMST.             02230000
022400     MOVE AP040-PO-NBR            TO AP070-PO-NBR.                02240000
022500     MOVE AP040-DOC-NBR           TO AP070-DOC-NBR.               02250000
022600     MOVE AP040-DOC-DTE           TO AP070-DOC-DTE.               02260000
022700     MOVE AP040-DEPT-NBR          TO AP070-DEPT-NBR.              02270000
022800     MOVE AP040-GL-ACCT-NBR       TO AP070-GL-ACCT-NBR.           02280000
022900     MOVE AP040-VND-NBR           TO AP070-VND-NBR.               02290000
023000     MOVE AP040-ENT-NME-DESC      TO AP070-ENT-NME-DESC.          02300000
023100     MOVE AP040-PSTG-TYP-CDE      TO AP070-PSTG-TYP-CDE.          02310000
023200     MOVE AP040-GRO-COST-AMT      TO AP070-GRO-COST-AMT.          02320000
023300     MOVE AP040-FRGT-AMT          TO AP070-FRGT-AMT.              02330000
023400     ADD  AP040-SALES-TAX-AMT     TO AP040-MISC-CHRG-AMT.         02340000
023500     MOVE AP040-MISC-CHRG-AMT     TO AP070-MISC-CHRG-AMT.         02350000
023600     MOVE AP040-NET-COST-AMT      TO AP070-NET-COST-AMT.          02360000
023700*    MOVE AP040-TERM-DISC-AMT     TO AP070-TERM-DISC-AMT.         02370000
023800*    MOVE AP040-NW-STR-DISC-AMT   TO AP070-NW-STR-DISC-AMT.       02380000
023900*    MOVE AP040-TRD-DISC-AMT      TO AP070-TRD-DISC-AMT.          02390000
024000     COMPUTE AP070-TOTAL-DISC-AMT = AP040-TERM-DISC-AMT           02400000
024100                                  + AP040-NW-STR-DISC-AMT         02410000
024200                                  + AP040-TRD-DISC-AMT            02420000
024300                                  + AP040-MOS-DISC-AMT            02430000
024400                                  + AP040-ADV-DISC-AMT            02440000
024500                                  + AP040-COOP-DISC-AMT           02450000
024600                                  + AP040-ALW-DISC-AMT            02460000
024700                                  + AP040-MSC-DISC-AMT.           02470000
024800                                                                  02480000
024900     MOVE AP040-RETL-AMT          TO AP070-RETL-AMT.              02490000
025000     MOVE AP040-BAR-CDE           TO AP070-BRCDE-NBR.             02500000
025100     MOVE SPACES                  TO AP070-VOUCHER-NBR            02510000
025200                                     AP070-BUSINESS-UNIT.         02520000
025300     MOVE ZEROS                   TO AP070-PAYT-REQ-SEQ-NBR.      02530000
025400                                                                  02540000
025500     EVALUATE TRUE                                                02550000
025600         WHEN AP040-ORGN-ADJUST                                   02560000
025700             SET ADJRNL-DB            TO TRUE                     02570000
025800         WHEN AP040-ORGN-CAPEX                                    02580000
025900             SET CAP-EXP-TRANS        TO TRUE                     02590000
026000         WHEN OTHER                                               02600000
026100             MOVE SPACES              TO AP070-SRC-CDE            02610000
026200     END-EVALUATE.                                                02620000
026300                                                                  02630000
026400*  NOTE: IF A TRANS POSTS TO PS, THE PS RECORD (COMPLETE WITH THE 02640000
026500*        VOUCHER NBR) WILL BE USED TO CREATE THE IMAGING KEYS     02650000
026600*        INSTEAD OF PULLING THIS IMCOMPLETE VERSION OF THE DATA.  02660000
026700*        SET THE IMAGING KEYS IND SWITCH ACCORDINGLY.             02670000
026800     EVALUATE TRUE                                                02680000
026900         WHEN AP040-POST-TO-PS-AP                                 02690000
027000             SET BYPASS-FOR-BUILD-KEYS TO TRUE                    02700000
027100         WHEN AP040-DO-NOT-POST-TO-PS-AP                          02710000
027200             SET USE-TO-BUILD-KEYS     TO TRUE                    02720000
027300     END-EVALUATE.                                                02730000
027400 EJECT                                                            02740000
027500*----------------------------------------------------------------*02750000
027600*  READS A RECORD FROM THE AP TRANSACTION FILE                   *02760000
027700*----------------------------------------------------------------*02770000
027800 R100-READ-TRANS.                                                 02780000
027900                                                                  02790000
028000     READ AP-TRANS-FILE INTO AP040-TRANSACTION-FILE               02800000
028100         AT END SET OUT-OF-TRANS TO TRUE.                         02810000
028200                                                                  02820000
028300     IF MORE-TRANS                                                02830000
028400         ADD +1 TO PV-READ-CNT                                    02840000
028500     END-IF.                                                      02850000
028600                                                                  02860000
028700 EJECT                                                            02870000
028800*----------------------------------------------------------------*02880000
028900*    GET THE DATE TO BE MATCHED AGAINST ENTER DATE ON THE AP     *02890000
029000*    TRANSACTION FILE.                                           *02900000
029100*    NOTE:  A NOT FOUND CONDITION IN THIS INSTANCE RESULTS IN    *02910000
029200*           AN ABEND AS THE PROGRAM LOGIC DEPENDS ON A VALUE     *02920000
029300*           FOR THE BATCH PROCESS DATE.                          *02930000
029400*----------------------------------------------------------------*02940000
029500 R200-GET-PROCESS-DATE.                                           02950000
029600                                                                  02960000
029700     EXEC SQL                                                     02970000
029800         SELECT                                                   02980000
029900             BTCH_PRC_DTE                                         02990000
030000         INTO                                                     03000000
030100             :APCNTL-BTCH-PRC-DTE                                 03010000
030200         FROM TAPCNTL                                             03020000
030300     END-EXEC.                                                    03030000
030400                                                                  03040000
030500     EVALUATE TRUE                                                03050000
030600         WHEN SQLCODE = ZERO                                      03060000
030700              CONTINUE                                            03070000
030800                                                                  03080000
030900         WHEN SQLCODE = +100                                      03090000
031000         WHEN SQLWARN0 NOT = SPACES                               03100000
031100         WHEN SQLCODE  NOT = ZERO                                 03110000
031200             MOVE 'R200-GET-PROCESS-DATE' TO AA-PARAGRAPH-NAME    03120000
031300             MOVE 'UNSUCCESSFUL SELECT'   TO AA-DB2-OPERATION     03130000
031400             MOVE 'TAPCNTL'               TO AA-DB2-TABLE-1       03140000
031500             MOVE SPACES                  TO AA-DB2-TABLE-2       03150000
031600                                             AA-DB2-TABLE-3       03160000
031700             PERFORM Z998-DB2-ABEND                               03170000
031800     END-EVALUATE.                                                03180000
031900 EJECT                                                            03190000
032000*---------------------------------------------------------------- 03200000
032100*    WRITES OUT THE INVOICE EXTRACT RECORD.                       03210000
032200*---------------------------------------------------------------- 03220000
032300 W100-WRITE-KEYED-DOCUMENT.                                       03230000
032400                                                                  03240000
032500     WRITE KEYED-DOC-EXTRACT-RECORD FROM AP070-EXTRACT-FILE.      03250000
032600     ADD +1 TO PV-WRITTEN-CNT.                                    03260000
032700 EJECT                                                            03270000
032800*----------------------------------------------------------------*03280000
032900*    ABEND ROUTINE FOR DB2 ERRORS                                *03290000
033000*----------------------------------------------------------------*03300000
033100 Z998-DB2-ABEND.                                                  03310000
033200                                                                  03320000
033300     CLOSE AP-TRANS-FILE                                          03330000
033400           KEYED-DOC-EXTRACT-FILE.                                03340000
033500                                                                  03350000
033600     DISPLAY AA-ABEND-LIT.                                        03360000
033700     DISPLAY AA-DB2-ERROR-LIT.                                    03370000
033800     DISPLAY AA-PROGRAM-LIT.                                      03380000
033900     DISPLAY AA-PARAGRAPH-LIT.                                    03390000
034000     DISPLAY AA-DB2-OPERATION-LIT.                                03400000
034100     DISPLAY AA-DB2-TABLE-1.                                      03410000
034200     DISPLAY AA-DB2-TABLE-2.                                      03420000
034300     DISPLAY AA-DB2-TABLE-3.                                      03430000
034400     SET AC-DB2-ERROR TO TRUE.                                    03440000
034500                                                                  03450000
034600     COPY DPPD004.                                                03460000
034700                                                                  03470000
034800     CALL 'ILBOABN0' USING ABEND-CODE.                            03480000
034900                                                                  03490000
