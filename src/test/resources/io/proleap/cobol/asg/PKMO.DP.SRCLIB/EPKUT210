019514*----------------------------------------------------------       01680000
019515* 2000-PROCESS-STORE-RECS                                         01690000
019516*     WRITES THE STATIC OFFER FILE FOR A GIVEN STORE AND GETS THE 01700000
019517*     NEXT STORE INFORMATION REQUESTING A FULL FILE.              01710000
019518*----------------------------------------------------------       01720000
019519 2000-PROCESS-STORE-RECS.                                         01730000
019520                                                                  01740000
019521     SET PS-NOT-END-OF-TBL TO TRUE.                               01750000
019522                                                                  01760000
019523     PERFORM 2200-LOAD-WRITE-OFFER VARYING PV-STA-OFR-IDX         01770000
019524               FROM 1 BY 1                                        01780000
019525               UNTIL PV-STA-OFR-IDX > PC-MAX-STATIC-OFR-TBL OR    01790000
019526                PS-END-OF-TBL                                     01800000
019527                                                                  01810000
019528     PERFORM 2300-GET-NEXT-STORE.                                 01820000
019529                                                                  01830000
019530*2000-EXIT.                                                       01840000
019531 EJECT.                                                           01850000
019532                                                                  01860000
019533*----------------------------------------------------------       01870000
019534* 2100-READ-LOAD-STATIC-OFFER                                     01880000
019535*    LOADS THE STATIC OFFER IN A TEMPORARY TABLE                  01890000
019536*----------------------------------------------------------       01900000
019537 2100-READ-LOAD-STATIC-OFFER.                                     01910000
019538                                                                  01920000
019539     PERFORM VARYING PV-STA-OFR-IDX                               01930000
019540                  FROM 1 BY 1                                     01940000
019541          UNTIL PV-STA-OFR-IDX > PC-MAX-STATIC-OFR-TBL OR         01950000
019542          PS-END-OF-ST-OFR                                        01951000
019543                                                                  01952000
019544         PERFORM 6100-READ-STATIC-OFFER                           01953000
019545                                                                  01954000
019546          IF PS-END-OF-ST-OFR                                     01954100
019547             IF PV-STA-OFR-IDX = 1                                01954200
019548                DISPLAY ' '                                       01954300
019549                DISPLAY ' NO STATIC OFFERS TO PROCESS '           01954400
019550                DISPLAY ' '                                       01954500
019551                SET PS-END-OF-PROCESSING TO TRUE                  01954600
019552             ELSE                                                 01954700
019553              CONTINUE                                            01954800
019554             END-IF                                               01954900
019555          ELSE                                                    01955000
019556             MOVE EP205-OFFER-EXTRACT  TO                         01955100
019557                 PV-OFFER-DATA (PV-STA-OFR-IDX)                   01955200
019558          END-IF                                                  01955300
019559                                                                  01955400
019560     END-PERFORM.                                                 01955500
019561                                                                  01955600
019562     IF PV-STA-OFR-IDX > PC-MAX-STATIC-OFR-TBL                    01955700
019563        DISPLAY 'LIMIT OF STATIC-OFFER TABLE REACHED'             01955800
019564        MOVE '2100-READ-LOAD-STATIC-OFFER' TO                     01955900
019565                               PV-PARAGRAPH-NAME                  01956000
019566        SET AC-4004-TABLE-OVERFLOW         TO TRUE                01956100
019567        PERFORM 9000-ABEND                                        01956200
019568     END-IF.                                                      01956300
019569                                                                  01956400
019570*2100-EXIT.                                                       01956500
019571 EJECT.                                                           01956600
019572                                                                  01956700
019573*----------------------------------------------------------       01956800
019574* 2200-LOAD-WRITE-OFFER                                           01956900
019575*  WRITES THE STATIC OFFER FOR A PARTICULAR STORE TO OUTPUT FILE  01957000
019576*----------------------------------------------------------       01957100
019577 2200-LOAD-WRITE-OFFER.                                           01957200
019578                                                                  01957300
019579     MOVE PV-OFFER-DATA (PV-STA-OFR-IDX) TO PV205-OFFER-EXTRACT.  01957400
019580     MOVE PV-CURR-STORE TO PV205-STR-NBR.                         01957500
019581     MOVE PV205-OFFER-EXTRACT TO  STATIC-OFFERS-FULL-RECORD.      01957600
019582                                                                  01957700
019583     EVALUATE TRUE                                                01957800
019584                                                                  01957900
019585         WHEN PV205-EVENT-ID = SPACES  OR                         01958000
019586              PV205-EVENT-ID = LOW-VALUES                         01958100
019587              SET PS-END-OF-TBL TO TRUE                           01958200
019588                                                                  01958300
019589         WHEN OTHER                                               01958400
019590                                                                  01958500
019591             WRITE STATIC-OFFERS-FULL-RECORD                      01958600
019592             ADD 1 TO PV-OFFER-WRITEN                             01958700
019593                                                                  01958800
019594             IF PV205-EVENT-ID  = EP206-EVENT-ID                  01958900
019595             AND PV205-OFFER-ID = EP206-OFFER-ID                  01959000
019596                CONTINUE                                          01959100
019597             ELSE                                                 01959200
019598                PERFORM 2250-LOAD-WRITE-STR-GROUP                 01959300
019599             END-IF                                               01959400
019600                                                                  01959500
019601     END-EVALUATE.                                                01959600
019602                                                                  01959700
019603*2200-EXIT.                                                       01959800
019604 EJECT.                                                           01959900
