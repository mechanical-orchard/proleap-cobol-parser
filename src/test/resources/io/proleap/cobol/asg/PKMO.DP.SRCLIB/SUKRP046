090100*---------------------------------------------------------------* 09010008
090200*   TRIP CURSOR - BRINGS BACK ALL SML/SKUS FOR A GIVEN TRIP     * 09020008
090300*---------------------------------------------------------------* 09030008
090400     EXEC SQL                                                     09040008
090500          DECLARE TRIP_SML_CSR CURSOR FOR                         09050008
090600              SELECT TRIP_OCRNC_ID                                09060008
090700                    ,DIVERT_TMST                                  09070008
090800                    ,SHIPT_UNT_ID                                 09080008
090900                    ,SKU_NBR                                      09090008
091000                    ,MDSE_LNE_CDE                                 09100008
091100                    ,HNG_FOLD_SEQ_NBR                             09110008
091200                    ,PRC_MSG_CDE                                  09120008
091300                    ,EAS_TAG_IND                                  09130008
091400                    ,UNT_QTY                                      09140008
091500                FROM SUT_TRIP_DTL_SUM                             09150008
091600               WHERE TRIP_OCRNC_ID  =  :SUT00095-TRIP-OCRNC-ID    09160008
091700          ORDER BY                                                09170008
091800                DIVERT_TMST DESC                                  09180099
091900               ,SHIPT_UNT_ID                                      09190008
092000               ,SKU_NBR                                           09200008
092100     END-EXEC.                                                    09210008
