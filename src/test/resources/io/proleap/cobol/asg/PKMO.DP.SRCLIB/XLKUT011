010800***************************************************************** 01080001
010900 PROCEDURE DIVISION.                                              01090001
011000***************************************************************** 01100001
011100     DISPLAY '**** BEGINNING XLKUT011 ****'.                      01110001
011200     DISPLAY ' '.                                                 01120001
011300                                                                  01130001
011400     OPEN INPUT  VSM-SKU-FILE                                     01140001
011401                 LOGISTIC-SKU-FILE                                01140001
011410          OUTPUT ERROR-FILE.                                            01
011420     SET PS-EMAIL-NO       TO TRUE.                                       
011500                                                                  01150001
011600     PERFORM 1000-READ-VSM-INPUT.                                 01160001
011610     PERFORM 1100-READ-LOGISTIC-INPUT.                            01160001
011700                                                                  01170001
012100     PERFORM 2000-PROCESS-INPUT-FILE                              01210001
012200       UNTIL PS-EOF-INPUT1-YES AND                                      01
012210             PS-EOF-INPUT2-YES.                                         01
012300                                                                  01230001
012310     CLOSE VSM-SKU-FILE                                                 01
012320           LOGISTIC-SKU-FILE                                            01
012330           ERROR-FILE.                                                  01
012500                                                                  01250001
012600     DISPLAY '  INPUT VSM SKU REC    = ' PV-RECORDS-READ-1.       01260001
012610     DISPLAY '  INPUT DOMAIN SKU REC = ' PV-RECORDS-READ-2.             01
012700     DISPLAY '  OUTPUT RECORDS WRITTEN = ' PV-RECORDS-WRITTEN.          01
012800     DISPLAY ' '.                                                 01280001
012900     DISPLAY '****  END OF  XLKUT011  ****'.                      01290001
013000                                                                  01300001
013100*    SET RETURN CODE IF E-MAIL SWITCH IS TRUE.                            
013200     IF PS-EMAIL-YES                                                      
013300         MOVE 4095 TO RETURN-CODE                                         
013301     END-IF.                                                              
013302     GOBACK.                                                      01310001
013303                                                                  01320001
013304                                                                  01330001
013400***************************************************************** 01340001
013500*  READ A VSM SKU RECORD                                          01350003
013600***************************************************************** 01360001
013700 1000-READ-VSM-INPUT.                                             01370001
013800                                                                  01380001
013900     MOVE '1000-READ-INPUT-FILE    ' TO PV-ABEND-PARAGRAPH.       01390001
014000                                                                  01400001
014010     INITIALIZE PV-VSM-INPUT.                                     01401003
014020                                                                  01402003
014100     READ VSM-SKU-FILE                                            01410004
014200          INTO PV-VSM-INPUT                                             01
014300            AT END                                                01430001
014400               SET PS-EOF-INPUT1-YES TO TRUE                            01
014900     END-READ.                                                    01490001
015000                                                                  01500001
015010     IF PS-EOF-INPUT1-YES                                                 
015020        CONTINUE                                                          
015030     ELSE                                                                 
015040        ADD +1 TO PV-RECORDS-READ-1                                       
015050     END-IF.                                                              
015100                                                                  01510001
015101***************************************************************** 01340001
015102*  READ A LOGISTIC SKU RECORD                                     01350003
015103***************************************************************** 01360001
015104 1100-READ-LOGISTIC-INPUT.                                        01370001
015105                                                                  01380001
015106     MOVE '1100-READ-LOGISTIC-INPUT' TO PV-ABEND-PARAGRAPH.       01390001
015107                                                                  01400001
015108     INITIALIZE PV-LOGISTIC-INPUT.                                01401003
015109                                                                  01402003
015110     READ LOGISTIC-SKU-FILE                                             04
015111          INTO PV-LOGISTIC-INPUT                                        01
015112            AT END                                                01430001
015113               SET PS-EOF-INPUT2-YES TO TRUE                            01
015114     END-READ.                                                    01490001
015115                                                                  01500001
015116     IF PS-EOF-INPUT2-YES                                                 
015117        CONTINUE                                                          
015118     ELSE                                                                 
015119        ADD +1 TO PV-RECORDS-READ-2                                       
015120     END-IF.                                                              
015130                                                                  01510001
015200***************************************************************** 01520001
015300*  REFORMAT THE VSM SKU EXTRACT FILE                            * 01530003
015400***************************************************************** 01540001
015500 2000-PROCESS-INPUT-FILE.                                         01550001
015600                                                                  01560001
015700     MOVE '2000-PROCESS-INPUT-FILE ' TO PV-ABEND-PARAGRAPH.       01570001
015800                                                                  01580001
015010     IF PS-EOF-INPUT1-YES                                                 
               AND PS-EOF-INPUT2-NO                                             
016010           PERFORM 2100-WRITE-VSM-ERROR                                   
016020           PERFORM 1000-READ-VSM-INPUT                                    
015800     END-IF.                                                      01580001
 15800                                                                  01580001
015010     IF PS-EOF-INPUT2-YES                                                 
015800       AND PS-EOF-INPUT1-NO                                       01580001
016113           PERFORM 2150-WRITE-LOGISTIC-ERROR                              
016114           PERFORM 1100-READ-LOGISTIC-INPUT                               
015800     END-IF.                                                      01580001
015800                                                                  01580001
015800     IF PS-EOF-INPUT1-NO                                          01580001
              AND PS-EOF-INPUT2-NO                                      01580001
015800           PERFORM 2001-PROCESS-INPUT-FILE                        01580001
           END-IF.                                                              
015800                                                                  01580001
015400***************************************************************** 01540001
015500 2001-PROCESS-INPUT-FILE.                                         01550001
                                                                                
015700     MOVE '2001-PROCESS-INPUT-FILE ' TO PV-ABEND-PARAGRAPH.       01570001
                                                                                
015900     IF XL008-INTR-UPC-ID OF PV-VSM-INPUT <                               
016000        XL008-INTR-UPC-ID OF PV-LOGISTIC-INPUT                            
016001*       ON VSM NOT ON DOMAIN                                              
016010           PERFORM 2100-WRITE-VSM-ERROR                                   
016020           PERFORM 1000-READ-VSM-INPUT                                    
016030     ELSE                                                                 
016040        IF XL008-INTR-UPC-ID OF PV-VSM-INPUT =                            
016050           XL008-INTR-UPC-ID OF PV-LOGISTIC-INPUT                         
016051*       ON VSM AND ON DOMAIN                                              
016060              IF XL008-DATA1 OF PV-VSM-INPUT =                            
016070                 XL008-DATA1 OF PV-LOGISTIC-INPUT                         
016071                 IF XL008-DATA2 OF PV-VSM-INPUT =                         
016072                    XL008-DATA2 OF PV-LOGISTIC-INPUT                      
016073                       PERFORM 1000-READ-VSM-INPUT                        
016074                       PERFORM 1100-READ-LOGISTIC-INPUT                   
016075                 ELSE                                                     
016076*       BUT SOMETHING DOESNT MATCH                                        
016077                       PERFORM 2100-WRITE-VSM-ERROR                       
016078                       PERFORM 2150-WRITE-LOGISTIC-ERROR                  
016079                       PERFORM 1000-READ-VSM-INPUT                        
016080                       PERFORM 1100-READ-LOGISTIC-INPUT                   
016081                 END-IF                                                   
016091              ELSE                                                        
016092*       BUT SOMETHING DOESNT MATCH                                        
016093                 PERFORM 2100-WRITE-VSM-ERROR                             
016094                 PERFORM 2150-WRITE-LOGISTIC-ERROR                        
016095                 PERFORM 1000-READ-VSM-INPUT                              
016096                 PERFORM 1100-READ-LOGISTIC-INPUT                         
016097              END-IF                                                      
016109        ELSE                                                              
016110           IF XL008-INTR-UPC-ID OF PV-VSM-INPUT >                         
016111              XL008-INTR-UPC-ID OF PV-LOGISTIC-INPUT                      
016112*       ON DOMAIN NOT ON VSM                                              
016113                PERFORM 2150-WRITE-LOGISTIC-ERROR                         
016114                PERFORM 1100-READ-LOGISTIC-INPUT                          
016120           END-IF                                                         
016130        END-IF                                                            
016140     END-IF.                                                              
016200                                                                          
