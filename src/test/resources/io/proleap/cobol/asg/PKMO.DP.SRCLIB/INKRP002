042300 PROCEDURE DIVISION.                                                      
042400******************************************************************        
042500                                                                          
042600******************************************************************        
042700 0000-MAINLINE-INKRP002.                                                  
042800******************************************************************        
042900                                                                          
043000     PERFORM 1000-INITIALIZATION.                                         
043100                                                                          
043200     PERFORM 2000-MAIN-PROCESS.                                           
043300                                                                          
043400     PERFORM 8000-EOJ.                                                    
043500                                                                          
044200     STOP RUN.                                                            
044300                                                                          
044400 EJECT                                                                    
044500                                                                          
044600******************************************************************        
044700 1000-INITIALIZATION.                                                     
044800******************************************************************        
044900*                                                                         
045200     OPEN INPUT  REPORT-SUMMARY-FILE.                                     
045300     OPEN OUTPUT STORE-EXP-SUM-ADJ-RPT.                                   
045400                                                                          
046700     INITIALIZE PV-PROGRAM-VARIABLES.                                     
046800                                                                          
046900     ACCEPT PV-DATE-YYMMDD FROM DATE.                                     
           MOVE PV-DATE-YYMMDD-YY      TO DP132O-RUN-YEAR.                      
047100     MOVE PV-DATE-YYMMDD-MM      TO DP132O-RUN-MONTH.                     
047200     MOVE PV-DATE-YYMMDD-DD      TO DP132O-RUN-DAY.                       
047300     ACCEPT PV-TIME-HHMMSS FROM TIME.                                     
047400     MOVE PV-TIME-HHMMSS-HH      TO DP132O-RUN-HOUR.                      
047500     MOVE PV-TIME-HHMMSS-MM      TO DP132O-RUN-MINUTE.                    
047600     MOVE PC-PROGRAN-NAME        TO DP132O-PROGRAM-NAME.                  
           MOVE 1                      TO DP132O-REPORT-NUMBER.                 
052300                                                                          
055600     PERFORM 4200-READ-REPORT-SUMM-FILE.                                  
                                                                                
054300     PERFORM 6100-WRITE-REPORT-TITLE.                                     
054400                                                                          
055000                                                                          
055100                                                                          
055200******************************************************************        
055300 2000-MAIN-PROCESS.                                                       
055400******************************************************************        
055500*                                                                         
055800     PERFORM                                                              
055900       UNTIL PS-NO-MORE-SUM-RECS                                          
056000         PERFORM 6000-CREATE-REPORT                                       
056100         PERFORM 4200-READ-REPORT-SUMM-FILE                               
056200     END-PERFORM.                                                         
056300                                                                          
056500                                                                          
060000******************************************************************        
060100 4200-READ-REPORT-SUMM-FILE.                                              
060200******************************************************************        
060300*                                                                         
060400     READ REPORT-SUMMARY-FILE                                             
060500       INTO IN030-REPORT-DETAIL                                           
060600         AT END                                                           
060700             SET PS-NO-MORE-SUM-RECS                                      
060800                                 TO TRUE                                  
060900     END-READ.                                                            
061000                                                                          
061100     IF PS-MORE-SUM-RECS                                                  
061200         ADD 1                   TO PA-SUM-RECORDS-READ                   
061300     END-IF.                                                              
061400                                                                          
061500 EJECT                                                                    
061600                                                                          
064700******************************************************************        
064800 6000-CREATE-REPORT.                                                      
064900******************************************************************        
065000                                                                          
065100     IF PV-LINE-COUNT > PC-MAX-LINE-COUNT                                 
065200         PERFORM 6100-WRITE-REPORT-TITLE                                  
           END-IF.                                                              
                                                                                
           MOVE IN030-LOC-NBR          TO DL-LOC-NBR                            
                                          XMT00001-LOC-NBR.                     
           PERFORM 6200-SELECT-XMT-LOC.                                         
           MOVE XMT00001-LOC-NM        TO DL-LOC-NAME.                          
           COMPUTE PV-ADJ-UNITS = IN030-ADJ-UNITS * -1.                         
           MOVE PV-ADJ-UNITS           TO DL-ADJ-UNITS.                         
           COMPUTE PV-EXTD-RLT-AMT = IN030-EXTD-RTL-AMT * -1.                   
           MOVE PV-EXTD-RLT-AMT        TO DL-EXTD-RTL-AMT.                      
           ADD PV-ADJ-UNITS            TO PA-TOT-ADJ-UNITS.                     
           ADD PV-EXTD-RLT-AMT         TO PA-TOT-EXTD-RTL-AMT.                  
066500                                                                          
067800     WRITE STORE-EXP-SUM-ADJ-REC FROM DL-LINE-1                           
067900         AFTER ADVANCING 1 LINE.                                          
068000     ADD 1                       TO PV-LINE-COUNT.                        
068100     ADD 1                       TO PA-REPORT-LINES-WRITTEN.              
068200                                                                          
068300 EJECT                                                                    
068400                                                                          
061700******************************************************************        
061800 6100-WRITE-REPORT-TITLE.                                                 
061900******************************************************************        
062000*                                                                         
062100     ADD 1                       TO PV-PAGE-COUNT.                        
062200     MOVE PV-PAGE-COUNT          TO DP132O-PAGE-NUMBER.                   
062300                                                                          
062400     WRITE STORE-EXP-SUM-ADJ-REC FROM                                     
062400                                    DP132O-STANDRD-HEADER-132-COLS        
062500         AFTER ADVANCING PAGE.                                            
062600                                                                          
062700     WRITE STORE-EXP-SUM-ADJ-REC FROM HD-REPORT-TITLE-2                   
062800         AFTER ADVANCING 1 LINE.                                          
062900                                                                          
063300     WRITE STORE-EXP-SUM-ADJ-REC FROM HD-COLUMN-HEADING-1                 
063400         AFTER ADVANCING 2 LINES.                                         
063500                                                                          
063900     MOVE SPACES                 TO STORE-EXP-SUM-ADJ-REC.                
064000     WRITE STORE-EXP-SUM-ADJ-REC                                          
064100         AFTER ADVANCING 1 LINE.                                          
064200                                                                          
064300     MOVE +5                     TO PV-LINE-COUNT.                        
064400                                                                          
064500 EJECT                                                                    
064600                                                                          
      ******************************************************************07560099
       6200-SELECT-XMT-LOC.                                             08340099
      ******************************************************************07560099
                                                                                
           EXEC SQL                                                             
                SELECT LOC_NM                                                   
                  INTO :XMT00001-LOC-NM                                         
                  FROM XMT_LOC                                                  
                  WHERE LOC_NBR       = :XMT00001-LOC-NBR                       
           END-EXEC.                                                            
                                                                                
           EVALUATE TRUE                                                        
               WHEN SQLCODE = ZERO                                              
                   CONTINUE                                                     
               WHEN SQLCODE = +100                                              
                   MOVE 'UNKNOWN'  TO XMT00001-LOC-NM                           
               WHEN OTHER                                                       
                   MOVE '6200-SELECT-XMT-LOC'                           20630099
                                       TO AA-PARAGRAPH-NAME             20640099
                   MOVE 'XNT_LOC TABLE ERROR'                           20650099
                                       TO AA-MESSAGE                    20660099
                   MOVE 'SELECT'       TO AA-DB2-OPERATION              20670099
                   PERFORM 9901-SQL-ERROR                               20680099
           END-EVALUATE.                                                        
                                                                                
068500******************************************************************        
068600 8000-EOJ.                                                                
068700******************************************************************        
068800*                                                                         
           MOVE PA-TOT-ADJ-UNITS    TO TL-ADJ-UNITS.                            
           MOVE PA-TOT-EXTD-RTL-AMT TO TL-EXTD-RTL-AMT.                         
                                                                                
           WRITE STORE-EXP-SUM-ADJ-REC FROM TL-LINE-1                           
               AFTER ADVANCING 2 LINES.                                         
                                                                                
069100     WRITE STORE-EXP-SUM-ADJ-REC FROM ER-END-OF-REPORT-LINE               
069200         AFTER ADVANCING 3 LINES.                                         
           ADD +5                   TO PA-REPORT-LINES-WRITTEN.                 
069300                                                                          
069400     PERFORM 8100-DISPLAY-RPT-MSG-SUMMARY.                                
069500                                                                          
069800     CLOSE REPORT-SUMMARY-FILE                                            
069900           STORE-EXP-SUM-ADJ-RPT.                                         
070000                                                                          
070100 EJECT                                                                    
070200                                                                          
