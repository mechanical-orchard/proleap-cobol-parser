022000 PROCEDURE DIVISION.                                              02180000
022100 0000-PROGRAM-MAINLINE.                                           02190000
022200                                                                  02200000
022300     INITIALIZE CA-COMMAREA                                       02210000
022400     MOVE DFHCOMMAREA                 TO CA-COMMAREA              02220000
022500                                                                  02230000
022600     EVALUATE TRUE                                                02240000
022700         WHEN EIBCALEN = ZERO                                     02250000
022800             MOVE PC-ENTER-THROUGH-MENU-MSG                       02260000
022900                                      TO PV-MESSAGE-AREA          02270000
023000             PERFORM 8110-RETURN                                  02280000
023100                                                                  02290000
023200         WHEN EIBTRNID = 'Z278'                                   02300000
023300             MOVE DFHCOMMAREA         TO PC192-COMMAREA           02310000
023400             MOVE PC192-HOST-SAVE-AREA                            02320000
023500                                      TO CA-COMMAREA              02330000
023600             SET PC033-PROCESS-SCREEN TO TRUE                     02340000
023700             MOVE PC192-HOST-SAVE-MAP TO PC856AO                  02350000
023800             MOVE -1                  TO ASMLL                    02360000
023900             PERFORM 8000-SEND-MAP                                02370000
024000             PERFORM 8100-RETURN-TRANID                           02380000
024100                                                                  02390000
024200         WHEN PC033-INITIALIZE-SCREEN                             02400000
024300         WHEN EIBAID = DFHCLEAR                                   02410000
024400             SET PC033-PROCESS-SCREEN TO TRUE                     02420000
024500             PERFORM 1000-INITIALIZE                              02430000
024600             PERFORM 8000-SEND-MAP                                02440000
024700             PERFORM 8100-RETURN-TRANID                           02450000
024800                                                                  02460000
024900         WHEN EIBAID = DFHPF3                                     02470000
025000             MOVE 'PCKRF853'          TO PV-TRANSFER-PROGRAM      02480000
025100             PERFORM 8200-XCTL                                    02490000
025200                                                                  02500000
025300         WHEN EIBAID = DFHENTER                                   02510000
025400             PERFORM 2000-PROCESS-SCREEN-FIELDS                   02520000
025500             IF PS-ERRORS-NO                                      02530000
025600                 IF PS-XCTL-E855                                  02540000
025700                     SET PC033-ADD-FAST-TO-LABEL                  02550000
025800                                      TO TRUE                     02560000
025900                     MOVE 'PCKRF855'  TO PV-TRANSFER-PROGRAM      02570000
026000                     PERFORM 8200-XCTL                            02580000
026100                 END-IF                                           02590000
026200             ELSE                                                 02600000
026300                 IF PS-XCTL-E852                                  02610000
026400                     PERFORM 8210-XCTL-ERROR-SCREEN               02620000
026500                 ELSE                                             02630000
026600                     PERFORM 8010-SEND-DATA-ONLY                  02640000
026700                     PERFORM 8100-RETURN-TRANID                   02650000
026800                 END-IF                                           02660000
026900             END-IF                                               02670000
027000                                                                  02680000
027100         WHEN EIBAID = DFHPF1                                     02690000
027200             PERFORM 8020-RECEIVE-MAP                             02700000
027300             MOVE PC856AO             TO PC192-HOST-SAVE-MAP      02710000
027400             PERFORM 1200-HELP-SCREEN                             02720000
027500                                                                  02730000
027600         WHEN OTHER                                               02740000
027700             PERFORM 8020-RECEIVE-MAP                             02750000
027800             MOVE -1                  TO ASMLL                    02760000
027900             MOVE PC-UNRECOGNIZED-PF-KEY-MSG                      02770000
028000                                      TO AMSGLNO                  02780000
028100             PERFORM 8000-SEND-MAP                                02790000
028200*            PERFORM 8010-SEND-DATA-ONLY                          02800000
028300             PERFORM 8100-RETURN-TRANID                           02810000
028400     END-EVALUATE.                                                02820000
028500                                                                  02830000
028600     GOBACK.                                                      02840000
028700                                                                  02850000
028800 1000-INITIALIZE.                                                 02860000
028900                                                                  02870000
029000     MOVE SPACES                      TO AMSGLNO                  02880000
029100                                         ASMLO                    02890000
029200     MOVE PC-TRANID                   TO PC033-TRAN-ID            02900000
029300     MOVE PC-CURRENT-PROGRAM-NAME     TO PC033-LAST-PROGRAM.      02910000
029400                                                                  02920000
029500 1200-HELP-SCREEN.                                                02930000
029600     MOVE 'DPKRF278'                  TO PV-TRANSFER-PROGRAM      02940000
029700     MOVE 'PCKRF856'                  TO PC192-PROGRAM-NAME       02950000
029800     COPY PCPD188.                                                02960000
029900     MOVE PC188-DISPLAY-DATE          TO PC192-DISPLAY-DATE.      02970000
030000     MOVE CA-COMMAREA                 TO PC192-HOST-SAVE-AREA     02980000
030100     PERFORM 8220-XCTL-TO-HELP.                                   02990000
030200                                                                  03000000
030300/                                                                 03010000
030400 2000-PROCESS-SCREEN-FIELDS.                                      03020000
030500                                                                  03030000
030600     PERFORM 8020-RECEIVE-MAP                                     03040000
030700                                                                  03050000
030800     PERFORM 2100-EDIT-INPUT.                                     03060000
030900                                                                  03070000
031000                                                                  03080000
031100     IF PS-VALID-INPUT                                            03090000
031200         SET PS-ERRORS-NO             TO TRUE                     03100000
031300         PERFORM 2200-CHECK-TABLES                                03110000
031400     ELSE                                                         03120000
031500         PERFORM 8010-SEND-DATA-ONLY                              03130000
031600         PERFORM 8100-RETURN-TRANID                               03140000
031700     END-IF.                                                      03150000
031800                                                                  03160000
031900/                                                                 03170000
032000 2100-EDIT-INPUT.                                                 03180000
032100                                                                  03190000
032200     IF  ASMLL > ZERO                                             03200000
032300         MOVE ASMLI TO PV-CTN                                     03210000
032400     ELSE                                                         03220000
032500         IF ASMLA = DP015-ERASE-EOF                               03230000
032600             MOVE SPACES TO PV-CTN                                03240000
032700         END-IF                                                   03250000
032800     END-IF.                                                      03260000
032900                                                                  03270000
033000     PERFORM 2110-EDIT-CTN.                                       03280000
033100                                                                  03290000
033200 2110-EDIT-CTN.                                                   03300000
033300                                                                  03310000
033400     SET PS-VALID-INPUT               TO TRUE                     03320000
033500* BARCODE CAN BE EITHER 20 DIGITS OR 18 DIGITS WITHOUT            03330000
033600* THE LEADING 2 ZEROS                                             03340000
033700                                                                  03350000
033800     PERFORM VARYING PV-SUB FROM 1 BY 1                           03360000
033900             UNTIL PV-SUB > 20                                    03370000
034000                                                                  03380000
034100         EVALUATE TRUE                                            03390000
034200         WHEN PV-CTN-CHAR (PV-SUB) = ZERO                         03400000
034300*            IF PV-SUB < 3 OR > 18                                03410000
034400             IF PV-SUB < 3                                        03420000
034500                 MOVE ZERO            TO PV-BARCODE-CHAR          03430000
034600                                         (PV-SUB)                 03440000
034700             ELSE                                                 03450000
034800                 MOVE '9'             TO PV-BARCODE-CHAR          03460000
034900                                         (PV-SUB)                 03470000
035000             END-IF                                               03480000
035100         WHEN PV-CTN-CHAR (PV-SUB) = '4'                          03490000
035200             IF PV-SUB < 4                                        03500000
035300                 MOVE '4'             TO PV-BARCODE-CHAR          03510000
035400                                         (PV-SUB)                 03520000
035500             ELSE                                                 03530000
035600                 MOVE '9'             TO PV-BARCODE-CHAR          03540000
035700                                         (PV-SUB)                 03550000
035800             END-IF                                               03560000
035900         WHEN PV-CTN-CHAR (PV-SUB) = SPACE                        03570000
036000             MOVE 'B'                 TO PV-BARCODE-CHAR          03580000
036100                                         (PV-SUB)                 03590000
036200         WHEN PV-CTN-CHAR (PV-SUB) NUMERIC                        03600000
036300             MOVE '9'                 TO PV-BARCODE-CHAR          03610000
036400                                         (PV-SUB)                 03620000
036500         WHEN OTHER                                               03630000
036600             MOVE 'X'                 TO PV-BARCODE-CHAR          03640000
036700                                         (PV-SUB)                 03650000
036800             MOVE PC-MUST-BE-NUM-MSG   TO AMSGLNO                 03660000
036900         END-EVALUATE                                             03670000
037000     END-PERFORM.                                                 03680000
037100                                                                  03690000
037200                                                                  03700000
037300     INITIALIZE DK-BRCDE-NBR                                      03710000
037400                PV-CTN-18                                         03720000
037500                PC033-CARTON                                      03730000
037600                PC033-FIRST-CARTON                                03740000
037700                PC033-LAST-CARTON                                 03750000
037800                PC033-BRCDE-NBR                                   03760000
037900                AMSGLNO                                           03770000
038000     MOVE DP015-UNP-ALP-NOR-MDT       TO ASMLA.                   03780000
038100                                                                  03790000
038200     IF PV-VALID-BARCODE                                          03800000
038300         EVALUATE TRUE                                            03810000
038400             WHEN PV-2-LEAD-ZEROES                                03820000
038500                 MOVE PV-CTN (3:18)   TO PV-CTN-18                03830000
038600                 MOVE PV-CTN          TO DK-BRCDE-NBR             03840000
038700             WHEN PV-LEAD-TRAIL-SPACES                            03850000
038800                 MOVE PV-CTN (2:18)   TO PV-CTN-18                03860000
038900                 MOVE ZEROES          TO DK-BRCDE-NBR (1:2)       03870000
039000                 MOVE PV-CTN (2:18)   TO DK-BRCDE-NBR (3:18)      03880000
039100             WHEN PV-2-LEAD-SPACES                                03890000
039200                 MOVE PV-CTN (3:18)   TO PV-CTN-18                03900000
039300                 MOVE ZEROES          TO DK-BRCDE-NBR (1:2)       03910000
039400                 MOVE PV-CTN (3:18)   TO DK-BRCDE-NBR (3:18)      03920000
039500             WHEN PV-TRAIL-SPACES                                 03930000
039600                 MOVE PV-CTN (1:18)   TO PV-CTN-18                03940000
039700                 MOVE ZEROES          TO DK-BRCDE-NBR (1:2)       03950000
039800                 MOVE PV-CTN (1:18)   TO DK-BRCDE-NBR (3:18)      03960000
039900         END-EVALUATE                                             03970000
040000                                                                  03980000
040100                                                                  03990000
040200         MOVE PV-CTN-N                TO PC033-CARTON             04000000
040300                                         PC033-FIRST-CARTON       04010000
040400                                         PC033-LAST-CARTON        04020000
040500         MOVE DK-BRCDE-NBR            TO PC033-BRCDE-NBR          04030000
040600         MOVE PC033-BRCDE-NBR (6:4)   TO PC033-STR-NBR            04040006
040700         MOVE PC033-CARTON            TO ASMLO                    04050000
040800                                                                  04060000
040900     ELSE                                                         04070000
041000         SET PS-INVALID-INPUT         TO TRUE                     04080000
041100         MOVE DP015-UNP-ALP-NOR-MDT   TO ASMLA                    04090000
041200         MOVE -1                      TO ASMLL                    04100000
041300                                                                  04110000
041400         MOVE PV-CTN                  TO ASMLO                    04120000
041500         MOVE 'INVALID CTN ID      '  TO AMSGLNO                  04130000
041600     END-IF.                                                      04140000
041700/                                                                 04150000
041800 2200-CHECK-TABLES.                                               04160000
041900                                                                  04170000
042000     PERFORM 7050-SELECT-OTBND-CART                               04180001
042100     IF PS-OTBND-CART-YES                                         04190001
042200         IF SUT00024-ACTV-IND = 'Y'                               04200005
042300             CONTINUE                                             04210000
042400         ELSE                                                     04220000
042500             SET PS-ERRORS-YES        TO TRUE                     04230000
042600             MOVE 'INACTIVE CARTON'   TO AMSGLNO                  04240000
042700         END-IF                                                   04250000
042800         MOVE PC033-FAST-NBR          TO PV-FAST-NBR              04260000
