046800 0000-MAINLINE.                                                   04680001
046900                                                                  04690001
047000     PERFORM 0010-INITIALIZATION.                                 04700001
047100                                                                  04710001
047200     PERFORM 1000-PROCESS-INPUT                                   04720001
047400       UNTIL PS-EOF-INPUT.                                        04740001
047600                                                                  04760001
047610     WRITE REPORT-RECORD                                          04761001
047620      FROM END-OF-REPORT-LINE                                     04762001
047630     AFTER ADVANCING 3 LINES.                                     04763001
047640                                                                  04764001
047641     IF  RC111-WEEKLY  AND                                        04764112
047642         RC111-REPORT-DATE   =  RC111-MONTH-END-DATE              04764212
047643         MOVE 4050 TO RETURN-CODE                                 04764312
047644     END-IF.                                                      04764412
047646                                                                  04764611
047650     CLOSE INPUT01-FILE                                           04765001
047660           REPORT-FILE.                                           04766001
047670                                                                  04767001
047700     GOBACK.                                                      04770001
047800                                                                  04780001
047810                                                                  04781001
047820*----------------------------------------------------------------*04782001
047830*                                                                 04783001
047840*----------------------------------------------------------------*04784001
048200 0010-INITIALIZATION.                                             04820001
048300                                                                  04830001
048400     OPEN INPUT  DATE-FILE                                        04840015
048410                 INPUT01-FILE                                     04841011
048500          OUTPUT REPORT-FILE.                                     04850001
048510                                                                  04851011
048520     READ DATE-FILE INTO                                          04852015
048530          RC111-DATE-CONTROL-REC                                  04853011
048540     AT END                                                       04854011
048550         CONTINUE                                                 04855011
048560     NOT AT END                                                   04856011
048570         DISPLAY 'DATE CONTROL CARD:'                             04857011
048580         DISPLAY RC111-DATE-CONTROL-REC                           04858011
048590         DISPLAY SPACES                                           04859011
048591     END-READ.                                                    04859111
048592                                                                  04859211
048593     IF  RC111-DAILY                                              04859311
048594         SET  HDR01-DAILY           TO  TRUE                      04859411
048595     ELSE                                                         04859511
048596         IF  RC111-WEEKLY                                         04859611
048597             SET HDR01-WEEKLY       TO  TRUE                      04859711
048598         ELSE                                                     04859811
048599             IF  RC111-MONTHLY                                    04859911
048602                 SET HDR01-MONTHLY  TO  TRUE                      04860211
048603             END-IF                                               04860311
048604         END-IF                                                   04860411
048605     END-IF.                                                      04860511
048606                                                                  04860611
048607     MOVE RC111-REPORT-DATE  TO  HEADING-2-DATE.                  04860716
048610                                                                  04861001
048700     ACCEPT PV-TODAYS-DATE   FROM DATE.                           04870001
048800     ACCEPT PV-TODAYS-TIME   FROM TIME.                           04880001
048900                                                                  04890001
049000     MOVE PV-TODAYS-DATE-MM      TO DP132O-RUN-MONTH.             04900011
049100     MOVE PV-TODAYS-DATE-DD      TO DP132O-RUN-DAY.               04910011
049200     MOVE PV-TODAYS-DATE-YY      TO DP132O-RUN-YEAR.              04920011
049300     MOVE PV-TODAYS-TIME-HH      TO DP132O-RUN-HOUR.              04930011
049400     MOVE PV-TODAYS-TIME-MM      TO DP132O-RUN-MINUTE.            04940011
049500     MOVE PC-PROGRAM-NAME        TO DP132O-PROGRAM-NAME.          04950011
049600     MOVE PC-REPORT-NBR          TO DP132O-REPORT-NUMBER.         04960011
049700                                                                  04970001
049710     PERFORM 7000-READ-INPUT.                                     04971001
049711                                                                  04971101
049712     IF  PS-EOF-INPUT                                             04971201
049713         PERFORM 9040-PAGE-HEADERS                                04971301
049714         WRITE REPORT-RECORD                                      04971401
049715          FROM NO-DATA-TO-REPORT-LINE                             04971501
049716         AFTER ADVANCING 2 LINES                                  04971601
049717     ELSE                                                         04971701
049720                                                                  04972001
049721         PERFORM 2000-GET-OPER-NAME                               04972101
049730         PERFORM 9040-PAGE-HEADERS                                04973001
049731                                                                  04973126
