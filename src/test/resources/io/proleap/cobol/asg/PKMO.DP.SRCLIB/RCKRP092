057240         PERFORM 9010-REPORT-OPER-TOTAL                           05724011
057250     END-IF.                                                      05725011
057300                                                                  05730001
057600                                                                  05760001
057700*----------------------------------------------------*            05770001
057800* GET OPER UNIT NAME                                 *            05780001
057900*----------------------------------------------------*            05790001
058000 2000-GET-OPER-NAME.                                              05800001
058100                                                                  05810001
058200     MOVE RC110-DIST-CNTR    TO  XLT00010-LOC-NBR.                05820011
058300                                                                  05830001
058400     EXEC SQL                                                     05840001
058500         SELECT A.LOC_NM                                          05850001
058600           INTO :XLT00010-LOC-NM                                  05860001
058700           FROM XLT_LOC A                                         05870001
058800          WHERE A.LOC_NBR     = :XLT00010-LOC-NBR                 05880001
058900           WITH UR                                                05890001
059000     END-EXEC.                                                    05900001
059100                                                                  05910001
059200     EVALUATE TRUE                                                05920001
059300       WHEN SQLCODE = ZERO                                        05930001
059310           MOVE XLT00010-LOC-NBR TO HDR02-DC                      05931004
059400           MOVE XLT00010-LOC-NM  TO HDR02-DC-DESCRIPTION          05940004
059410                                                                  05941004
059500       WHEN OTHER                                                 05950001
059510           MOVE ZEROES           TO HDR02-DC                      05951004
059600           MOVE SPACES           TO HDR02-DC-DESCRIPTION          05960004
059610                                                                  05961004
059700     END-EVALUATE.                                                05970001
059800                                                                  05980001
