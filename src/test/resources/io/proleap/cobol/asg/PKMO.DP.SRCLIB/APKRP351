017100 PROCEDURE DIVISION.                                              00890000
017200*-----------------------*                                         00900000
017300                                                                  00910000
017400     PERFORM B100-INITIALIZATION.                                 00920000
017500     PERFORM B200-PROCESS-EXTRACT-FILE                            00930000
017600       UNTIL ADJ-INPUT-EOF.                                       00940000
017700     PERFORM W300-WRITE-END-OF-REPORT.                            00950000
017800     PERFORM B300-END-OF-JOB.                                     00960000
017900     GOBACK.                                                      00970000
018000                                                                  00980000
018100*--------------------------------------------------------------*  00990000
018200* INITIALIZATION PROCESSING                                    *  01000000
018300*--------------------------------------------------------------*  01010000
018400 B100-INITIALIZATION.                                             01020000
018500                                                                  01030000
018510     OPEN INPUT  ADJ-ERR-EXCPT-FILE                               01040000
018520          OUTPUT ADJ-ERR-EXCPT-RPT-FILE.                          01050000
018530                                                                  01060000
018600     ACCEPT SYSTEM-DATE FROM DATE.                                01070000
018700     MOVE SYSTEM-YEAR                    TO DP200O-RUN-YEAR.      01080008
018800     MOVE SYSTEM-MONTH                   TO DP200O-RUN-MONTH.     01090008
018900     MOVE SYSTEM-DAY                     TO DP200O-RUN-DAY.       01100008
019000     ACCEPT SYSTEM-TIME FROM TIME.                                01110000
019100     MOVE SYSTEM-HOUR                    TO DP200O-RUN-HOUR.      01120008
019200     MOVE SYSTEM-MINUTE                  TO DP200O-RUN-MINUTE.    01130008
019300     MOVE PC-PROGRAM-NAME                TO DP200O-PROGRAM-NAME.  01140008
019400     MOVE 1                              TO DP200O-REPORT-NUMBER. 01150008
019500     DISPLAY 'RUN DATE = ' SYSTEM-DATE.                           01160000
019600     DISPLAY 'RUN TIME = ' SYSTEM-TIME.                           01170000
019700                                                                  01180000
019800     ACCEPT CONTROL-CARD-RECORD.                                  01190000
019810     IF CC-REPORT-SUBTITLE NOT EQUAL SPACES                       01200000
019900        MOVE CC-REPORT-SUBTITLE          TO HL-SUBTITLE           01210000
020000     END-IF.                                                      01220000
020100                                                                  01230000
021500     PERFORM W100-WRITE-REPORT-HEADERS.                           01240000
021510     PERFORM R100-READ-ADJ-ERR-EXCPT-FILE.                        01250000
021511                                                                  01260000
021520     IF ADJ-INPUT-EOF                                             01270000
021530        PERFORM W400-WRITE-NO-INPUT-DATA                          01280000
021531     ELSE                                                         01290000
021532        MOVE 4055           TO RETURN-CODE                        01300000
021540     END-IF.                                                      01310000
021600                                                                  01320000
022300*--------------------------------------------------------------*  01330000
022400* PROCESS PURCHASE ORDERS                                      *  01340000
022500*--------------------------------------------------------------*  01350000
022600 B200-PROCESS-EXTRACT-FILE.                                       01360000
022700                                                                  01370000
022800     MOVE AP122-VND-NBR                    TO DL-VENDOR-NBR.      01380000
022900     MOVE AP122-DOC-NBR                    TO DL-DOC-NBR.         01390000
023000     MOVE AP122-DOC-DTE                    TO DL-DOC-DTE.         01400000
023100     MOVE AP122-AP-TRN-CDE                 TO DL-AP-TRN-CDE.      01410000
023200     MOVE AP122-PO-NBR                     TO DL-PO-NBR.          01420000
023300     MOVE AP122-NET-COST                   TO DL-NET-COST-AMT.    01430000
023301     MOVE AP122-NET-RETAIL                 TO DL-NET-RETL-AMT.    01440000
023302     MOVE AP122-ERROR-TYP-CDE              TO DL-ERR-CDE.         01450000
023303     MOVE AP122-ERROR-FIELD                TO DL-ERROR-FIELD.     01460000
023400     MOVE AP122-ERROR-VALUE                TO DL-ERROR-VALUE.     01470000
023410     MOVE AP122-ERROR-MESSAGE              TO DL-ERROR-MESSAGE.   01480000
