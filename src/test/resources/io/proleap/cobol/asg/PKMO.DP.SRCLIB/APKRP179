022900***** REPORT LAYOUT                                               02290000
023000     COPY DPWS132O.                                               02300000
023100                                                                  02310000
023200***** INPUT FILE LAYOUT                                           02320000
023300     COPY APRD042.                                                02330000
023400                                                                  02340000
023500 PROCEDURE DIVISION.                                              02350000
023600 A100-MAINLINE.                                                   02360000
023700     PERFORM B100-INITIALIZATION.                                 02370000
023800                                                                  02380000
023900     PERFORM B200-PROCESS-VENDOR-DATA                             02390000
024000             UNTIL END-OF-INTRANSIT-DATA.                         02400000
024100                                                                  02410000
