024100*---------------------------------------------------------------* 01550000
024200     EXEC SQL                                                     01560000
024300          INCLUDE SQLCA                                           01570000
024400     END-EXEC.                                                    01580000
024500                                                                  01590000
024600*---------------------------------------------------------------* 01600000
024700*    DEFINE CURSOR FOR ACCESSING TAUCSBL                        * 01610000
024800*---------------------------------------------------------------* 01620000
024900     EXEC SQL                                                     01630000
025000         DECLARE TABLE-CURSOR CURSOR FOR                          01640000
025100           SELECT                                                 01650000
025200              CSTM_ENTR_ID                                        01660000
025300             ,MSTR_BOL_NBR                                        01670000
025400             ,HSE_BOL_NBR                                         01680000
025700             ,LAST_UPD_BY_USR_ID                                  01710000
025800             ,LAST_UPD_TMST                                       01720000
025500             ,AUD_FUNC_CDE                                        01721000
025600             ,AUD_TMST                                            01722000
027900             FROM TAUCSBL                                         01730000
028100            WHERE CSTM_ENTR_ID = :AH028-CSTM-ENTR-ID              01740002
028500         FOR FETCH ONLY                                           01750000
028600         WITH UR                                                  01760000
028700         OPTIMIZE FOR 1 ROW                                       01770000
028800     END-EXEC.                                                    01780000
029000                                                                  01790000
029100 PROCEDURE DIVISION.                                              01800000
029200                                                                  01810000
029300 0000-PROGRAM-MAINLINE.                                           01820000
029400                                                                  01830000
029500     PERFORM 1000-INITIALIZE.                                     01840000
029600                                                                  01850000
029700     PERFORM 1500-READ-DRIVER-RECORD                              01860000
029800         UNTIL END-OF-DRIVER.                                     01870000
029900                                                                  01880000
030000     PERFORM 6000-EOJ-ROUTINE.                                    01890000
030100     STOP RUN.                                                    01900000
030200                                                                  01910000
030400******************************************************************01920000
030500*  INITIALIZATIONS                                               *01930000
030600******************************************************************01940000
030700 1000-INITIALIZE.                                                 01950000
030800                                                                  01960000
030900     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  01970000
031000     DISPLAY DM-DISPLAY-MESSAGE.                                  01980000
031400                                                                  01990000
031500     OPEN INPUT  IN-DRIVER-FILE.                                  02000000
031500     OPEN OUTPUT OUT-ARCHIVE-FILE.                                02010000
031200     MOVE PC-BEGINNING            TO DP-STATUS-DESCRIPTION.       02020000
032500     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02030000
033100     INITIALIZE DCLTAUCSBL                                        02040000
033200                AH030-ARCHIVE-RECORD-LAYOUT.                      02050002
033800                                                                  02060000
031200 1500-READ-DRIVER-RECORD.                                         02070000
031400     READ IN-DRIVER-FILE INTO AH028-ARCHIVE-RECORD-LAYOUT         02080002
031600          AT END SET END-OF-DRIVER  TO TRUE.                      02090000
031700                                                                  02100000
031800     IF MORE-DRIVER                                               02110000
031900         ADD 1 TO PV-TAUCSCN-INPUT-COUNT                          02120000
029600                                                                  02130000
032600         SET PS-TABLE-LOOP-NOT-DONE TO TRUE                       02140000
032700         PERFORM 9100-OPEN-TABLE-CURSOR                           02150000
033000                                                                  02160000
029700         PERFORM 2000-GET-NEXT-FROM-CURSOR                        02170000
029800             UNTIL PS-TABLE-LOOP-DONE                             02180000
                                                                        02190000
032700         PERFORM 9130-CLOSE-TABLE-CURSOR                          02200000
032000     END-IF.                                                      02210000
                                                                        02220000
033900******************************************************************02230000
034000*  FETCH NEXT FROM CURSOR & WRITE OUTPUT DRIVER FILE RECORD.     *02240000
034100******************************************************************02250000
034200 2000-GET-NEXT-FROM-CURSOR.                                       02260000
034300                                                                  02270000
036900     PERFORM 9110-FETCH-TABLE-CURSOR.                             02280000
           IF SQLCODE = 0                                               02290000
034400         MOVE  AUCSBL-CSTM-ENTR-ID TO DISPLAY-CSTM-ENTR-ID        02300000
034410*        DISPLAY 'FETCHED CUSTOMS ENTRY = ' DISPLAY-CSTM-ENTR-ID  02310000
034610                                                                  02320000
036100         PERFORM 2010-POPULATE-ARCHIVE-RECORD                     02330000
036200         PERFORM 9650-WRITE-ARCHIVE-RECORD                        02340000
036300         PERFORM 2100-CHECK-PGM-PROGRESS                          02350000
036600     END-IF.                                                      02360000
037100******************************************************************02370000
037200*  MOVE COLUMNS FROM TABLES TO FIELDS IN ARCHIVE FILE RECORD.    *02380000
037300******************************************************************02390000
037400 2010-POPULATE-ARCHIVE-RECORD.                                    02400000
037500                                                                  02410000
           MOVE AUCSBL-CSTM-ENTR-ID       TO AH030-CSTM-ENTR-ID.        02420000
           MOVE AUCSBL-MSTR-BOL-NBR       TO AH030-MSTR-BOL-NBR.        02430000
           MOVE AUCSBL-HSE-BOL-NBR        TO AH030-HSE-BOL-NBR.         02440000
           MOVE AUCSBL-LAST-UPD-BY-USR-ID TO AH030-LAST-UPD-BY-USR-ID.  02470000
           MOVE AUCSBL-LAST-UPD-TMST      TO AH030-LAST-UPD-TMST.       02480000
           MOVE AUCSBL-AUD-FUNC-CDE       TO AH030-AUD-FUNC-CDE.        02481001
           MOVE AUCSBL-AUD-TMST           TO AH030-AUD-TMST.            02482000
042200                                                                  02490000
040700******************************************************************02500000
040800*      EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT       *02510000
040900*      RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST     *02520000
041000*      WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.     *02530000
041100******************************************************************02540000
041200 2100-CHECK-PGM-PROGRESS.                                         02550000
041300     DIVIDE    PV-ARCHIVE-WRITTEN-CNT                             02560000
041400         BY        PC-HOW-OFTEN-TO-DISPLAY                        02570000
041500         GIVING    PV-DISPLAY-NTH-MULTIPLE                        02580000
041600         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      02590000
041700                                                                  02600000
041800     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           02610000
041900         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      02620000
042000         PERFORM 6200-DISPLAY-PGM-PROGRESS                        02630000
042100     END-IF.                                                      02640000
042200                                                                  02650000
046400******************************************************************02660000
046500*  CLOSE CURSOR & OUTPUT FILE, DISPLAY END OF RUN MESSAGE.       *02670000
046600******************************************************************02680000
046700 6000-EOJ-ROUTINE.                                                02690000
046800                                                                  02700000
047100     CLOSE IN-DRIVER-FILE.                                        02710000
047100     CLOSE OUT-ARCHIVE-FILE.                                      02720000
047200                                                                  02730000
046900     MOVE PC-ENDING  TO  DP-STATUS-DESCRIPTION.                   02740000
047500     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02750000
047600                                                                  02760000
047700     DISPLAY 'TOTAL PO TRACKING CUSTOMS BOL   = '                 02770000
047700                                  PV-ARCHIVE-WRITTEN-CNT.         02780000
047900                                                                  02790000
048000******************************************************************02800000
048100*  DISPLAY PROGRAM PROGRESS.                                     *02810000
048200******************************************************************02820000
048300 6200-DISPLAY-PGM-PROGRESS.                                       02830000
048400                                                                  02840000
048500     EXEC SQL                                                     02850000
048600          SET :PV-TMST = CURRENT TIMESTAMP                        02860000
048700     END-EXEC.                                                    02870000
048800                                                                  02880000
048900     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                02890000
049000     MOVE PV-TMST                   TO DP-TIMESTAMP.              02900000
049100     MOVE PV-TABLE-ROW-FETCHED-CNT  TO DP-TABLE-ROW-FETCHED-CNT.  02910000
049200     MOVE PV-ARCHIVE-WRITTEN-CNT     TO DP-DRIVER-WRITTEN-CNT.    02920000
049300                                                                  02930000
049400     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             02940000
049600                                                                  02950000
049700******************************************************************02960000
049800*  OPEN   TABLE CURSOR                                           *02970000
049900******************************************************************02980000
050000 9100-OPEN-TABLE-CURSOR.                                          02990000
050100     EXEC SQL                                                     03000000
050200          OPEN TABLE-CURSOR                                       03010000
050300     END-EXEC.                                                    03020000
050400                                                                  03030000
050500     EVALUATE TRUE                                                03040000
050600         WHEN SQLCODE = ZERO                                      03050000
050700              CONTINUE                                            03060000
050800         WHEN SQLCODE = +100                                      03070000
050900              SET PS-TABLE-LOOP-DONE TO TRUE                      03080000
051000              MOVE '9100-OPEN-TABLE-CURSOR'                       03090000
051100                                 TO PV-PARAGRAPH-NAME             03100000
051200              MOVE 'OPEN TABLE CURSOR - NO ROWS SELECTED'         03110000
051300                                 TO PV-DB2-OPERATION-MSG-1        03120000
051400              MOVE 'TAUCSBL' TO PV-DB2-TABLE-NAME-1               03130000
051500              MOVE SPACES        TO PV-DB2-TABLE-NAME-2           03140000
051600              PERFORM 9999-DB2-ABEND                              03150000
051700         WHEN SQLWARN0 NOT EQUAL SPACE                            03160000
051800         WHEN SQLCODE  NOT EQUAL ZERO                             03170000
051900              MOVE '9100-OPEN-TABLE-CURSOR'                       03180000
052000                                 TO PV-PARAGRAPH-NAME             03190000
052100              MOVE 'OPEN TABLE CURSOR'                            03200000
052200                                 TO PV-DB2-OPERATION-MSG-1        03210000
052300              MOVE 'TAUCSBL' TO PV-DB2-TABLE-NAME-1               03220000
052400              MOVE SPACES        TO PV-DB2-TABLE-NAME-2           03230000
052500              PERFORM 9999-DB2-ABEND                              03240000
052600     END-EVALUATE.                                                03250000
052700                                                                  03260000
052900******************************************************************03270000
053000*  FETCH TABLE CURSOR                                            *03280000
053100******************************************************************03290000
053200 9110-FETCH-TABLE-CURSOR.                                         03300000
053300     EXEC SQL                                                     03310000
053400         FETCH TABLE-CURSOR                                       03320000
053500          INTO  :AUCSBL-CSTM-ENTR-ID                              03330000
053600               ,:AUCSBL-MSTR-BOL-NBR                              03340000
053700               ,:AUCSBL-HSE-BOL-NBR                               03350000
054000               ,:AUCSBL-LAST-UPD-BY-USR-ID                        03380000
054100               ,:AUCSBL-LAST-UPD-TMST                             03390000
053800               ,:AUCSBL-AUD-FUNC-CDE                              03391000
053900               ,:AUCSBL-AUD-TMST                                  03392000
056300     END-EXEC.                                                    03400000
056400                                                                  03410000
056500     EVALUATE TRUE                                                03420000
056600         WHEN SQLCODE = ZERO                                      03430000
056700              ADD +1                 TO PV-TABLE-ROW-FETCHED-CNT  03440000
056800         WHEN SQLCODE = +100                                      03450000
056900              SET PS-TABLE-LOOP-DONE TO TRUE                      03460000
057000         WHEN SQLWARN0 NOT EQUAL SPACE                            03470000
057100         WHEN SQLCODE NOT EQUAL ZERO                              03480000
057200              MOVE '9110-FETCH-TABLE-CURSOR'                      03490000
057300                                 TO PV-PARAGRAPH-NAME             03500000
057400              MOVE 'FETCH TABLE CURSOR'                           03510000
057500                                 TO PV-DB2-OPERATION-MSG-1        03520000
057600              MOVE 'TAUCSCN' TO PV-DB2-TABLE-NAME-1               03530000
057700              MOVE SPACES        TO PV-DB2-TABLE-NAME-2           03540000
057800              PERFORM 9999-DB2-ABEND                              03550000
057900     END-EVALUATE.                                                03560000
058100                                                                  03570000
058200******************************************************************03580000
058300*  CLOSE TABLE CURSOR                                            *03590000
058400******************************************************************03600000
058500 9130-CLOSE-TABLE-CURSOR.                                         03610000
058600     EXEC SQL                                                     03620000
058700          CLOSE TABLE-CURSOR                                      03630000
058800     END-EXEC.                                                    03640000
058900                                                                  03650000
059000     EVALUATE TRUE                                                03660000
059100         WHEN SQLCODE = ZERO                                      03670000
059200              CONTINUE                                            03680000
059300         WHEN SQLWARN0 NOT EQUAL SPACE                            03690000
059400         WHEN SQLCODE NOT EQUAL ZERO                              03700000
059500              MOVE '9130-CLOSE-TABLE-CURSOR'                      03710000
059600                                 TO PV-PARAGRAPH-NAME             03720000
059700              MOVE 'CLOSE TABLE CURSOR'                           03730000
