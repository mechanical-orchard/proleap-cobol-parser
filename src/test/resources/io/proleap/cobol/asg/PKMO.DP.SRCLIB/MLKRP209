061000         IF (ML988-TRN-TYP-CDE = PC-PCMD-TRN-TYP-CDE OR                   
                   ML988-TRN-TYP-CDE = PC-PCMU-TRN-TYP-CDE)                     
                   CONTINUE                                                     
               ELSE                                                             
059000             MOVE '4220-READ-DIFF-PRCHG-RPT-FILE'                         
059100                                 TO AA-PARAGRAPH-NAME                     
059200             MOVE 'TRN TYP CDE NOT EQUAL TO PRCHG         '               
059300                                 TO AA-MESSAGE                            
                   MOVE ML988-TRN-TYP-CDE TO AA-TRN-TYP-CDE                     
059400             MOVE +4003          TO ABEND-CODE                            
059500             PERFORM 9900-COMMON-ABEND-ROUTINE                            
               END-IF                                                           
061300     END-IF.                                                              
061400                                                                          
061500 EJECT                                                                    
061600                                                                          
060000******************************************************************        
060100 4230-READ-DIFF-INV-RPT-FILE.                                             
060200******************************************************************        
060300*                                                                         
060400     READ DATA-INTEGRITY-INV-DIFF-FILE                                    
060500       INTO ML985-INV-ADJ-REPORT-RECS                                     
060600         AT END                                                           
060700             SET PS-NO-MORE-DIFF-RECS                                     
060800                                 TO TRUE                                  
060900     END-READ.                                                            
061000                                                                          
061100     IF PS-MORE-DIFF-RECS                                                 
061200         ADD 1                   TO PA-DIFF-RECORDS-READ                  
               IF ML985-TRN-TYP-CDE NOT EQUAL PC-INVADJ-TRN-TYP-CDE             
059000            MOVE '4230-READ-DIFF-INV-RPT-FILE'                            
059100                                 TO AA-PARAGRAPH-NAME                     
059200            MOVE 'TRN TYP CDE NOT EQUAL TO INV ADJ       '                
059300                                 TO AA-MESSAGE                            
                  MOVE ML985-TRN-TYP-CDE TO AA-TRN-TYP-CDE                      
059400            MOVE +4003          TO ABEND-CODE                             
059500            PERFORM 9900-COMMON-ABEND-ROUTINE                             
               END-IF                                                           
061300     END-IF.                                                              
061400                                                                          
061500 EJECT                                                                    
