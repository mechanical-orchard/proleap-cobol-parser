064800 6100-CREATE-SALES-REPORT.                                                
064900******************************************************************        
065000*                                                                         
           INITIALIZE DL-ERROR-MESSAGE.                                         
065100     IF PV-LINE-COUNT > PC-MAX-LINE-COUNT                                 
065200         PERFORM 6000-WRITE-SALES-REPORT-TITLE.                           
065300                                                                          
065400     MOVE ML986-FSCL-WK-END-DTE  TO DL-FSCL-WK-END-DTE.                   
065600     MOVE ML986-DEPT-NBR         TO DL-DEPT.                              
065700     MOVE ML986-MAJ-CL-NBR       TO DL-MCL.                               
065800     MOVE ML986-SUB-CL-NBR       TO DL-SCL.                               
065900     MOVE ML986-LOC-NBR          TO DL-STORE.                             
           MOVE ML986-ML-LO-MDSE-LVL-ID   TO DL-DKEY.                           
066000     MOVE ML986-WKSUM-SLS-AMT    TO DL-LOW-TBL-AMT.                       
066100     MOVE ML986-AUDIT-SLS-AMT    TO DL-HIGH-TBL-AMT.                      
066200     COMPUTE PV-DIFFERENCE =                                              
066300         ML986-WKSUM-SLS-AMT - ML986-AUDIT-SLS-AMT.                       
066400     MOVE PV-DIFFERENCE          TO DL-DIFFERENCE-AMT.                    
P46161     ADD  PV-DIFFERENCE          TO PV-TOTAL-DIFF.                        
066500                                                                          
066600     SET ERR-IDX                 TO 1.                                    
066700     SEARCH TABLE-ERROR-ENTRIES                                           
066800         AT END                                                           
066900             MOVE 'NO ERROR MESSAGE'                                      
067000                                 TO DL-ERROR-MESSAGE                      
067100         WHEN TBL-ERR-NBR (ERR-IDX) = ML986-TRN-TYP-CDE                   
067200*            MOVE TBL-ERR-MSG (ERR-IDX)                                   
                   STRING 'SLS_TYP_CDE' DELIMITED BY SPACES                     
                   ' ' DELIMITED BY SIZE                                        
                   ML986-SLS-TYP-CDE DELIMITED BY SPACES                        
                   ' ' DELIMITED BY SIZE                                        
                   ML986-COMPARE-FIELD-NAME DELIMITED BY SPACES                 
067300                               INTO DL-ERROR-MESSAGE                      
                   END-STRING                                                   
067400             ADD +1              TO TBL-ERR-TOTAL (ERR-IDX)               
067500     END-SEARCH.                                                          
067600                                                                          
067700                                                                          
067800     WRITE DATA-INTEGRITY-SALES-REC FROM DL-LINE-1                        
067900         AFTER ADVANCING 1 LINE.                                          
068000     ADD 1                       TO PV-LINE-COUNT.                        
068100     ADD 1                       TO PA-REPORT-LINES-WRITTEN.              
068300 EJECT                                                                    
068400                                                                          
