020000*                                                                 02000000
020100******************************************************************02010000
020200**  CURSOR USED TO RETRIEVE THE DEPARTMENTS FOR A BUYER           02020000
020300******************************************************************02030000
020400     EXEC SQL                                                     02040000
020500          DECLARE DPT_CSR CURSOR FOR                              02050000
020600           SELECT DEPT_NBR                                        02060000
020700             FROM TMDSEAR A,                                      02070000
020800                  TAPLAVL B                                       02080000
020900            WHERE A.MDSEAR_DKEY = B.MDSEAR_DKEY                   02090000
021000              AND BYR_NBR       = :MDSEAR-BYR-NBR                 02100000
021100              AND MDSELV_CDE    = :MDSEAR-MDSELV-CDE              02110000
021200              AND (:PV-EFF-DATE                                   02120000
021300                           BETWEEN CHAR(STRT_DTE,ISO)             02130000
021400                               AND CHAR(END_DTE,ISO))             02140000
021500              AND (:PV-EFF-DATE                                   02150000
021600                           BETWEEN CHAR(B.MDSEAR_STRT_DTE,ISO)    02160000
021700                               AND CHAR(B.MDSEAR_END_DTE,ISO))    02170000
021800              AND B.APLSYS_CDE  = :APLAVL-APLSYS-CDE              02180000
021900            ORDER BY DEPT_NBR                                     02190000
022000     END-EXEC.                                                    02200000
022100*                                                                 02210000
022200*                                                                 02220000
022300******************************************************************02230000
022400**  CURSOR USED TO RETRIEVE THE ANALYSTS FOR A DEPARTMENT         02240000
022500******************************************************************02250000
022600     EXEC SQL                                                     02260000
022700          DECLARE ANL_CSR CURSOR FOR                              02270000
022800           SELECT C.WORKER_FIR_NAME,                              02280000
022900                  C.WORKER_LAST_NAME                              02290000
023000             FROM TMDSEAR A,                                      02300000
023100                  TAPLAVL B,                                      02310000
023200                  TRSPBAR C                                       02320000
023300            WHERE A.MDSEAR_DKEY   = B.MDSEAR_DKEY                 02330000
023400              AND A.MDSEAR_DKEY   = C.MDSEAR_DKEY                 02340000
023500              AND A.OPERCO_NBR    = :MDSEAR-OPERCO-NBR            02350000
023600              AND A.DEPT_NBR      = :MDSEAR-DEPT-NBR              02360000
023700              AND A.MDSELV_CDE    = :MDSEAR-MDSELV-CDE            02370000
023800              AND B.APLSYS_CDE    = :APLAVL-APLSYS-CDE            02380000
023900              AND C.RSPLVL_CDE    = :RSPBAR-RSPLVL-CDE            02390000
024000              AND (CURRENT DATE                                   02400000
024100                           BETWEEN CHAR(STRT_DTE,ISO)             02410000
024200                               AND CHAR(END_DTE,ISO))             02420000
024300              AND (:PV-EFF-DATE                                   02430000
024400                           BETWEEN CHAR(B.MDSEAR_STRT_DTE,ISO)    02440000
024500                               AND CHAR(B.MDSEAR_END_DTE,ISO))    02450000
024600              AND (:PV-EFF-DATE                                   02460000
024700                           BETWEEN CHAR(C.WORKER_STRT_DTE,ISO)    02470000
024800                               AND CHAR(C.WORKER_END_DTE,ISO))    02480000
024900            ORDER BY C.WORKER_LAST_NAME,                          02490000
025000                     C.WORKER_FIR_NAME                            02500000
025100     END-EXEC.                                                    02510000
