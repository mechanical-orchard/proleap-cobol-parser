035473 1200-CHECK-IF-PRODUCTION.                                        03547301
035474                                                                  03547401
035475     MOVE '1200-CHECK-IF-PRODUCTION'                              03547501
035476          TO DS009-PARAGRAPH.                                     03547601
035477     MOVE 'SELECT PRDCHK TKRPRPM '                                03547701
035478          TO DS009-OPERATION.                                     03547801
035479                                                                  03547901
035480     EXEC SQL                                                     03548001
035481       SELECT FUNC_TXT                                            03548101
035482         INTO :KRPRPM-FUNC-TXT                                    03548201
035483       FROM   TKRPRPM                                             03548301
035484       WHERE  SYS_PRC_FUNC_CDE  = :PC-PRDCHK                      03548407
035485         AND  INTFC_SYS_CDE     = :PC-KHL                         03548501
035486         AND  LOC_ID            = :PC-90                          03548601
035487         AND  FUNC_TXT          = :PC-PARM-TEXT                   03548701
035488     END-EXEC                                                     03548801
