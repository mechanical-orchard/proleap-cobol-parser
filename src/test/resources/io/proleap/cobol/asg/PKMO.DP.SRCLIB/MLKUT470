      /*****************************************************************02220008
       Z210-OPEN-CURSOR-TWKDPTC.                                        02230007
      ******************************************************************02240000
      *                                                                 02250000
           EXEC SQL                                                     02260000
               OPEN TWKDPTC_CURSOR                                      02270007
           END-EXEC.                                                    02280000
                                                                        02290000
           EVALUATE TRUE                                                02300000
               WHEN SQLCODE < 0                                         02310000
                   MOVE 'Z210-OPEN-CURSOR-TWKDPTC'                      02320007
                                       TO AA-PARAGRAPH-NAME             02330000
                   MOVE 'TWKDPTC TABLE ERROR'                           02340007
                                       TO AA-MESSAGE                    02350000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02360000
                   PERFORM Z998-SQL-ERROR                               02370000
               WHEN SQLCODE > 0                                         02380000
               WHEN SQLWARN0 NOT = SPACES                               02390000
                   MOVE 'Z210-OPEN-CURSOR-TWKDPTC'                      02400007
                                       TO AA-PARAGRAPH-NAME             02410000
                   MOVE 'TWKDPTC TABLE WARNING'                         02420007
                                       TO AA-MESSAGE                    02430000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02440000
                   PERFORM Z998-SQL-ERROR                               02450000
           END-EVALUATE.                                                02460000
                                                                        02470000
      /*****************************************************************02500008
       Z220-FETCH-TWKDPTC.                                              02510007
      ******************************************************************02520000
      *                                                                 02530000
W31475*    EXEC SQL                                                     02540011
  |   *        FETCH TWKDPTC_CURSOR                                     02550011
  |   *         INTO :WKDPTC-DEPT-NBR                                   02560011
  |   *             ,:WKDPTC-CHNL-CDE                                   02580011
  |   *    END-EXEC.                                                    02590011
  |        INITIALIZE HOST-VARIABLES.                                   02590111
  |                                                                     02590211
  |        EXEC SQL                                                     02591011
  |            FETCH NEXT ROWSET FROM TWKDPTC_CURSOR FOR 100 ROWS       02592011
  |             INTO :HV-WKDPTC-DEPT-NBR                                02593011
  |                 ,:HV-WKDPTC-CHNL-CDE                                02594011
W31475     END-EXEC.                                                    02595011
                                                                        02600000
           EVALUATE TRUE                                                02610000
               WHEN SQLCODE = 0                                         02620000
W31475*            ADD +1              TO PA-TWKDPTC-ROWS-FETCHED       02630014
W31475             ADD +100            TO PA-TWKDPTC-ROWS-FETCHED       02631014
               WHEN SQLCODE = +100                                      02640000
                   MOVE 'Y'            TO PS-TWKDPTC-DONE               02650007
W31475             ADD SQLERRD (3)     TO PA-TWKDPTC-ROWS-FETCHED       02651011
  |                PERFORM VARYING ROW-ID FROM 1 BY 1                   02652011
  |                UNTIL ROW-ID > SQLERRD (3)                           02653011
  |                     PERFORM C100-WRITE-EXTRACT                      02654011
W31475             END-PERFORM                                          02655011
               WHEN SQLCODE < 0                                         02660000
                   MOVE 'Z220-FETCH-TWKDPTC'                            02670007
                                       TO AA-PARAGRAPH-NAME             02680000
                   MOVE 'TWKDPTC TABLE ERROR'                           02690007
                                       TO AA-MESSAGE                    02700000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02710000
                   PERFORM Z998-SQL-ERROR                               02720000
               WHEN ((SQLCODE > 0) AND (SQLCODE NOT = +100))            02730000
               WHEN SQLWARN0 NOT = SPACES                               02740000
                   MOVE 'Z220-FETCH-TWKDPTC'                            02750007
                                       TO AA-PARAGRAPH-NAME             02760000
                   MOVE 'TWKDPTC TABLE WARNING'                         02770007
                                       TO AA-MESSAGE                    02780000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02790000
                   PERFORM Z998-SQL-ERROR                               02800000
           END-EVALUATE.                                                02810000
                                                                        02820000
      /*****************************************************************02850008
       Z230-CLOSE-CURSOR-TWKDPTC.                                       02860007
      ******************************************************************02870000
      *                                                                 02880000
           EXEC SQL                                                     02890000
               CLOSE TWKDPTC_CURSOR                                     02900007
           END-EXEC.                                                    02910000
                                                                        02920000
           EVALUATE TRUE                                                02930000
               WHEN SQLCODE < 0                                         02940000
