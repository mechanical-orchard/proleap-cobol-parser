019600                                                                  01960000
019700*---------------------------------------------------------------* 01970000
019800*  AHRD002  - RECORD LAYOUT FOR PO DRIVER FILE, INPUT.          * 01980007
019900*---------------------------------------------------------------* 01990000
020000     COPY AHRD002.                                                02000007
020100                                                                  02010000
020200                                                                  02020000
020300*---------------------------------------------------------------* 02030000
020400*    DB2 AREA FOR TRECDIS                                       * 02040001
020500*---------------------------------------------------------------* 02050000
020600     EXEC SQL                                                     02060000
020700          INCLUDE TRECDIS                                         02070001
020800     END-EXEC.                                                    02080000
020900                                                                  02090000
021000*---------------------------------------------------------------* 02100000
021100*    DB2 AREA FOR COMMUNICATIONS                                * 02110000
021200*---------------------------------------------------------------* 02120000
021300     EXEC SQL                                                     02130000
021400          INCLUDE SQLCA                                           02140000
021500     END-EXEC.                                                    02150000
021600                                                                  02160000
021700*---------------------------------------------------------------* 02170000
021800*    DEFINE CURSOR FOR TRECDIS :                                * 02180001
021900*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02190000
022000*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02200000
022100*---------------------------------------------------------------* 02210000
022200     EXEC SQL                                                     02220000
022300          DECLARE TABLE-CURSOR CURSOR FOR                         02230000
022400           SELECT ORD_NBR                                         02240000
022500                 ,ORD_LNE_NBR                                     02250001
022600                 ,REC_SEQ_NBR                                     02260001
022700                 ,OPER_UNT_ID                                     02270001
022800                 ,FCLTY_ID                                        02280001
022900                 ,CRE_TMST                                        02290001
023000                 ,CRE_UID                                         02300001
023100                 ,VER_STATUS_CDE                                  02310001
023200                 ,EXPTED_ITM_QTY                                  02320001
023300                 ,VERIFY_ITM_QTY                                  02330001
023400                 ,AP_VERIFY_ITM_QTY                               02340001
023500                 ,STR_NBR                                         02350001
023600                 ,STR_FCLTY_ID                                    02360001
023700                 ,PLAN_ITM_QTY                                    02370001
023800                 ,AP_PRC_CDE                                      02380001
023900                 ,AP_PRC_DTE                                      02390001
024000* ******** FROM   TKMA235.TRECDIS                                 02400001
024100           FROM   TRECDIS                                         02410001
024200          WHERE  (ORD_NBR = :PV-PO-NBR-COMP)                      02420000
024300*      ORDER BY                                                   02430000
024400            FOR  FETCH ONLY                                       02440000
024500           WITH  UR                                               02450000
024600     END-EXEC.                                                    02460000
024700                                                                  02470000
024800                                                                  02480000
024900 PROCEDURE DIVISION.                                              02490000
025000********************                                              02500000
025100                                                                  02510000
025200 0000-PROGRAM-MAINLINE.                                           02520000
025300                                                                  02530000
025400     PERFORM 1000-INITIALIZE.                                     02540000
025500                                                                  02550000
025600     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02560000
025700     PERFORM 2000-PROCESS-DRIVER-FILE                             02570000
025800         UNTIL PS-IN-DRIVER-EOF.                                  02580000
025900                                                                  02590000
026000     PERFORM 6000-EOJ-ROUTINE.                                    02600000
026100     STOP RUN.                                                    02610000
026200                                                                  02620000
026300                                                                  02630000
026400* ************************************************************** *02640000
026500* * INITIALIZATIONS                                            * *02650000
026600* ************************************************************** *02660000
026700 1000-INITIALIZE.                                                 02670000
026800                                                                  02680000
026900     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02690000
027000     DISPLAY DM-DISPLAY-MESSAGE.                                  02700000
027100                                                                  02710000
027200     MOVE PC-BEGINNING              TO DP-STATUS-DESCRIPTION.     02720000
027300     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02730000
027400                                                                  02740000
027500     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02750000
027600     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02760000
027700                                                                  02770000
027800                                                                  02780000
027900* ************************************************************** *02790000
028000* * 1) GET NEXT RECORD FROM DRIVER FILE, AND                   * *02800000
028100* *    1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR        * *02810000
028200* *    1B- OPEN TABLE CURSOR                                   * *02820000
028300* *    1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL               * *02830000
028400* *        ROWS FOR THIS INPUT DRIVER RECORD                   * *02840000
028500* *    2) FOR EACH ROW,                                        * *02850000
028600* *       2A- INCREMENT COUNTS                                 * *02860000
028700* *       2B- WRITE EXTRACT RECORD                             * *02870000
028800* *    1D- CLOSE TABLE CURSOR                                  * *02880000
028900* ************************************************************** *02890000
029000 2000-PROCESS-DRIVER-FILE.                                        02900000
029100                                                                  02910000
029200     PERFORM 9050-READ-IN-DRIVER-FILE.                            02920000
029300     IF PS-IN-DRIVER-EOF                                          02930000
029400         CONTINUE                                                 02940000
029500     ELSE                                                         02950000
029600         MOVE AH002-PO-NBR TO         PV-PO-NBR-COMP              02960007
029700                                      PV-PO-NBR-NUM               02970000
029800         PERFORM 2100-CHECK-PGM-PROGRESS                          02980000
029900         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     02990000
030000         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     03000000
030100         PERFORM 9100-OPEN-TABLE-CURSOR                           03010000
030200         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     03020000
030300             UNTIL PS-TABLE-LOOP-DONE                             03030000
030400         PERFORM 9190-CLOSE-TABLE-CURSOR                          03040000
030500     END-IF.                                                      03050000
030600                                                                  03060000
030700                                                                  03070000
030800* ************************************************************** *03080000
030900* *    EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT     * *03090000
031000* *    RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST   * *03100000
031100* *    WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.   * *03110000
031200* ************************************************************** *03120000
031300 2100-CHECK-PGM-PROGRESS.                                         03130000
031400     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03140000
031500         BY        PC-HOW-OFTEN-TO-DISPLAY                        03150000
031600         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03160000
031700         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03170000
031800                                                                  03180000
031900     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03190000
032000         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03200000
032100         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03210000
032200     END-IF.                                                      03220000
032300                                                                  03230000
032400                                                                  03240000
032500* ************************************************************** *03250000
032600* * EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD     * *03260000
032700* ************************************************************** *03270000
032800 3000-EXTRACT-ROWS-FROM-TABLE.                                    03280000
032900     INITIALIZE DCLTRECDIS.                                       03290001
033000     PERFORM 9150-FETCH-TABLE-CURSOR.                             03300000
033100     IF PS-TABLE-LOOP-DONE                                        03310000
033200         CONTINUE                                                 03320000
033300     ELSE                                                         03330000
033400         PERFORM 9750-WRITE-EXTRACT-RECORD                        03340000
033500     END-IF.                                                      03350000
033600                                                                  03360000
033700                                                                  03370000
033800* ************************************************************** *03380000
033900* * DISPLAY END OF RUN MESSAGE, CLOSE THE OUTPUT FILES         * *03390003
034000* ************************************************************** *03400000
034100 6000-EOJ-ROUTINE.                                                03410000
034200                                                                  03420000
034300     PERFORM 9090-CLOSE-IN-DRIVER-FILE.                           03430000
034400     PERFORM 9790-CLOSE-OUT-EXTRACT-FILE.                         03440000
034500                                                                  03450000
034600     MOVE PC-ENDING                 TO DP-STATUS-DESCRIPTION.     03460000
034700     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           03470000
034800                                                                  03480000
034900                                                                  03490000
035000* ************************************************************** *03500000
035100* * DISPLAY PROGRAM PROGRESS.                                  * *03510000
035200* ************************************************************** *03520000
035300 6200-DISPLAY-PGM-PROGRESS.                                       03530000
035400                                                                  03540000
035500     EXEC SQL                                                     03550000
035600          SET :PV-TMST = CURRENT TIMESTAMP                        03560000
035700     END-EXEC.                                                    03570000
035800                                                                  03580000
035900     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                03590000
036000     MOVE PV-TIMESTAMP-19           TO DP-TIMESTAMP-19.           03600000
036100     MOVE PV-IN-DRIVER-RECS-READ-CNT                              03610000
036200                                    TO DP-IN-CNT.                 03620000
036300*    MOVE PV-RECS-WITH-MATCHES-CNT  TO DP-MATCHES-FOUND-CNT.      03630000
036400     MOVE PV-TABLE-ROW-SELECT-CNT   TO DP-ROWS-SELECTED-CNT.      03640000
036500     MOVE PV-EXTRACT-WRITTEN-CNT    TO DP-OUT-CNT.                03650000
036600                                                                  03660000
036700     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             03670000
036800                                                                  03680000
036900                                                                  03690000
037000* ************************************************************** *03700000
037100* * FILE AND DB2 TABLE ACCESS PARAGRAPHS.                      * *03710000
037200* * 90XX- INPUT:  DRIVER FILE                                  * *03720000
037300* * 91XX- INPUT:  TABLE CURSOR                                 * *03730000
037400* * 97XX- OUTPUT: TABLE EXTRACT                                * *03740000
037500* ************************************************************** *03750000
037600                                                                  03760000
037700* ************************************************************** *03770000
037800* * OPEN  INPUT DRIVER FILE                                    * *03780000
037900* ************************************************************** *03790000
038000 9000-OPEN-IN-DRIVER-FILE.                                        03800000
038100     OPEN  INPUT  IN-DRIVER-FILE.                                 03810000
038200                                                                  03820000
038300                                                                  03830000
038400* ************************************************************** *03840000
038500* * READ  INPUT DRIVER FILE OF ELIGIBLE DATA TO EXTRACT        * *03850000
038600* ************************************************************** *03860000
038700 9050-READ-IN-DRIVER-FILE.                                        03870000
038800     READ IN-DRIVER-FILE                                          03880000
038900          INTO AH002-DRIVER-RECORD-LAYOUT                         03890007
039000          AT END                                                  03900000
039100              SET PS-IN-DRIVER-EOF TO TRUE.                       03910000
039200                                                                  03920000
039300     IF PS-IN-DRIVER-EOF                                          03930000
039400         CONTINUE                                                 03940000
039500     ELSE                                                         03950000
039600         ADD +1 TO PV-IN-DRIVER-RECS-READ-CNT                     03960000
039700     END-IF.                                                      03970000
039800                                                                  03980000
039900                                                                  03990000
040000* ************************************************************** *04000000
040100* * CLOSE INPUT DRIVER FILE                                    * *04010000
040200* ************************************************************** *04020000
040300 9090-CLOSE-IN-DRIVER-FILE.                                       04030000
040400     CLOSE IN-DRIVER-FILE.                                        04040000
040500                                                                  04050000
040600                                                                  04060000
040700* ************************************************************** *04070000
040800* * OPEN  TABLE CURSOR                                         * *04080000
040900* ************************************************************** *04090000
041000 9100-OPEN-TABLE-CURSOR.                                          04100000
041100     EXEC SQL                                                     04110000
041200          OPEN TABLE-CURSOR                                       04120000
041300     END-EXEC.                                                    04130000
041400                                                                  04140000
041500     EVALUATE TRUE                                                04150000
041600         WHEN SQLCODE = ZERO                                      04160000
041700              ADD +1 TO PV-RECS-WITH-MATCHES-CNT                  04170000
041800              SET PS-CURSOR-HAS-DATA TO TRUE                      04180000
041900         WHEN SQLCODE = +100                                      04190000
042000              SET PS-TABLE-LOOP-DONE TO TRUE                      04200000
042100         WHEN SQLWARN0 NOT EQUAL SPACE                            04210000
042200         WHEN SQLCODE  NOT EQUAL ZERO                             04220000
042300              MOVE '9100-OPEN-TABLE-CURSOR'                       04230000
042400                                 TO PV-PARAGRAPH-NAME             04240000
042500              MOVE 'OPEN TABLE CURSOR'                            04250000
042600                                 TO PV-DB2-OPERATION-MSG-1        04260000
042700              MOVE 'TRECDIS'     TO PV-DB2-TABLE-NAME             04270001
042800              PERFORM 9999-DB2-ABEND                              04280000
