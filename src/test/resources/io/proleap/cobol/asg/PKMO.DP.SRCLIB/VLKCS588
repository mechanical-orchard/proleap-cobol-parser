122800         SET PS-ERROR TO TRUE                                     POKCS088
122900         PERFORM 3410-POSITION-AT-ERROR                           POKCS088
123000     END-IF.                                                      POKCS088
123100                                                                  POKCS088
123200******************************************************************POKCS088
123300** BECAUSE RANGES CAN SPAN ACROSS MULTIPLE PAGES, A SEPARATE    **POKCS088
123400** PARAGRAPH (THIS ONE) IS NEEDED TO DETERMINE WHERE THE CURSOR **POKCS088
123500** SHOULD BE POSITIONED ON A RANGE ERROR.                       **POKCS088
123600******************************************************************POKCS088
123700 3410-POSITION-AT-ERROR.                                          POKCS088
123800                                                                  POKCS088
123900     SET DP030-SET-CURSOR-APPL-1                                  POKCS088
124000                                   TO TRUE.                       POKCS088
124100     COMPUTE PV-SUB = ASC-START-OF-SELECTED-RANGE                 POKCS088
124200                    - PV-TOP-ITEM + 1.                            POKCS088
124300     IF  PV-SUB < +1 OR > PC-ITEMS-PER-PANEL                      POKCS088
124400         CONTINUE                                                 POKCS088
124500     ELSE                                                         POKCS088
124600         MOVE -1                   TO MR-SELECTL (PV-SUB)         POKCS088
124700         MOVE DP015-REVERSE        TO MR-SELECTH (PV-SUB)         POKCS088
124800         MOVE DP015-RED            TO MR-SELECTC (PV-SUB)         POKCS088
124900     END-IF.                                                      POKCS088
125000                                                                  POKCS088
125100     COMPUTE PV-SUB = ASC-END-OF-SELECTED-RANGE                   POKCS088
125200                    - PV-TOP-ITEM + 1.                            POKCS088
125300     IF  PV-SUB < +1 OR > PC-ITEMS-PER-PANEL                      POKCS088
125400         CONTINUE                                                 POKCS088
125500     ELSE                                                         POKCS088
125600         MOVE -1                   TO MR-SELECTL (PV-SUB)         POKCS088
125700         MOVE DP015-REVERSE        TO MR-SELECTH (PV-SUB)         POKCS088
125800         MOVE DP015-RED            TO MR-SELECTC (PV-SUB)         POKCS088
125900     END-IF.                                                      POKCS088
126000                                                                  POKCS088
126100*----------------------------------------------------------------*POKCS088
126200*    WHENEVER THE USER SELECTS AN ITEM AS THE BEGINNING RANGE    *POKCS088
126300*    AND ONE AS THE ENDING RANGE, MARK THESE ITEMS AS WELL AS    *POKCS088
126400*    THOSE THAT FALL IN THE RANGE AS BEING SELECTED.             *POKCS088
126500*----------------------------------------------------------------*POKCS088
126600 3500-MARK-RANGE-AS-SELECTED.                                     POKCS088
126700                                                                  POKCS088
126800     MOVE +1                       TO  ASC-BLK-SUB.               POKCS088
126900     PERFORM 3510-MARK-SELECTED-INDS                              POKCS088
127000         VARYING ASC-ITEM-SUB                                     POKCS088
127100         FROM 1 BY 1                                              POKCS088
127200         UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL.                 POKCS088
127300                                                                  POKCS088
127400     ADD +1                        TO ASC-BLK-SUB.                POKCS088
127500     PERFORM 3510-MARK-SELECTED-INDS                              POKCS088
127600         VARYING ASC-ITEM-SUB                                     POKCS088
127700         FROM 1 BY 1                                              POKCS088
127800         UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL.                 POKCS088
127900                                                                  POKCS088
128000     MOVE ASC-LIST-ITEM-NUMBER(1 1)                               POKCS088
128100                                   TO  PV-LIST-ITEM-NUMBER.       POKCS088
128200     COMPUTE PV-FIRST-BLOCK-IN-ARRAY =                            POKCS088
128300         1 + ((PV-LIST-ITEM-NUMBER - 1) /                         POKCS088
128400               PC-ITEMS-PER-PANEL).                               POKCS088
128500                                                                  POKCS088
128600     MOVE ASC-START-OF-SELECTED-RANGE                             POKCS088
128700                                   TO PV-START-OF-SELECTED-RANGE. POKCS088
128800     COMPUTE PV-FIRST-BLOCK-IN-RANGE =                            POKCS088
128900         1 + ((PV-START-OF-SELECTED-RANGE - 1) /                  POKCS088
129000               PC-ITEMS-PER-PANEL).                               POKCS088
129100                                                                  POKCS088
129200     MOVE ASC-END-OF-SELECTED-RANGE                               POKCS088
129300                                   TO  PV-END-OF-SELECTED-RANGE.  POKCS088
129400     COMPUTE PV-LAST-BLOCK-IN-RANGE =                             POKCS088
129500         1 + ((PV-END-OF-SELECTED-RANGE - 1) /                    POKCS088
129600               PC-ITEMS-PER-PANEL).                               POKCS088
129700     COMPUTE PV-NEXT-BLOCK-IN-ARRAY =                             POKCS088
129800         PV-FIRST-BLOCK-IN-ARRAY + 1.                             POKCS088
129900                                                                  POKCS088
130000     IF (PV-FIRST-BLOCK-IN-RANGE < PV-FIRST-BLOCK-IN-ARRAY) OR    POKCS088
130100        (PV-LAST-BLOCK-IN-RANGE  > PV-NEXT-BLOCK-IN-ARRAY)        POKCS088
130200         MOVE +1                   TO ASC-BLK-SUB                 POKCS088
130300         PERFORM 5100-WRITE-TEMP-STORAGE                          POKCS088
130400         COMPUTE PV-PREV-BLOCK-IN-ARRAY =                         POKCS088
130500             PV-FIRST-BLOCK-IN-ARRAY - 1                          POKCS088
130600                                                                  POKCS088
130700         PERFORM                                                  POKCS088
130800             UNTIL PV-FIRST-BLOCK-IN-RANGE >                      POKCS088
130900                   PV-PREV-BLOCK-IN-ARRAY                         POKCS088
131000             MOVE PV-FIRST-BLOCK-IN-RANGE                         POKCS088
131100                                   TO PV-BLOCK-NUMBER             POKCS088
131200             PERFORM 4410-READ-TEMP-STORAGE                       POKCS088
131300             PERFORM 3510-MARK-SELECTED-INDS                      POKCS088
131400                 VARYING ASC-ITEM-SUB                             POKCS088
131500                 FROM 1 BY 1                                      POKCS088
131600                 UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL          POKCS088
131700             PERFORM 5100-WRITE-TEMP-STORAGE                      POKCS088
131800             ADD +1                TO PV-FIRST-BLOCK-IN-RANGE     POKCS088
131900         END-PERFORM                                              POKCS088
132000                                                                  POKCS088
132100         COMPUTE PV-NEXT-BLOCK-IN-ARRAY =                         POKCS088
132200             PV-FIRST-BLOCK-IN-ARRAY + PC-MAX-BLOCKS-IN-ARRAY     POKCS088
132300         PERFORM                                                  POKCS088
132400             UNTIL PV-NEXT-BLOCK-IN-ARRAY >                       POKCS088
132500                   PV-LAST-BLOCK-IN-RANGE                         POKCS088
132600             MOVE PV-NEXT-BLOCK-IN-ARRAY                          POKCS088
132700                                   TO PV-BLOCK-NUMBER             POKCS088
132800             PERFORM 4410-READ-TEMP-STORAGE                       POKCS088
132900             PERFORM 3510-MARK-SELECTED-INDS                      POKCS088
133000                 VARYING ASC-ITEM-SUB                             POKCS088
133100                 FROM 1 BY 1                                      POKCS088
133200                 UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL          POKCS088
133300             PERFORM 5100-WRITE-TEMP-STORAGE                      POKCS088
133400             ADD +1                TO PV-NEXT-BLOCK-IN-ARRAY      POKCS088
133500         END-PERFORM                                              POKCS088
133600                                                                  POKCS088
133700         MOVE PV-FIRST-BLOCK-IN-ARRAY                             POKCS088
133800                                   TO PV-BLOCK-NUMBER             POKCS088
133900         PERFORM 4410-READ-TEMP-STORAGE                           POKCS088
134000     END-IF.                                                      POKCS088
134100                                                                  POKCS088
