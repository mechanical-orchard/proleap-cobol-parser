170100*----------------------------------------------------------------*POKCS088
170200* MOVE THE DISPLAYED FIELDS FROM THE COMMAREA TO THE SCREEN.     *POKCS088
170300*----------------------------------------------------------------*POKCS088
170400 4400-MOVE-COMMAREA-TO-SCREEN.                                    POKCS088
170500                                                                  POKCS088
170600     IF  (((PV-TOP-ITEM > ASC-LIST-ITEM-NUMBER (1 1))             POKCS088
170700         AND (PV-TOP-ITEM > ASC-LIST-ITEM-NUMBER (2 1)))          POKCS088
170800         OR  (PV-TOP-ITEM < ASC-LIST-ITEM-NUMBER (1 1)))          POKCS088
170900             MOVE +1    TO ASC-BLK-SUB                            POKCS088
171000             PERFORM 5100-WRITE-TEMP-STORAGE                      POKCS088
171100             ADD +1     TO ASC-BLK-SUB                            POKCS088
171200             PERFORM 5100-WRITE-TEMP-STORAGE                      POKCS088
171300             DIVIDE PV-TOP-ITEM BY PC-ITEMS-PER-PANEL             POKCS088
171400                 GIVING PV-BLOCK-NUMBER                           POKCS088
171500                 REMAINDER PV-REMAINDER                           POKCS088
171600             IF  PV-REMAINDER > ZERO                              POKCS088
171700                 ADD +1 TO PV-BLOCK-NUMBER                        POKCS088
171800             END-IF                                               POKCS088
171900             MOVE +1    TO ASC-BLK-SUB                            POKCS088
172000             PERFORM 4410-READ-TEMP-STORAGE                       POKCS088
172100             ADD +1     TO ASC-BLK-SUB                            POKCS088
172200                           PV-BLOCK-NUMBER                        POKCS088
172300             PERFORM 4410-READ-TEMP-STORAGE                       POKCS088
172400     END-IF.                                                      POKCS088
172500     MOVE +1            TO ASC-BLK-SUB.                           POKCS088
172600     COMPUTE ASC-ITEM-SUB =                                       POKCS088
172700         ((PV-TOP-ITEM - ASC-LIST-ITEM-NUMBER (1 1))              POKCS088
172800         + PC-ITEMS-PER-PANEL).                                   POKCS088
172900                                                                  POKCS088
173000     IF  ASC-ITEM-SUB > PC-ITEMS-PER-PANEL                        POKCS088
173100         ADD +1         TO ASC-BLK-SUB                            POKCS088
173200         SUBTRACT PC-ITEMS-PER-PANEL FROM ASC-ITEM-SUB            POKCS088
173300     END-IF.                                                      POKCS088
173400                                                                  POKCS088
173500     COMPUTE ASC-ITEM-AT-BOT-OF-PANEL =                           POKCS088
173600         ((PV-TOP-ITEM + PC-ITEMS-PER-PANEL) - 1).                POKCS088
173700     PERFORM 4430-CHECK-FOR-BEGIN-AND-END.                        POKCS088
173800     PERFORM 4420-FORMAT-LIST-LINES                               POKCS088
173900         VARYING PV-SUB                                           POKCS088
174000         FROM PC-ITEMS-PER-PANEL BY -1                            POKCS088
174100         UNTIL PV-SUB < +1.                                       POKCS088
174200                                                                  POKCS088
