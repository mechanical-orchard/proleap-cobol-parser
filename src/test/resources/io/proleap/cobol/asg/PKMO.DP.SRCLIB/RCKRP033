273000                 FSCL_YR_NBR                                      27290033
273100               , FSCL_MN_NBR                                      27300033
273200               , FSCL_MN_END_DTE                                  27310033
273300         INTO   :PV-FISCAL-YEAR                                   27320033
273400               ,:PV-FISCAL-MONTH                                  27330033
273500               ,:DTATTR-FSCL-MN-END-DTE                           27340033
273600         FROM TDTATTR                                             27350033
273700         WHERE KY_DTE  = :COCNTL-BTCH-PRC-DTE                     27360033
273800     END-EXEC                                                     27370034
273900                                                                  27380033
274000     EVALUATE TRUE                                                27390033
