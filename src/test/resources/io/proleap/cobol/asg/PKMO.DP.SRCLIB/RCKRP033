131600     IF  CC-CONSTANT = 'CONTROL'                                  13160033
131700         CONTINUE                                                 13170033
131800     ELSE                                                         13180033
131900         DISPLAY '** ABEND **'                                    13190033
132000         DISPLAY '** PROGRAM   ** = ' PC-CURRENT-PROGRAM-NAME     13200033
132100         DISPLAY '** PARAGRAPH ** = 1100-VALIDATE-CONTROL-CARD'   13210033
132200         DISPLAY '** CONTROL CONSTANT INVALID'                    13220033
132300         DISPLAY '** CONTROL CONSTANT = ' CC-CONSTANT             13230033
132400         DISPLAY '** CHANGE TO CONTROL'                           13240033
132500         MOVE +4003              TO PV-ABEND-CODE                 13250034
132600         CALL 'ILBOABN0' USING PV-ABEND-CODE                      13260034
132700     END-IF.                                                      13270033
132800                                                                  13280033
132900     IF  CC-OVERRIDE-NO                                           13290033
133000         PERFORM 7200-SELECT-TCOCNTL                              13300033
133100         PERFORM 7300-SELECT-TDTATTR                              13310033
133200     ELSE                                                         13320033
133300         PERFORM 7400-SELECT-TDTATTR.                             13330035
133400                                                                  13340033
133500     DISPLAY '** FISCAL YEAR = ' PV-FISCAL-YEAR.                  13350035
133600     DISPLAY '** FISCAL MONTH = ' PV-FISCAL-MONTH.                13360035
133700     DISPLAY '** END DATE = ' DTATTR-FSCL-MN-END-DTE.             13370033
133800                                                                  13380033
133900     MOVE DTATTR-FSCL-MN-END-DTE TO PV-MN-END-DTE.                13390033
134000     MOVE PV-MN-END-YYYY         TO HL2-YYYY.                     13400033
134100     MOVE PV-MN-END-MM           TO HL2-MM.                       13410033
134200     MOVE PV-MN-END-DD           TO HL2-DD.                       13420033
134300                                                                  13430033
134400/                                                                 13440033
134500***************************************************************** 13450001
134600* SELECT MONTH-ENDING DATE FROM TDTATTR.                          13460018
134700***************************************************************** 13470001
134800 1150-SELECT-MONTH-END-DATE.                                      13480018
134900                                                                  13490001
135000         EXEC SQL                                                 13500001
135100           SELECT FSCL_MN_END_DTE                                 13510018
135200             INTO :DTATTR-FSCL-MN-END-DTE                         13520018
135300           FROM TDTATTR                                           13530001
135400             WHERE FSCL_MN_NBR = :PV-KEY-MM                       13540025
135500               AND FSCL_YR_NBR = :PV-KEY-YYYY                     13550025
135600         END-EXEC                                                 13560001
135700                                                                  13570001
135800     EVALUATE TRUE                                                13580001
135900         WHEN SQLCODE = ZERO                                      13590001
136000         WHEN SQLCODE = -811                                      13600025
136100             MOVE DTATTR-FSCL-MN-END-DTE TO PV-MN-END-DTE         13610018
136200             MOVE PV-MN-END-YYYY         TO HL2-YYYY              13620018
136300             MOVE PV-MN-END-MM           TO HL2-MM                13630018
136400             MOVE PV-MN-END-DD           TO HL2-DD                13640018
136500         WHEN SQLCODE = +100                                      13650001
136600             CONTINUE                                             13660001
136700         WHEN SQLCODE < ZERO                                      13670001
136800             DISPLAY '** ABEND - RCKRP033 **'                     13680006
136900             DISPLAY '** 1150-SELECT-CURR-DATES **'               13690001
137000             PERFORM 9200-SQL-ABEND                               13700001
137100         WHEN SQLCODE > ZERO                                      13710001
137200         WHEN SQLWARN0 NOT = SPACES                               13720001
137300             DISPLAY '** ABEND - RCKRP033 **'                     13730006
137400             DISPLAY '** 1150-SELECT-CURR-DATES **'               13740001
137500             PERFORM 9300-SQL-WARNING                             13750001
137600     END-EVALUATE.                                                13760001
137700/                                                                 13770001
137800***************************************************************** 13780001
137900 1200-SELECT-OPER-DESC.                                           13790001
138000***************************************************************** 13800001
138100*                                                                 13810001
138200     INITIALIZE XL071-COMMAREA-REC.                               13820053
138300     MOVE PV-PREV-OPER-UNT-ID   TO XL071-LOCATION-NUMBER          13830053
138400                                                                  13840053
138500     CALL XL071-LOCATION-MODULE USING XL071-COMMAREA-REC.         13850053
138600                                                                  13860053
138700     EVALUATE TRUE                                                13870053
138800     WHEN XL071-NO-ERRORS                                         13880053
138900     WHEN XL071-LOC-NOT-FOUND                                     13890053
139000     WHEN XL071-DC-ASGMT-NOT-FND                                  13900053
139100         CONTINUE                                                 13910053
139200     WHEN XL071-BAD-DB2-RETURN                                    13920053
139300         DISPLAY '1200-SELECT-OPER-DESC'                          13930054
139400         DISPLAY 'CALL LOCATION MOD '                             13940054
139500         DISPLAY 'XLT00010'                                       13950054
139600         MOVE XL071-SQLCA             TO SQLCA                    13960053
139800         PERFORM 9200-SQL-ABEND                                   13970053
139900     END-EVALUATE.                                                13980053
140000/                                                                 13990053
