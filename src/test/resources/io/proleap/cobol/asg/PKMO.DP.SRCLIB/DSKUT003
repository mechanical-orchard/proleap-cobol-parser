019400***************************************************************** 01940011
019500*     FOR EACH TABLE, DELETE ANY EXISTING DATA AND THEN INSERT    01950011
019600*     THE DATA FROM THE FILE.                                     01960011
019700***************************************************************** 01970011
019800*                                                                 01980011
019900     READ TBLE-FILE INTO DS002-RECORD                             01990011
020000         AT END                                                   02000011
020100             SET TABLE-EOF TO TRUE                                02010011
020200         NOT AT END                                               02020011
020300             ADD 1 TO PV-READ-COUNT                               02030011
020400     END-READ.                                                    02040011
020500                                                                  02050011
020600     IF NOT-TABLE-EOF                                             02060011
020700        MOVE DS002-DATA-SIZE TO DS009-TBLE-DATA-SIZE              02070011
020800        MOVE DS002-NAME TO DS009-PROCESS-TABLE                    02080011
020900        MOVE DS002-KEYS TO DS009-PKYS                             02090011
021000        PERFORM DS002-DO-PROCESS                                  02100011
021100                                                                  02110011
021200        IF DS009-DO-PROCESS                                       02120011
021300        EVALUATE TRUE                                             02130011
021400*        WHEN DS009-PROCESS-TABLE = 'TSK'                         02140011
021500*            MOVE DS002-DATA-ITEM TO WS-SK                        02150011
021600*            MOVE WS-TSK-ITEM-NMBR       TO HOLD-ITEM-NBR         02160011
021700*            MOVE WS-TSK-MERCH-DEPT-NMBR                          02170011
021800*                                TO HOLD-MERCH-DEPT-NMBR          02180011
021900*            MOVE WS-TSK-MERCH-CLASS-NMBR                         02190011
022000*                                TO HOLD-MERCH-CLASS-NMBR         02200011
022100*            MOVE WS-TSK-SEQ-NMBR                                 02210011
022200*                                TO HOLD-SEQ-NMBR                 02220011
022300*            PERFORM R100-DELETE                                  02230011
022400*        WHEN DS009-PROCESS-TABLE = 'TUPC'                        02240011
022500*            MOVE DS002-DATA-ITEM TO WS-UPC                       02250011
022600*            MOVE WS-UPC-UPC-ID           TO HOLD-UPC-ID          02260011
022700*            PERFORM R100-DELETE                                  02270011
022800*        WHEN DS009-PROCESS-TABLE = 'TUPCSKU'                     02280011
022900*            MOVE DS002-DATA-ITEM TO WS-UPCSKU                    02290011
023000*            MOVE WS-TUPCSKU-UPC-NBR      TO HOLD-UPC-NBR         02300011
023100*            PERFORM R100-DELETE                                  02310011
023200*        WHEN DS009-PROCESS-TABLE = 'TUPCPLS'                     02320011
023300*            MOVE DS002-DATA-ITEM TO WS-UPCPLS                    02330011
