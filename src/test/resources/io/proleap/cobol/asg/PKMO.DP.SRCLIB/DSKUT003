043100                                                                  04310011
043200*        DISPLAY 'DELETE RETURN SQLCODE:' SQLCODE                 04320011
043300*        DISPLAY 'NUMBER OF ROWS DELETED:' SQLERRD (3)            04330011
043400*        IF DS009-UPDATE-COUNT > DS009-COMMIT-COUNT               04340011
043500*           EXEC SQL                                              04350011
043600*               COMMIT                                            04360011
043700*           END-EXEC                                              04370011
043800*           IF SQLCODE NOT = ZERO                                 04380011
043900*              PERFORM DS002-SQL-ABEND-COMMIT                     04390011
044000*           END-IF                                                04400011
044100*           MOVE +0  TO DS009-UPDATE-COUNT                        04410011
044200*           DISPLAY 'COMMIT TAKEN AFTER RECORD: '                 04420011
044300*                                     DS009-TUPDATE-COUNT         04430011
044400*        END-IF                                                   04440011
044500                                                                  04450011
044600         PERFORM DS002-COMMIT                                     04460011
044700                                                                  04470011
044800         ADD SQLERRD(3) TO DS009-UPDATE-COUNT                     04480011
044900         ADD SQLERRD(3) TO DS009-TUPDATE-COUNT                    04490011
045000                                                                  04500011
045100                                                                  04510011
045200        END-IF                                                    04520011
045300     END-IF.                                                      04530011
045400                                                                  04540011
045500 B101-ADD-TO-DELETE-COUNTS.                                       04550011
045600     IF NOT-TABLE-EOF AND DS009-DO-PROCESS                        04560011
045700         SET DS009-SRCH-IDX TO +1                                 04570011
045800         SEARCH DS009-CNTL-RECORDS                                04580011
045900                 VARYING DS009-SRCH-IDX                           04590011
046000             AT END                                               04600011
046100                 CONTINUE                                         04610011
046200             WHEN CNTL-TABLE (DS009-SRCH-IDX) = DS002-NAME        04620011
046300                 ADD SQLERRD(3)                                   04630011
046400                        TO DS009-READ-COUNT (DS009-SRCH-IDX)      04640011
046500         END-SEARCH                                               04650011
046600     END-IF.                                                      04660011
046700 EJECT                                                            04670011
