008300     ELSE                                                         00830017
008400        MOVE 'N' TO EPWS208-LPTQT-OVRFL-FL (EPWS208-NETC-INDX)    00840025
008500        SET EPWS208-OFFER-DATA-IN-RANGE TO TRUE                   00850017
008600     END-IF.                                                      00860017
008700                                                                  00870017
008710     IF EPWS208-OFFER-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00871036
008720     OR EPWS208-EVENT-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00872036
008730     OR EPWS208-LPTQT-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00873036
008740     OR EPWS208-TIERN-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00874036
008750     OR EPWS208-MDSES-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00875036
008760     OR EPWS208-RLTVN-OVRFL-FL (EPWS208-NETC-INDX) = 'Y'          00876036
008770                                                                  00877036
008780        SET EPWS208-OFR-DATA-NOT-IN-RANGE TO TRUE                 00878036
008790     END-IF.                                                      00879036
008800                                                                  00880017
008900     IF EPWS208-OFR-DATA-NOT-IN-RANGE                             00890018
009000        SET EPWS208-NETCOOL-ALERT TO TRUE                         00900017
009100        MOVE 'Y' TO EPWS208-RECORD-IN-ERROR (EPWS208-NETC-INDX)   00910017
009200        DISPLAY ' EPWS208-RECORD-IN-ERROR (EPWS208-NETC-INDX)'    00920017
009300                 EPWS208-RECORD-IN-ERROR (EPWS208-NETC-INDX)      00930017
009400        SET EPWS208-NETC-INDX UP BY 1                             00940017
009500     END-IF.                                                      00950017
009600                                                                  00960017
009700     IF EPWS208-NETC-INDX IS > 10                                 00970017
009800        DISPLAY ' LIMIT OF NETCOOL TABLE REACHED'                 00980017
009900        MOVE 'EP208-1000-DETERMNE-FLD-OVRFLW'                     00990017
010000                          TO PV-PARAGRAPH-NAME                    01000019
010100        MOVE ' LIMIT OF NETCOOL TABLE REACHED' TO                 01010017
010200                          PV-OPERATION-MSG-1                      01020019
010300        SET EPWS208-AC-4004-TBL-OVRFLW-ERR TO TRUE                01030024
010400        PERFORM EP208-9900-ABEND                                  01040023
010500     END-IF.                                                      01050017
010600                                                                  01060017
010700                                                                  01070017
010800******************************************************************01080027
010900* EP208-1100-DETERMNE-POS-OVRFLW                                  01090017
011000*                                                                 01100017
011100*  THIS PARAGRAPH DETERMINES IF THE FOLLOWING VARIABLES ARE       01110017
011200*  WITHIN THE BOUNDARIES ACCEPTABLE AT STORE SERVER DATABASE.     01120017
011300*                                                                 01130017
