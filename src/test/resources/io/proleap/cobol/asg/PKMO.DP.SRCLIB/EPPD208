017700             UNTIL EPWS208-NETC-INDX > 10 OR                      01770017
017800          EPWS208-RECORD-IN-ERROR (EPWS208-NETC-INDX) = SPACE.    01780017
017900                                                                  01790017
018000******************************************************************01800027
018100*  THIS PARAGRAPH WILL BE CALLED BEFORE WRAP=UP AND WOULD FORMAT  01810017
018200*  ERROR MESSAGES AND WILL SEND OUT A NETCOOL ERROR MESSAGE       01820017
018300*  IF ANY VARIABLE IS NOT WITHIN ACCEPTABLE BOUNDARIES.           01830017
018400******************************************************************01840027
018500 EP208-2000-FORMAT-ERROR-MSG.                                     01850017
018600                                                                  01860017
018700      IF EPWS208-OFFER-OVRFL-FL(EPWS208-NETC-INDX) = 'Y'          01870025
018800         STRING ' OFFER ID NOT IN RANGE '     DELIMITED BY SIZE   01880017
018900         EPWS208-ERR-OFFER-ID (EPWS208-NETC-INDX)                 01890017
019000                                             DELIMITED BY SIZE    01900017
019100                ':' DELIMITED BY SIZE                             01910017
019200                INTO EPWS208-STRING1 (EPWS208-NETC-INDX)          01920028
019300      ELSE                                                        01930017
019400         IF EPWS208-OFFER-OVRFL-FL(EPWS208-NETC-INDX) = 'N'       01940025
019500            STRING  ':' DELIMITED BY SIZE                         01950017
019600                INTO EPWS208-STRING1 (EPWS208-NETC-INDX)          01960028
019700         END-IF                                                   01970017
019800      END-IF.                                                     01980017
019900                                                                  01990017
020000      IF EPWS208-EVENT-OVRFL-FL(EPWS208-NETC-INDX) = 'Y'          02000025
020100          STRING ' EVENT ID NOT IN RANGE '     DELIMITED BY SIZE  02010017
020200                EPWS208-ERR-EVENT-ID (EPWS208-NETC-INDX)          02020017
020300                                               DELIMITED BY SIZE  02030017
020400                ':' DELIMITED BY SIZE                             02040017
020500                INTO EPWS208-STRING2 (EPWS208-NETC-INDX)          02050028
020600      ELSE                                                        02060017
020700         IF EPWS208-EVENT-OVRFL-FL(EPWS208-NETC-INDX) = 'N'       02070025
020800            STRING ':' DELIMITED BY SIZE                          02080017
020900                INTO EPWS208-STRING2 (EPWS208-NETC-INDX)          02090028
021000      END-IF.                                                     02100017
021100                                                                  02110017
