      ******************************************************************01080000
      *  DB2 ERROR MESSAGES FORMAT AREA.                                01090000
      ******************************************************************01100000
           COPY DPWS004.                                                01110005
                                                                        01120000
      ******************************************************************01130000
      *  DB2 ERRORS                                                     01140000
      ******************************************************************01150000
           COPY SQLCA2.                                                 01160000
                                                                        01170000
      ******************************************************************01180000
      *  DB2 COMMUNICATION AREA                                         01190000
      ******************************************************************01200000
           EXEC SQL                                                     01210000
                INCLUDE SQLCA                                           01220000
           END-EXEC.                                                    01230000
                                                                        01240000
      ******************************************************************01250000
      *  DB2 TMNMCL TABLE LAYOUT                                        01260000
      ******************************************************************01270000
           EXEC SQL                                                     01280000
                INCLUDE TMNMCL                                          01290000
           END-EXEC.                                                    01300000
                                                                        01310000
      ******************************************************************01320000
      *  DB2 TMNMCL CURSOR                                              01330000
      ******************************************************************01340000
           EXEC SQL                                                     01350000
               DECLARE TMNMCL_CURSOR CURSOR FOR                         01360000
                SELECT FSCL_MN_END_DTE,                                 01370000
                       FSCL_MN_NBR,                                     01380000
                       DEPT_NBR,                                        01390000
                       MAJ_CL_NBR,                                      01400001
                       SLS_RTL_AMT,                                     01410000
                       MKDN_RTL_AMT,                                    01420000
                       MKUP_RTL_AMT,                                    01430000
                       XFER_IN_RTL_AMT,                                 01440000
                       XFER_OUT_RTL_AMT,                                01450000
                       RCPT_RTL_AMT,                                    01460000
                       RCPT_NET_COST_AMT,                               01470000
                       PADJ_RTL_AMT,                                    01480000
                       PADJ_NET_COST_AMT,                               01490000
                       INV_ADJ_RTL_AMT,                                 01500000
                       SHNK_RTL_AMT,                                    01510010
                       END_INV_RTL_AMT                                  01511010
                  FROM TMNMCL                                           01520000
                 WHERE (FSCL_MN_END_DTE >= :ML110-STRT-DTE)             01530000
                   AND (FSCL_MN_END_DTE <= :ML110-END-DTE)              01540000
              ORDER BY FSCL_MN_END_DTE,                                 01550000
                       DEPT_NBR,                                        01560001
                       MAJ_CL_NBR                                       01570001
W26603        WITH UR                                                   01571011
           END-EXEC.                                                    01580000
                                                                        01590000
       EJECT                                                            01600000
                                                                        01610000
      ******************************************************************01620000
       PROCEDURE DIVISION.                                              01630000
      ******************************************************************01640000
                                                                        01650000
      ******************************************************************01660000
       0000-MAINLINE-MLKUT416.                                          01670006
      ******************************************************************01680000
      *                                                                 01690000
           PERFORM 1000-INITIALIZATION.                                 01700006
                                                                        01710000
           PERFORM 2000-MAIN-PROCESS.                                   01720006
                                                                        01730000
           PERFORM 8000-EOJ.                                            01740006
                                                                        01750000
           STOP RUN.                                                    01760000
                                                                        01770000
       EJECT                                                            01780000
                                                                        01790000
      ******************************************************************01800000
       1000-INITIALIZATION.                                             01810006
      ******************************************************************01820000
      *                                                                 01830000
           OPEN INPUT  DATA-INTEGRITY-CTL-FILE.                         01840000
           OPEN OUTPUT DATA-INTEGRITY-EXTR-FILE.                        01850000
                                                                        01860000
           PERFORM 4000-READ-CONTROL-CARD.                              01870006
                                                                        01880000
           DISPLAY ' '.                                                 01890000
           DISPLAY '*** MLKUT416 INPUT CONTROL CARD VALUES ***'         01900000
           DISPLAY ML110-DATA-INTEGRITY-CC-REC.                         01910000
                                                                        01920000
       EJECT                                                            01930000
                                                                        01940000
      ******************************************************************01950000
       2000-MAIN-PROCESS.                                               01960006
      ******************************************************************01970000
      *                                                                 01980000
           PERFORM 7100-OPEN-CURSOR-TMNMCL.                             01990006
                                                                        02000000
           PERFORM 7200-FETCH-TMNMCL.                                   02010006
                                                                        02020000
           PERFORM                                                      02030000
             UNTIL PS-TMNMCL-IS-DONE                                    02040000
               PERFORM 5000-WRITE-EXTRACT                               02050006
               PERFORM 7200-FETCH-TMNMCL                                02060006
           END-PERFORM.                                                 02070000
                                                                        02080000
           PERFORM 7300-CLOSE-CURSOR-TMNMCL.                            02090006
                                                                        02100000
       EJECT                                                            02110000
                                                                        02120000
      ******************************************************************02130006
       4000-READ-CONTROL-CARD.                                          02140006
      ******************************************************************02150006
      *                                                                 02160006
           READ DATA-INTEGRITY-CTL-FILE                                 02170006
             INTO ML110-DATA-INTEGRITY-CC-REC                           02180006
               AT END                                                   02190006
                   MOVE '4000-READ-CONTROL-CARD'                        02200006
                                       TO AA-PARAGRAPH-NAME             02210006
                   MOVE 'CONTROL CARD IS MISSING'                       02220006
                                       TO AA-MESSAGE                    02230006
                   MOVE +4001          TO ABEND-CODE                    02240006
                   PERFORM Z990-COMMON-ABEND-ROUTINE                    02250006
           END-READ.                                                    02260006
                                                                        02270006
       EJECT                                                            02280006
                                                                        02290006
      ******************************************************************02300006
       5000-WRITE-EXTRACT.                                              02310006
      ******************************************************************02320006
      *                                                                 02330006
           MOVE MNMCL-FSCL-MN-END-DTE  TO ML111-FSCL-MN-END-DTE.        02340006
           MOVE MNMCL-FSCL-MN-NBR      TO ML111-FSCL-MN-NBR.            02350006
           MOVE MNMCL-DEPT-NBR         TO ML111-DEPT-NBR.               02360006
           MOVE MNMCL-MAJ-CL-NBR       TO ML111-MAJ-CL-NBR.             02370006
           MOVE '00'                   TO ML111-SUB-CL-NBR.             02380006
           MOVE 0                      TO ML111-STR-NBR.                02390006
           MOVE MNMCL-SLS-RTL-AMT      TO ML111-SLS-RTL-AMT.            02400006
           MOVE MNMCL-MKDN-RTL-AMT     TO ML111-MKDN-RTL-AMT.           02410006
           MOVE MNMCL-MKUP-RTL-AMT     TO ML111-MKUP-RTL-AMT.           02420006
           MOVE MNMCL-XFER-IN-RTL-AMT  TO ML111-XFER-IN-RTL-AMT.        02430006
           MOVE MNMCL-XFER-OUT-RTL-AMT TO ML111-XFER-OUT-RTL-AMT.       02440006
           MOVE MNMCL-RCPT-RTL-AMT     TO ML111-RCPT-RTL-AMT.           02450006
           MOVE MNMCL-RCPT-NET-COST-AMT                                 02460006
                                       TO ML111-RCPT-NET-COST-AMT.      02470006
           MOVE MNMCL-PADJ-RTL-AMT     TO ML111-PADJ-RTL-AMT.           02480006
           MOVE MNMCL-PADJ-NET-COST-AMT                                 02490006
                                       TO ML111-PADJ-NET-COST-AMT.      02500006
           MOVE MNMCL-INV-ADJ-RTL-AMT  TO ML111-INV-ADJ-RTL-AMT.        02510006
           MOVE MNMCL-SHNK-RTL-AMT     TO ML111-SHNK-RTL-AMT.           02520006
           MOVE MNMCL-END-INV-RTL-AMT  TO ML111-END-INV-RTL-AMT.        02521008
                                                                        02530006
           WRITE DATA-INTEGRITY-EXTR-REC                                02540006
               FROM ML111-DATA-INTEGRITY-EXTR-REC.                      02550006
           ADD 1                       TO PA-OUTPUT-RECS-WRITTEN.       02560006
                                                                        02570006
       EJECT                                                            02580006
                                                                        02590006
      ******************************************************************02600006
       7100-OPEN-CURSOR-TMNMCL.                                         02610006
      ******************************************************************02620006
      *                                                                 02630006
           EXEC SQL                                                     02640006
               OPEN TMNMCL_CURSOR                                       02650006
           END-EXEC.                                                    02660006
                                                                        02670006
           EVALUATE TRUE                                                02680006
               WHEN SQLCODE = 0                                         02690007
                   CONTINUE                                             02690107
               WHEN OTHER                                               02691007
                   MOVE '7100-OPEN-CURSOR-TMNMCL'                       02700006
                                       TO AA-PARAGRAPH-NAME             02710006
                   MOVE 'TMNMCL TABLE ERROR'                            02720006
                                       TO AA-MESSAGE                    02730006
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02740006
                   PERFORM Z998-SQL-ERROR                               02750006
           END-EVALUATE.                                                02840006
                                                                        02850006
       EJECT                                                            02860006
                                                                        02870006
      ******************************************************************02880006
       7200-FETCH-TMNMCL.                                               02890006
      ******************************************************************02900006
      *                                                                 02910006
           EXEC SQL                                                     02920006
               FETCH TMNMCL_CURSOR                                      02930006
                INTO :MNMCL-FSCL-MN-END-DTE,                            02940006
                     :MNMCL-FSCL-MN-NBR,                                02950006
                     :MNMCL-DEPT-NBR,                                   02960006
                     :MNMCL-MAJ-CL-NBR,                                 02970006
                     :MNMCL-SLS-RTL-AMT,                                02980006
                     :MNMCL-MKDN-RTL-AMT,                               02990006
                     :MNMCL-MKUP-RTL-AMT,                               03000006
                     :MNMCL-XFER-IN-RTL-AMT,                            03010006
                     :MNMCL-XFER-OUT-RTL-AMT,                           03020006
                     :MNMCL-RCPT-RTL-AMT,                               03030006
                     :MNMCL-RCPT-NET-COST-AMT,                          03040006
                     :MNMCL-PADJ-RTL-AMT,                               03050006
                     :MNMCL-PADJ-NET-COST-AMT,                          03060006
                     :MNMCL-INV-ADJ-RTL-AMT,                            03070006
                     :MNMCL-SHNK-RTL-AMT,                               03080010
                     :MNMCL-END-INV-RTL-AMT                             03081010
           END-EXEC.                                                    03090006
                                                                        03100006
           EVALUATE TRUE                                                03110006
               WHEN SQLCODE = 0                                         03120006
                   ADD +1              TO PA-TMNMCL-ROWS-FETCHED        03130006
               WHEN SQLCODE = +100                                      03140006
                   MOVE 'Y'            TO PS-TMNMCL-DONE                03150006
               WHEN OTHER                                               03160007
                   MOVE '7200-FETCH-TMNMCL'                             03170006
                                       TO AA-PARAGRAPH-NAME             03180006
                   MOVE 'TMNMCL TABLE ERROR'                            03190006
                                       TO AA-MESSAGE                    03200006
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              03210006
                   PERFORM Z998-SQL-ERROR                               03220006
           END-EVALUATE.                                                03310006
                                                                        03320006
       EJECT                                                            03330006
                                                                        03340006
      ******************************************************************03350006
       7300-CLOSE-CURSOR-TMNMCL.                                        03360006
      ******************************************************************03370006
      *                                                                 03380006
           EXEC SQL                                                     03390006
               CLOSE TMNMCL_CURSOR                                      03400006
           END-EXEC.                                                    03410006
                                                                        03420006
           EVALUATE TRUE                                                03430006
               WHEN SQLCODE = 0                                         03431007
                   CONTINUE                                             03432007
               WHEN OTHER                                               03433007
                   MOVE '7300-CLOSE-CURSOR-TMNMCL'                      03450006
                                       TO AA-PARAGRAPH-NAME             03460006
                   MOVE 'TMNMCL TABLE ERROR'                            03470006
                                       TO AA-MESSAGE                    03480006
                   MOVE 'CLOSE CURSOR' TO AA-DB2-OPERATION              03490006
                   PERFORM Z998-SQL-ERROR                               03500006
           END-EVALUATE.                                                03590006
                                                                        03600006
       EJECT                                                            03610006
                                                                        03620006
      ******************************************************************03630000
       8000-EOJ.                                                        03640006
      ******************************************************************03650000
