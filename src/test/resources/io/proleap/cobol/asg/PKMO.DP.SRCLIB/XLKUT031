017300*---------------------------------------------------------------* 01730002
017400*  DB2 FORMATTED MESSAGE AREA                                     01740002
017500*---------------------------------------------------------------* 01750002
017600     COPY DPWS004.                                                01760002
017700                                                                  01770002
017800                                                                  01780002
017900*---------------------------------------------------------------* 01790002
018000*  XLRD013  - RECORD LAYOUT FOR PO DRIVER FILE, INPUT.          * 01800002
018100*---------------------------------------------------------------* 01810002
018200     COPY XLRD013.                                                01820002
018300                                                                  01830002
018400                                                                  01840002
018500*---------------------------------------------------------------* 01850002
018600*    DB2 AREA FOR TPOINST                                       * 01860002
018700*---------------------------------------------------------------* 01870002
018800     EXEC SQL                                                     01880002
018900          INCLUDE TPOINST                                         01890002
019000     END-EXEC.                                                    01900002
019100                                                                  01910002
019110*---------------------------------------------------------------* 01911002
019120*    DB2 AREA FOR TINSTCD                                       * 01912002
019130*---------------------------------------------------------------* 01913002
019140     EXEC SQL                                                     01914002
019150          INCLUDE TINSTCD                                         01915002
019160     END-EXEC.                                                    01916002
019170                                                                  01917002
019200*---------------------------------------------------------------* 01920002
019300*    DB2 AREA FOR COMMUNICATIONS                                * 01930002
019400*---------------------------------------------------------------* 01940002
019500     EXEC SQL                                                     01950002
019600          INCLUDE SQLCA                                           01960002
019700     END-EXEC.                                                    01970002
019800                                                                  01980002
019900*---------------------------------------------------------------* 01990002
020000*    DEFINE CURSOR FOR TPOINST WITH JOIN TO TINSTCD             * 02000002
020100*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02010002
020200*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02020002
020300*---------------------------------------------------------------* 02030002
020400     EXEC SQL                                                     02040002
020500          DECLARE TABLE-CURSOR CURSOR FOR                         02050002
020600           SELECT A.PO_NBR                                        02060002
020700                 ,A.RECIP_CDE                                     02070002
020800                 ,A.INSTR_NBR                                     02080002
020900                 ,A.CRE_BY_UID                                    02090002
021000                 ,A.CRE_TMST                                      02100002
021100                 ,B.INSTR_NBR                                     02110002
021200                 ,B.INSTR_DESC                                    02120002
021300                 ,B.CRE_BY_UID                                    02130002
021400                 ,B.CRE_DTE                                       02140002
023300           FROM   TPOINST A                                       02330002
023310              ,   TINSTCD B                                       02331002
023400          WHERE  A.PO_NBR = :PV-PO-NBR-COMP                       02340002
023500            AND  B.INSTR_NBR = A.INSTR_NBR                        02350002
023600            FOR  FETCH ONLY                                       02360002
023700           WITH  UR                                               02370002
023800     END-EXEC.                                                    02380002
023900                                                                  02390002
024000                                                                  02400002
024100 PROCEDURE DIVISION.                                              02410002
024200********************                                              02420002
024300                                                                  02430002
024400 0000-PROGRAM-MAINLINE.                                           02440002
024500                                                                  02450002
024600     PERFORM 1000-INITIALIZE.                                     02460002
024700                                                                  02470002
024800     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02480002
024900     PERFORM 2000-PROCESS-DRIVER-FILE                             02490002
025000         UNTIL PS-IN-DRIVER-EOF.                                  02500002
025100                                                                  02510002
025200     PERFORM 6000-EOJ-ROUTINE.                                    02520002
025300     STOP RUN.                                                    02530002
025400                                                                  02540002
025500                                                                  02550002
025600* ************************************************************** *02560002
025700* * INITIALIZATIONS                                            * *02570002
025800* ************************************************************** *02580002
025900 1000-INITIALIZE.                                                 02590002
026000                                                                  02600002
026100     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02610002
026200     DISPLAY DM-DISPLAY-MESSAGE.                                  02620002
026300                                                                  02630002
026400     MOVE PC-BEGINNING              TO DP-STATUS-DESCRIPTION.     02640002
026500     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02650002
026600                                                                  02660002
026700     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02670002
026800     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02680002
026900                                                                  02690002
027000                                                                  02700002
027100* ************************************************************** *02710002
027200* * 1) GET NEXT RECORD FROM DRIVER FILE, AND                   * *02720002
027300* *    1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR        * *02730002
027400* *    1B- OPEN TABLE CURSOR                                   * *02740002
027500* *    1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL               * *02750002
027600* *        ROWS FOR THIS INPUT DRIVER RECORD                   * *02760002
027700* *    2) FOR EACH ROW,                                        * *02770002
027800* *       2A- INCREMENT COUNTS                                 * *02780002
027900* *       2B- WRITE EXTRACT RECORD                             * *02790002
028000* *    1D- CLOSE TABLE CURSOR                                  * *02800002
028100* ************************************************************** *02810002
028200 2000-PROCESS-DRIVER-FILE.                                        02820002
028300                                                                  02830002
028400     PERFORM 9050-READ-IN-DRIVER-FILE.                            02840002
028500     IF PS-IN-DRIVER-EOF                                          02850002
028600         CONTINUE                                                 02860002
028700     ELSE                                                         02870002
028800         MOVE XL013-PO-NBR TO         PV-PO-NBR-COMP              02880002
028900                                      PV-PO-NBR-NUM               02890002
029000         PERFORM 2100-CHECK-PGM-PROGRESS                          02900002
029100         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     02910002
029200         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     02920002
029300         PERFORM 9100-OPEN-TABLE-CURSOR                           02930002
029400         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     02940002
029500             UNTIL PS-TABLE-LOOP-DONE                             02950002
029600         PERFORM 9190-CLOSE-TABLE-CURSOR                          02960002
029700     END-IF.                                                      02970002
029800                                                                  02980002
029900                                                                  02990002
030000* ************************************************************** *03000002
030100* *    EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT     * *03010002
030200* *    RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST   * *03020002
030300* *    WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.   * *03030002
030400* ************************************************************** *03040002
030500 2100-CHECK-PGM-PROGRESS.                                         03050002
030600     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03060002
030700         BY        PC-HOW-OFTEN-TO-DISPLAY                        03070002
030800         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03080002
030900         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03090002
031000                                                                  03100002
031100     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03110002
031200         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03120002
031300         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03130002
031400     END-IF.                                                      03140002
031500                                                                  03150002
031600                                                                  03160002
031700* ************************************************************** *03170002
031800* * EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD     * *03180002
031900* ************************************************************** *03190002
032000 3000-EXTRACT-ROWS-FROM-TABLE.                                    03200002
032100     INITIALIZE DCLTPOINST.                                       03210002
032110     INITIALIZE DCLTINSTCD.                                       03211002
032200     PERFORM 9150-FETCH-TABLE-CURSOR.                             03220002
032300     IF PS-TABLE-LOOP-DONE                                        03230002
032400         CONTINUE                                                 03240002
032500     ELSE                                                         03250002
032600         PERFORM 9750-WRITE-EXTRACT-RECORD                        03260002
032700     END-IF.                                                      03270002
032800                                                                  03280002
032900                                                                  03290002
033000* ************************************************************** *03300002
033100* * DISPLAY END OF RUN MESSAGE, CLOSE THE OUTPUT FILES         * *03310002
033200* ************************************************************** *03320002
033300 6000-EOJ-ROUTINE.                                                03330002
033400                                                                  03340002
033500     PERFORM 9090-CLOSE-IN-DRIVER-FILE.                           03350002
033600     PERFORM 9790-CLOSE-OUT-EXTRACT-FILE.                         03360002
033700                                                                  03370002
033800     MOVE PC-ENDING                 TO DP-STATUS-DESCRIPTION.     03380002
033900     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           03390002
034000                                                                  03400002
034100                                                                  03410002
034200* ************************************************************** *03420002
034300* * DISPLAY PROGRAM PROGRESS.                                  * *03430002
034400* ************************************************************** *03440002
034500 6200-DISPLAY-PGM-PROGRESS.                                       03450002
034600                                                                  03460002
034700     EXEC SQL                                                     03470002
034800          SET :PV-TMST = CURRENT TIMESTAMP                        03480002
034900     END-EXEC.                                                    03490002
035000                                                                  03500002
035100     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                03510002
035200     MOVE PV-TIMESTAMP-19           TO DP-TIMESTAMP-19.           03520002
035300     MOVE PV-IN-DRIVER-RECS-READ-CNT                              03530002
035400                                    TO DP-IN-CNT.                 03540002
035500*    MOVE PV-RECS-WITH-MATCHES-CNT  TO DP-MATCHES-FOUND-CNT.      03550002
035600     MOVE PV-TABLE-ROW-SELECT-CNT   TO DP-ROWS-SELECTED-CNT.      03560002
035700     MOVE PV-EXTRACT-WRITTEN-CNT    TO DP-OUT-CNT.                03570002
035800                                                                  03580002
035900     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             03590002
036000                                                                  03600002
036100                                                                  03610002
036200* ************************************************************** *03620002
036300* * FILE AND DB2 TABLE ACCESS PARAGRAPHS.                      * *03630002
036400* * 90XX- INPUT:  DRIVER FILE                                  * *03640002
036500* * 91XX- INPUT:  TABLE CURSOR                                 * *03650002
036600* * 97XX- OUTPUT: TABLE EXTRACT                                * *03660002
036700* ************************************************************** *03670002
036800                                                                  03680002
036900* ************************************************************** *03690002
037000* * OPEN  INPUT DRIVER FILE                                    * *03700002
037100* ************************************************************** *03710002
037200 9000-OPEN-IN-DRIVER-FILE.                                        03720002
037300     OPEN  INPUT  IN-DRIVER-FILE.                                 03730002
037400                                                                  03740002
037500                                                                  03750002
037600* ************************************************************** *03760002
037700* * READ  INPUT DRIVER FILE OF ELIGIBLE DATA TO EXTRACT        * *03770002
037800* ************************************************************** *03780002
037900 9050-READ-IN-DRIVER-FILE.                                        03790002
038000     READ IN-DRIVER-FILE                                          03800002
038100          INTO XL013-DRIVER-RECORD-LAYOUT                         03810002
038200          AT END                                                  03820002
038300              SET PS-IN-DRIVER-EOF TO TRUE.                       03830002
038400                                                                  03840002
038500     IF PS-IN-DRIVER-EOF                                          03850002
038600         CONTINUE                                                 03860002
038700     ELSE                                                         03870002
038800         ADD +1 TO PV-IN-DRIVER-RECS-READ-CNT                     03880002
038900     END-IF.                                                      03890002
039000                                                                  03900002
039100                                                                  03910002
039200* ************************************************************** *03920002
039300* * CLOSE INPUT DRIVER FILE                                    * *03930002
039400* ************************************************************** *03940002
039500 9090-CLOSE-IN-DRIVER-FILE.                                       03950002
039600     CLOSE IN-DRIVER-FILE.                                        03960002
039700                                                                  03970002
039800                                                                  03980002
039900* ************************************************************** *03990002
040000* * OPEN  TABLE CURSOR                                         * *04000002
040100* ************************************************************** *04010002
040200 9100-OPEN-TABLE-CURSOR.                                          04020002
040300     EXEC SQL                                                     04030002
040400          OPEN TABLE-CURSOR                                       04040002
040500     END-EXEC.                                                    04050002
040600                                                                  04060002
040700     EVALUATE TRUE                                                04070002
040800         WHEN SQLCODE = ZERO                                      04080002
040900              ADD +1 TO PV-RECS-WITH-MATCHES-CNT                  04090002
041000              SET PS-CURSOR-HAS-DATA TO TRUE                      04100002
041100         WHEN SQLCODE = +100                                      04110002
041200              SET PS-TABLE-LOOP-DONE TO TRUE                      04120002
041300         WHEN SQLWARN0 NOT EQUAL SPACE                            04130002
041400         WHEN SQLCODE  NOT EQUAL ZERO                             04140002
041500              MOVE '9100-OPEN-TABLE-CURSOR'                       04150002
041600                                 TO PV-PARAGRAPH-NAME             04160002
041700              MOVE 'OPEN TABLE CURSOR'                            04170002
041800                                 TO PV-DB2-OPERATION-MSG-1        04180002
041900              MOVE 'TPOINST'     TO PV-DB2-TABLE-NAME             04190002
042000              PERFORM 9999-DB2-ABEND                              04200002
042100     END-EVALUATE.                                                04210002
042200                                                                  04220002
042300                                                                  04230002
042400* ************************************************************** *04240002
042500* * FETCH TABLE CURSOR                                         * *04250002
042600* ************************************************************** *04260002
042700 9150-FETCH-TABLE-CURSOR.                                         04270002
042800     EXEC SQL                                                     04280002
042900         FETCH TABLE-CURSOR                                       04290002
043000          INTO  :POINST-PO-NBR                                    04300002
043100               ,:POINST-RECIP-CDE                                 04310002
