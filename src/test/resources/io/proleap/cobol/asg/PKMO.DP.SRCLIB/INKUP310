           05  PV-AUDIT-STR-NBR              PIC  S9(4) COMP.           01010001
           05  PV-AUDIT-FSCL-MN-END-DTE      PIC   X(10).               01020001
           05  PV-AUDIT-SLS-RTL-AMT          PIC  S9(11)V9(2) COMP-3.   01030001
           05  PV-AUDIT-SHNK-RTL-AMT         PIC  S9(11)V9(2) COMP-3.   01040001
                                                                        01060000
      ******************************************************************01070000
      *  DB2 SQL ERROR MESSAGE FORMAT AREA                              01080000
      ******************************************************************01090000
           COPY DPWS004.                                                01100000
                                                                        01110000
      ******************************************************************01120000
      * SHRINK TABLE INT_INV_SHRK                                       01130002
      ******************************************************************01140000
           EXEC SQL                                                     01150000
             INCLUDE INT00006                                           01160002
           END-EXEC.                                                    01170000
      ***************************************************************** 01420000
      *    DB2 AREA FOR COMMUNICATIONS                                * 01430000
      ***************************************************************** 01440000
           EXEC SQL                                                     01450000
                INCLUDE SQLCA                                           01460000
           END-EXEC.                                                    01470000
                                                                        01480000
                                                                        01580000
           EXEC SQL                                                     01590000
              DECLARE SHRINK_CSR CURSOR FOR                             01600001
                SELECT                                                  01610000
                       FSCL_WK_END_DTE                                  01620001
                     , ML_PSTG_MN_END_DTE                               01630001
                     , DEPT_NBR                                         01640001
                     , SUB_CL_NBR                                       01650000
                     , STR_NBR                                          01660001
                     , FSCL_MN_END_DTE                                  01670001
                     , SLS_RTL_AMT                                      01680001
                     , SHNK_RTL_AMT                                     01690001
                    FROM INT_INV_SHRK                                   02160002
                WHERE STR_NBR           = :CC-STR-NBR                   02170002
                  AND FSCL_MN_END_DTE   = :CC-FSCL-MN-END-DTE           02180002
                  AND ML_PSTG_MN_END_DTE= :CC-ML-PSTG-MN-END-DTE        02181002
           END-EXEC.                                                    02234000
      ******************************************************************02235000
       PROCEDURE DIVISION.                                              02236000
      ******************************************************************02237000
                                                                        02238000
       0000-MAINLINE.                                                   02239000
                                                                        02240000
           PERFORM 1000-INITIALIZE.                                     02250000
                                                                        02260000
           PERFORM 2000-PROCESS-RECS                                    02261002
             UNTIL PS-CONTROL-FILE-AT-END-YES.                          02262002
                                                                        02262400
           DISPLAY 'PA-AUDIT-COUNT     ' PA-AUDIT-COUNT.                02262502
           DISPLAY 'PA-DELETE-COUNT    ' PA-DELETE-COUNT.               02262900
                                                                        02263002
           IF PA-AUDIT-COUNT EQUAL PA-DELETE-COUNT                      02263102
              IF PA-AUDIT-COUNT EQUAL ZERO                              02263209
                 DISPLAY 'ZERO RECORDS PROCESSED/ DELETED'              02263309
              ELSE                                                      02263409
                 DISPLAY 'RUN STATS BALANCE'                            02263509
                 DISPLAY 'ALL DELETED RECS ARE IN AUDIT FILE'           02263609
              END-IF                                                    02263709
           ELSE                                                         02263802
              DISPLAY 'PROGRAM STATS OUT OF BALANCE'                    02263902
              DISPLAY '** ERROR ** ERROR ** ERROR **'                   02264002
           END-IF.                                                      02264102
                                                                        02264200
           CLOSE CNTL-STR-DATE-FILE                                     02264302
                 AUDIT-FILE.                                            02264400
                                                                        02264500
           STOP RUN.                                                    02264600
                                                                        02265000
                                                                        02430000
      ******************************************************************02440000
      *  PERFORM THE NECESSARY INITIALIZATION STEPS                     02450000
      ******************************************************************02460000
       1000-INITIALIZE.                                                 02470000
                                                                        02480000
           OPEN INPUT  CNTL-STR-DATE-FILE                               02490001
                OUTPUT AUDIT-FILE.                                      02500000
                                                                        02510000
           INITIALIZE PV-AUDIT-REC.                                     02520000
                                                                        02530000
           PERFORM 1100-READ-CNTL-STR-DATE-FILE.                        02540002
                                                                        02550000
                                                                        02560002
      ******************************************************************02570002
      * 1100-READ-CNTL-STR-DATE-FILE.                                   02570102
      ******************************************************************02570202
                                                                        02570302
       1100-READ-CNTL-STR-DATE-FILE.                                    02570402
           READ CNTL-STR-DATE-FILE INTO CC-CONTROL-REC                  02570502
               AT END                                                   02570602
                   DISPLAY '** PROGRAM ABEND **'                        02570702
                   DISPLAY 'INKUP310-CNTL-STR-DATE-FILE EMPTY'          02570803
                   MOVE PC-4005 TO ABEND-CODE                           02570902
                   CALL 'ILBOABN0' USING ABEND-CODE.                    02571002
                                                                        02571102
      ******************************************************************02571202
      * 2000-PROCESS-RECS.                                              02571302
      ******************************************************************02571402
                                                                        02571502
       2000-PROCESS-RECS.                                               02571602
                                                                        02571702
           PERFORM 2100-OPEN-SHRINK-CURSOR.                             02571802
                                                                        02572000
           PERFORM 2200-FETCH-SHRINK-CURSOR                             02573002
             UNTIL PS-SHRINK-CSR-AT-END-YES.                            02574002
                                                                        02575002
           PERFORM 2300-CLOSE-SHRINK-CURSOR.                            02576002
                                                                        02577002
           READ CNTL-STR-DATE-FILE INTO CC-CONTROL-REC                  02578002
               AT END SET PS-CONTROL-FILE-AT-END-YES TO TRUE.           02579002
                                                                        02580002
       2100-OPEN-SHRINK-CURSOR.                                         03290002
                                                                        03300000
           EXEC SQL                                                     03310000
               OPEN SHRINK_CSR                                          03320002
           END-EXEC.                                                    03330000
                                                                        03340000
           IF SQLCODE NOT = ZERO                                        03350000
               MOVE '2100-OPEN-SHRINK-CURSOR '                          03360002
                                       TO PV-PARAGRAPH-NAME             03370000
               PERFORM 9999-SQL-ABEND                                   03380000
           END-IF.                                                      03390000
                                                                        03400000
                                                                        03410002
       2200-FETCH-SHRINK-CURSOR.                                        03410102
                                                                        03410202
           EXEC SQL                                                     03410302
               FETCH SHRINK_CSR                                         03410402
               INTO    :INT00006-FSCL-WK-END-DTE                        03410502
                      ,:INT00006-ML-PSTG-MN-END-DTE                     03410602
                      ,:INT00006-DEPT-NBR                               03410702
                      ,:INT00006-SUB-CL-NBR                             03410802
                      ,:INT00006-STR-NBR                                03410902
                      ,:INT00006-FSCL-MN-END-DTE                        03411002
                      ,:INT00006-SLS-RTL-AMT                            03411102
                      ,:INT00006-SHNK-RTL-AMT                           03411202
           END-EXEC.                                                    03411302
                                                                        03411402
           EVALUATE TRUE                                                03411502
               WHEN SQLCODE = ZERO                                      03411602
                   SET PS-SHRINK-CSR-AT-END-NO  TO TRUE                 03411702
                   PERFORM 2210-WRITE-TO-AUDIT-FILE                     03411802
                   PERFORM 2220-DELETE-FROM-SHRINK-TBL                  03411910
               WHEN SQLCODE = +100                                      03412002
                   SET PS-SHRINK-CSR-AT-END-YES TO TRUE                 03412102
               WHEN OTHER                                               03412202
                   MOVE '2200-FETCH-SHRINK-CURSOR   '                   03412302
                                       TO PV-PARAGRAPH-NAME             03412402
                   PERFORM 9999-SQL-ABEND                               03412502
           END-EVALUATE.                                                03412602
                                                                        03412702
                                                                        03412802
       2300-CLOSE-SHRINK-CURSOR.                                        03413002
                                                                        03420000
           EXEC SQL                                                     03430000
               CLOSE SHRINK_CSR                                         03440002
           END-EXEC.                                                    03450000
                                                                        03460000
           IF SQLCODE NOT = ZERO                                        03470000
               MOVE '2300-CLOSE-SHRINK-CURSOR '                         03480002
                                       TO PV-PARAGRAPH-NAME             03490000
               PERFORM 9999-SQL-ABEND                                   03500000
           END-IF.                                                      03510000
                                                                        03520000
       2210-WRITE-TO-AUDIT-FILE.                                        04660002
                                                                        05790000
           MOVE INT00006-FSCL-WK-END-DTE                                05800002
             TO PV-AUDIT-FSCL-WK-END-DTE.                               05801002
           MOVE INT00006-ML-PSTG-MN-END-DTE                             05810002
             TO PV-AUDIT-ML-PSTG-MN-END-DTE.                            05811002
           MOVE INT00006-DEPT-NBR                                       05820002
             TO PV-AUDIT-DEPT-NBR.                                      05821002
           MOVE INT00006-SUB-CL-NBR                                     05830002
             TO PV-AUDIT-SUB-CL-NBR.                                    05831002
           MOVE INT00006-STR-NBR                                        05840002
             TO PV-AUDIT-STR-NBR.                                       05841002
           MOVE INT00006-FSCL-MN-END-DTE                                05850002
             TO PV-AUDIT-FSCL-MN-END-DTE.                               05851002
           MOVE INT00006-SLS-RTL-AMT                                    05860002
             TO PV-AUDIT-SLS-RTL-AMT.                                   05861002
           MOVE INT00006-SHNK-RTL-AMT                                   05870002
             TO PV-AUDIT-SHNK-RTL-AMT.                                  05880002
                                                                        06010000
           WRITE AUDIT-REC FROM PV-AUDIT-REC.                           06011002
             ADD 1  TO PA-AUDIT-COUNT.                                  06012002
                                                                        06020000
      ****************************************************************  07230000
      * 2220-DELETE-FROM-SHRINK-TBL.                                    07240002
      ****************************************************************  07250000
                                                                        07250100
       2220-DELETE-FROM-SHRINK-TBL.                                     07250202
                                                                        07250300
           EXEC SQL                                                     07250400
             DELETE FROM INT_INV_SHRK                                   07250502
               WHERE FSCL_WK_END_DTE    = :INT00006-FSCL-WK-END-DTE     07250604
                 AND ML_PSTG_MN_END_DTE = :INT00006-ML-PSTG-MN-END-DTE  07250704
                 AND DEPT_NBR           = :INT00006-DEPT-NBR            07250804
                 AND SUB_CL_NBR         = :INT00006-SUB-CL-NBR          07250904
                 AND STR_NBR            = :INT00006-STR-NBR             07251004
                 AND FSCL_MN_END_DTE    = :INT00006-FSCL-MN-END-DTE     07251104
                 AND SLS_RTL_AMT        = :INT00006-SLS-RTL-AMT         07251204
                 AND SHNK_RTL_AMT       = :INT00006-SHNK-RTL-AMT        07251304
                                                                        07252102
           END-EXEC.                                                    07252200
                                                                        07252300
           EVALUATE SQLCODE                                             07252400
               WHEN ZERO                                                07252500
                   ADD 1                      TO PA-DELETE-COUNT        07252600
               WHEN OTHER                                               07253100
                    DISPLAY '*************************************'     07253200
                    DISPLAY 'ROW IN PROCESS : '                         07253300
                    'FSCL-WK-END-DTE :' INT00006-FSCL-WK-END-DTE,       07253402
                    'ML-PSTG-DTE     :' INT00006-ML-PSTG-MN-END-DTE,    07253502
                    'DEPT-NBR        :' INT00006-DEPT-NBR,              07253602
                    'SUB-CL-NBR      :' INT00006-SUB-CL-NBR,            07253702
                    'STR-NBR         :' INT00006-STR-NBR,               07253802
                    'FSCL-MN-END-DTE :' INT00006-FSCL-MN-END-DTE,       07253902
                    'SLS-RTL-AMT     :' INT00006-SLS-RTL-AMT,           07254002
                    'SHNK-RTL-AMT    :' INT00006-SHNK-RTL-AMT           07254102
                    DISPLAY '*************************************'     07254202
                    MOVE '2220-DELETE-FROM-SHRINK-TBL'                  07254302
                                          TO PV-PARAGRAPH-NAME          07254402
                    PERFORM 9999-SQL-ABEND                              07254502
           END-EVALUATE.                                                07254602
                                                                        07254702
      ******************************************************************07863000
      *  STANDARD DB2 ERROR ABEND ROUTINE                               07864000
      *  THIS ROUTINE FORCES AN ABEND DUE TO A DB2 PROBLEM.             07865000
      ******************************************************************07866000
       9999-SQL-ABEND.                                                  07867000
                                                                        07868000
           DISPLAY '**  ABEND     **'.                                  07870000
           DISPLAY '**  PROGRAM   **  = ' PC-PGM-NAME.                  07880000
           DISPLAY '**  PARAGRAPH **  = ' PV-PARAGRAPH-NAME.            07890000
                                                                        07900000
      *    THE FOLLOWING COPYBOOK UTILIZES THE DB2 ABEND MODULE         07910000
      *    DSNTIAR TO CONVERT SQLCA INTO UNDERSTANDABLE VERBAGE.        07920000
                                                                        07930000
           COPY DPPD004.                                                07940000
                                                                        07950000
           CALL 'ILBOABN0'  USING ABEND-CODE.                           07960000
