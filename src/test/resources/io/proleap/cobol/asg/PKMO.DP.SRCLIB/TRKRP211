184200******************************************************************18420099
184300* SELECT VENDOR NAME FROM  PURCHS.                                18430099
184400******************************************************************18440099
184500                                                                  18450099
184600 7050-SELECT-VENDOR-NAME.                                         18460099
184700                                                                  18470099
184800     IF TR211-CONPOA-PO-NBR  NUMERIC                              18480099
184810        MOVE TR211-CONPOA-PO-NBR  TO  PURCHS-PO-NBR               18481099
184811     ELSE                                                         18481199
184820        MOVE 0                    TO  PURCHS-PO-NBR               18482099
184830     END-IF.                                                      18483099
184900     MOVE PC-A                 TO  PURCHS-VER-STATUS-CDE.         18490099
185000                                                                  18500099
185100     EXEC SQL                                                     18510099
185200           SELECT DISTINCT                                        18520099
185300                  A.ENT_NAME_DESC                                 18530099
185400           INTO   :ENTNME-ENT-NAME-DESC                           18540099
185500           FROM   TENTNME A,                                      18550099
185600                  TPURCHS B                                       18560099
185700           WHERE  B.PO_NBR       = :PURCHS-PO-NBR                 18570099
185800           AND    A.ENT_ID       = B.ENT_ID                       18580099
185900           AND    VER_STATUS_CDE = :PURCHS-VER-STATUS-CDE         18590099
186000           AND    A.TYPE_CDE    = :PC-01                          18600099
186100     END-EXEC.                                                    18610099
186200                                                                  18620099
186300     EVALUATE TRUE                                                18630099
186400         WHEN SQLCODE = 0                                         18640099
186500             MOVE ENTNME-ENT-NAME-DESC   TO DL06-VENDOR-NAME      18650099
186600         WHEN SQLCODE = +100                                      18660099
186700             MOVE SPACES                 TO DL06-VENDOR-NAME      18670099
186800         WHEN SQLCODE = -904                                      18680099
186900         WHEN SQLCODE = -911                                      18690099
187000             MOVE SPACES                 TO DL06-VENDOR-NAME      18700099
187100             CONTINUE                                             18710099
187200     END-EVALUATE.                                                18720099
