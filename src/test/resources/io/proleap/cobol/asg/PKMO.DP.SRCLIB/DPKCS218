001900******************************************************************00190000
002000                                                                  00200000
002100******************************************************************00210000
002200 DATA DIVISION.                                                   00220000
002300******************************************************************00230000
002400                                                                  00240000
002500 WORKING-STORAGE SECTION.                                         00250000
002600 01  WS-INDICATORS.                                               00260000
002710     05 WI-PORT1           PIC 9(4) VALUE 4998.                   00271000
002711     05 WI-PORT2           PIC 9(4) VALUE 5998.                   00271101
002712     05 WI-PORT3           PIC 9(4) VALUE 7998.                   00271202
002720******************************************************************00272000
002730 PROCEDURE DIVISION.                                              00273000
002740******************************************************************00274000
002750 0000-MAIN-LINE.                                                  00275000
002800     EXEC CICS START                                              00280000
002900               TRANSID('A500')                                    00290000
003000               FROM(WI-PORT1)                                     00300000
003100     END-EXEC.                                                    00310000
003200     EXEC CICS START                                              00320002
003300               TRANSID('A505')                                    00330001
003400               FROM(WI-PORT2)                                     00340001
003500     END-EXEC.                                                    00350001
003510     EXEC CICS START                                              00351002
003520               TRANSID('A519')                                    00352002
003530               FROM(WI-PORT3)                                     00353002
003531     END-EXEC.                                                    00353102
003540     EXEC CICS RETURN                                             00354000
003550     END-EXEC.                                                    00355000
