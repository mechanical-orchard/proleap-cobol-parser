028300                                                                  02830001
028400*    WE WANT TO SEE IF THIS GROUP CONSISTS OF JUST 1 CLEAN REC    02840001
028500     IF PC051-TRN-TYP-CDE = 'CL'                                  02850001
028600         SET PS-CLEAN-FOUND-YES       TO TRUE                     02860001
028700     END-IF.                                                      02870001
028800                                                                  02880001
028801***************************************************************** 02880101
028802* STORE EXPECTED MEMO CODES                                    ** 02880201
028803*     UM - STORE UNLOAD MEMO                                   ** 02880301
028804*     NS - NOT SCANNED ON EXPTED TRLR                          ** 02880401
028805*     MM - STORE MISLOAD MEMO                                  ** 02880501
028806*     AM - ASSUMPTIVE UNLOAD MEMO                              ** 02880601
028807*     SM - STORE MISLOAD UNLOAD MEMO                           ** 02880701
028808***************************************************************** 02880801
028810     IF  PC051-TRN-TYP-CDE = 'UM' OR                              02881001
028811         PC051-TRN-TYP-CDE = 'NS' OR                              02881101
028812         PC051-TRN-TYP-CDE = 'MM' OR                              02881201
028813         PC051-TRN-TYP-CDE = 'AM' OR                              02881301
028814         PC051-TRN-TYP-CDE = 'SM'                                 02881401
028820         NEXT SENTENCE                                            02882001
028830     ELSE                                                         02883001
028840         ADD PC051-TRN-QTY            TO PV-GROUP-TOTAL           02884001
028850     END-IF.                                                      02885001
028860                                                                  02886001
028870     ADD 1                            TO PV-RCVR-REC-COUNT        02887001
028880                                         PV-GROUP-MEMBER-COUNT.   02888001
028890                                                                  02889001
028900 2200-PO-RCVR-BREAK.                                              02890001
029000                                                                  02900001
029100     MOVE '2200-PO-RCVR-BREAK'        TO PV-ABEND-PARAGRAPH       02910001
029200*    DISPLAY PV-ABEND-PARAGRAPH                                   02920001
029300                                                                  02930001
029400     INITIALIZE PC050-SE-SUMMARY-REC                              02940001
029500     MOVE PV-PREV-PO                  TO PC050-PO-NBR             02950001
029600     MOVE PV-PREV-RCVR                TO PC050-RCVR-SEQ-NBR       02960001
029700     MOVE PV-PREV-UPC                 TO PC050-INTR-UPC-ID        02970001
029800     MOVE PV-PREV-LOC                 TO PC050-LOC-NBR            02980001
029900     MOVE PV-GROUP-TOTAL              TO PC050-GROUP-BAL-QTY      02990001
030000     MOVE PV-END-RCVR-DATE            TO PC050-LATEST-TRN-DTE     03000001
030100     MOVE PV-SAVE-INV-DTE             TO PC050-LOC-INV-DTE        03010001
030200     MOVE PS-EOF-CLEAN-SW             TO PC050-ALL-CLEAN-GRP-SW   03020001
030300     MOVE PV-GROUP-MEMBER-COUNT       TO PC050-GRP-MEMBER-COUNT   03030001
030400     MOVE ZERO                        TO PC050-SKU-NBR            03040001
030500                                                                  03050001
030600     EVALUATE TRUE                                                03060001
030700         WHEN PV-GROUP-MEMBER-COUNT = 1 AND PS-CLEAN-FOUND-YES    03070001
030800*            (THIS GROUP CONSISTS OF 1 CLEAN REC ONLY)            03080001
030900             PERFORM 8200-WRITE-LONESOME-CLEAN                    03090001
031000             DISPLAY 'LONESOME CLEAN - PO='                       03100001
031100                  PV-PREV-PO ' RCVR='                             03110001
031200                  PV-PREV-RCVR ' UPC='                            03120001
031300                  PV-PREV-UPC ' LOC='                             03130001
031400                  PV-PREV-LOC                                     03140001
031500             ADD 1                    TO PV-WRITE-3-COUNTER       03150001
031600                                                                  03160001
031700         WHEN PC050-LOC-INV-DTE < PC050-LATEST-TRN-DTE            03170001
031800             CONTINUE                                             03180001
031900         WHEN PC050-GROUP-BAL-QTY NOT = ZERO                      03190001
032000             CONTINUE                                             03200001
032100                                                                  03210001
032200          WHEN OTHER                                              03220001
032300             PERFORM 8100-WRITE-PC050                             03230001
032400                                                                  03240001
032500     END-EVALUATE.                                                03250001
032600                                                                  03260001
032700     MOVE PV-CURR-KEY                 TO PV-PREV-KEY              03270001
032800     MOVE '9999-09-09'                TO PV-START-RCVR-DATE       03280001
032900     MOVE '0001-01-01'                TO PV-END-RCVR-DATE         03290001
033000     INITIALIZE PV-GROUP-MEMBER-COUNT                             03300001
033100     SET PS-CLEAN-FOUND-NO            TO TRUE                     03310001
033200                                                                  03320001
033300     MOVE ZERO                        TO PV-GROUP-TOTAL.          03330001
