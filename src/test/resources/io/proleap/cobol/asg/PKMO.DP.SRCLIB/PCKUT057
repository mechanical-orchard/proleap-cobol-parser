021200/                                                                 02120001
021300 1000-INITIALIZATION.                                             02130001
021400     MOVE '1000-INITIALIZATION         '  TO PV-ABEND-PARAGRAPH.  02140001
021500*    DISPLAY PV-ABEND-PARAGRAPH                                   02150001
021600                                                                  02160001
021700     DISPLAY SPACES.                                              02170001
021800     DISPLAY ' STARTING PCKUT057'                                 02180001
021900                                                                  02190001
022000     MOVE WHEN-COMPILED               TO PV-WHEN-COMPILED         02200001
022100                                         PGM-COMPILED             02210001
022200     DISPLAY SPACES                                               02220001
022300     DISPLAY 'COMPILE DATE: ' PV-COMPILE-DATE                     02230001
022400             ' ' PV-COMPILE-TIME                                  02240001
022500                                                                  02250001
022600     OPEN INPUT ALL-INV-SE-FILE                                   02260001
022700          OUTPUT SUMMARY-FILE                                     02270001
022800                 LONESOME-FILE                                    02280001
022900                                                                  02290001
023000     PERFORM 8000-READ-DETAIL-EXTRACT                             02300001
023100     MOVE PV-CURR-KEY                 TO PV-PREV-KEY              02310001
023200                                                                  02320001
023300     IF PS-EOF-DETAIL-YES                                         02330001
023400         DISPLAY SPACES                                           02340001
023500         DISPLAY ALL '*'                                          02350001
023600         DISPLAY '  DETAIL INPUT FILE IS EMPTY '                  02360001
023700         DISPLAY ALL '*'                                          02370001
023800         DISPLAY SPACES                                           02380001
023900     ELSE                                                         02390001
024000         MOVE PV-CURR-KEY             TO PV-PREV-KEY              02400001
024100     END-IF.                                                      02410001
024200                                                                  02420001
024300     MOVE '9999-09-09'                TO PV-START-RCVR-DATE       02430001
024400     MOVE '0001-01-01'                TO PV-END-RCVR-DATE         02440001
024500                                                                  02450001
024600     INITIALIZE PV-GROUP-MEMBER-COUNT.                            02460001
024700/                                                                 02470001
024800 2000-PROCESS.                                                    02480001
024900                                                                  02490001
025000     MOVE '2000-PROCESS'                  TO PV-ABEND-PARAGRAPH.  02500001
025100*    DISPLAY PV-ABEND-PARAGRAPH                                   02510001
025200                                                                  02520001
025300     IF PV-CURR-KEY = PV-PREV-KEY                                 02530001
025400         PERFORM 2005-SAVE-RCVR-DATES                             02540001
025500         PERFORM 2006-ADD-TO-RCVR-TOTS                            02550001
025600     ELSE                                                         02560001
025700         PERFORM 2200-PO-RCVR-BREAK                               02570001
025800         PERFORM 2005-SAVE-RCVR-DATES                             02580001
025900         PERFORM 2006-ADD-TO-RCVR-TOTS                            02590001
026000     END-IF.                                                      02600001
026100                                                                  02610001
