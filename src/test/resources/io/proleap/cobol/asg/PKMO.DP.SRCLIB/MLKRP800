635300 6000-CHECK-AP-DETAIL-GT-ZERO.                                    63490000
635400******************************************************************63500000
635500*   6000-CHECK-AP-DETAIL-GT-ZERO.                                *63510000
635600*      CHECK THE DOLLAR FIELDS IN THE DETAIL LINE IN ORDER TO    *63520000
635700*      DETERMINE IF ONE IS GOING TO BE GREATER THAN ZERO.  IF    *63530000
635800*      NO FIELDS WILL BE GREATER THAN ZERO THE DETAIL LINE IS    *63540000
635900*      SUBSEQUENTLY NOT PRINTED.                                 *63550000
636000*                                                                *63560000
636100******************************************************************63570000
636200                                                                  63580000
636300     SET PI-PRINT-DETAIL TO TRUE.                                 63590000
636400* DO NOT CHECK LAST YEAR IF LAST YEAR WAS 1998                    63600000
636500     IF PV-HDG-START-CCYY-LY = PC-1998                            63610000
636600         IF DL-AP-RECEIPTS-COST-TY       = SPACES AND             63620000
636700            DL-AP-RECEIPTS-RETAIL-TY     = SPACES AND             63630000
636800            DL-AP-PURCHASE-ADJ-COST-TY   = SPACES AND             63640000
636900            DL-AP-PURCHASE-ADJ-RETAIL-TY = SPACES AND             63650000
637000            DL-AP-INIT-MARKON-TY         = SPACES AND             63660000
637100            DL-AP-NET-MARKON-TY          = SPACES AND             63670000
637200            DL-AP-INIT-MARKON-TY         = SPACES AND             63680000
637300            DL-AP-NET-MARKON-TY          = SPACES                 63690000
637400         SET PI-NO-PRINT-DETAIL TO TRUE                           63700000
637500         INITIALIZE DL-AP-DETAIL-LINE                             63710000
637600         END-IF                                                   63720000
637700     ELSE                                                         63730000
637800         IF DL-AP-RECEIPTS-COST-TY       = SPACES AND             63740000
637900            DL-AP-RECEIPTS-RETAIL-TY     = SPACES AND             63750000
