658700 7000-SELECT-TMLCNTL.                                             65830000
658800******************************************************************65840000
658900*   7000-SELECT-TMLCNTL.                                         *65850000
659000*      OBTAIN THE OPEN FISCAL MONTH DATE IN ORDER TO ACCESS      *65860000
659100*      TDTATTR TABLE FIELDS.                                     *65870000
659200******************************************************************65880000
659300*                                                                 65890000
659400     EXEC SQL                                                     65900000
W02191*******  SELECT OPN_FSCL_MN_DTE                                   65910000
W02191         SELECT PSTG_FSCL_MN_DTE                                  65920000
659600         INTO  :PV-OPN-FSCL-MN-DTE                                65930000
659700         FROM TMLCNTL                                             65940000
659800     END-EXEC.                                                    65950000
659900                                                                  65960000
660000     EVALUATE TRUE                                                65970000
660100         WHEN SQLCODE = ZERO                                      65980000
660200             CONTINUE                                             65990000
660300         WHEN OTHER                                               66000000
660400             MOVE '7000-SELECT-TMLCNTL'                           66010000
660500                                 TO AA-PARAGRAPH-NAME             66020000
660600             MOVE 'TMLCNTL ROW NOT FOUND'                         66030000
660700                                 TO AA-MESSAGE                    66040000
660800             MOVE 'SELECT'       TO AA-DB2-OPERATION              66050000
660900             PERFORM 9901-SQL-ERROR                               66060000
661000     END-EVALUATE.                                                66070000
661100                                                                  66080000
661200 EJECT                                                            66090000
661300                                                                  66100000
661400 7100-SELECT-CURR-YR-TDTATTR.                                     66110000
661500******************************************************************66120000
661600*   7100-SELECT-CURR-YR-TDTATTR.                                 *66130000
661700*      OBTAIN THE OPEN FISCAL MONTH DATE'S FISCAL YEAR AND MONTH *66140000
661800*      NUMBERS.                                                  *66150000
661900******************************************************************66160000
662000*                                                                 66170000
662100     EXEC SQL                                                     66180000
662200         SELECT FSCL_YR_NBR,                                      66190000
662300                FSCL_MN_NBR,                                      66200000
662400                SEA_BEG_DTE,                                      66210000
662500                LY_THIS_DAY_KY_DTE                                66220000
662600         INTO   :DTATTR-FSCL-YR-NBR,                              66230000
662700                :DTATTR-FSCL-MN-NBR,                              66240000
662800                :DTATTR-SEA-BEG-DTE,                              66250000
662900                :DTATTR-LY-THIS-DAY-KY-DTE                        66260000
663000         FROM   TDTATTR                                           66270000
663100         WHERE  KY_DTE = :PV-OPN-FSCL-MN-DTE                      66280000
663200     END-EXEC.                                                    66290000
663300                                                                  66300000
663400     EVALUATE TRUE                                                66310000
663500         WHEN SQLCODE = 0                                         66320000
663600             MOVE DTATTR-FSCL-YR-NBR (1:4)                        66330000
663700                                 TO PV-FISCAL-TY-CCYY             66340000
663800             MOVE DTATTR-FSCL-MN-NBR                              66350000
663900                                 TO PV-FISCAL-TY-END-MONTH        66360000
664000         WHEN SQLCODE = +100                                      66370000
664100             MOVE '7100-SELECT-CURR-YR-TDTATTR'                   66380000
664200                                 TO AA-PARAGRAPH-NAME             66390000
664300             MOVE 'TDTATTR ROW NOT FOUND'                         66400000
664400                                 TO AA-MESSAGE                    66410000
664500             MOVE 'SELECT'       TO AA-DB2-OPERATION              66420000
664600             PERFORM 9901-SQL-ERROR                               66430000
664700         WHEN OTHER                                               66440000
664800             MOVE '7100-SELECT-CURR-YR-TDTATTR'                   66450000
664900                                 TO AA-PARAGRAPH-NAME             66460000
665000             MOVE 'TDTATTR TABLE ERROR'                           66470000
665100                                 TO AA-MESSAGE                    66480000
665200             MOVE 'SELECT'       TO AA-DB2-OPERATION              66490000
665300             PERFORM 9901-SQL-ERROR                               66500000
665400     END-EVALUATE.                                                66510000
665500                                                                  66520000
665600 EJECT                                                            66530000
665710                                                                  66540000
665720 7125-GET-LY-THIS-DAY-MIN-7DAYS.                                  66550000
665730******************************************************************66560000
665740*   7125-GET-LY-THIS-DAY-MIN-7DAYS                               *66570000
665750*      GET LY-THIS-DAY WHEN WE ARE IN THE LAST MONTH OF A 53RD   *66571000
665760*      WEEK YEAR.                                                *66572000
665770******************************************************************66573000
665780*                                                                 66574000
665790     EXEC SQL                                                     66575000
665794         SELECT LY_THIS_DAY_KY_DTE                                66576000
665795         INTO   :DTATTR-LY-THIS-DAY-KY-DTE                        66577000
665799         FROM   TDTATTR                                           66578000
665800         WHERE  KY_DTE = DATE(:PV-OPN-FSCL-MN-DTE) - 7 DAYS       66579000
665801     END-EXEC.                                                    66580000
665802                                                                  66580100
665803     EVALUATE TRUE                                                66580200
665804         WHEN SQLCODE = 0                                         66580300
665805             CONTINUE                                             66580400
665809         WHEN SQLCODE = +100                                      66580500
665810             MOVE '7125-GET-LY-THIS-DAY-MIN-7DAYS'                66580600
665811                                 TO AA-PARAGRAPH-NAME             66580700
665812             MOVE 'TDTATTR ROW NOT FOUND'                         66580800
665813                                 TO AA-MESSAGE                    66580900
665814             MOVE 'SELECT'       TO AA-DB2-OPERATION              66581000
665815             PERFORM 9901-SQL-ERROR                               66581100
665816         WHEN OTHER                                               66581200
665817             MOVE '7125-GET-LY-THIS-DAY-MIN-7DAYS'                66581300
665818                                 TO AA-PARAGRAPH-NAME             66581400
665819             MOVE 'TDTATTR TABLE ERROR'                           66581500
665820                                 TO AA-MESSAGE                    66581600
665821             MOVE 'SELECT'       TO AA-DB2-OPERATION              66581700
665822             PERFORM 9901-SQL-ERROR                               66581800
665823     END-EVALUATE.                                                66581900
665824                                                                  66582000
