363700             ADD +1 TO PV-HUD-WHSE-RPLN-WRITTEN-PRD               36370009
363800         WHEN PV-FOUND-APS                                        36380009
363900             ADD +1 TO PV-APS-WHSE-REPLEN-READ                    36390009
364000             ADD +1 TO PV-APS-WHSE-RPLN-WRITTEN-PRD               36400009
364100         WHEN PV-FOUND-GLB                                        36410009
364200             ADD +1 TO PV-GLB-WHSE-REPLEN-READ                    36420009
364300             ADD +1 TO PV-GLB-WHSE-RPLN-WRITTEN-PRD               36430009
364400         WHEN PV-FOUND-UWD                                        36440009
364500             ADD +1 TO PV-UWD-WHSE-REPLEN-READ                    36450009
364600             ADD +1 TO PV-UWD-WHSE-RPLN-WRITTEN-PRD               36460009
364700     END-EVALUATE.                                                36470009
364800                                                                  36480009
364900     IF PS-CTT-PRESENT OR PS-IEA-PRESENT                          36490009
365000         EVALUATE TRUE                                            36500009
365100             WHEN PV-FOUND-TRIANGLE                               36510009
365200                 ADD +1 TO PV-TRI-WHSE-RPLN-WRITTEN-PRD           36520009
365300             WHEN PV-FOUND-HUDD                                   36530009
365400                 ADD +1 TO PV-HUD-WHSE-RPLN-WRITTEN-PRD           36540009
365500             WHEN PV-FOUND-APS                                    36550009
365600                 ADD +1 TO PV-APS-WHSE-RPLN-WRITTEN-PRD           36560009
365700             WHEN PV-FOUND-GLB                                    36570009
365800                 ADD +1 TO PV-GLB-WHSE-RPLN-WRITTEN-PRD           36580009
365900             WHEN PV-FOUND-UWD                                    36590009
366000                 ADD +1 TO PV-UWD-WHSE-RPLN-WRITTEN-PRD           36600009
366100         END-EVALUATE                                             36610009
366200     END-IF.                                                      36620009
