005000 FILE-CONTROL.                                                    00500009
005100     SELECT INPUT-FILE                                            00510009
005200         ASSIGN                       TO INP01.                   00520009
005300     SELECT REPORT-FILE                                           00530009
005400         ASSIGN                       TO RPT01.                   00540009
005500     SELECT TRIANGLE-PACKSLIPS-P-FILE                             00550009
005600         ASSIGN                       TO OUT01.                   00560009
005700     SELECT CONSOL-IMPORT-BKG-P-FILE                              00570009
005800         ASSIGN                       TO OUT02.                   00580009
005900     SELECT CONSOL-CONMAN-P-FILE                                  00590009
006000         ASSIGN                       TO OUT03.                   00600009
006100     SELECT CONSOL-ARRIVAL-P-FILE                                 00610009
006200         ASSIGN                       TO OUT04.                   00620009
006300     SELECT CONSOL-CUSTOMS-P-FILE                                 00630009
006400         ASSIGN                       TO OUT05.                   00640009
006500     SELECT CONSOL-EXCP-FILE                                      00650009
006600         ASSIGN                       TO OUT06.                   00660009
006700     SELECT CONSOL-IMPORT-BKG-T-FILE                              00670009
006800         ASSIGN                       TO OUT07.                   00680009
006900     SELECT CONSOL-CONMAN-T-FILE                                  00690009
007000         ASSIGN                       TO OUT08.                   00700009
007100     SELECT CONSOL-ARRIVAL-T-FILE                                 00710009
007200         ASSIGN                       TO OUT09.                   00720009
007300     SELECT CONSOL-CUSTOMS-T-FILE                                 00730009
007400         ASSIGN                       TO OUT10.                   00740009
007500     SELECT TRIANGLE-PACKSLIPS-T-FILE                             00750009
007600         ASSIGN                       TO OUT11.                   00760009
007700     SELECT CONSOL-IMPORT-RCVG-T-FILE                             00770009
007800         ASSIGN                       TO OUT12.                   00780009
007900     SELECT CONSOL-IMPORT-RCVG-P-FILE                             00790009
008000         ASSIGN                       TO OUT13.                   00800009
008100     SELECT DELIVERY-P-FILE                                       00810009
008200         ASSIGN                       TO OUT14.                   00820009
008300     SELECT DELIVERY-T-FILE                                       00830009
008400         ASSIGN                       TO OUT15.                   00840009
008500* PB 06/30/2010 BEG                                               00850009
008600     SELECT WHSE-REPLEN-P-FILE                                    00860009
008700         ASSIGN                       TO OUT16.                   00870009
008800     SELECT WHSE-REPLEN-T-FILE                                    00880009
008900         ASSIGN                       TO OUT17.                   00890009
009000* PB 06/30/2010 END                                               00900009
