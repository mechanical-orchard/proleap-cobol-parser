368000                                                                  36800009
368100******************************************************************36810009
368200*  QUERY TO RETURN WHICH VENDOR WE ARE DEALING WITH              *36820009
368300******************************************************************36830009
368400*RJ                                                               36840009
368500 7000-GET-VENDOR.                                                 36850009
368700     MOVE +1 TO PV-POINTER.                                       36870009
368800                                                                  36880009
368900     STRING TR090-IMP-VEND-INCHG-ID DELIMITED BY ' '              36890009
369000           INTO   PV-VENDOR-NAME-S                                36900009
369100           WITH   POINTER PV-POINTER.                             36910009
369200     SUBTRACT +1 FROM PV-POINTER.                                 36920009
369300                                                                  36930009
369400            EXEC SQL                                              36940009
369500              SELECT DISTINCT                                     36950009
369600                     B.ENT_NAME_DESC,                             36960009
369700                     A.INACTV_DTE                                 36970009
369800              INTO  :ENTNME-ENT-NAME-DESC,                        36980009
369900                    :TRDPRT-INACTV-DTE                            36990009
370000              FROM   TTRDPRT A,                                   37000009
370100                     TENTNME B,                                   37010009
370200                     TEXTENT C                                    37020009
370300              WHERE  A.ENT_ID           = B.ENT_ID                37030009
370400                AND (B.ENT_NAME_DESC LIKE :DK-WHERE-3             37040009
370500                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-4             37050009
370600                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-5             37060009
370700                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-6             37070009
370800                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-7             37080009
370900                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-8             37090009
371000                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-9             37100009
371100                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-10            37110009
371200                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-11            37120009
371300                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-12            37130009
371400                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-13            37140009
371500                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-14            37150009
371600                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-15            37160009
371700                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-16            37170009
371800                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-17            37180009
371900                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-18            37190009
372000                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-19            37200009
372100                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-20            37210009
372200                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-21            37220009
372300                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-22            37230009
372400                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-23            37240009
372500                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-24            37250009
372600                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-25            37260009
372700                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-26            37270009
372800                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-27            37280009
372900                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-28            37290009
373000                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-29            37300009
373100                 OR  B.ENT_NAME_DESC LIKE :DK-WHERE-30)           37310009
373200                AND B.TYPE_CDE          = '01'                    37320009
373300                AND A.VEND_INCHG_ID     =                         37330009
373400                    SUBSTR(:PV-VENDOR-NAME-S,1,:PV-POINTER)       37340009
373500                AND A.VEND_INCHG_ID_DESC =:                       37350009
373600                                     TR090-IMP-VEND-INCHG-ID-DESC 37360009
373700                AND CURRENT DATE        <= A.INACTV_DTE           37370009
373800                AND B.ENT_ID = C.ENT_ID                           37380009
373900                AND C.CARRIER_IND = 'Y'                           37390009
374000            END-EXEC.                                             37400009
374100                                                                  37410009
374200      EVALUATE TRUE                                               37420009
374300          WHEN SQLCODE = +100                                     37430009
374400            SET PS-VENDOR-FOUND-NO TO TRUE                        37440009
374500          WHEN SQLCODE = ZERO                                     37450009
374600          WHEN SQLCODE = -811                                     37460009
374700            SET PS-VENDOR-FOUND    TO TRUE                        37470009
374900             EVALUATE PV-NAME-SUB                                 37490009
375000                 WHEN 1                                           37500009
375100                   SET PV-FOUND-TRIANGLE  TO TRUE                 37510009
375200                 WHEN 2                                           37520009
375300                   SET PV-FOUND-HELLMAN   TO TRUE                 37530009
375400                 WHEN 3                                           37540009
375500                   SET PV-FOUND-EXPEDITOR TO TRUE                 37550009
375600                 WHEN 4                                           37560009
375700                   SET PV-FOUND-MAERSK    TO TRUE                 37570009
375800                 WHEN 5                                           37580009
375900                   SET PV-FOUND-NYK       TO TRUE                 37590009
376000                 WHEN 6                                           37600009
376100                   SET PV-FOUND-HUDD      TO TRUE                 37610009
376200                 WHEN 7                                           37620009
376300                   SET PV-FOUND-UWD       TO TRUE                 37630009
376400                 WHEN 8                                           37640009
376500                   SET PV-FOUND-APS       TO TRUE                 37650009
376600                 WHEN 9                                           37660009
376700                   SET PV-FOUND-GLB       TO TRUE                 37670009
376800                 WHEN 10                                          37680009
376900                   SET PV-FOUND-APS       TO TRUE                 37690009
377000                 WHEN 11                                          37700009
377100                   SET PV-FOUND-LIVINGSTON TO TRUE                37710009
377200* IF NEW VENDORS ARE ADDED, ADD WHEN 12, ETC...                   37720009
377300                 WHEN OTHER                                       37730009
377400                   SET PV-FOUND-NONE      TO TRUE                 37740009
377500             END-EVALUATE                                         37750009
377600                                                                  37760009
377700          WHEN SQLCODE > 0                                        37770009
377800          WHEN SQLCODE < 0                                        37780009
377900          WHEN SQLWARN0 NOT EQUAL SPACE                           37790009
378000             MOVE '7000-GET-VENDOR' TO    PV-PARAGRAPH-NAME       37800009
378100             DISPLAY ' PV-VENDOR-NAME-S='                         37810009
378200                        PV-VENDOR-NAME-S                          37820009
378300             DISPLAY    'PV-POINTER='                             37830009
378400                        PV-POINTER                                37840009
378500             DISPLAY ' TR090-IMP-VEND-INCHG-ID-DESC='             37850009
378600                       TR090-IMP-VEND-INCHG-ID-DESC               37860009
378700             DISPLAY  DK-WHERE-11                                 37870009
378800             DISPLAY  DK-WHERE-12                                 37880009
378900             DISPLAY  DK-WHERE-13                                 37890009
379000             DISPLAY  DK-WHERE-14                                 37900009
379100             DISPLAY  DK-WHERE-15                                 37910009
379200             DISPLAY  DK-WHERE-16                                 37920009
379300             DISPLAY  DK-WHERE-17                                 37930009
379400             DISPLAY  DK-WHERE-18                                 37940009
379500             DISPLAY  DK-WHERE-19                                 37950009
379600             DISPLAY  DK-WHERE-20                                 37960009
379700             DISPLAY  DK-WHERE-21                                 37970009
379800             PERFORM 9900-SQL-ERROR                               37980009
379900      END-EVALUATE.                                               37990009
