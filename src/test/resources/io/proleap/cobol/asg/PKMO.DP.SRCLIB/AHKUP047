023500     PERFORM X600-UPDATE-TCKRSTCNTL.                              02350005
023600                                                                  02360005
023800*----------------------------------------------------------------*02380005
023900*    DELETES FROM THE RCT_PO_RCVR_TRKG TABLE.                    *02390015
024200*----------------------------------------------------------------*02420005
024300 D100-DELETE-TRACKING.                                            02430015
024400                                                                  02440005
024500     MOVE 'D100-DELETE-TRACKING' TO PV-CURRENT-PARAGRAPH.         02450015
024600                                                                  02460005
024700     PERFORM WITH TEST AFTER                                      02470005
024800        VARYING PC-RETRY-COUNT FROM 1 BY 1                        02480005
024900          UNTIL PC-RETRY-COUNT > PC-RETRY-MAX                     02490005
025000             OR SQLCODE = ZERO                                    02500005
025100                                                                  02510005
025200         EXEC SQL                                                 02520005
025300              DELETE                                              02530005
025400                FROM RCT_PO_RCVR_TRKG                             02540022
025500               WHERE PO_NBR         = :RCT00017-PO-NBR            02550023
025600                 AND RCVR_SEQ_NBR   = :RCT00017-RCVR-SEQ-NBR      02560023
025700                 AND CRE_BY_USR_ID  = :RCT00017-CRE-BY-USR-ID     02570023
025800                 AND CRE_TMST       = :RCT00017-CRE-TMST          02580023
025900         END-EXEC                                                 02590005
026000                                                                  02600005
026100         EVALUATE TRUE                                            02610005
026200             WHEN SQLCODE = ZERO                                  02620005
026300                 ADD 1 TO PV-TRACKING-DELETE-COUNT                02630024
026400                 MOVE SQLERRD(3) TO PV-SQLERRD3                   02640007
026500                 ADD PV-SQLERRD3 TO PV-SQL-DELETE-COUNT           02650005
026600                                                                  02660005
026700             WHEN SQLCODE = -904                                  02670005
026800             WHEN SQLCODE = -913                                  02680005
026900                  CONTINUE                                        02690005
027000                                                                  02700005
027100             WHEN SQLCODE = +100                                  02710005
027200                 MOVE PV-CURRENT-PARAGRAPH                        02720005
027300                                     TO AA-PARAGRAPH-NAME         02730005
027400                 DISPLAY '********* PO NUMBER: '                  02740026
027500                          PV-DISP-PO-NBR                          02750018
027600                 DISPLAY '****** RECIEVER NBR: '                  02760026
027700                          PV-DISP-RCVR-SEQ-NBR                    02770019
027800                 DISPLAY '**** CREATE USER ID: '                  02780026
027900                          PV-DISP-CRE-BY-USR-ID                   02790021
028000                 DISPLAY '** CREATE TIMESTAMP: '                  02800026
028100                          PV-DISP-CRE-TMST                        02810022
028200                 MOVE 'ROW NOT ON TABLE ********'                 02820005
028300                          TO AA-MESSAGE-1                         02830005
028400                 MOVE SPACES TO AA-MESSAGE-2                      02840005
028500                 MOVE 'UNSUCCESSFUL DELETE'                       02850005
028600                                         TO AA-DB2-OPERATION      02860026
028700                 MOVE 'RCT_PO_RCVR_TRKG' TO AA-DB2-TABLE          02870026
028800                 PERFORM Z999-DB2-ABEND                           02880005
028900             WHEN SQLWARN0 NOT = SPACES                           02890005
029000             WHEN SQLCODE  NOT = ZERO                             02900005
029100                 MOVE PV-CURRENT-PARAGRAPH                        02910005
029200                                     TO AA-PARAGRAPH-NAME         02920005
029300                 DISPLAY '********* PO NUMBER: '                  02930026
029400                          PV-DISP-PO-NBR                          02940018
029500                 DISPLAY '****** RCVR-SEQ-NBR: '                  02950026
029600                          PV-DISP-RCVR-SEQ-NBR                    02960019
029700                 DISPLAY '**** CREATE USER ID: '                  02970026
029800                          PV-DISP-CRE-BY-USR-ID                   02980021
029900                 DISPLAY '** CREATE TIMESTAMP: '                  02990026
030000                          PV-DISP-CRE-TMST                        03000021
030100                 MOVE SPACES TO AA-MESSAGE-1                      03010005
030200                                AA-MESSAGE-2                      03020005
030300                 MOVE 'UNSUCCESSFUL DELETE'                       03030005
030400                                         TO AA-DB2-OPERATION      03040026
030500                 MOVE 'RCT_PO_RCVR_TRKG' TO AA-DB2-TABLE          03050026
030600                 PERFORM Z999-DB2-ABEND                           03060005
030700         END-EVALUATE                                             03070005
030800     END-PERFORM.                                                 03080005
030900                                                                  03090005
031000     IF PC-RETRY-COUNT > PC-RETRY-MAX                             03100005
031100         MOVE PV-CURRENT-PARAGRAPH                                03110005
031200                             TO AA-PARAGRAPH-NAME                 03120005
031300         DISPLAY '********* PO NUMBER: '                          03130026
031400                          PV-DISP-PO-NBR                          03140018
031500         DISPLAY '****** RCVR-SEQ-NBR: '                          03150026
031600                          PV-DISP-RCVR-SEQ-NBR                    03160019
031700         DISPLAY '**** CREATE USER ID: '                          03170026
031800                          PV-DISP-CRE-BY-USR-ID                   03180021
031900         DISPLAY '** CREATE TIMESTAMP: '                          03190026
032000                          PV-DISP-CRE-TMST                        03200022
032100         MOVE SPACES TO AA-MESSAGE-1                              03210005
032200                        AA-MESSAGE-2                              03220005
032300         MOVE 'MAX RETRIES EXCEEDED ON DELETE'                    03230005
032400                                 TO AA-DB2-OPERATION              03240026
