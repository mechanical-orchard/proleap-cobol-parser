030700******************************************************************03070001
030800* OPEN CURSOR                                                     03080001
030900******************************************************************03090001
031000 3000-OPEN-DELETE-CSR.                                            03100001
031100                                                                  03110001
031200     EXEC SQL                                                     03120001
031300         OPEN DELETE_CSR                                          03130001
031400     END-EXEC.                                                    03140001
031500                                                                  03150001
031600     EVALUATE TRUE                                                03160001
031700         WHEN SQLCODE = +0                                        03170001
031800             CONTINUE                                             03180001
031900                                                                  03190001
032000         WHEN SQLWARN0 NOT = SPACES                               03200001
032100         WHEN SQLCODE < ZERO                                      03210001
032200         WHEN SQLCODE > ZERO                                      03220001
032300             MOVE '3000-OPEN-DELETE-CSR' TO PV-PARAGRAPH-NAME     03230001
032400             MOVE 'OPEN DELETE CURSOR'   TO PV-DB2-OPERATION      03240001
032500                                                                  03250001
032600             PERFORM 9100-SQL-ABEND                               03260001
032700     END-EVALUATE.                                                03270001
032800                                                                  03280001
032900                                                                  03290001
033000******************************************************************03300001
033100* FETCH CURSOR                                                    03310001
033200******************************************************************03320001
033300 3100-FETCH-DELETE-CSR.                                           03330001
033400                                                                  03340001
033500     INITIALIZE DCLTTFMSTR.                                       03350001
033600                                                                  03360001
033700     EXEC SQL                                                     03370001
033800         FETCH  DELETE_CSR                                        03380001
033900          INTO  :TFMSTR-XFER-DKEY-ID                              03390001
034000     END-EXEC.                                                    03400001
034100                                                                  03410001
034200     EVALUATE TRUE                                                03420001
034300         WHEN SQLCODE = +0                                        03430001
034400             ADD 1 TO PA-READ                                     03440001
034500                                                                  03450001
034600         WHEN SQLCODE = +100                                      03460001
034700             SET PS-END-OF-DELETES TO TRUE                        03470001
034800                                                                  03480001
034900         WHEN SQLWARN0 NOT = SPACES                               03490001
035000         WHEN SQLCODE < ZERO                                      03500001
035100         WHEN SQLCODE > ZERO                                      03510001
035200             MOVE '3100-FETCH-DELETE-CSR' TO PV-PARAGRAPH-NAME    03520001
035300             MOVE 'FETCH DELETE CURSOR'   TO PV-DB2-OPERATION     03530001
035400                                                                  03540001
035500             PERFORM 9100-SQL-ABEND                               03550001
035600     END-EVALUATE.                                                03560001
035700                                                                  03570001
035800                                                                  03580001
035900******************************************************************03590001
036000* CLOSE CURSOR                                                    03600001
036100******************************************************************03610001
