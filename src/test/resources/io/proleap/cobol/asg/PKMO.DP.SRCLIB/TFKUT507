036300                                                                  03630001
036400     EXEC SQL                                                     03640001
036500         CLOSE DELETE_CSR                                         03650001
036600     END-EXEC.                                                    03660001
036700                                                                  03670001
036800     EVALUATE TRUE                                                03680001
036900         WHEN SQLCODE = +0                                        03690001
037000             CONTINUE                                             03700001
037100                                                                  03710001
037200         WHEN SQLWARN0 NOT = SPACES                               03720001
037300         WHEN SQLCODE < ZERO                                      03730001
037400         WHEN SQLCODE > ZERO                                      03740001
037500             MOVE '3200-CLOSE-DELETE-CSR' TO PV-PARAGRAPH-NAME    03750001
037600             MOVE 'CLOSE DELETE CURSOR'   TO PV-DB2-OPERATION     03760001
037700                                                                  03770001
037800             PERFORM 9100-SQL-ABEND                               03780001
037900     END-EVALUATE.                                                03790001
