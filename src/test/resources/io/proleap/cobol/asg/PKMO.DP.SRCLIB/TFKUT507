019900* INITIALIZATION ROUTINE.                                         01990001
020000******************************************************************02000001
020100 1000-INITIALIZE.                                                 02010001
020200                                                                  02020001
020300     OPEN INPUT DATE-FILE                                         02030001
020400          OUTPUT KEY-FILE.                                        02040001
020500                                                                  02050001
020600     MOVE FUNCTION CURRENT-DATE TO SYS-DATE-TIME.                 02060001
020700                                                                  02070001
020800     MOVE SYS-TIME (1:2) TO ST-BEG-HR.                            02080001
020900     MOVE SYS-TIME (3:2) TO ST-BEG-MN.                            02090001
021000                                                                  02100001
021100     MOVE SYS-DATE (1:2) TO SD-CENT.                              02110001
021200     MOVE SYS-DATE (3:2) TO SD-YEAR.                              02120001
021300     MOVE SYS-DATE (5:2) TO SD-MONTH.                             02130001
021400     MOVE SYS-DATE (7:2) TO SD-DAY.                               02140001
021500                                                                  02150001
021600     PERFORM 1100-READ-CARDIN.                                    02160001
021700     PERFORM 1200-CALL-CALENDAR.                                  02170001
021800                                                                  02180001
021900     PERFORM 3000-OPEN-DELETE-CSR.                                02190001
022000     PERFORM 3100-FETCH-DELETE-CSR.                               02200001
022100                                                                  02210001
022200                                                                  02220001
022300******************************************************************02230001
022400* READ THE INPUT DATE FILE                                        02240001
022500******************************************************************02250001
022600 1100-READ-CARDIN.                                                02260001
022700                                                                  02270001
022800     READ DATE-FILE INTO CC-CARDIN                                02280001
022900         AT END SET PS-EOF-CARDIN TO TRUE                         02290001
023000     END-READ.                                                    02300001
023100                                                                  02310001
023200     IF PS-EOF-CARDIN                                             02320001
023300        MOVE '1100-READ-CARDIN'               TO PV-PARAGRAPH-NAME02330001
023400        MOVE 'CARD (CURR DATE) FILE IS EMPTY' TO PV-OPERATION-MSG 02340001
023500                                                                  02350001
023600        SET ABEND-4003-CTRL-CARD-ERROR TO TRUE                    02360001
023700                                                                  02370001
023800        PERFORM 9000-ABEND                                        02380001
023900     END-IF.                                                      02390001
024000                                                                  02400001
024100                                                                  02410001
024200******************************************************************02420001
024300* CALL CALENDAR ROUTINE TO DECREMENT CURRENT DATE BY 750 DAYS,    02430001
024400* OR APPROXIMATELY 25 MONTHS                                      02440001
024500******************************************************************02450001
024600 1200-CALL-CALENDAR.                                              02460001
024700                                                                  02470001
024800     INITIALIZE DPG51 DPG52 DPG53 DPG54 DPG55 DPG56.              02480003
024900                                                                  02490003
025000     MOVE CC-CURR-DATE TO DPG52-DB2-ISO-DATE-FMT.                 02500002
025100                                                                  02510002
025200     SET DPG51-ACTUAL-CALENDAR-ONLY                               02520001
025300         DPG51-DECREMENT-DATE                                     02530001
025400         DPG52-LK-DTE-ISO-FMT TO TRUE.                            02540001
025500                                                                  02550001
025600     MOVE PC-750-DAYS  TO DPG51-INCR-DECR-DAYS-9.                 02560003
025700                                                                  02570001
025800     CALL DP500-KOHLS-CALENDAR-ROUTINE                            02580001
025900                USING DPG51 DPG52 DPG53 DPG54 DPG55 DPG56.        02590001
026000                                                                  02600001
026100     EVALUATE TRUE                                                02610001
026200         WHEN DPG54-DATE-VALID                                    02620001
026300             MOVE DPG55-DB2-ISO-DATE-FMT                          02630001
026400                 TO PV-CURR-DATE-MINUS-750                        02640001
026500                                                                  02650001
026600         WHEN OTHER                                               02660001
026700             DISPLAY '                                       '    02670001
026800             DISPLAY '** INVALID RETURN FROM CALENDAR ROUTINE'    02680001
026900             DISPLAY '** ERROR MESSAGE = ' DPG54-ERROR-MESSAGE    02690001
027000             DISPLAY '** DPG51 = ' DPG51                          02700001
027100             DISPLAY '** DPG52 = ' DPG52                          02710001
027200             DISPLAY '** DPG53 = ' DPG53                          02720001
027300             DISPLAY '** DPG54 = ' DPG54                          02730001
027400             DISPLAY '** CC-CURR-DATE = ' CC-CURR-DATE            02740002
027500                                                                  02750001
027600             MOVE '1200-CALL-CALENDAR'   TO PV-PARAGRAPH-NAME     02760002
027700             MOVE 'INVALID CNTL DATE'    TO PV-OPERATION-MSG      02770002
027800                                                                  02780001
027900             SET ABEND-4019-CAL-SUB-ERROR TO TRUE                 02790004
028000                                                                  02800001
028100             PERFORM 9000-ABEND                                   02810001
028200     END-EVALUATE.                                                02820001
028300                                                                  02830001
028400                                                                  02840001
028500***************************************************************** 02850001
028600* RETRIEVE RECORDS FOR PROCESSING                                 02860001
028700***************************************************************** 02870001
028800 2000-PROCESS-DELETES.                                            02880001
028900                                                                  02890001
029000     PERFORM 2100-WRITE-DEL-KEY.                                  02900001
029100                                                                  02910001
