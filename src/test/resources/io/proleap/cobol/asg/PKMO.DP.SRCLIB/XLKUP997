000100***************************************************************** 00010000
000200 IDENTIFICATION DIVISION.                                         00020000
000300***************************************************************** 00030000
000400 PROGRAM-ID.    XLKUP997.                                         00040000
000500 AUTHOR.        DAN HINTZ.                                        00050000
000600 INSTALLATION.  KOHLS.                                            00060000
000700 DATE-WRITTEN   JUNE 2003.                                        00070000
000800 DATE-COMPILED.                                                   00080000
000900***************************************************************** 00090000
001000*  THIS BATCH DB2 PROGRAM LOADS XLT_PK FROM PACKHEAD EXTRACT      00100000
001000*  ERROR FILE.  ONLY THOSE RECORDS THAT STATE 'NOT ON DOMAIN      00101000
001000*  PACK HEADER' WILL BE INSERTED INTO THE TABLE.  THIS IS BEING   00102000
001000*  DONE FOR DATA CLEANUP PURPOSES THAT CAN BE DONE WHILE THE      00103000
001000*  TABLES ARE ACTIVE.                                             00104000
001400***************************************************************** 00105000
001500 ENVIRONMENT DIVISION.                                            00106000
001600 INPUT-OUTPUT SECTION.                                            00107000
001700                                                                  00108000
001800 FILE-CONTROL.                                                    00109000
001900                                                                  00110000
002000     SELECT XLT-PK-IN                                             00120000
002010         ASSIGN TO INP01.                                         00130000
002100                                                                  00140000
002200***************************************************************** 00150000
002300 DATA DIVISION.                                                   00160000
002400***************************************************************** 00170000
002500 FILE SECTION.                                                    00180000
002600                                                                  00190000
002700 FD  XLT-PK-IN                                                    00200000
002800     RECORDING MODE IS F                                          00210000
002810     BLOCK CONTAINS 0 RECORDS                                     00220000
002900     LABEL RECORDS ARE STANDARD.                                  00230000
003000                                                                  00240000
003300 01  XLT-PK-IN-RECORD                 PIC X(144).                 00250000
003700                                                                  00260000
003800                                                                  00270000
003900***************************************************************** 00280000
004000 WORKING-STORAGE SECTION.                                         00290000
004100                                                                  00300000
       01  PA-DB2-COMMIT-COUNT              PIC S9(09) VALUE +0         00310000
                                                          COMP-3.       00320000
004200 01  PS-PROGRAM-SWITCHES.                                         00330000
004300     05  PS-EOF-INPUT-SW              PIC X(01)  VALUE 'N'.       00340000
004400         88  PS-EOF-INPUT-YES                    VALUE 'Y'.       00350000
004500         88  PS-EOF-INPUT-NO                     VALUE 'N'.       00360000
004600                                                                  00370000
004700 01  PC-PROGRAM-CONSTANTS.                                        00380000
004800     05  PC-PROGRAM-NAME              PIC  X(08) VALUE 'XLKUP997'.00390000
005000                                                                  00400000
005100 01  PV-PROGRAM-VARIABLES.                                        00410000
005900     05  PV-RECORDS-READ              PIC  9(09) VALUE  0.        00420000
006000     05  PV-RECORDS-INSERTED          PIC  9(09) VALUE  0.        00430000
006000     05  PV-RECORDS-SKIP              PIC  9(09) VALUE  0.        00440000
006400     05  PV-DISP-UPC-ID               PIC  9(09) VALUE  0.        00450000
           05  PV-DISP-UPC-NBR              PIC  9(15) VALUE  0.        00460000
006700     05  PV-SQLCODE                   PIC  S9(9) VALUE +0.        00470000
006800     05  PV-DISP-SQLCODE              PIC ZZZZZZ9-.               00480000
006900                                                                  00490000
007000                                                                  00500000
007100 01  PV-ABEND-FIELDS.                                             00510000
007200     05  PV-ABEND-CODE                PIC  S9(4) VALUE +0         00520000
007300                                                 COMP SYNC.       00530000
007400     05  PV-ABEND-PROGRAM             PIC  X(08) VALUE 'XLKUP997'.00540000
007500     05  PV-ABEND-PARAGRAPH           PIC  X(50) VALUE SPACES.    00550000
007600     05  PV-ABEND-OPERATION           PIC  X(50) VALUE SPACES.    00560000
007700     05  PV-ABEND-TABLE               PIC  X(12) VALUE SPACES.    00570000
007900     05  PV-DB2-OPERATION-MESSAGE-1   PIC  X(79) VALUE SPACES.    00580000
008000     05  PV-DB2-OPERATION-MESSAGE-2   PIC  X(79) VALUE SPACES.    00590000
008100     05  PV-DB2-OPERATION-MESSAGE-3   PIC  X(79) VALUE SPACES.    00600000
008200                                                                  00610000
008300                                                                  00620000
008400***********************************************************       00630000
008500*  COPYBOOK USED FOR THE PACKHEAD INPUT FILE              *       00640000
008600***********************************************************       00650000
008700     COPY XLRD027.                                                00660000
008800                                                                  00670000
008900***********************************************************       00680000
009000*  USED FOR DB2 ERROR MESSAGE BUILDING                    *       00690000
009100***********************************************************       00700000
009200     COPY DPWS004.                                                00710000
009300                                                                  00720000
009400***********************************************************       00730000
009500*  DB2 SQL COMMUNICATION AREA                             *       00740000
009600***********************************************************       00750000
009700     EXEC SQL                                                     00760000
009800          INCLUDE SQLCA                                           00770000
009900     END-EXEC.                                                    00780000
010000                                                                  00790000
010100***********************************************************       00800000
010200*  XLT_PK TABLE                                          *        00810000
010300***********************************************************       00820000
010400     EXEC SQL                                                     00830000
010500          INCLUDE XLT00007                                        00840000
010600     END-EXEC.                                                    00850000
010700                                                                  00860000
010800***************************************************************** 00870000
010900 PROCEDURE DIVISION.                                              00880000
011000***************************************************************** 00890000
011100     DISPLAY '**** BEGINNING XLKUP997 ****'.                      00900000
011200     DISPLAY ' '.                                                 00910000
011300                                                                  00920000
011400     OPEN INPUT  XLT-PK-IN.                                       00930000
011500                                                                  00940000
011600     PERFORM 1000-READ-INPUT-FILE.                                00950000
011700                                                                  00960000
012100     PERFORM 2000-PROCESS-INPUT-FILE                              00970000
012200       UNTIL PS-EOF-INPUT-YES.                                    00980000
012300                                                                  00990000
012400     CLOSE XLT-PK-IN.                                             01000000
012500                                                                  01010000
012510     PERFORM 3000-TABLE-COMMIT.                                   01020000
012520                                                                  01030000
012600     DISPLAY '  INPUT PK RECS READ    = ' PV-RECORDS-READ.        01040000
012700     DISPLAY '  XLT_PK ROWS INSERTED  = ' PV-RECORDS-INSERTED.    01050000
012700     DISPLAY '  XLT_PK ROWS SKIPPED   = ' PV-RECORDS-SKIP.        01060000
012800     DISPLAY ' '.                                                 01070000
012900     DISPLAY '****  END OF  XLKUP997  ****'.                      01080000
013000                                                                  01090000
013100     STOP RUN.                                                    01100000
013200                                                                  01110000
013300                                                                  01120000
013400***************************************************************** 01130000
013500*  READ A PACKHEAD EXTRACT RECORD THAT WILL BE USED TO INSERT    *01140000
013510*  A ROW OF DATA INTO THE XLT_PK TABLE.                        *  01150000
013600***************************************************************** 01160000
013700 1000-READ-INPUT-FILE.                                            01170000
013800                                                                  01180000
013900     MOVE '1000-READ-INPUT-FILE    ' TO PV-ABEND-PARAGRAPH.       01190000
014000                                                                  01200000
           INITIALIZE PA-DB2-COMMIT-COUNT.                              01210000
014020                                                                  01220000
014100     READ XLT-PK-IN                                               01230000
014200          INTO XL027-PACK-DATA                                    01240000
014300            AT END                                                01250000
014400               SET PS-EOF-INPUT-YES TO TRUE                       01260000
014500                                                                  01270000
014600            NOT AT END                                            01280000
014700               SET PS-EOF-INPUT-NO  TO TRUE                       01290000
014800               ADD +1 TO PV-RECORDS-READ                          01300000
014900     END-READ.                                                    01310000
015000                                                                  01320000
015100                                                                  01330000
015200***************************************************************** 01340000
015300*  LOAD THE INFORMATION FROM THE PACKHEAD EXTRACT FILE INTO THE * 01350000
015310*  HOST VARIABLES AND PERFORM A PARAGRAPH TO INSERT THE NEW ROW * 01360000
015320*  INTO THE XLT_PK  TABLE.                                      * 01370000
015400***************************************************************** 01380000
015500 2000-PROCESS-INPUT-FILE.                                         01390000
015600                                                                  01400000
015700     MOVE '2000-PROCESS-INPUT-FILE ' TO PV-ABEND-PARAGRAPH.       01410000
015800                                                                  01420000
016361     MOVE XL027-PACK-PK-SKU-NBR      TO XLT00007-PK-SKU-NBR.      01430000
016362     MOVE XL027-PACK-DEPT-NBR        TO XLT00007-DEPT-NBR.        01440000
016363     MOVE XL027-PACK-MAJ-CL-NBR      TO XLT00007-MAJ-CL-NBR.      01450000
016364     MOVE XL027-PACK-SUB-CL-NBR      TO XLT00007-SUB-CL-NBR.      01460000
016365     MOVE XL027-PACK-PK-DESC         TO XLT00007-PK-DESC.         01470000
016366     MOVE XL027-PACK-DC-ORD-MULT-QTY TO XLT00007-DC-ORD-MULT-QTY. 01480000
016380                                                                  01490000
016400     IF  XL027-PACK-ERROR-MESSAGE = 'NOT ON DOMAIN PACK HEADER'   01500000
016400         PERFORM 2100-INSERT-XLT-PK                               01510000
016400     END-IF.                                                      01520000
017700                                                                  01530000
017800     PERFORM 1000-READ-INPUT-FILE.                                01540000
018500                                                                  01550000
018600                                                                  01560000
021600***************************************************************** 01570000
021700*  INSERT A ROW TO THE XLT_PK TABLE.                           *  01580000
021800***************************************************************** 01590000
021900 2100-INSERT-XLT-PK.                                              01600000
022000                                                                  01610000
022100     EXEC SQL                                                     01620000
022200          INSERT                                                  01630000
022300          INTO  XLT_PK                                            01640000
023093              ( PK_SKU_NBR                                        01650000
023094               ,PK_DESC                                           01660000
023095               ,DEPT_NBR                                          01670000
023096               ,MAJ_CL_NBR                                        01680000
023097               ,SUB_CL_NBR                                        01690000
023098               ,DC_ORD_MULT_QTY                                   01700005
                     ,LAST_UPD_TMST)                                    01701005
023200          VALUES                                                  01710000
023993             (  :XLT00007-PK-SKU-NBR                              01720000
023994               ,:XLT00007-PK-DESC                                 01730000
023995               ,:XLT00007-DEPT-NBR                                01740000
023996               ,:XLT00007-MAJ-CL-NBR                              01750000
023997               ,:XLT00007-SUB-CL-NBR                              01760000
023998               ,:XLT00007-DC-ORD-MULT-QTY                         01770005
                     ,CURRENT TIMESTAMP)                                01771005
024010     END-EXEC.                                                    01780000
024100                                                                  01790000
024200     EVALUATE TRUE                                                01800000
024300         WHEN SQLCODE = ZERO                                      01810000
024400              ADD +1                 TO PV-RECORDS-INSERTED       01820000
                    ADD +1                 TO PA-DB2-COMMIT-COUNT       01830000
                    IF PA-DB2-COMMIT-COUNT > 4500                       01840000
                        PERFORM 3000-TABLE-COMMIT                       01850000
                        MOVE 0 TO PA-DB2-COMMIT-COUNT                   01860000
                    END-IF                                              01870000
               WHEN SQLCODE = -803                                      01880000
               WHEN SQLCODE = -530                                      01890000
024400              ADD +1                 TO PV-RECORDS-SKIP           01900000
024500         WHEN SQLCODE NOT = +0                                    01910000
024600              MOVE '2100-INSERT-XLT-PK'                           01920000
024700                                     TO PV-ABEND-PARAGRAPH        01930000
024800              MOVE 'INSERT XLT_PK'                                01940000
024810                                     TO PV-ABEND-OPERATION        01950000
025500              STRING 'SKU NBR: '           DELIMITED BY SIZE      01960000
025600                     XL027-PACK-PK-SKU-NBR DELIMITED BY SIZE      01970000
026700                                   INTO PV-DB2-OPERATION-MESSAGE-101980000
026800                                                                  01990000
026900              MOVE 'XLT_PK'          TO PV-ABEND-TABLE            02000000
027000              PERFORM Z999-SQL-ABEND                              02010000
027100     END-EVALUATE.                                                02020000
027200                                                                  02030000
027300                                                                  02040000
027400***************************************************************** 02050000
027500* COMMIT THE PENDING TABLE CHANGES.                             * 02060000
027600***************************************************************** 02070000
027700 3000-TABLE-COMMIT.                                               02080000
027800                                                                  02090000
027900     EXEC SQL                                                     02100000
028000          COMMIT                                                  02110000
028100     END-EXEC.                                                    02120000
028200                                                                  02130000
028300                                                                  02140000
028400***************************************************************** 02150000
028500*  DISPLAY DB2 ERROR INFORMATION, ROLLBACK ANY PENDING CHANGES, * 02160000
028510*  AND ABEND THE PROGRAM.                                       * 02170000
028600***************************************************************** 02180000
028700 Z999-SQL-ABEND.                                                  02190000
028800                                                                  02200000
028900     MOVE SQLCODE            TO PV-SQLCODE.                       02210000
029000     MOVE PV-SQLCODE         TO PV-DISP-SQLCODE.                  02220000
029100     DISPLAY '*** DB2 ERROR '.                                    02230000
029200     DISPLAY '*** SQLCODE   = ' PV-DISP-SQLCODE.                  02240000
029300     DISPLAY '*** PROGRAM   = ' PV-ABEND-PROGRAM.                 02250000
029400     DISPLAY '*** PARAGRAPH = ' PV-ABEND-PARAGRAPH.               02260000
029500     DISPLAY '*** OPERATION = ' PV-ABEND-OPERATION.               02270000
029600     DISPLAY '*** MESSAGE   = ' PV-DB2-OPERATION-MESSAGE-1.       02280000
029700     DISPLAY '***           = ' PV-DB2-OPERATION-MESSAGE-2.       02290000
029800     DISPLAY '***           = ' PV-DB2-OPERATION-MESSAGE-3.       02300000
029900     DISPLAY '*** TABLE 1   = ' PV-ABEND-TABLE.                   02310000
030000                                                                  02320000
030100     COPY DPPD004.                                                02330000
030200                                                                  02340000
030300     MOVE +4013         TO PV-ABEND-CODE.                         02350000
030400     CALL 'ILBOABN0' USING PV-ABEND-CODE.                         02360000
