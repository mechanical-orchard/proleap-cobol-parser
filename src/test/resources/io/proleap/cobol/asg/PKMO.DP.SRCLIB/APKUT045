019800                                                                  01980038
019900     GOBACK.                                                      01990038
020000 EJECT                                                            02000038
020100*---------------------------------------------------------------  02010038
020200*    INITIALIZATION PROCESSING                                    02020038
020300*    -INITIAL READ                                                02030038
020400*---------------------------------------------------------------  02040038
020500 B100-INITIALIZE.                                                 02050038
020600                                                                  02060038
020700     OPEN INPUT  UNJRNLZD-FILE                                    02070038
020800         OUTPUT  RECEIPT-SUM-FILE.                                02080038
020900                                                                  02090038
021000     MOVE ZERO TO        PA-UNJRNLZD-IN-COUNT                     02100038
021100                         PA-SUM-OUT-COUNT.                        02110038
021200*---------------------------------------------------------------  02120038
021300* THE FIRST READ SHOULD ALWAYS RETURN A RECORD.  IT WOULD HAVE    02130038
021400* TO BE AN EXCEPTIONAL SITUATION IF THERE WERE NO UNJRNLZD        02140038
021500*---------------------------------------------------------------  02150038
021600                                                                  02160038
021700     PERFORM R100-READ-UNJRNLZD.                                  02170038
021800                                                                  02180038
021900     IF OUT-OF-UNJRNLZD                                           02190038
022000         DISPLAY '*****************************************'      02200038
022100         DISPLAY 'B100-INITIALIZE'                                02210038
022200         DISPLAY 'EMPTY UNJRNLZD FILE'                            02220038
022300         DISPLAY 'EMPTY UNJRNLZD FILE'                            02230038
022400         DISPLAY 'EMPTY UNJRNLZD FILE'                            02240038
022500     ELSE                                                         02250038
022600         PERFORM C100-DETERMINE-FISCAL-PERIOD                     02260038
022700     END-IF.                                                      02270038
022800                                                                  02280038
022900     MOVE ZERO            TO  PA-RCV-ACCUM-RETAIL                 02290038
023000                              PA-RCV-ACCUM-COST                   02300038
023100                              PA-RCV-ACCUM-QTY                    02310038
023200                              PA-NIR-ACCUM-RETAIL                 02320038
023300                              PA-NIR-ACCUM-COST                   02330038
023400                              PA-NIR-ACCUM-QTY.                   02340038
023500                                                                  02350038
023600 EJECT                                                            02360038
023700                                                                  02370038
023800*---------------------------------------------------------------  02380038
023900*   -REFORMAT THE DATE                                            02390038
024000*   -DETERMINE THE FISCAL PERIOD                                  02400038
024100*   -ACCUM THE FIELDS BY FISCAL PERIOD                            02410038
024200*   -WRITE OUT THE SUMMED RECORD                                  02420038
024300*---------------------------------------------------------------  02430038
024400 B200-PROCESS-UNJRNLZD.                                           02440038
024500                                                                  02450038
024600     PERFORM WITH TEST BEFORE                                     02460038
024700         UNTIL AP042-VRFD-DTE < PV-FIRST-FP-DATE                  02470038
024800         OR OUT-OF-UNJRNLZD                                       02480038
024900                                                                  02490038
025000         IF  AP042-ORIG-RCVR                                      02500038
025100             ADD  AP042-CALCD-VRFD-RTL-AMT TO PA-RCV-ACCUM-RETAIL 02510038
025200             ADD  AP042-CALCD-VRFD-COST-AMT TO PA-RCV-ACCUM-COST  02520038
025300             ADD  AP042-PO-VRFD-QTY         TO PA-RCV-ACCUM-QTY   02530038
025400         ELSE                                                     02540038
025500             ADD  AP042-CALCD-VRFD-RTL-AMT TO PA-NIR-ACCUM-RETAIL 02550038
025600             ADD  AP042-CALCD-VRFD-COST-AMT TO PA-NIR-ACCUM-COST  02560038
025700             ADD  AP042-PO-VRFD-QTY         TO PA-NIR-ACCUM-QTY   02570038
025800         END-IF                                                   02580038
025900                                                                  02590038
026000         ADD  AP042-CALCD-VRFD-RTL-AMT  TO PA-TOT-ACCUM-RETAIL    02600038
026100         ADD  AP042-CALCD-VRFD-COST-AMT TO PA-TOT-ACCUM-COST      02610038
026200         ADD  AP042-PO-VRFD-QTY         TO PA-TOT-ACCUM-QTY       02620038
026300                                                                  02630038
026400         PERFORM R100-READ-UNJRNLZD                               02640038
026500                                                                  02650038
026600     END-PERFORM.                                                 02660038
026700                                                                  02670038
026800     PERFORM C300-CREATE-OUT-RECORD.                              02680038
026900     MOVE ZERO          TO PA-RCV-ACCUM-RETAIL                    02690038
027000                           PA-RCV-ACCUM-COST                      02700038
027100                           PA-RCV-ACCUM-QTY                       02710038
027200                           PA-NIR-ACCUM-RETAIL                    02720038
027300                           PA-NIR-ACCUM-COST                      02730038
027400                           PA-NIR-ACCUM-QTY                       02740038
027500                           PA-TOT-ACCUM-RETAIL                    02750038
027600                           PA-TOT-ACCUM-COST                      02760038
027700                           PA-TOT-ACCUM-QTY.                      02770038
027800                                                                  02780038
027900     IF OUT-OF-UNJRNLZD                                           02790038
028000         CONTINUE                                                 02800038
028100     ELSE                                                         02810038
028200         PERFORM C100-DETERMINE-FISCAL-PERIOD                     02820038
028300     END-IF.                                                      02830038
028400                                                                  02840038
028500*---------------------------------------------------------------  02850038
028600*    ENDING PROCESSING: DISPLAYS COUNT OF POS AND CLOSES FILES.   02860038
028700*---------------------------------------------------------------  02870038
028800 B300-END-PROGRAM.                                                02880038
028900                                                                  02890038
029000     CLOSE  UNJRNLZD-FILE                                         02900038
029100            RECEIPT-SUM-FILE.                                     02910038
029200                                                                  02920038
029300     MOVE PA-UNJRNLZD-IN-COUNT   TO PA-IN-COUNT-DISP.             02930038
029400     MOVE PA-SUM-OUT-COUNT       TO PA-SUM-OUT-DISP.              02940038
029500                                                                  02950038
029600     DISPLAY '*****   RECORD COUNTS FROM APKUT045    ******'.     02960038
029700     DISPLAY '---------------------------------------------'.     02970038
029800     DISPLAY 'UNJRNLZD READ         = ' PA-IN-COUNT-DISP.         02980038
029900     DISPLAY '                                             '.     02990038
030000     DISPLAY 'SUMMED RECS OUT       = ' PA-SUM-OUT-DISP.          03000038
030100     DISPLAY '---------------------------------------------'.     03010038
030200                                                                  03020038
030300 EJECT                                                            03030038
030400                                                                  03040038
030500*---------------------------------------------------------------  03050038
030600* FIND THE FIRST DAY OF PERIOD IN WHICH THE INT-MKDATE            03060038
030700* DATE FALLS.  USE THE DATE ROUTINE TO GIVE THE PERIOD AND YEAR.  03070038
030800*---------------------------------------------------------------- 03080038
030900*                                                                 03090038
031000 C100-DETERMINE-FISCAL-PERIOD.                                    03100038
031100                                                                  03110038
031200     INITIALIZE DPG51, DPG52, DPG53, DPG54, DPG55, DPG56.         03120038
031300                                                                  03130038
031400     SET DPG51-FISCAL-454-CALENDAR                                03140038
031500         DPG51-DO-NOT-INCR-DECR-DATE                              03150038
031600         DPG52-LK-DTE-ISO-FMT       TO TRUE.                      03160038
031700     MOVE AP042-VRFD-DTE            TO DPG52-LK-DATE-INPUT.       03170038
031800                                                                  03180038
031900     CALL DP500-KOHLS-CALENDAR-ROUTINE                            03190038
032000         USING DPG51 DPG52 DPG53 DPG54 DPG55 DPG56.               03200038
032100                                                                  03210038
032200     IF (DPG54-SEVERE-ERROR OR                                    03220038
032300         DPG54-WARNING-ERROR)                                     03230038
032400         MOVE 'C100-DETERMINE-FISC' TO AA-PARAGRAPH-NAME          03240038
032500         MOVE 'BAD DATE CALCULATION' TO AA-MESSAGE                03250038
032600         SET AC-DATE-ERROR          TO TRUE                       03260038
032700         PERFORM Z999-ABEND                                       03270038
032800     ELSE                                                         03280038
032900         MOVE DPG56-FIRST-FP-DB2-ISO-FMT-YR TO  PV-FIRST-PER-CCYY 03290038
033000         MOVE DPG56-FIRST-FP-DB2-ISO-FMT-MO TO  PV-FIRST-PER-MM   03300038
033100         MOVE DPG56-FIRST-FP-DB2-ISO-FMT-DY TO  PV-FIRST-PER-DD   03310038
033200         MOVE DPG56-FISCAL-YEAR-CCYY        TO  PV-FP-YEAR        03320038
033300         MOVE DPG56-FISCAL-PERIOD-NBR       TO  PV-FP-NBR         03330038
033400     END-IF.                                                      03340038
033500                                                                  03350038
033600                                                                  03360038
033700*---------------------------------------------------------------- 03370038
033800* BUILD THE COPYBOOK RECORD APRD030                               03380038
033900*                                                                 03390038
034000*---------------------------------------------------------------- 03400038
034100*                                                                 03410038
034200 C300-CREATE-OUT-RECORD.                                          03420038
034300                                                                  03430038
034400     MOVE PV-FP-YEAR(1:2)         TO  AP030-FISCAL-CENTURY.       03440038
034500     MOVE PV-FP-YEAR(3:2)       TO  AP030-FISCAL-YEAR.            03450038
034600     MOVE PV-FP-NBR             TO  AP030-FISCAL-PERIOD.          03460038
034700                                                                  03470038
034800     MOVE PA-RCV-ACCUM-COST     TO  AP030-RCV-COST-TOTAL.         03480038
034900     MOVE PA-RCV-ACCUM-RETAIL   TO  AP030-RCV-RETAIL-TOTAL.       03490038
035000     MOVE PA-RCV-ACCUM-QTY      TO  AP030-RCV-ITEM-QTY.           03500038
035100     MOVE PA-NIR-ACCUM-COST     TO  AP030-NIR-COST-TOTAL.         03510038
035200     MOVE PA-NIR-ACCUM-RETAIL   TO  AP030-NIR-RETAIL-TOTAL.       03520038
035300     MOVE PA-NIR-ACCUM-QTY      TO  AP030-NIR-ITEM-QTY.           03530038
035400     MOVE PA-TOT-ACCUM-COST     TO  AP030-TOTAL-COST.             03540038
035500     MOVE PA-TOT-ACCUM-RETAIL   TO  AP030-TOTAL-RETAIL.           03550038
035600     MOVE PA-TOT-ACCUM-QTY      TO  AP030-TOTAL-ITEM-QTY.         03560038
035700                                                                  03570038
035800     WRITE APRD030-REC FROM AP030-UNJ-NI-RCPT-SUMMARY.            03580038
035900     ADD 1 TO PA-SUM-OUT-COUNT.                                   03590038
036000                                                                  03600038
036100 EJECT                                                            03610038
036200                                                                  03620038
036300                                                                  03630038
