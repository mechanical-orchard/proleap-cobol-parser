001500******************************************************************00150005
001600                                                                  00160005
001700******************************************************************00170005
001800 ENVIRONMENT DIVISION.                                            00180005
001900******************************************************************00190005
002000                                                                  00200005
002100******************************************************************00210005
002200 DATA DIVISION.                                                   00220005
002300******************************************************************00230005
002400                                                                  00240005
002500 WORKING-STORAGE SECTION.                                         00250005
002600 01  WS-INDICATORS.                                               00260005
002700     05 WI-PORT            PIC 9(4) VALUE 2997.                   00270005
002710     05 WI-PORT1           PIC 9(4) VALUE 4997.                   00271005
002711     05 WI-PORT2           PIC 9(4) VALUE 7997.                   00271105
002712******************************************************************00271205
002713 PROCEDURE DIVISION.                                              00271305
002714******************************************************************00271405
002715 0000-MAIN-LINE.                                                  00271505
002716     EXEC CICS START                                              00271605
002717               TRANSID('Z300')                                    00271705
002718               FROM(WI-PORT)                                      00271805
002719     END-EXEC.                                                    00271905
002720     EXEC CICS START                                              00272005
002730               TRANSID('A500')                                    00273005
002740               FROM(WI-PORT1)                                     00274005
002750     END-EXEC.                                                    00275005
002760     EXEC CICS START                                              00276005
002770               TRANSID('A519')                                    00277005
002780               FROM(WI-PORT2)                                     00278005
002790     END-EXEC.                                                    00279005
002800     EXEC CICS START                                              00280005
002900               TRANSID('A400')                                    00290005
003000               INTERVAL(000010)                                   00300005
003100     END-EXEC.                                                    00310005
003200     EXEC CICS START                                              00320005
003300               TRANSID('L201')                                    00330005
003400     END-EXEC.                                                    00340005
003500     EXEC CICS RETURN                                             00350005
003600     END-EXEC.                                                    00360005
