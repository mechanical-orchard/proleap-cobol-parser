020000*    LINKAGE SECTION                                       *      02000000
020100************************************************************      02010000
020200*                                                                 02020000
020300 LINKAGE SECTION.                                                 02030000
020400*                                                                 02040000
020500 01  LINK-FIELDS.                                                 02050000
020600     05  LINK-LEN            PIC 9(4)    COMP.                    02060000
020700     05  DBRM-USER               PIC X(7).                        02070000
020800     05  DBRM-SYS                PIC X(4).                        02080000
020900     05  DBRM-LINK-NAME.                                          02090000
021000         10  DBRM-SID            PIC XX.                          02100000
021100         10  FILLER              PIC X(6).                        02110000
021200*                                                                 02120000
021300 PROCEDURE DIVISION USING LINK-FIELDS.                            02130000
021400                                                                  02140000
021500************************************************************      02150000
021600*    VDEFINES FOR PANEL                                    *      02160000
021700************************************************************      02170000
021800     CALL 'ISPLINK' USING ISP-VDEFINE NAM-ZTEMPF VAR-ZTEMPF       02180000
021900                          ISP-FORMAT LEN-ZTEMPF.                  02190000
022000     CALL 'ISPLINK' USING ISP-VDEFINE NAM-SMSG  VAR-SMSG          02200000
022100                          ISP-FORMAT LEN-SMSG.                    02210000
022200     CALL 'ISPLINK' USING ISP-VDEFINE NAM-LMSG  VAR-LMSG          02220000
022300                          ISP-FORMAT LEN-LMSG.                    02230000
022400     CALL 'ISPLINK' USING ISP-VDEFINE                             02240000
022500         NAM-CMNEKSID  VAR-CMNEKSID  CHAR  LEN-CMNEKSID.          02250000
022600     CALL 'ISPLINK' USING ISP-VDEFINE                             02260000
022700         NAM-CMNEKDBR  VAR-CMNEKDBR  CHAR  LEN-CMNEKDBR.          02270000
022800     CALL 'ISPLINK' USING ISP-VDEFINE                             02280000
022900         NAM-CMNEKLIB  VAR-CMNEKLIB  CHAR  LEN-CMNEKLIB.          02290000
023000     CALL 'ISPLINK' USING ISP-VDEFINE                             02300000
023100         NAM-CMNEKAPP  VAR-CMNEKAPP  CHAR  LEN-CMNEKAPP.          02310000
023200     CALL 'ISPLINK' USING ISP-VDEFINE                             02320000
023300         NAM-USER  VAR-USER  CHAR  LEN-USER.                      02330000
023400                                                                  02340000
023500************************************************************      02350000
023600*    VGETS FROM PROFILES                                   *      02360000
023700************************************************************      02370000
023800                                                                  02380000
023900     CALL 'ISPLINK' USING ISP-VGET NAM-ZTEMPF ISP-SHARED.         02390000
024000     MOVE SPACES TO ISP-MSG.                                      02400000
024100     MOVE ZERO TO RETURN-CODE.                                    02410000
024200                                                                  02420000
024300     MOVE DBRM-LINK-NAME TO VAR-CMNEKDBR.                         02430000
024400     MOVE DBRM-SID TO VAR-CMNEKSID.                               02440000
024500     MOVE 'TKMA' TO VAR-CMNEKLIB.                                 02450000
024600     MOVE SPACES TO VAR-CMNEKAPP.                                 02460000
024700                                                                  02470000
024800     PERFORM 1700-DISPLAY-PANEL THRU 1700-EXIT UNTIL              02480000
024900          RETURN-CODE NOT = 0.                                    02490000
025000     MOVE SPACES TO VAR-CMNEKDBR.                                 02500000
025100                                                                  02510000
025200     GOBACK.                                                      02520000
025300                                                                  02530000
025400************************************************************      02540000
025500*    DISPLAY THE PANEL                                     *      02550000
025600************************************************************      02560000
025700*                                                                 02570000
025800 1700-DISPLAY-PANEL.                                              02580000
025900                                                                  02590000
026000     IF DBRM-SYS = 'DB2P'                                         02600004
026100         MOVE 'CMNEKSID' TO ISP-CSR                               02610000
026200         MOVE 'CMNEKR5' TO ISP-PANEL.                             02620002
026700     CALL 'ISPLINK' USING ISP-DISPLAY,                            02670000
026800                          ISP-PANEL,                              02680000
026900                          ISP-MSG,                                02690000
027000                          ISP-CSR.                                02700000
027100                                                                  02710000
027200     MOVE SPACES TO ISP-MSG.                                      02720000
027300     MOVE SPACES TO VAR-SMSG.                                     02730000
027400     MOVE SPACES TO VAR-LMSG.                                     02740000
027500                                                                  02750000
027600     IF RETURN-CODE NOT = 0                                       02760000
027700         GO TO 1700-EXIT.                                         02770000
027800                                                                  02780000
027900     PERFORM 2100-EDIT-INPUT THRU 2100-EXIT.                      02790000
028000     IF PS-USER-INPUT-NOT-VALID                                   02800000
028100         GO TO 1700-EXIT.                                         02810000
028200                                                                  02820000
028300     PERFORM 9900-VPUTS THRU 9900-EXIT.                           02830000
028400                                                                  02840000
028500     IF MEM-CRE = 'Y'                                             02850000
028600         MOVE 'JOB SUBMITTED ' TO VAR-SMSG                        02860000
028700         MOVE 'CMN000' TO ISP-MSG.                                02870000
028800                                                                  02880000
028900                                                                  02890000
029000 1700-EXIT.                                                       02900000
029100     EXIT.                                                        02910000
029200     EJECT                                                        02920000
029300************************************************************      02930000
029400* SET SWITCHES TO INDICATE WHAT OPTION HAS BEEN SPECIFIED  *      02940000
029500* BY THE USER                                              *      02950000
029600************************************************************      02960000
029700 2100-EDIT-INPUT.                                                 02970000
029800                                                                  02980000
029900     SET PS-NO-DB2-ERROR TO TRUE.                                 02990000
030000     MOVE 'Y' TO PS-USER-INPUT.                                   03000000
030100                                                                  03010000
030200     IF DBRM-SYS = 'DB2P'                                         03020000
030300         EXEC SQL                                                 03030000
030400            SELECT '1'                                            03040000
030500              INTO :WS-FIELD                                      03050000
030600              FROM SYSIBM.SYSPACKLIST                             03060000
030700              WHERE SUBSTR(COLLID,1,2) = :VAR-CMNEKSID            03070000
030800          END-EXEC                                                03080000
032700     END-IF.                                                      03270000
032800                                                                  03280000
032900      IF SQLCODE  = 0                                             03290000
033000        MOVE 'Y' TO MEM-CRE                                       03300000
033100        GO TO 2100-EXIT.                                          03310000
033200      IF SQLCODE  = -811                                          03320000
033300        MOVE 'Y' TO MEM-CRE                                       03330000
033400        GO TO 2100-EXIT.                                          03340000
033500      IF SQLCODE  =  100                                          03350000
033600          SET PS-DB2-ERROR TO TRUE                                03360000
033700          MOVE 'N' TO PS-USER-INPUT                               03370000
033800          MOVE 'SYSTEM ID INVALID' TO VAR-SMSG-FILLER             03380000
033900          STRING 'SYSTEM ID '       DELIMITED BY SIZE             03390000
034000                 VAR-CMNEKSID       DELIMITED BY ' '              03400000
034100                 ' IS NOT VALID '   DELIMITED BY SIZE             03410000
034200              INTO VAR-LMSG                                       03420000
034300          MOVE 'CMN000  ' TO ISP-MSG                              03430000
034400          GO TO 2100-EXIT.                                        03440000
034500                                                                  03450000
034600      SET PS-DB2-ERROR TO TRUE                                    03460000
034700      MOVE 'SQLCODE =' TO VAR-SMSG-FILLER                         03470000
034800      MOVE SQLCODE TO VAR-SMSG-SQLCODE                            03480000
034900      MOVE '2100-EDIT-INPUT  ABEND, CONTACT DATA RESOURCE'        03490000
035000           TO VAR-LMSG                                            03500000
035100      MOVE 'CMN000  ' TO ISP-MSG.                                 03510000
035200                                                                  03520000
035300 2100-EXIT.                                                       03530000
035400     EXIT.                                                        03540000
035500     EJECT                                                        03550000
035600************************************************************      03560000
035700* CREATE BIND JOB                                          *      03570000
035800************************************************************      03580000
035900 9900-VPUTS.                                                      03590000
036000                                                                  03600000
036100     MOVE DBRM-USER TO VAR-USER.                                  03610000
036110     MOVE 'CMNEK50' TO ISP-SKEL.                                  03611001
037600                                                                  03760000
037700     CALL 'ISPLINK' USING ISP-FTOPEN ISP-TEMP.                    03770000
037800     CALL 'ISPLINK' USING ISP-FTINCL ISP-SKEL.                    03780000
037900     CALL 'ISPLINK' USING ISP-FTCLOSE.                            03790000
038000     MOVE VAR-ZTEMPF TO ISP-NAME.                                 03800000
038100     CALL 'ISPLINK' USING ISP-SELECT                              03810000
038200                          ISP-BL                                  03820000
038300                          ISP-BUFFER.                             03830000
038400 9900-EXIT.                                                       03840000
038500     EXIT.                                                        03850000
