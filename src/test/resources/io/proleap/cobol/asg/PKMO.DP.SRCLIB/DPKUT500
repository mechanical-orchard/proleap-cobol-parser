086980         MOVE PV-DPG55-CMPR-GREG-CC-FMT-YR                        08692009
086990           TO    DPG55-CMPR-GREG-CC-FMT-YR                        08693009
087000         MOVE PV-CREL-CMPR-DIFF-ACT  TO DPG55-DAYS-DIFFERENCE     08694009
087010         MOVE PV-CREL-CMPR-DIFF-BUS  TO DPG55-BUS-DAYS-DIFFERENCE 08695009
087020     END-IF.                                                      08696009
087030                                                                  08697009
087040     PERFORM 3820-IDENTIFY-HOL-OR-BUS-DAY.                        08698009
087050     IF  PV-INTERNAL-PRCSS-OK                                     08699009
087060         PERFORM 3830-IDENTIFY-1ST-DAY-OF-WEEK                    08700009
087070     END-IF.                                                      08701009
087080                                                                  08702009
087090     IF  PV-INTERNAL-PRCSS-OK                                     08703009
087100         PERFORM 3840-IDENTIFY-LAST-DAY-OF-WEEK                   08704009
087110     END-IF.                                                      08705009
087120                                                                  08706009
087130                                                                  08707009
087140* ************************************************************** *08708009
087150* * IDENTIFY "PROCESS DATE" AS A HOLIDAY OR BUSINESS DAY.      * *08709009
087160* ************************************************************** *08710009
087170 3820-IDENTIFY-HOL-OR-BUS-DAY.                                    08711009
087180                                                                  08712009
087190                                                                  08713009
087200     IF PV-SET-IDX-CCYY-N = PC-HT-BASELINE-YEAR                   08714009
087210         MOVE PC-01                     TO PV-HT-STARTING-POINT   08715009
087220     ELSE                                                         08716009
087230         COMPUTE PV-HT-STARTING-POINT = ((PV-SET-IDX-CCYY-N       08717009
087240                                      -  PC-HT-BASELINE-YEAR)     08718009
087250                                      *  PC-HT-HOLIDAYS-PER-YR)   08719009
087260                                      +  PC-ONE                   08720009
087270     END-IF.                                                      08721009
087280                                                                  08722009
087290*    *********************************************************** *08723009
087300*    *  ERROR TRAP TO PREVENT INDEX ERROR                      * *08724009
087310*    *  (FUTURE ENHANCEMENT COULD CAUSE CONSTANTS OUT-OF-SYNC) * *08725009
087320*    *  (WITH HT-TABLE OCCURS CLAUSE OR DATA.                ) * *08726009
087330*    *********************************************************** *08727009
087340     IF  PV-HT-STARTING-POINT < PC-01                             08728009
087350         MOVE PC-01                     TO PV-HT-STARTING-POINT   08729009
087360         SET PV-INTERNAL-PRCSS-ERR      TO TRUE                   08730009
087370         MOVE '3820- A'                 TO PV-ERRMSG99-PARA       08731009
087380         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      08732009
087390                                        TO DPG54-ERROR-MESSAGE    08733009
087400     ELSE                                                         08734009
087410         IF  PV-HT-STARTING-POINT > PC-HT-MAX-HOLIDAY-ENTRIES     08735009
087420             IF  PV-SET-IDX-CCYY-N = PC-HT-MAX-YEAR               08736009
087430                 MOVE PC-ERRMSG95-MAX-DATE-RANGE                  08737009
087440                                        TO DPG54-ERROR-MESSAGE    08738009
087450                 SET PV-EDIT-SW-ERROR                             08739009
087460                     DPG54-SEVERE-ERROR TO TRUE                   08740009
087470             ELSE                                                 08741009
087480                 MOVE PC-HT-MAX-HOLIDAY-ENTRIES                   08742009
087490                                        TO PV-HT-STARTING-POINT   08743009
087500                 SET PV-INTERNAL-PRCSS-ERR                        08744009
087510                                        TO TRUE                   08745009
087520                 MOVE '3820- B'         TO PV-ERRMSG99-PARA       08746009
087530                 MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR              08747009
087540                                        TO DPG54-ERROR-MESSAGE    08748009
087550         END-IF                                                   08749009
087560     END-IF.                                                      08750009
087570                                                                  08751009
087580     SET HT-INDEX                       TO PV-HT-STARTING-POINT.  08752009
087590     SET PV-HOLIDAY-MATCH-SW-INIT       TO TRUE.                  08753009
087600                                                                  08754009
087610     PERFORM 3821-CHECK-HOLIDAYS-THIS-YEAR                        08755009
087620        VARYING PV-HOLIDAYS-CHECKED-COUNT                         08756009
087630        FROM    PC-01                                             08757009
087640        BY      PC-01                                             08758009
087650        UNTIL   HT-INDEX > PC-HT-MAX-HOLIDAY-ENTRIES              08759009
087660           OR   PV-HOLIDAYS-CHECKED-COUNT >                       08760009
087670                                           PC-HT-HOLIDAYS-PLUS-ONE08761009
087680           OR   PV-HOLIDAY-PASSED                                 08762009
087690           OR   PV-HOLIDAY-MATCH-FOUND.                           08763009
087700                                                                  08764009
087710                                                                  08765009
087720* ************************************************************** *08766009
087730* * CHECK "CREL" REPRESENTATIONS OF HOLIDAYS IN THIS YEAR TO   * *08767009
087740* * DETERMINE WHETHER "PROCESS DATE" IS A HOLIDAY -OR- BUSINESS* *08768009
087750* * DAY.                                                       * *08769009
087760* ************************************************************** *08770009
087770 3821-CHECK-HOLIDAYS-THIS-YEAR.                                   08771009
087780                                                                  08772009
087790     IF PV-CREL-PROCESS-DATE  = HT-DAYS-SINCE-BASELINE (HT-INDEX) 08773009
087800         SET  PV-HOLIDAY-MATCH-FOUND         TO TRUE              08774009
087810         MOVE HT-HOLIDAY-NAME (HT-INDEX)     TO DPG55-HOLIDAY-NAME08775009
087820         IF  PV-DAY-OF-WEEK-X = PC-SATURDAY                       08776009
087830         OR  PV-DAY-OF-WEEK-X = PC-SUNDAY                         08777009
087850             SET DPG55-NOT-CORPORATE-HOLIDAY TO TRUE              08778009
087880         ELSE                                                     08779009
087900             SET DPG55-CORPORATE-HOLIDAY     TO TRUE              08780009
087930         END-IF                                                   08790009
087931         SET DPG55-NOT-CORPORATE-BUS-DAY     TO TRUE              08791009
087932         SET DPG55-ACTUAL-HOLIDAY            TO TRUE              08792009
087933         PERFORM 3823-CHECK-IF-STORE-HOLIDAY                      08793009
087940     ELSE                                                         08794009
087950         IF PV-DAY-OF-WEEK-X = PC-SATURDAY                        08795009
087960         OR PV-DAY-OF-WEEK-X = PC-SUNDAY                          08796009
087970             MOVE SPACES                     TO DPG55-HOLIDAY-NAME08797009
087980             SET DPG55-NOT-CORPORATE-BUS-DAY TO TRUE              08798009
087990             SET DPG55-NOT-CORPORATE-HOLIDAY TO TRUE              08799009
088000             SET DPG55-NOT-ACTUAL-HOLIDAY    TO TRUE              08800009
088010             SET DPG55-NOT-STORE-HOLIDAY     TO TRUE              08801009
088020         ELSE                                                     08802009
088030             IF  PV-DAY-OF-WEEK-X = PC-FRIDAY                     08803009
088040                 COMPUTE PV-CREL-NEXT-DAY =                       08804009
088050                         PV-CREL-PROCESS-DATE + PC-01             08805009
088060                 IF  PV-CREL-NEXT-DAY =                           08806009
088070                     HT-DAYS-SINCE-BASELINE (HT-INDEX)            08807009
088080                     IF  HT-HOLIDAY-NAME (HT-INDEX) =             08808009
088090                         PC-HT-M-NEW-YEARS-DAY  OR                08809009
088100                         PC-HT-M-FOURTH-OF-JULY OR                08810009
088110                         PC-HT-M-CHRISTMAS-DAY                    08811009
088120                         SET  PV-HOLIDAY-MATCH-FOUND     TO TRUE  08812009
088130                         MOVE HT-HOLIDAY-NAME (HT-INDEX)          08813009
088140                                             TO DPG55-HOLIDAY-NAME08814009
088150                         PERFORM 3822-ITS-A-CELEBRATED-HOLIDAY    08815009
088160                     ELSE                                         08816009
088170                         PERFORM 3824-ITS-A-BUSINESS-DAY          08817009
088180                    END-IF                                        08818009
088190                 ELSE                                             08819009
088200                     PERFORM 3824-ITS-A-BUSINESS-DAY              08820009
088210                 END-IF                                           08821009
088220             ELSE                                                 08822009
088230                 IF  PV-DAY-OF-WEEK-X = PC-MONDAY                 08823009
088240                     COMPUTE PV-CREL-PREV-DAY =                   08824009
088250                             PV-CREL-PROCESS-DATE - PC-01         08825009
088260                     IF  PV-CREL-PREV-DAY =                       08826009
088270                         HT-DAYS-SINCE-BASELINE (HT-INDEX)        08827009
088280                         IF  HT-HOLIDAY-NAME (HT-INDEX) =         08828009
088290                             PC-HT-M-NEW-YEARS-DAY  OR            08829009
088300                             PC-HT-M-FOURTH-OF-JULY OR            08830009
088310                             PC-HT-M-CHRISTMAS-DAY                08831009
088320                             SET  PV-HOLIDAY-MATCH-FOUND TO TRUE  08832009
088330                             MOVE HT-HOLIDAY-NAME (HT-INDEX)      08833009
088340                                             TO DPG55-HOLIDAY-NAME08834009
088350                             PERFORM 3822-ITS-A-CELEBRATED-HOLIDAY08835009
088360                         ELSE                                     08836009
088370                             PERFORM 3824-ITS-A-BUSINESS-DAY      08837009
088380                         END-IF                                   08838009
088390                     ELSE                                         08839009
088400                         PERFORM 3824-ITS-A-BUSINESS-DAY          08840009
088410                     END-IF                                       08841009
088420                 ELSE                                             08842009
088430                     PERFORM 3824-ITS-A-BUSINESS-DAY              08843009
088440                 END-IF                                           08844009
088450             END-IF                                               08845009
088460         END-IF                                                   08846009
088470     END-IF.                                                      08847009
088480                                                                  08848009
088490     IF PV-HOLIDAY-MATCH-FOUND                                    08849009
088500         CONTINUE                                                 08850009
088510     ELSE                                                         08851009
088520         IF PV-CREL-PROCESS-DATE <                                08852009
088530                                 HT-DAYS-SINCE-BASELINE (HT-INDEX)08853009
088540             SET PV-HOLIDAY-PASSED TO TRUE                        08854009
088550         ELSE                                                     08855009
088560             SET HT-INDEX UP BY PC-01                             08856009
088570         END-IF                                                   08857009
088580     END-IF.                                                      08858009
