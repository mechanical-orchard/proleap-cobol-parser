060620         CONTINUE                                                 06062009
060630     ELSE                                                         06063009
060640         IF  PV-ACT-LEN-OF-CMPR-DT  = PV-MAX-LEN-OF-CMPR-DT       06064009
060650             CONTINUE                                             06065009
060660         ELSE                                                     06066009
060670*            IF  PV-ACT-LEN-OF-CMPR-DT >                          06067009
060680*                                     PV-MAX-LEN-OF-CMPR-DT       06068009
060690*                    SET PV-GF-EXTRANEOUS-INPUT                   06069009
060700*                                             TO TRUE             06070009
060710*            ELSE                                                 06071009
060720                 SET DPG54-CMPR-DATE-INVALID                      06072009
060730                     PV-EDIT-SW-ERROR                             06073009
060740                     DPG54-SEVERE-ERROR       TO TRUE             06074009
060750                 MOVE PC-ERRMSG14-INVAL-CMPR-DATE                 06075009
060760                                              TO                  06076009
060770                                             DPG54-ERROR-MESSAGE  06077009
060780*            END-IF                                               06078009
060790         END-IF                                                   06079009
060800     END-IF.                                                      06080009
060810                                                                  06081009
060820                                                                  06082009
060830                                                                  06083009
060840* ************************************************************** *06084009
060850* * THE FUNCTIONS OF THIS PARAGRAPH ARE:                       * *06085009
060860* * 1. CONVERT DATE INPUT AS FORMAT                            * *06086009
060870* *    DPG53-LK-CMPR-DTE-GREG-FMT   (MM/DD/YY)                 * *06087009
060880* *    OR                                                      * *06088009
060890* *    DPG53-LK-CMPR-DTE-GREG       (MMDDYY)                   * *06089009
060900* *    OR                                                      * *06090009
060910* *    TO FORMAT:                                              * *06091009
060920* *    DPG52-LK-DTE-GREG-CC         (MMDDCCYY)                 * *06092009
060930* * 2. CALCULATE CENTURY (CC)       (MMDD19YY OR MMDD20YY)     * *06093009
060940* ************************************************************** *06094009
060950 2320-ADD-CENTURY-TO-COMPARE.                                     06095009
060960                                                                  06096009
060970     IF DPG53-LK-CMPR-DTE-GREG-FMT                                06097009
060980* ************************************************************** *06098009
060990* *     DPG53-LK-CMPR-DTE-GREG-FMT  (MM/DD/YY)                 * *06099009
061000* ************************************************************** *06100009
061010         IF  DPG53-GREG-DATE-FMT-S1 = PC-SLASH                    06101009
061020         AND DPG53-GREG-DATE-FMT-S2 = PC-SLASH                    06102009
061030         AND DPG53-GREG-DATE-FMT-MO-X NUMERIC                     06103009
061040         AND DPG53-GREG-DATE-FMT-DY-X NUMERIC                     06104009
061050         AND DPG53-GREG-DATE-FMT-YR-X NUMERIC                     06105009
061060             IF  DPG53-GREG-DATE-FMT-MO >  ZERO                   06106009
061070             AND DPG53-GREG-DATE-FMT-MO <= PC-MAX-MONTH           06107009
061080             AND DPG53-GREG-DATE-FMT-DY >  ZERO                   06108009
061090             AND DPG53-GREG-DATE-FMT-DY <=                        06109009
061100                                        PC-MAX-DAY-IN-ANY-MONTH   06110009
061101****                                                              06110109
061102*  SINCE SOME COMPARE OPERATIONS WORK DIRECTLY OFF THE DPG53      06110209
061103*  AREAS, AND THOSE AREAS ARE EXPANDED TO INCLUDE CENTURY PRIOR   06110309
061104*  DURING THE VALIDATION PROCESS, THESE INPUT FIELDS WILL BE      06110409
061105*  RESTORED JUST PRIOR TO THE RETURN TO THE CALLING PROGRAM.      06110509
061106*                                                                 06110609
061107                 MOVE DPG53-LK-CMPR-DATE-FORMAT-CODE TO           06110709
061108                           PV-ORIG-CMPR-DATE-FORMAT-CODE          06110809
061109                 MOVE DPG53-LK-CMPR-DATE-INPUT       TO           06110909
061110                           PV-ORIG-CMPR-DATE-INPUT                06111009
061111*                                                                 06111109
061112****                                                              06111209
061113                 MOVE DPG53-GREG-DATE-FMT-MO TO PV-MMDDCCYY-MM-N  06111309
061120                 MOVE DPG53-GREG-DATE-FMT-DY TO PV-MMDDCCYY-DD-N  06111409
061130                 MOVE DPG53-GREG-DATE-FMT-YR TO PV-CALC-CCYY-N    06111509
061140                 PERFORM 5920-CALC-CC                             06111609
061150                 MOVE PV-MMDDCCYY-MM-N   TO DPG53-GREG-CC-DATE-MO 06111709
061160                 MOVE PV-MMDDCCYY-DD-N   TO DPG53-GREG-CC-DATE-DY 06111809
061170                 MOVE PV-CALC-CCYY-N     TO DPG53-GREG-CC-DATE-YR 06111909
