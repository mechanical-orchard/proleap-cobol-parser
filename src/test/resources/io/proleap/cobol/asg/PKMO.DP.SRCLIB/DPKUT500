083460* ************************************************************** *08340009
083470* * LOOP THRU EACH DAY WITHIN THE RANGE, IDENTIFYING WHETHER   * *08341009
083480* * IT IS A BUSINESS DAY, OR A WEEKEND/HOLIDAY.                * *08342009
083490* ************************************************************** *08343009
083500 3630-LOOP-FORWARD.                                               08344009
083510                                                                  08345009
083520     MOVE PV-CREL-DATE-TO-CHECK         TO PV-DAYS-SINCE-BASELINE.08346009
083530     PERFORM 5700-DETERMINE-DAY-OF-WEEK.                          08347009
083540                                                                  08348009
083550     IF PV-CREL-DATE-TO-CHECK = HT-DAYS-SINCE-BASELINE (HT-INDEX) 08349009
083560         ADD PC-01                      TO  PV-COUNT-NON-BUSDAY   08350009
083570         IF  PV-DAY-OF-WEEK-X = PC-SATURDAY                       08351009
083580         OR  PV-DAY-OF-WEEK-X = PC-SUNDAY                         08352009
083590             ADD PC-01                  TO  PV-COUNT-WKNDHOL      08353009
083600         ELSE                                                     08354009
083610             ADD PC-01                  TO  PV-COUNT-HOL          08355009
083620         END-IF                                                   08356009
083630         SET HT-INDEX UP BY PC-01                                 08357009
083640     ELSE                                                         08358009
083650         IF PV-DAY-OF-WEEK-X = PC-SATURDAY                        08359009
083660         OR PV-DAY-OF-WEEK-X = PC-SUNDAY                          08360009
083670             ADD PC-01                  TO  PV-COUNT-NON-BUSDAY   08361009
083680             ADD PC-01                  TO  PV-COUNT-WKND         08362009
083690         ELSE                                                     08363009
083700             IF  PV-DAY-OF-WEEK-X = PC-FRIDAY                     08364009
083710                 SET HT-INDEX-NEXT      TO  HT-INDEX              08365009
083720                 PERFORM 3740-CHECK-AHEAD-FOR-HOL                 08366009
083730             ELSE                                                 08367009
083740                 IF  PV-DAY-OF-WEEK-X = PC-MONDAY                 08368009
083750                     SET HT-INDEX-PREV  TO  HT-INDEX              08369009
083760                     SET HT-INDEX-PREV  DOWN BY PC-01             08370009
083770                     PERFORM 3750-CHECK-BACK-FOR-HOL              08371009
083780                 ELSE                                             08372009
083790                     ADD PC-01          TO PV-COUNT-BUSDAY        08373009
083800                 END-IF                                           08374009
083810             END-IF                                               08375009
083820         END-IF                                                   08376009
083830     END-IF.                                                      08377009
083840                                                                  08378009
083850     IF  PV-BUS-DAYS-INCR-RUNNING                                 08379009
083860         IF  PV-COUNT-BUSDAY  >  PV-BUS-STOP                      08380009
083870             SET PV-BUS-DAYS-IDENTIFIED TO TRUE                   08381009
083880         ELSE                                                     08382009
083890             ADD PC-01                  TO PV-CREL-DATE-TO-CHECK  08383009
083900         END-IF                                                   08384009
083910     ELSE                                                         08385009
083920         IF  PV-BUS-DAYS-CMPR-RUNNING                             08386009
