059400* ************************************************************** *05940009
059410* * PERFORM A QUICK CHECK FOR MISMATCH OF DATES / FORMATS.     * *05941009
059420* ************************************************************** *05942009
059430 2310-CHECK-DATE-LENGTH-AND-FMT.                                  05943009
059440                                                                  05944009
059450     INITIALIZE PV-TALLY-OF-CMPR.                                 05945009
059460     INSPECT DPG53-LK-CMPR-DATE-INPUT                             05946009
059470         TALLYING PV-TALLY-OF-CMPR                                05947009
059480         FOR  ALL SPACES                                          05948009
059490                  LOW-VALUES.                                     05949009
059500                                                                  05950009
059510     COMPUTE PV-ACT-LEN-OF-CMPR-DT =                              05951009
059520             (LENGTH OF DPG53-LK-CMPR-DATE-INPUT -                05952009
059530              PV-TALLY-OF-CMPR).                                  05953009
059540                                                                  05954009
059550                                                                  05955009
059560     EVALUATE TRUE                                                05956009
059570                                                                  05957009
059580         WHEN DPG53-LK-CMPR-DTE-ISO-FMT                           05958009
059590*        ******************************************************* *05959009
059600*        *    LENGTH OF FORMAT:                   (CCYY-MM-DD) * *05960009
059610*        ******************************************************* *05961009
059620             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05962009
059630                     LENGTH OF DPG53-DB2-ISO-DATE-FMT             05963009
059640                                                                  05964009
059650         WHEN DPG53-LK-CMPR-DTE-ISO                               05965009
059660*        ******************************************************* *05966009
059670*        *    LENGTH OF FORMAT:                   (CCYYMMDD)   * *05967009
059680*        ******************************************************* *05968009
059690             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05969009
059700                     LENGTH OF DPG53-DB2-ISO-DATE                 05970009
059710                                                                  05971009
059720         WHEN DPG53-LK-CMPR-DTE-GREG-CC-FMT                       05972009
059730*        ******************************************************* *05973009
059740*        *    LENGTH OF FORMAT:                   (MM/DD/CCYY) * *05974009
059750*        ******************************************************* *05975009
059760             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05976009
059770                     LENGTH OF DPG53-GREG-CC-DATE-FMT             05977009
059780                                                                  05978009
059790         WHEN DPG53-LK-CMPR-DTE-GREG-CC                           05979009
059800*        ******************************************************* *05980009
059810*        *    LENGTH OF FORMAT:                   (MMDDCCYY)   * *05981009
059820*        ******************************************************* *05982009
059830             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05983009
059840                     LENGTH OF DPG53-GREG-CC-DATE                 05984009
059850                                                                  05985009
059860         WHEN DPG53-LK-CMPR-DTE-GREG-FMT                          05986009
059870*        ******************************************************* *05987009
059880*        *    LENGTH OF FORMAT:                   (MM/DD/YY)   * *05988009
059890*        ******************************************************* *05989009
059900             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05990009
059910                     LENGTH OF DPG53-GREG-DATE-FMT                05991009
059920                                                                  05992009
059930         WHEN DPG53-LK-CMPR-DTE-GREG                              05993009
059940*        ******************************************************* *05994009
059950*        *    LENGTH OF FORMAT:                   (MMDDYY)     * *05995009
059960*        ******************************************************* *05996009
059970             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      05997009
059980                     LENGTH OF DPG53-GREG-DATE                    05998009
059990                                                                  05999009
060000         WHEN DPG53-LK-CMPR-DTE-JULIAN-CC                         06000009
060010*        ******************************************************* *06001009
