110290* ************************************************************** *11029009
110300* * INITIALIZE VALUES USED & OUTPUT FROM THE PARAGRAPH TO:     * *11030009
110310* * DETERMINE THE DAY-OF-WEEK FOR A GIVEN DATE.                * *11031009
110320* ************************************************************** *11032009
110330 5701-5700-INITIALIZATIONS.                                       11033009
110340     INITIALIZE         PV-DAYS-FACTOR                            11034009
110350                        PV-YEARS-FACTOR                           11035009
110360                        PV-LEAP-FACTOR                            11036009
110370                        PV-NUMBER-OF-WEEKS                        11037009
110380                        PV-DAY-RELATIVE-TO-BASELINE.              11038009
110390     MOVE PC-UNKNOWN TO PV-DAY-OF-WEEK-X.                         11039009
110400                                                                  11040009
110410                                                                  11041009
110420* ************************************************************** *11042009
110430* * DETERMINE THE DAY-OF-WEEK FOR A GIVEN DATE.                * *11043009
110440* ************************************************************** *11044009
110450 5702-OK-DETERMINE-DAY.                                           11045009
110460     DIVIDE        PV-DAYS-SINCE-BASELINE                         11046009
110470         BY        PC-DAYS-PER-WEEK                               11047009
110480         GIVING    PV-NUMBER-OF-WEEKS                             11048009
110490         REMAINDER PV-DAY-RELATIVE-TO-BASELINE.                   11049009
110500                                                                  11050009
110510     IF PV-LEAP-YEAR                                              11051009
110520         IF  PV-DAYS-SINCE-BASELINE = ZERO                        11052009
110530             MOVE PC-01 TO PV-DAYS-FACTOR                         11053009
110540         ELSE                                                     11054009
110550             COMPUTE PV-LEAP-FACTOR =                             11055009
110560                  (PV-DAYS-SINCE-BASELINE / PC-DAYS-PER-YEAR)     11056009
110570             COMPUTE PV-YEARS-FACTOR =                            11057009
110580                  (PV-LEAP-FACTOR * PC-DAYS-PER-YEAR)             11058009
110590             COMPUTE PV-DAYS-FACTOR =                             11059009
110600                  (PV-DAYS-SINCE-BASELINE -                       11060009
110610                   PV-YEARS-FACTOR)                               11061009
110620         END-IF                                                   11062009
110630     END-IF.                                                      11063009
110640                                                                  11064009
110650                                                                  11065009
110660     IF  PV-DAY-RELATIVE-TO-BASELINE <= PC-HT-BASELINE-COMPLIMENT 11066009
110670         COMPUTE PV-DAY-OF-WEEK-N        =                        11067009
110680             PV-DAY-RELATIVE-TO-BASELINE +                        11068009
110690             PC-HT-BASELINE-OFFSET                                11069009
110700     ELSE                                                         11070009
110710         COMPUTE PV-DAY-OF-WEEK-N        =                        11071009
110720             PV-DAY-RELATIVE-TO-BASELINE -                        11072009
110730             PC-HT-BASELINE-COMPLIMENT                            11073009
110740     END-IF.                                                      11074009
110750                                                                  11075009
110760                                                                  11076009
110770     MOVE PC-DAY-OF-WEEK (PV-DAY-OF-WEEK-N)                       11077009
110780                                       TO PV-DAY-OF-WEEK-X.       11078009
110790                                                                  11079009
110800                                                                  11080009
110810* ************************************************************** *11081009
110820* * CHECK WHETHER THE GIVEN YEAR IS A LEAP YEAR.               * *11082009
110830* ************************************************************** *11083009
110840 5800-CHECK-LEAP-YEAR.                                            11084009
110850     PERFORM 5801-5800-INITIALIZATIONS.                           11085009
110860                                                                  11086009
110870     IF PV-CHK-LEAPYR-YY-N = ZERO                                 11087009
110880         DIVIDE PC-400-YEARS                                      11088009
110890             INTO      PV-CHK-LEAPYR-CCYY-N                       11089009
110900             GIVING    PV-LEAP-FACTOR ROUNDED                     11090009
110910             REMAINDER PV-LEAP-YEAR-REMAINDER                     11091009
110920     ELSE                                                         11092009
110930         DIVIDE PC-4-YEARS                                        11093009
110940             INTO      PV-CHK-LEAPYR-CCYY-N                       11094009
110950             GIVING    PV-LEAP-FACTOR ROUNDED                     11095009
110960             REMAINDER PV-LEAP-YEAR-REMAINDER                     11096009
110970     END-IF.                                                      11097009
110980                                                                  11098009
110990     IF  PV-LEAP-YEAR-REMAINDER = ZERO                            11099009
111000         SET PV-LEAP-YEAR        TO  TRUE                         11100009
111010     END-IF.                                                      11101009
111020                                                                  11102009
111030                                                                  11103009
