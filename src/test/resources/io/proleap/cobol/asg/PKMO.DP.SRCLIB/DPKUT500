066110* ************************************************************** *06605009
066120* * CONVERT THE COMPARISON DATE "FISCAL WEEK" ENTERED, TO AN   * *06606009
066130* * "ACTUAL DATE" FOR FURTHER "ACTUAL CALENDAR" PROCESSING.    * *06607009
066140* *                                                            * *06608009
066150* * - VALUE   OF DATE WILL BE: SATURDAY OF THE FISCAL WEEK.    * *06609009
066160* * - FORMATS OF DATE WILL BE:                                 * *06610009
066170* *   1) CREL (DAYS SINCE BASELINE)                            * *06611009
066180* *   2) WORKING FORMAT (CCYYMMDD)                             * *06612009
066190* ************************************************************** *06613009
066200 2350-CONVERT-FISCAL-WEEK.                                        06614009
066210                                                                  06615009
066220     MOVE DPG53-FISCAL-CC-WEEK-YR    TO PV-FWFP-INPUT-CCYY-N      06616009
066230                                        PV-FISCAL-PROPOSED-CCYY   06617009
066240                                        PV-FISCAL-JULIAN-CCYY.    06618009
066250                                                                  06619009
066260     MOVE DPG53-FISCAL-CC-WEEK-WK    TO PV-EDIT-FW.               06620009
066270     IF  PV-EDIT-FW > PC-52-FISCAL-WKS                            06621009
066280         MOVE PC-365-DAYS TO PV-FISCAL-JULIAN-DDD                 06622009
066290     ELSE                                                         06623009
066300         COMPUTE PV-FISCAL-JULIAN-DDD =                           06624009
066310            (DPG53-FISCAL-CC-WEEK-WK * PC-DAYS-PER-WEEK)          06625009
066320     END-IF.                                                      06626009
066330                                                                  06627009
066340     MOVE PV-FISCAL-JULIAN-CCYY      TO PV-CCYYDDD-CCYY-X.        06628009
066350     MOVE PV-FISCAL-JULIAN-DDD       TO PV-CCYYDDD-DDD-X.         06629009
066360     PERFORM 5600-CONVERT-JUL-TO-CCYYMMDD.                        06630009
066370                                                                  06631009
066380     IF  PV-EDIT-SW-OK-SO-FAR                                     06632009
066390     AND PV-INTERNAL-PRCSS-OK                                     06633009
066400         PERFORM 5100-VALIDATE-FORMAT-CCYYMMDD                    06634009
066410     ELSE                                                         06635009
066420         SET PV-EDIT-SW-ERROR                                     06636009
066430             DPG54-CMPR-DATE-INVALID TO TRUE                      06637009
066440     END-IF.                                                      06638009
066450                                                                  06639009
066460     IF  PV-EDIT-SW-OK-SO-FAR                                     06640009
066470     AND PV-INTERNAL-PRCSS-OK                                     06641009
066480         MOVE PV-CCYYMMDD-CCYY-N     TO PV-CCYYMMDD-FWFP-CCYY-N   06642009
066490                                        PV-CCYYMMDD-FWFP-C-CCYY-N 06643009
066500         IF  PV-CCYYMMDD-MM-N = PC-1ST-MONTH                      06644009
066510            ADD PC-ONE               TO PV-CCYYMMDD-MM-N          06645009
066520         END-IF                                                   06646009
066530         MOVE PV-CCYYMMDD-MM-N       TO PV-CCYYMMDD-FWFP-MM-N     06647009
066540                                        PV-CCYYMMDD-FWFP-C-MM-N   06648009
066550         MOVE PV-CCYYMMDD-DD-N       TO PV-CCYYMMDD-FWFP-DD-N     06649009
066560                                        PV-CCYYMMDD-FWFP-C-DD-N   06650009
066570         PERFORM 5200-CONVERT-CCYYMMDD-TO-DAYS                    06651009
066580     ELSE                                                         06652009
066590         SET PV-EDIT-SW-ERROR                                     06653009
066600             DPG54-CMPR-DATE-INVALID TO TRUE                      06654009
066610     END-IF.                                                      06655009
066620                                                                  06656009
066630     IF  PV-EDIT-SW-OK-SO-FAR                                     06657009
066640     AND PV-INTERNAL-PRCSS-OK                                     06658009
066650         MOVE PV-DAYS-SINCE-BASELINE TO PV-CREL-FWFP-CMPR         06659009
066660                                        PV-CREL-CMPR-INPUT        06660009
066670                                        PV-CREL-PROCESS-DATE      06661009
066680     ELSE                                                         06662009
066690         SET PV-EDIT-SW-ERROR                                     06663009
066700             DPG54-CMPR-DATE-INVALID TO TRUE                      06664009
066710     END-IF.                                                      06665009
066720                                                                  06666009
066730     IF  PV-EDIT-SW-OK-SO-FAR                                     06667009
066740     AND PV-INTERNAL-PRCSS-OK                                     06668009
066750         SET PV-THIS-PASS-EDITS-FW   TO TRUE                      06669009
066760         PERFORM 2400-EDIT-FWFP-TO-SAT-IN-FY                      06670009
066770     ELSE                                                         06671009
066780         SET PV-EDIT-SW-ERROR                                     06672009
066790             DPG54-CMPR-DATE-INVALID TO TRUE                      06673009
066800     END-IF.                                                      06674009
066810                                                                  06675009
066820     IF  PV-EDIT-SW-OK-SO-FAR                                     06676009
066830     AND PV-INTERNAL-PRCSS-OK                                     06677009
066840         MOVE PV-FISCAL-BEGIN-CCYY-N TO                           06678009
066850                                     PV-SAVE-FWFP-DIFF-CMPR-CCYY  06679009
066860         MOVE PV-SAVE-FISCAL-PRD     TO PV-SAVE-FWFP-DIFF-CMPR-FP 06680009
066870         MOVE PV-FISCAL-WEEK-NBR     TO PV-SAVE-FWFP-DIFF-CMPR-FW 06681009
066880         PERFORM 5700-DETERMINE-DAY-OF-WEEK                       06682009
066890         MOVE PV-DAY-OF-WEEK-N       TO                           06683009
066900                                     PV-SAVE-FWFP-DIFF-CMPR-WKDY  06684009
066910         MOVE PV-DAYS-SINCE-BASELINE TO                           06685009
066920                                     PV-SAVE-FWFP-DIFF-CMPR-CREL  06686009
066930     ELSE                                                         06687009
066940         SET PV-EDIT-SW-ERROR                                     06688009
066950             DPG54-DATE-INVALID      TO TRUE                      06689009
066960     END-IF.                                                      06690009
