060050                     LENGTH OF DPG53-JULIAN-DATE-CCYYDDD          06005009
060060                                                                  06006009
060070         WHEN DPG53-LK-CMPR-DTE-JULIAN-FMT                        06007009
060080*        ******************************************************* *06008009
060090*        *    LENGTH OF FORMAT:                   (YY/DDD)     * *06009009
060100*        ******************************************************* *06010009
060110             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      06011009
060120                     LENGTH OF DPG53-JULIAN-DATE-FMT              06012009
060130                                                                  06013009
060140         WHEN DPG53-LK-CMPR-DTE-JULIAN                            06014009
060150*        ******************************************************* *06015009
060160*        *    LENGTH OF FORMAT:                   (YYDDD)      * *06016009
060170*        ******************************************************* *06017009
060180             COMPUTE PV-MAX-LEN-OF-CMPR-DT =                      06018009
060190                     LENGTH OF DPG53-JULIAN-DATE                  06019009
060200                                                                  06020009
060210         WHEN DPG53-LK-CMPR-DTE-FISC-WEEK-CC                      06021009
060220*        ******************************************************* *06022009
