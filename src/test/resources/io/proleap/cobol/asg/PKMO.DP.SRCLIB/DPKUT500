067990 2390-HOLD-VALID-COMPARE-DATE.                                    06793009
068000                                                                  06794009
068010     MOVE PV-CCYYMMDD-MM-N        TO PV-DPG55-CMPR-GREG-CC-FMT-MO.06795009
068020     MOVE PC-SLASH                TO PV-DPG55-CMPR-GREG-CC-FMT-S1.06796009
068030     MOVE PV-CCYYMMDD-DD-N        TO PV-DPG55-CMPR-GREG-CC-FMT-DY.06797009
068040     MOVE PC-SLASH                TO PV-DPG55-CMPR-GREG-CC-FMT-S2.06798009
068050     MOVE PV-CCYYMMDD-CCYY-N      TO PV-DPG55-CMPR-GREG-CC-FMT-YR.06799009
068060                                                                  06800009
068070     PERFORM 5200-CONVERT-CCYYMMDD-TO-DAYS.                       06801009
068080     IF  PV-EDIT-SW-OK-SO-FAR                                     06802009
068090     AND PV-INTERNAL-PRCSS-OK                                     06803009
068100         MOVE PV-DAYS-SINCE-BASELINE TO PV-CREL-CMPR-INPUT        06804009
068110         SET DPG54-CMPR-DATE-VALID   TO TRUE                      06805009
068120     ELSE                                                         06806009
068130         INITIALIZE                     PV-CREL-CMPR-INPUT        06807009
068140         SET DPG54-CMPR-DATE-INVALID TO TRUE                      06808009
068150     END-IF.                                                      06809009
068160                                                                  06810009
068170                                                                  06811009
068180     IF  PV-EDIT-SW-OK-SO-FAR                                     06812009
068190     AND PV-INTERNAL-PRCSS-OK                                     06813009
068200         IF  DPG51-VALID-FISCAL-CALENDAR                          06814009
068210                                                                  06815009
068220             EVALUATE TRUE                                        06816009
068230                 WHEN DPG53-LK-CMPR-DTE-FISC-WEEK-CC              06817009
068240                 WHEN DPG53-LK-CMPR-DTE-FISC-PRD-CC               06818009
068250                     CONTINUE                                     06819009
068260                                                                  06820009
068270                 WHEN OTHER                                       06821009
068280                     PERFORM 5700-DETERMINE-DAY-OF-WEEK           06822009
068290                     MOVE PV-DAY-OF-WEEK-N                        06823009
068300                                 TO  PV-SAVE-FWFP-DIFF-CMPR-WKDY  06824009
068310                     MOVE PV-DAYS-SINCE-BASELINE                  06825009
068320                                 TO  PV-SAVE-FWFP-DIFF-CMPR-CREL  06826009
068330                                     PV-CREL-PROCESS-DATE         06827009
068340                     MOVE PV-CCYYMMDD-CCYY-N                      06828009
068350                                 TO  PV-FISCAL-PROPOSED-CCYY      06829009
068360                     PERFORM 4000-PROC-FISCAL-CALENDAR            06830009
068370                     MOVE PV-FISCAL-BEGIN-CCYY-N                  06831009
068380                                 TO  PV-SAVE-FWFP-DIFF-CMPR-CCYY  06832009
068390                     MOVE PV-SAVE-FISCAL-PRD                      06833009
068400                                 TO  PV-SAVE-FWFP-DIFF-CMPR-FP    06834009
068410                     MOVE PV-FISCAL-WEEK-NBR                      06835009
068420                                 TO  PV-SAVE-FWFP-DIFF-CMPR-FW    06836009
068430             END-EVALUATE                                         06837009
068440                                                                  06838009
068450         END-IF                                                   06839009
068460     ELSE                                                         06840009
068470         SET PV-EDIT-SW-ERROR                                     06841009
068480             DPG54-CMPR-DATE-INVALID                              06842009
068490                                 TO TRUE                          06843009
068500     END-IF.                                                      06844009
068510                                                                  06845009
068520                                                                  06846009
068530     IF  PV-EDIT-SW-OK-SO-FAR                                     06847009
068540     AND PV-INTERNAL-PRCSS-OK                                     06848009
068550*        ******************************************************* *06849009
068560*        * RESTORE "PRIMARY" INPUT DATE AS THE "PROCESS DATE"; * *06850009
068570*        * IT WAS TEMPORARILY OVERLAYED BY "COMPARE" DATE.     * *06851009
068580*        ******************************************************* *06852009
068590         MOVE PV-CREL-PRIMARY-INPUT TO PV-CREL-PROCESS-DATE       06853009
068600                                       PV-DAYS-SINCE-BASELINE     06854009
068610         PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD                    06855009
068620         MOVE PV-CCYYMMDD-CCYY-N    TO PV-PROCESS-CCYYMMDD-CCYY-N 06856009
068630         MOVE PV-CCYYMMDD-MM-N      TO PV-PROCESS-CCYYMMDD-MM-N   06857009
068640         MOVE PV-CCYYMMDD-DD-N      TO PV-PROCESS-CCYYMMDD-DD-N   06858009
068650     ELSE                                                         06859009
068660         SET PV-EDIT-SW-ERROR                                     06860009
068670             DPG54-CMPR-DATE-INVALID                              06861009
068680                                 TO TRUE                          06862009
068690     END-IF.                                                      06863009
068700                                                                  06864009
068710                                                                  06865009
068720* ************************************************************** *06866009
068730* * THIS PARAGRAPH IS THE COMMON EDIT:                         * *06867009
068740* * FOR  CONVERTING FISCAL WEEK INPUT FORMAT                   * *06868009
068750* * -OR- CONVERTING FISCAL PERIOD INPUT FORMAT                 * *06869009
068760* * WHEN ENTERED AS "PRIMARY" INPUT DATE                       * *06870009
068770* * -OR- ENTERED AS "COMPARE" DATE                             * *06871009
068780* * -OR- CALCULATED AS THE FISCAL PERIOD "INCR/DECR" RESULT.   * *06872009
068790* *                                                            * *06873009
068800* * EDIT FISCAL WEEK / FISCAL PERIOD, PROVIDING AN "ACTUAL DATE" *06874009
068810* * FOR FURTHER "ACTUAL CALENDAR" PROCESSING.                  * *06875009
068820* * ESTABLISH DATE AS A SATURDAY IN THE SPECIFIED FISCAL YEAR: * *06876009
068830* * - WHEN FW ENTERED,                                         * *06877009
068840* *   VALUE OF DATE WILL BE: SATURDAY OF THE FISCAL WEEK.      * *06878009
068850* * - WHEN FP ENTERED,                                         * *06879009
068860* *   VALUE OF DATE WILL BE: 2ND SATURDAY OF THE FISCAL PERIOD.* *06880009
068870* *                                                            * *06881009
068880* * NOTE: PLACE THE 4-DIGIT YEAR IN PV-FISCAL-PROPOSED-CCYY    * *06882009
068890* *       BEFORE EXECUTING THIS PARAGRAPH.                     * *06883009
068900* ************************************************************** *06884009
068910 2400-EDIT-FWFP-TO-SAT-IN-FY.                                     06885009
068920                                                                  06886009
068930*    *********************************************************** *06887009
068940*    *  1ST CALL: ESTABLISH A "BEGIN DATE" FOR THE FISCAL YEAR,* *06888009
068950*    *            FROM WHICH OTHER VALUES MAY BE CALCULATED.   * *06889009
068960*    *********************************************************** *06890009
068970     PERFORM 4100-EXPLORE-FISCAL-YEAR.                            06891009
068980                                                                  06892009
068990     IF  PV-EDIT-SW-OK-SO-FAR                                     06893009
069000     AND PV-INTERNAL-PRCSS-OK                                     06894009
069010         PERFORM 2410-CHECK-FISCAL-YEAR                           06895009
069020     END-IF.                                                      06896009
069030                                                                  06897009
069040     IF  PV-EDIT-SW-OK-SO-FAR                                     06898009
069050     AND PV-INTERNAL-PRCSS-OK                                     06899009
069060         COMPUTE PV-CREL-PROCESS-DATE   =                         06900009
069070                 PV-CREL-PROCESS-DATE +                           06901009
069080                 PV-FWFP-ADJUST-CREL                              06902009
069090                                                                  06903009
069100         MOVE PV-CREL-PROCESS-DATE  TO  PV-DAYS-SINCE-BASELINE    06904009
069110         PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD                    06905009
069120         MOVE PV-CCYYMMDD-CCYY-N    TO  PV-FISCAL-PROPOSED-CCYY   06906009
069130                                        PV-CCYYMMDD-FWFP-CCYY-N   06907009
069140         MOVE PV-CCYYMMDD-MM-N      TO  PV-CCYYMMDD-FWFP-MM-N     06908009
069150         MOVE PV-CCYYMMDD-DD-N      TO  PV-CCYYMMDD-FWFP-DD-N     06909009
069160     END-IF.                                                      06910009
069170                                                                  06911009
069180*    *********************************************************** *06912009
069190*    *  2ND CALL: ESTABLISH A "BEGIN DATE" FOR THE FISCAL YEAR,* *06913009
069200*    *            FROM WHICH OTHER VALUES MAY BE CALCULATED.   * *06914009
069210*    *          - ESTABLISH SATURDAY DATE, BASED ON            * *06915009
069220*    *            ADJUSTMENT JUST MADE TO THE PROCESS DATE.    * *06916009
069230*    *********************************************************** *06917009
069240     IF  PV-EDIT-SW-OK-SO-FAR                                     06918009
069250     AND PV-INTERNAL-PRCSS-OK                                     06919009
069260         PERFORM 4100-EXPLORE-FISCAL-YEAR                         06920009
069270     END-IF.                                                      06921009
069280                                                                  06922009
069290     IF  PV-EDIT-SW-OK-SO-FAR                                     06923009
069300     AND PV-INTERNAL-PRCSS-OK                                     06924009
069310         IF  PV-FWFP-INPUT-CCYY-N  =  PV-FISCAL-BEGIN-CCYY-N      06925009
069320             CONTINUE                                             06926009
069330         ELSE                                                     06927009
069340             PERFORM 2410-CHECK-FISCAL-YEAR                       06928009
069350                                                                  06929009
069360             IF  PV-EDIT-SW-OK-SO-FAR                             06930009
069370             AND PV-INTERNAL-PRCSS-OK                             06931009
069380                 COMPUTE PV-CREL-PROCESS-DATE   =                 06932009
069390                         PV-CREL-PROCESS-DATE +                   06933009
069400                         PV-FWFP-ADJUST-CREL                      06934009
069410                                                                  06935009
069420                 MOVE PV-CREL-PROCESS-DATE                        06936009
069430                                       TO  PV-DAYS-SINCE-BASELINE 06937009
069440                 PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD            06938009
069450                 MOVE PV-CCYYMMDD-CCYY-N                          06939009
069460                                       TO  PV-FISCAL-PROPOSED-CCYY06940009
069470                                           PV-CCYYMMDD-FWFP-CCYY-N06941009
069480                 MOVE PV-CCYYMMDD-MM-N TO  PV-CCYYMMDD-FWFP-MM-N  06942009
069490                 MOVE PV-CCYYMMDD-DD-N TO  PV-CCYYMMDD-FWFP-DD-N  06943009
069500                                                                  06944009
069510*                *********************************************** *06945009
069520*                * 3RD CALL: CONDITIONAL. ADJUST YR IF NEEDED. * *06946009
069530*                *********************************************** *06947009
069540                 PERFORM 4100-EXPLORE-FISCAL-YEAR                 06948009
069550             END-IF                                               06949009
069560         END-IF                                                   06950009
069570     END-IF.                                                      06951009
069580                                                                  06952009
069590     IF  PV-EDIT-SW-OK-SO-FAR                                     06953009
069600     AND PV-INTERNAL-PRCSS-OK                                     06954009
069610         IF  PV-THIS-PASS-EDITS-FW                                06955009
069620             IF  PV-EDIT-FW    > PV-NBR-FISCAL-WEEKS              06956009
069630                 SET PV-EDIT-SW-ERROR      TO TRUE                06957009
069640                 MOVE PV-EDIT-FW           TO PV-ERRMSG19-FW      06958009
069650                 MOVE PC-ERRMSG19-INVAL-FW TO DPG54-ERROR-MESSAGE 06959009
069660             END-IF                                               06960009
069670         ELSE                                                     06961009
069680             IF  PV-EDIT-FP    > PC-MAX-PRDS-IN-YR                06962009
069690                 SET PV-EDIT-SW-ERROR      TO TRUE                06963009
069700                 MOVE PV-EDIT-FP           TO PV-ERRMSG20-FP      06964009
069710                MOVE PC-ERRMSG20-INVAL-FP TO DPG54-ERROR-MESSAGE  06965009
069720             END-IF                                               06966009
069730         END-IF                                                   06967009
069740     END-IF.                                                      06968009
069750                                                                  06969009
069760     IF  PV-EDIT-SW-OK-SO-FAR                                     06970009
069770     AND PV-INTERNAL-PRCSS-OK                                     06971009
069780         IF  PV-THIS-PASS-EDITS-FW                                06972009
069790             COMPUTE PV-CREL-PROCESS-DATE      =                  06973009
069800                     PV-CREL-BEGIN-FISCAL-SUN +                   06974009
069810                     (PV-EDIT-FW * PC-DAYS-PER-WEEK) - PC-ONE     06975009
069820         ELSE                                                     06976009
069830             INITIALIZE PV-FISCAL-PRD                             06977009
069840                        PV-FISCAL-WHOLE-QTRS                      06978009
069850                        PV-FISCAL-PRD-REMAINDER                   06979009
069860                        PV-FISCAL-CUM-WKS                         06980009
069870             PERFORM                                              06981009
069880                 PV-EDIT-FP TIMES                                 06982009
069890*                *********************************************** *06983009
069900*                * DETERMINE WEEKS IN PERIOD:                  * *06984009
069910*                *********************************************** *06985009
069920                 ADD PC-ONE TO PV-FISCAL-PRD                      06986009
069930                 DIVIDE        PV-FISCAL-PRD                      06987009
069940                     BY        PC-PRDS-PER-QTR                    06988009
069950                     GIVING    PV-FISCAL-WHOLE-QTRS               06989009
069960                     REMAINDER PV-FISCAL-PRD-REMAINDER            06990009
069970                 IF  PV-FISCAL-PRD = PV-EDIT-FP                   06991009
069980*                    ******************************************* *06992009
069990*                    * SET DATE TO 2ND WEEK OF PERIOD          * *06993009
070000*                    ******************************************* *06994009
070010                     ADD PC-2ND-WK-OF-PRD                         06995009
070020                                        TO PV-FISCAL-CUM-WKS      06996009
070030                 ELSE                                             06997009
070040                     IF DPG51-FISCAL-454-CALENDAR                 06998009
070050*                        *************************************** *06999009
070060*                        * ... FOR "F2" CALENDAR.              * *07000009
070070*                        *************************************** *07001009
070080                         IF PV-FISCAL-PRD-REMAINDER =             07002009
070090                            PC-2ND-PRD-IN-QTR                     07003009
070100                         OR (PV-FISCAL-PRD = PC-MAX-PRDS-IN-YR    07004009
070110                            AND                                   07005009
070120                            PV-NBR-F2-WEEKS = PC-53-FISCAL-WKS)   07006009
070130                             ADD PC-5WK-PRD TO PV-FISCAL-CUM-WKS  07007009
070140                         ELSE                                     07008009
070150                             ADD PC-4WK-PRD TO PV-FISCAL-CUM-WKS  07009009
070160                         END-IF                                   07010009
070170                     END-IF                                       07011009
070180                     IF DPG51-FISCAL-JAN-DEC-F2                   07012009
070190*                        *************************************** *07013009
070200*                        * ... FOR "F4" CALENDAR.              * *07014009
070210*                        *************************************** *07015009
070220                         IF PV-FISCAL-PRD-REMAINDER =             07016009
070230                            PC-3RD-PRD-IN-QTR                     07017009
070240                         OR (PV-FISCAL-PRD = PC-1ST-PRD           07018009
070250                            AND                                   07019009
070260                            PV-NBR-F4-WEEKS = PC-53-FISCAL-WKS)   07020009
070270                             ADD PC-5WK-PRD TO PV-FISCAL-CUM-WKS  07021009
070280                         ELSE                                     07022009
070290                            ADD PC-4WK-PRD TO PV-FISCAL-CUM-WKS   07023009
070300                         END-IF                                   07024009
070310                     END-IF                                       07025009
070320                 END-IF                                           07026009
070330             END-PERFORM                                          07027009
070340             COMPUTE PV-CREL-PROCESS-DATE      =                  07028009
070350                     PV-CREL-BEGIN-FISCAL-SUN +                   07029009
070360                     (PV-FISCAL-CUM-WKS * PC-DAYS-PER-WEEK) -     07030009
070370                     PC-ONE                                       07031009
070380             MOVE PV-CREL-PROCESS-DATE  TO  PV-DAYS-SINCE-BASELINE07032009
070390             PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD                07033009
070400             IF  PV-EDIT-SW-OK-SO-FAR                             07034009
070410             AND PV-INTERNAL-PRCSS-OK                             07035009
070420                 MOVE PV-CCYYMMDD-CCYY-N                          07036009
070430                                        TO PV-FISCAL-PROPOSED-CCYY07037009
070440                                           PV-CCYYMMDD-FWFP-CCYY-N07038009
070450                 MOVE PV-CCYYMMDD-MM-N  TO PV-CCYYMMDD-FWFP-MM-N  07039009
070460                 MOVE PV-CCYYMMDD-DD-N  TO PV-CCYYMMDD-FWFP-DD-N  07040009
070470*            *************************************************** *07041009
070480*            *  4TH CALL: FOR FISCAL PERIOD                    * *07042009
070490*            *************************************************** *07043009
070500                 PERFORM 4100-EXPLORE-FISCAL-YEAR                 07044009
070510             END-IF                                               07045009
070520             IF  PV-EDIT-SW-OK-SO-FAR                             07046009
070530             AND PV-INTERNAL-PRCSS-OK                             07047009
070540                 COMPUTE PV-WK-OF-FSCL-PRD  =                     07048009
070550                      ((PV-CREL-PROCESS-DATE -                    07049009
070560                      PV-CREL-FISCAL-PRD-1ST-DAY) /               07050009
070570                      PC-DAYS-PER-WEEK)                           07051009
070580                      + PC-ONE                                    07052009
070590             END-IF                                               07053009
070600         END-IF                                                   07054009
070610     END-IF.                                                      07055009
070620                                                                  07056009
070630     IF  PV-EDIT-SW-OK-SO-FAR                                     07057009
070640     AND PV-INTERNAL-PRCSS-OK                                     07058009
070650         MOVE PV-CREL-PROCESS-DATE  TO  PV-DAYS-SINCE-BASELINE    07059009
070660         PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD                    07060009
070670     END-IF.                                                      07061009
070680                                                                  07062009
070690     IF  PV-EDIT-SW-OK-SO-FAR                                     07063009
070700     AND PV-INTERNAL-PRCSS-OK                                     07064009
070710         MOVE PV-CCYYMMDD-CCYY-N    TO PV-PROCESS-CCYYMMDD-CCYY-N 07065009
070720                                       PV-CCYYMMDD-FWFP-CCYY-N    07066009
070730         MOVE PV-CCYYMMDD-MM-N      TO PV-PROCESS-CCYYMMDD-MM-N   07067009
070740                                       PV-CCYYMMDD-FWFP-MM-N      07068009
070750         MOVE PV-CCYYMMDD-DD-N      TO PV-PROCESS-CCYYMMDD-DD-N   07069009
070760                                       PV-CCYYMMDD-FWFP-DD-N      07070009
070770     END-IF.                                                      07071009
070780                                                                  07072009
070790                                                                  07073009
070800* ************************************************************** *07074009
070810* * VERIFY WHETHER THE FW/FP PROVIDED AS INPUT FALLS WITHIN    * *07075009
070820* * THE FISCAL YEAR WHOSE CHARACTERISTICS WE'VE JUST EXPLORED. * *07076009
070830* *                                                            * *07077009
070840* * ADJUST DATE IF NEEDED, TO EXPLORE ANOTHER FISCAL YEAR.     * *07078009
070850* * ESTABLISH SATURDAY DATE.                                   * *07079009
070860* ************************************************************** *07080009
070870 2410-CHECK-FISCAL-YEAR.                                          07081009
070880                                                                  07082009
070890     INITIALIZE PV-FWFP-ADJUST-CREL.                              07083009
070900                                                                  07084009
070910     IF  PV-FWFP-INPUT-CCYY-N  <  PV-FISCAL-BEGIN-CCYY-N          07085009
070920             COMPUTE PV-FWFP-ADJUST-CREL  =                       07086009
070930                     PV-FWFP-ADJUST-CREL +                        07087009
070940                     PC-DAYS-IN-52-WEEK-YR                        07088009
070950     ELSE                                                         07089009
070960         IF  PV-FWFP-INPUT-CCYY-N  >  PV-FISCAL-BEGIN-CCYY-N      07090009
070970             COMPUTE PV-FWFP-ADJUST-CREL  =                       07091009
070980                     PV-FWFP-ADJUST-CREL -                        07092009
070990                     PC-DAYS-IN-52-WEEK-YR                        07093009
071000     END-IF.                                                      07094009
071010                                                                  07095009
071020                                                                  07096009
071030     MOVE PV-CREL-PROCESS-DATE       TO PV-DAYS-SINCE-BASELINE.   07097009
071040     PERFORM 5700-DETERMINE-DAY-OF-WEEK.                          07098009
071050     IF PV-DAY-OF-WEEK-X = PC-SATURDAY                            07099009
071060         CONTINUE                                                 07100009
071070     ELSE                                                         07101009
071080         COMPUTE PV-FWFP-ADJUST-CREL  =                           07102009
071090                (PV-FWFP-ADJUST-CREL +                            07103009
071100                (PC-DAYS-PER-WEEK - PV-DAY-OF-WEEK-N))            07104009
071110     END-IF.                                                      07105009
071120                                                                  07106009
071130                                                                  07107009
071140* ************************************************************** *07108009
071150* * THIS PERFORMS VALIDATION ACROSS MULTIPLE INPUT WORKAREAS.  * *07109009
071160* *                                                            * *07110009
071170* * 01) PRIMARY INPUT DATE FORMAT MUST BE COMPATIBLE WITH      * *07111009
071180* *     CALENDAR TYPE SPECIFIED.                               * *07112009
071190* *                                                            * *07113009
071200* * 02) INCR/DECR AND COMPARE FUNCTIONS ARE MUTUALLY EXCLUSIVE * *07114009
071210* *     REQUESTS.                                              * *07115009
071220* *                                                            * *07116009
071230* * 03) INCR/DECR FUNCTIONS CANNOT BE PERFORMED ON SOME        * *07117009
071240* *     PRIMARY INPUT DATE FORMATS.                            * *07118009
071250* *                                                            * *07119009
071260* * 04) IF AN INCR/DECR FUNCTION ENTERED,                      * *07120009
071270* *     A. ONE AND ONLY ONE INCR/DECR TIMEFRAME MUST BE PRESENT. *07121009
071280* *     B. "0" MAY BE A VALID INCR/DECR TIMEFRAME  -AND/OR-    * *07122009
071290* *        MAY SIGNIFY AN INITIALIZED STATE.                   * *07123009
071300* *     C. FURTHER CALENDAR-DEPENDENT EDITS WILL BE PERFORMED  * *07124009
071310* *        FOR COMPATIBILITY OF THE INCR/DECR TIMEFRAME.       * *07125009
071320* *                                                            * *07126009
071330* * ADDITIONAL "ACTUAL" CALENDAR INCR/DECR CROSS-CHECK EDITS:  * *07127009
071340* * -----------------------------=========-------------------  * *07128009
071350* * 05) "FISCAL" INCR/DECR TIMEFRAME MUST NOT BE ENTERED.      * *07129009
071360* * 06) IF AN INCR/DECR FUNCTION ENTERED,                      * *07130009
071370* *     A VALID INCR/DECR TIMEFRAME MUST BE ENTERED            * *07131009
071380* *     (IE- BUSINESS DAYS, CALENDAR DAYS)                     * *07132009
071390* *     (NOTE: SPACE/LOW_VALUES IS GRANDFATHERED IN AS '0'.)   * *07133009
071400* * 07) IF A VALID INCR/DECR TIMEFRAME ENTERED                 * *07134009
071410* *     AN INCR/DECR FUNCTION MUST BE ENTERED.                 * *07135009
071420* *                                                            * *07136009
071430* * ADDITIONAL "ACTUAL" CALENDAR COMPARE CROSS-CHECK EDITS:    * *07137009
071440* * -----------------------------=======-------------------    * *07138009
071450* * 08) "FISCAL" COMPARE DATE AND FORMAT MUST NOT BE ENTERED.  * *07139009
071460* *     (IE- FISCAL WEEKS, FISCAL PERIODS)                     * *07140009
071470* *                                                            * *07141009
071480* *                                                            * *07142009
071490* * ADDITIONAL "FISCAL" CALENDAR INCR/DECR CROSS-CHECK EDITS:  * *07143009
071500* * -----------------------------=========-------------------  * *07144009
071510* * 09) INCR/DECR FW FP FUNCTIONS CAN BE PERFORMED ONLY ON     * *07145009
071520* *     FISCAL WEEK OR FISCAL PERIOD PRIMARY INPUT DATE FORMATS. *07146009
071530* * 10) IF AN INCR/DECR FUNCTION ENTERED,                      * *07147009
071540* *     A VALID INCR/DECR TIMEFRAME MUST BE ENTERED            * *07148009
071550* *     (IE- BUSINESS DAYS, CALENDAR DAYS,                     * *07149009
071560* *          FISCAL WEEKS, FISCAL PERIODS)                     * *07150009
071570* * 11) IF A VALID INCR/DECR TIMEFRAME ENTERED                 * *07151009
071580* *     AN INCR/DECR FUNCTION MUST BE ENTERED.                 * *07152009
071590* * 12) THE INCR/DECR TIMEFRAME MUST BE COMPATIBLE WITH THE    * *07153009
071600* *     FISCAL WEEK OR FISCAL PERIOD PRIMARY INPUT DATE FORMAT.* *07154009
071610* *                                                            * *07155009
071620* * ADDITIONAL "FISCAL" CALENDAR COMPARE CROSS-CHECK EDITS:    * *07156009
071630* * -----------------------------=======-------------------    * *07157009
071640* * 13) "ACTUAL" COMPARE DATE AND FORMAT MUST NOT BE ENTERED   * *07158009
071650* *     IF PRIMARY INPUT FORMAT WAS FW OR FP.                  * *07159009
071660* * 14) THE COMPARE DATE & FORMAT MUST BE COMPATIBLE WITH THE  * *07160009
071670* *     FISCAL WEEK OR FISCAL PERIOD PRIMARY INPUT DATE FORMAT.* *07161009
071680* *                                                            * *07162009
071690* *                                                            * *07163009
071700* * OTHER CROSS-CHECK EDITS MAY BE ADDED IN THE FUTURE.        * *07164009
071710* ************************************************************** *07165009
071720 2500-CROSS-CHECK-EDITS.                                          07166009
071730                                                                  07167009
071740* ************************************************************** *07168009
071750* * 01) PRIMARY INPUT DATE FORMAT MUST BE COMPATIBLE WITH      * *07169009
071760* *     CALENDAR TYPE SPECIFIED.                               * *07170009
071770* ************************************************************** *07171009
071780     EVALUATE TRUE                                                07172009
071790                                                                  07173009
071800         WHEN DPG52-LK-DTE-FISC-WEEK-CC                           07174009
071810         WHEN DPG52-LK-DTE-FISC-WEEK                              07175009
071820         WHEN DPG52-LK-DTE-FISC-PRD-CC                            07176009
071830         WHEN DPG52-LK-DTE-FISC-PRD                               07177009
071840             IF  DPG51-VALID-FISCAL-CALENDAR                      07178009
071850                 CONTINUE                                         07179009
071860             ELSE                                                 07180009
071870                 MOVE PC-ERRMSG-A1-INCOMPATIBLE-CAL               07181009
071880                                        TO DPG54-ERROR-MESSAGE    07182009
071890                 SET  PV-EDIT-SW-ERROR                            07183009
071900                      DPG54-CALENDAR-TYPE-INVALID                 07184009
071910                      DPG54-DATE-INVALID                          07185009
071920                                        TO TRUE                   07186009
071930             END-IF                                               07187009
071940                                                                  07188009
071950     END-EVALUATE.                                                07189009
071960                                                                  07190009
071970* ************************************************************** *07191009
071980* * 02) INCR/DECR AND COMPARE FUNCTIONS ARE MUTUALLY EXCLUSIVE * *07192009
071990* *     REQUESTS.                                              * *07193009
072000* ************************************************************** *07194009
072010     IF  PV-EDIT-SW-OK-SO-FAR                                     07195009
072020     AND PV-INTERNAL-PRCSS-OK                                     07196009
072030         IF  DPG51-INCR-DECR-REQUESTED                            07197009
072040             IF (DPG53-LK-CMPR-DATE-FORMAT-CODE  =  SPACES)       07198009
072050                CONTINUE                                          07199009
072060             ELSE                                                 07200009
072070                MOVE PC-ERRMSG-B1-INVAL-REQUEST                   07201009
072080                                        TO DPG54-ERROR-MESSAGE    07202009
072090                SET  PV-EDIT-SW-ERROR                             07203009
072100                     DPG54-INCR-DECR-INVALID                      07204009
072110                     DPG54-CMPR-DATE-INVALID                      07205009
072120                                        TO TRUE                   07206009
072130             END-IF                                               07207009
072140         END-IF                                                   07208009
072150     END-IF.                                                      07209009
072160                                                                  07210009
072170                                                                  07211009
072180* ************************************************************** *07212009
072190* * 04) IF AN INCR/DECR FUNCTION ENTERED,                      * *07213009
072200* *     A. ONE AND ONLY ONE INCR/DECR TIMEFRAME MUST BE PRESENT. *07214009
072210* *     B. "0" MAY BE A VALID INCR/DECR TIMEFRAME  -AND/OR-    * *07215009
072220* *        MAY SIGNIFY AN INITIALIZED STATE.                   * *07216009
072230* *     C. FURTHER CALENDAR-DEPENDENT EDITS WILL BE PERFORMED  * *07217009
072240* *        FOR COMPATIBILITY OF THE INCR/DECR TIMEFRAME.       * *07218009
072250* ************************************************************** *07219009
072260     IF  PV-EDIT-SW-OK-SO-FAR                                     07220009
072270     AND PV-INTERNAL-PRCSS-OK                                     07221009
072280         IF  DPG51-INCR-DECR-REQUESTED                            07222009
072290             IF  PV-COUNT-VALID-TIMEFRAMES = PC-01                07223009
072300                 CONTINUE                                         07224009
072310             ELSE                                                 07225009
072320                 IF  PV-COUNT-VALID-TIMEFRAMES > PC-01            07226009
072330                     SET DPG54-INCR-DECR-INVALID                  07227009
072340                         PV-EDIT-SW-ERROR  TO TRUE                07228009
072350                     IF  PV-BUSDAYS-VALID                         07229009
072360                     AND PV-CALDAYS-VALID                         07230009
072370                     AND PV-NO-FISCLWK-INCRDECR                   07231009
072380                     AND PV-NO-FISCLPRD-INCRDECR                  07232009
072390                         MOVE PC-ERRMSG-C2-TOO-MANY-INCRDECR      07233009
072400                                            TO DPG54-ERROR-MESSAGE07234009
072410                     ELSE                                         07235009
072420                         MOVE PC-ERRMSG-C1-TOO-MANY-INCRDECR      07236009
072430                                            TO DPG54-ERROR-MESSAGE07237009
072440                     END-IF                                       07238009
072450                 ELSE                                             07239009
072460                     IF  PV-COUNT-0-TIMEFRAMES > ZERO             07240009
072470                         CONTINUE                                 07241009
072480                     ELSE                                         07242009
072490                         IF  PV-NO-BUSDAYS-RQST                   07243009
072500                         OR  PV-NO-CALDAYS-RQST                   07244009
072510                             SET  PV-GF-GRANDFATHERED-ERR         07245009
072520                                            TO TRUE               07246009
072530                         ELSE                                     07247009
072540                             MOVE PC-ERRMSG-D1-NO-ID-TMFRM        07248009
072550                                            TO DPG54-ERROR-MESSAGE07249009
072560                             SET  DPG54-INCR-DECR-INVALID         07250009
072570                                  PV-EDIT-SW-ERROR                07251009
072580                                            TO TRUE               07252009
072590                         END-IF                                   07253009
072600                     END-IF                                       07254009
072610                 END-IF                                           07255009
072620             END-IF                                               07256009
072630         END-IF                                                   07257009
072640     END-IF.                                                      07258009
072650                                                                  07259009
072660                                                                  07260009
072670* ************************************************************** *07261009
072680* * ADDITIONAL EDITS ARE CALENDAR-DEPENDENT.                   * *07262009
072690* ************************************************************** *07263009
072700     IF  PV-EDIT-SW-OK-SO-FAR                                     07264009
072710     AND PV-INTERNAL-PRCSS-OK                                     07265009
072720         IF DPG51-ACTUAL-CALENDAR-ONLY                            07266009
072730             PERFORM 2510-ACTUAL-CALENDAR-XCHECK                  07267009
072740         ELSE                                                     07268009
072750             PERFORM 2520-FISCAL-CALENDAR-XCHECK                  07269009
072760         END-IF                                                   07270009
072770     END-IF.                                                      07271009
072780                                                                  07272009
072790                                                                  07273009
072800* ************************************************************** *07274009
072810* * ADDITIONAL "ACTUAL" CALENDAR INCR/DECR CROSS-CHECK EDITS.  * *07275009
072820* * ADDITIONAL "ACTUAL" CALENDAR COMPARE CROSS-CHECK EDITS.    * *07276009
072830* ************************************************************** *07277009
072840 2510-ACTUAL-CALENDAR-XCHECK.                                     07278009
072850                                                                  07279009
072860* ************************************************************** *07280009
072870* * 05) "FISCAL" INCR/DECR TIMEFRAME MUST NOT BE ENTERED.      * *07281009
072880* * 06) IF AN INCR/DECR FUNCTION ENTERED,                      * *07282009
072890* *     A VALID INCR/DECR TIMEFRAME MUST BE ENTERED            * *07283009
072900* *     (IE- BUSINESS DAYS, CALENDAR DAYS)                     * *07284009
072910* *     (NOTE: SPACE/LOW_VALUES IS GRANDFATHERED IN AS '0'.)   * *07285009
072920* ************************************************************** *07286009
072930                                                                  07287009
072940     IF  PV-EDIT-SW-OK-SO-FAR                                     07288009
072950     AND PV-INTERNAL-PRCSS-OK                                     07289009
072960         IF  DPG51-INCR-DECR-REQUESTED                            07290009
072970             IF  PV-FISCLWK-VALID OR                              07291009
072980                 PV-FISCLPRD-VALID                                07292009
072990*                *********************************************** *07293009
073000*                * FISCAL TIMEFRAME ENTERED                    * *07294009
073010*                *********************************************** *07295009
073020                 MOVE PC-ERRMSG-G1-MISMATCH-TO-A0                 07296009
073030                                            TO DPG54-ERROR-MESSAGE07297009
073040                 SET PV-EDIT-SW-ERROR                             07298009
073050                     DPG54-INCR-DECR-INVALID                      07299009
073060                                            TO TRUE               07300009
073070             END-IF                                               07301009
073080         END-IF                                                   07302009
073090     END-IF.                                                      07303009
073100                                                                  07304009
073110                                                                  07305009
073120     IF  PV-EDIT-SW-OK-SO-FAR                                     07306009
073130     AND PV-INTERNAL-PRCSS-OK                                     07307009
073140         IF  DPG51-INCR-DECR-REQUESTED                            07308009
073150             IF  PV-BUSDAYS-VALID                                 07309009
073160             OR  PV-CALDAYS-VALID                                 07310009
073170             OR  PV-BUSDAYS-ZERO                                  07311009
073180             OR  PV-CALDAYS-ZERO                                  07312009
073190                 CONTINUE                                         07313009
073200             ELSE                                                 07314009
073210                 IF  PV-NO-BUSDAYS-RQST                           07315009
073220                 OR  PV-NO-CALDAYS-RQST                           07316009
073230                     SET  PV-GF-GRANDFATHERED-ERR                 07317009
073240                                            TO TRUE               07318009
073250                 ELSE                                             07319009
073260*                    ******************************************* *07320009
073270*                    * NEITHER ENTERED                         * *07321009
073280*                    ******************************************* *07322009
073290                     MOVE PC-ERRMSG-D2-NO-ID-TMFRM                07323009
073300                                            TO DPG54-ERROR-MESSAGE07324009
073310                     SET DPG54-INCR-DECR-INVALID                  07325009
073320                         PV-EDIT-SW-ERROR   TO TRUE               07326009
073330                 END-IF                                           07327009
073340             END-IF                                               07328009
073350          END-IF                                                  07329009
073360      END-IF.                                                     07330009
073370                                                                  07331009
073380                                                                  07332009
073390* ************************************************************** *07333009
073400* * 07) IF A VALID INCR/DECR TIMEFRAME ENTERED                 * *07334009
073410* *     AN INCR/DECR FUNCTION MUST BE ENTERED.                 * *07335009
073420* ************************************************************** *07336009
073430     IF  PV-EDIT-SW-OK-SO-FAR                                     07337009
