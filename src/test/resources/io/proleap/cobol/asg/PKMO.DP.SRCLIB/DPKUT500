075950                                                                  07589009
075960                                                                  07590009
075970* ************************************************************** *07591009
075980* * 13) "ACTUAL" COMPARE DATE AND FORMAT MUST NOT BE ENTERED   * *07592009
075990* *     IF PRIMARY INPUT FORMAT WAS FW OR FP.                  * *07593009
076000* ************************************************************** *07594009
076010     IF  PV-EDIT-SW-OK-SO-FAR                                     07595009
076020     AND PV-INTERNAL-PRCSS-OK                                     07596009
076030         IF DPG53-LK-CMPR-DATE-FORMAT-CODE = SPACES               07597009
076040             CONTINUE                                             07598009
076050         ELSE                                                     07599009
076060             EVALUATE TRUE                                        07600009
076070                                                                  07601009
076080                 WHEN DPG53-LK-CMPR-DTE-ISO-FMT                   07602009
076090                 WHEN DPG53-LK-CMPR-DTE-ISO                       07603009
076100                 WHEN DPG53-LK-CMPR-DTE-GREG-CC-FMT               07604009
076110                 WHEN DPG53-LK-CMPR-DTE-GREG-CC                   07605009
076120                 WHEN DPG53-LK-CMPR-DTE-GREG-FMT                  07606009
076130                 WHEN DPG53-LK-CMPR-DTE-GREG                      07607009
076140                 WHEN DPG53-LK-CMPR-DTE-JULIAN-CC                 07608009
076150                 WHEN DPG53-LK-CMPR-DTE-JULIAN-FMT                07609009
076160                 WHEN DPG53-LK-CMPR-DTE-JULIAN                    07610009
076170                 IF DPG52-LK-DTE-FISC-WEEK-CC                     07611009
076180                 OR DPG52-LK-DTE-FISC-WEEK                        07612009
076190                 OR DPG52-LK-DTE-FISC-PRD-CC                      07613009
076200                 OR DPG52-LK-DTE-FISC-PRD                         07614009
076210*                    ******************************************* *07615009
076220*                    * COMPARE DATE NOT IN A FISCAL FORMAT     * *07616009
076230*                    * BUT PRIMARY INPUT IS                    * *07617009
076240*                    ******************************************* *07618009
076250                     MOVE PC-ERRMSG-H3-MISMATCH-TO-FISCL          07619009
076260                                            TO DPG54-ERROR-MESSAGE07620009
076270                     SET  PV-EDIT-SW-ERROR  TO TRUE               07621009
076280                 END-IF                                           07622009
076290             END-EVALUATE                                         07623009
076300         END-IF                                                   07624009
076310     END-IF.                                                      07625009
076320                                                                  07626009
076330                                                                  07627009
076340* ************************************************************** *07628009
076350* * 14) THE COMPARE DATE & FORMAT MUST BE COMPATIBLE WITH THE  * *07629009
076360* *     FISCAL WEEK OR FISCAL PERIOD PRIMARY INPUT DATE FORMAT.* *07630009
076370* ************************************************************** *07631009
076380     IF  PV-EDIT-SW-OK-SO-FAR                                     07632009
076390     AND PV-INTERNAL-PRCSS-OK                                     07633009
076400         IF DPG53-LK-CMPR-DATE-FORMAT-CODE = SPACES               07634009
076410             CONTINUE                                             07635009
076420         ELSE                                                     07636009
076430             IF DPG53-LK-CMPR-DTE-FISC-WEEK-CC                    07637009
076440             OR DPG53-LK-CMPR-DTE-FISC-WEEK                       07638009
076450                 IF DPG52-LK-DTE-FISC-WEEK-CC                     07639009
076460                 OR DPG52-LK-DTE-FISC-WEEK                        07640009
076470                     CONTINUE                                     07641009
076480                 ELSE                                             07642009
076490                     MOVE PC-ERRMSG-I1-FW-FP-MISMATCH             07643009
076500                                            TO DPG54-ERROR-MESSAGE07644009
076510                     SET PV-EDIT-SW-ERROR   TO TRUE               07645009
076520                 END-IF                                           07646009
076530             END-IF                                               07647009
076540             IF DPG53-LK-CMPR-DTE-FISC-PRD-CC                     07648009
076550             OR DPG53-LK-CMPR-DTE-FISC-PRD                        07649009
076560                 IF DPG52-LK-DTE-FISC-PRD-CC                      07650009
076570                 OR DPG52-LK-DTE-FISC-PRD                         07651009
076580                     CONTINUE                                     07652009
076590                 ELSE                                             07653009
076600                     MOVE PC-ERRMSG-J1-FP-FW-MISMATCH             07654009
076610                                            TO DPG54-ERROR-MESSAGE07655009
076620                     SET PV-EDIT-SW-ERROR   TO TRUE               07656009
076630                 END-IF                                           07657009
076640             END-IF                                               07658009
076650         END-IF                                                   07659009
076660     END-IF.                                                      07660009
076670                                                                  07661009
076680                                                                  07662009
076690* ************************************************************** *07663009
076700* * THIS PERFORMS DATE FUNCTIONS BASED ON:                     * *07664009
076710* * 1) THE ACTUAL GREGORIAN CALENDAR                           * *07665009
076720* * 2) A 7-DAY-WEEK BEGINNING ON SUNDAY AND ENDING ON SATURDAY.* *07666009
076730* *                                                            * *07667009
076740* ************************************************************** *07668009
076750* * DATE DIFFERENCES FOR INCR/DECR AND COMPARE ARE CALCULATED  * *07669009
076760* * FOLLOWING THESE RULES:                                     * *07670009
076770* *                                                            * *07671009
076780* * RULES WHICH APPLY TO BOTH INCR/DECR AND COMPARE:           * *07672009
076790* * ********************************************************** * *07673009
076800* * - THE PRIMARY DATE IS NOT COUNTED                          * *07674009
076810* *                                                            * *07675009
076820* * ADDITIONAL RULES WHICH APPLY TO COMPARE:                   * *07676009
076830* * ********************************************************** * *07677009
076840* * - WHEN THE PRIMARY DATE IS GREATER THAN THE COMPARE DATE,  * *07678009
076850* *   THE DIFFERENCE IS EXPRESSED AS A POSITIVE NUMBER OF DAYS.* *07679009
076860* * - WHEN THE PRIMARY DATE IS LESS THAN THE COMPARE DATE,     * *07680009
076870* *   THE DIFFERENCE IS EXPRESSED AS A NEGATIVE NUMBER OF DAYS.* *07681009
076880* *                                                            * *07682009
076890* * ADDITIONAL RULES WHICH APPLY TO BOTH INCR/DECR AND COMPARE * *07683009
076900* * OF BUSINESS DAYS:                                          * *07684009
076910* * ********************************************************** * *07685009
076920* * - A DATE FALLING ON A WEEKEND IS NOT COUNTED.              * *07686009
076930* * - A DATE FALLING ON AN ACTUAL HOLIDAY IS NOT COUNTED.      * *07687009
076940* * - A DATE FALLING ON A "CORPORATE" HOLIDAY IS NOT COUNTED.  * *07688009
076950* *                                                            * *07689009
076960* * EXAMPLES: INCR/DECR AND COMPARE FOR "ACTUAL" DAYS          * *07690009
076970* * ********************************************************** * *07691009
076980* * EXAMPLES OF RESULTS, GIVEN 2 CONSECUTIVE DATES,            * *07692009
076990* * "A" AND "B", WHERE "A" IS THE DAY BEFORE "B":              * *07693009
077000* * 01. INCR: PRIM DATE "A" + 1 ACTUAL DAY = DATE "B".         * *07694009
077010* * 02. DECR: PRIM DATE "B" - 1 ACTUAL DAY = DATE "A".         * *07695009
077020* * 03. CMPR: PRIM DATE "A", CMPR DATE "B" = -1 ACTUAL.        * *07696009
077030* * 04. CMPR: PRIM DATE "B", CMPR DATE "A" = +1 ACTUAL.        * *07697009
077040* *                                                            * *07698009
077050* *     NOTES ON "ACTUAL" DAYS COMPARE RESULTS:                * *07699009
077060* *     A. 'COUNT OF DAYS' COMPONENT OF RESULTS.               * *07700009
077070* *        WHEN DEALING IN ACTUAL DAYS, THE RESULT WILL        * *07701009
077080* *        YIELD THE SAME COUNT OF DAYS (WITH ONLY A CHANGE IN * *07702009
077090* *        SIGN +/-) REGARDLESS OF WHICH DATE IS <PRIMARY> OR  * *07703009
077100* *        <COMPARE>.                                          * *07704009
077110* *        (SEE EXAMPLES 3&4.)                                 * *07705009
077120* *     B. 'SIGN' COMPONENT OF RESULTS.                        * *07706009
077130* *        THINK OF THE COMPARE'S SIGN AS RESULTING FROM:      * *07707009
077140* *        "WHAT IS DONE TO CMPR DT TO ARRIVE AT PRIMARY DT?"  * *07708009
077150* *        THIS WILL TELL YOU WHETHER TO EXPECT "+" OR "-".    * *07709009
077160* *        (SEE EXAMPLES 3&4.)                                 * *07710009
077170* *                                                            * *07711009
077180* * EXAMPLES: INCR/DECR AND COMPARE FOR "BUSINESS" DAYS        * *07712009
077190* * ********************************************************** * *07713009
077200* *   EXAMPLES OF RESULTS, GIVEN 4 CONSECUTIVE NON-HOLIDAY DATES *07714009
077210* *   FRI"D", SAT"E", SUN"F", MON"G":                          * *07715009
077220* *   05. INCR: PRIM DT "F" (SUN) + 1 BUS DAY = DT "G" (MON).  * *07716009
077230* *   06. DECR: PRIM DT "G" (MON) - 1 BUS DAY = DT "D" (FRI).  * *07717009
077240* *   07. CMPR: PRIM DT "F" (SUN), CMPR DT "G" (MON) = -1BUS.  * *07718009
077250* *   08. CMPR: PRIM DT "G" (MON), CMPR DT "F" (SUN) =  0BUS.  * *07719009
077260* *   09. CMPR: PRIM DT "G" (MON), CMPR DT "D" (FRI) = +1BUS.  * *07720009
077270* *   10. CMPR: PRIM DT "F" (SUN), CMPR DT "D" (FRI) = +1BUS.  * *07721009
077280* *                                                            * *07722009
077290* *   EXAMPLES OF RESULTS, GIVEN 4 CONSECUTIVE DATES,          * *07723009
077300* *   FRI"D", SAT"E", SUN"F", MON"G", WHERE MONDAY IS A HOLIDAY  *07724009
077310* *   ("ACTUAL" -OR- "CORPORATE").                             * *07725009
077320* *   11. INCR: PRIM DT "F" (SUN) + 1 BUS DAY = DT "H" (TUE).  * *07726009
077330* *   12. DECR: PRIM DT "G" (MON) - 1 BUS DAY = DT "D" (FRI).  * *07727009
077340* *   13. CMPR: PRIM DT "F" (SUN), CMPR DT "G" (MON) =  0BUS.  * *07728009
077350* *   14. CMPR: PRIM DT "G" (MON), CMPR DT "F" (SUN) =  0BUS.  * *07729009
077360* *   15. CMPR: PRIM DT "G" (MON), CMPR DT "D" (FRI) = +1BUS.  * *07730009
077370* *   16. CMPR: PRIM DT "F" (SUN), CMPR DT "D" (FRI) = +1BUS.  * *07731009
077380* *                                                            * *07732009
077390* *   EXAMPLES OF RESULTS, GIVEN 4 CONSECUTIVE DATES,          * *07733009
077400* *   FRI"D", SAT"E", SUN"F", MON"G", WHERE FRIDAY IS A HOLIDAY  *07734009
077410* *   ("ACTUAL" -OR- "CORPORATE").                             * *07735009
077420* *   17. INCR: PRIM DT "F" (SUN) + 1 BUS DAY = DT "G" (MON).  * *07736009
077430* *   18. DECR: PRIM DT "G" (MON) - 1 BUS DAY = DT "C" (THURS).* *07737009
077440* *   19. CMPR: PRIM DT "F" (SUN), CMPR DT "G" (MON) = -1BUS.  * *07738009
077450* *   20. CMPR: PRIM DT "G" (MON), CMPR DT "F" (SUN) =  0BUS.  * *07739009
077460* *   21. CMPR: PRIM DT "G" (MON), CMPR DT "D" (FRI) =  0BUS.  * *07740009
077470* *   22. CMPR: PRIM DT "F" (SUN), CMPR DT "D" (FRI) =  0BUS.  * *07741009
077480* *                                                            * *07742009
077490* *     NOTES ON "BUSINESS" DAYS COMPARE RESULTS:              * *07743009
077500* *     A. 'COUNT OF DAYS' COMPONENT OF RESULTS.               * *07744009
077510* *        UNLIKE THE RULES FOR ACTUAL DAYS, THE RULES FOR     * *07745009
077520* *        BUSINESS DAYS WILL *NOT* NECESSARILY YIELD THE SAME * *07746009
077530* *        COUNT OF DAYS (WITH ONLY A CHANGE IN SIGN +/-)      * *07747009
077540* *        WHEN GIVEN THE INVERSE TEST CASE.                   * *07748009
077550* *        THEREFORE "COMPARE" CALLS MUST BE THOUGHTFULLY      * *07749009
077560* *        CONSTRUCTED AS TO WHICH DATE IS <PRIMARY> OR <COMPARE>.07750009
077570* *        (SEE EXAMPLES 7&8, 13&14, 19&20.)                   * *07751009
077580* *     B. 'SIGN' COMPONENT OF RESULTS.                        * *07752009
077590* *        THINK OF THE COMPARE'S SIGN AS RESULTING FROM:      * *07753009
077600* *        "WHAT IS DONE TO CMPR DT TO ARRIVE AT PRIMARY DT?"  * *07754009
077610* *        THIS WILL TELL YOU WHETHER TO EXPECT "+" OR "-".    * *07755009
077620* *        (SEE EXAMPLES 7&8, 9&10, 13&14, 15&16, 19&20, 21&22)* *07756009
077630* *                                                            * *07757009
077640* ************************************************************** *07758009
077650 3000-ACTUAL-DATES.                                               07759009
077660                                                                  07760009
077670     IF  (DPG51-INCREMENT-DATE  OR                                07761009
077680          DPG51-DECREMENT-DATE)                                   07762009
077690         PERFORM 3100-ACTUAL-CAL-INCR-DECR                        07763009
077700     ELSE                                                         07764009
077710         IF  DPG53-LK-CMPR-DATE-FORMAT-CODE  NOT =  SPACES        07765009
077720             PERFORM 3500-ACTUAL-CAL-COMPARE                      07766009
077730         END-IF                                                   07767009
077740     END-IF.                                                      07768009
077750                                                                  07769009
077760     IF  PV-EDIT-SW-ERROR                                         07770009
077770     OR  PV-INTERNAL-PRCSS-ERR                                    07771009
077780         SET DPG54-SEVERE-ERROR        TO TRUE                    07772009
077790         IF DPG54-ERROR-MESSAGE = SPACES                          07773009
077800             MOVE PC-ERRMSG98-ACT-DTS-ERR                         07774009
077810                                       TO DPG54-ERROR-MESSAGE     07775009
077820         END-IF                                                   07776009
077830     END-IF.                                                      07777009
077840                                                                  07778009
077850     PERFORM 3800-POPULATE-DPG55-OUTPARMS.                        07779009
077860                                                                  07780009
077870     IF  PV-EDIT-SW-ERROR                                         07781009
077880     OR  PV-INTERNAL-PRCSS-ERR                                    07782009
077890         SET DPG54-SEVERE-ERROR        TO TRUE                    07783009
077900         IF DPG54-ERROR-MESSAGE = SPACES                          07784009
077910             MOVE PC-ERRMSG55-55OUTPARM-ERR                       07785009
077920                                       TO DPG54-ERROR-MESSAGE     07786009
077930         END-IF                                                   07787009
077940     END-IF.                                                      07788009
077950                                                                  07789009
077960                                                                  07790009
077970* ************************************************************** *07791009
077980* * THIS CONTROLS PROCESSING TO INCREMENT/DECREMENT THE        * *07792009
077990* * PRIMARY INPUT DATE BY THE SPECIFIED NUMBER OF:             * *07793009
078000* * - BUSINESS DAYS, OR                                        * *07794009
078010* * - CALENDAR DAYS, OR                                        * *07795009
078020* * - FISCAL WEEKS,  OR                                        * *07796009
078030* * - FISCAL PERIODS.                                          * *07797009
078040* ************************************************************** *07798009
078050 3100-ACTUAL-CAL-INCR-DECR.                                       07799009
078060                                                                  07800009
078070     IF  PV-NO-BUSDAYS-INCRDECR                                   07801009
078080     AND PV-NO-CALDAYS-INCRDECR                                   07802009
078090     AND PV-NO-FISCLWK-INCRDECR                                   07803009
078100     AND PV-NO-FISCLPRD-INCRDECR                                  07804009
078110         MOVE PV-CREL-PRIMARY-INPUT TO                            07805009
078120              PV-CREL-PRIMARY-INCR-DECR                           07806009
078130     END-IF.                                                      07807009
078140                                                                  07808009
078150*    *********************************************************** *07809009
078160*    * CALENDAR DAYS                                           * *07810009
078170*    *********************************************************** *07811009
078180     IF  DPG51-INCR-DECR-DAYS-X    NUMERIC                        07812009
078190         IF  PV-CALDAYS-VALID                                     07813009
078200             IF DPG51-INCREMENT-DATE                              07814009
078210                 COMPUTE PV-CREL-PRIMARY-INCR-DECR =              07815009
078220                         PV-CREL-PRIMARY-INPUT     +              07816009
078230                         DPG51-INCR-DECR-DAYS-9                   07817009
078240             ELSE                                                 07818009
078250                 COMPUTE PV-CREL-PRIMARY-INCR-DECR =              07819009
078260                         PV-CREL-PRIMARY-INPUT     -              07820009
078270                         DPG51-INCR-DECR-DAYS-9                   07821009
078280             END-IF                                               07822009
078290         END-IF                                                   07823009
078300     END-IF.                                                      07824009
078310                                                                  07825009
078320*    *********************************************************** *07826009
078330*    * BUSINESS DAYS                                           * *07827009
078340*    *********************************************************** *07828009
078350     IF  DPG51-INCR-DECR-BUS-DAYS-X    NUMERIC                    07829009
078360         IF  PV-BUSDAYS-VALID                                     07830009
078370             PERFORM 3200-IDENTIFY-INCRDECR-WKNDHOL               07831009
078380         END-IF                                                   07832009
078390     END-IF.                                                      07833009
078400                                                                  07834009
078410*    *********************************************************** *07835009
078420*    * FISCAL WEEKS                                            * *07836009
078430*    *********************************************************** *07837009
078440     IF  DPG51-INCR-DECR-FISCAL-WEEK-X NUMERIC                    07838009
078450         IF  PV-FISCLWK-VALID                                     07839009
078460             IF DPG51-INCREMENT-DATE                              07840009
078470                 COMPUTE PV-CREL-PRIMARY-INCR-DECR =              07841009
078480                         PV-CREL-PRIMARY-INPUT     +              07842009
078490                        (DPG51-INCR-DECR-FISCAL-WEEK-9 *          07843009
078500                         PC-DAYS-PER-WEEK)                        07844009
078510             ELSE                                                 07845009
078520                 COMPUTE PV-CREL-PRIMARY-INCR-DECR =              07846009
078530                         PV-CREL-PRIMARY-INPUT     -              07847009
078540                        (DPG51-INCR-DECR-FISCAL-WEEK-9 *          07848009
078550                         PC-DAYS-PER-WEEK)                        07849009
078560             END-IF                                               07850009
078570         END-IF                                                   07851009
078580     END-IF.                                                      07852009
078590                                                                  07853009
078600*    *********************************************************** *07854009
078610*    * FISCAL PERIODS                                          * *07855009
078620*    *********************************************************** *07856009
078630     IF  DPG51-INCR-DECR-FISCAL-PRD-X NUMERIC                     07857009
078640         IF  PV-FISCLPRD-VALID                                    07858009
078650             IF DPG51-INCREMENT-DATE                              07859009
078660                PERFORM 3110-FP-INCR-LOOK-FORWARD                 07860009
078670             ELSE                                                 07861009
078680                PERFORM 3120-FP-DECR-LOOK-BACK                    07862009
078690             END-IF                                               07863009
078700         END-IF                                                   07864009
078710     END-IF.                                                      07865009
078720                                                                  07866009
078730*    *********************************************************** *07867009
078740*    * SAVE RESULTS                                            * *07868009
078750*    *********************************************************** *07869009
078760     PERFORM 3300-HOLD-INCR-DECRD-DATE.                           07870009
078770                                                                  07871009
078780                                                                  07872009
078790* ************************************************************** *07873009
078800* * FISCAL PERIOD INCREMENT; LOOK FORWARD.                     * *07874009
078810* * 1) INCREMENT THE INPUT FISCAL YEAR AND PERIOD              * *07875009
078820* *    BY THE NUMBER OF FISCAL PERIODS REQUESTED,              * *07876009
078830* * 2) THEN CONVERT THIS TO 2ND SATURDAY OF THE FISCAL PERIOD. * *07877009
078840* ************************************************************** *07878009
078850 3110-FP-INCR-LOOK-FORWARD.                                       07879009
078860                                                                  07880009
078870     INITIALIZE PV-FP-ID-TEST-PRD                                 07881009
078880                PV-FISCAL-PRD-REMAINDER.                          07882009
078890                                                                  07883009
078900     COMPUTE PV-FP-ID-TEST-PRD =                                  07884009
078910             DPG52-FISCAL-CC-PRD-PP +                             07885009
078920             DPG51-INCR-DECR-FISCAL-PRD-9.                        07886009
078930                                                                  07887009
078940     IF  PV-FP-ID-TEST-PRD > PC-MAX-PRDS-IN-YR                    07888009
078950         COMPUTE PV-FP-ID-NET-PERIODS   =                         07889009
078960                 DPG51-INCR-DECR-FISCAL-PRD-9 -                   07890009
078970                 PC-MAX-PRDS-IN-YR +                              07891009
078980                 DPG52-FISCAL-CC-PRD-PP                           07892009
078990         DIVIDE  PV-FP-ID-NET-PERIODS                             07893009
079000                 BY        PC-MAX-PRDS-IN-YR                      07894009
079010                 GIVING    PV-FP-ID-YRS                           07895009
079020                 REMAINDER PV-FP-ID-PRD                           07896009
079030         IF  PV-FP-ID-PRD     = ZERO                              07897009
079040             MOVE PC-MAX-PRDS-IN-YR  TO  PV-FP-ID-PRD             07898009
079050         ELSE                                                     07899009
079060             ADD  PC-ONE             TO  PV-FP-ID-YRS             07900009
079070         END-IF                                                   07901009
079080         COMPUTE PV-FP-ID-RESULT-CCYY =                           07902009
079090                 DPG52-FISCAL-CC-PRD-YR +                         07903009
079100                 PV-FP-ID-YRS                                     07904009
079110         MOVE    PV-FP-ID-PRD        TO PV-FP-ID-RESULT-PRD       07905009
079120         IF PV-FP-ID-RESULT-CCYY > PC-HT-MAX-YEAR                 07906009
079130             MOVE PC-ERRMSG95-MAX-DATE-RANGE                      07907009
079140                                     TO DPG54-ERROR-MESSAGE       07908009
079150             SET PV-EDIT-SW-ERROR                                 07909009
079160                 DPG54-SEVERE-ERROR  TO TRUE                      07910009
079170         END-IF                                                   07911009
079180     ELSE                                                         07912009
079190         MOVE DPG52-FISCAL-CC-PRD-YR TO PV-FP-ID-RESULT-CCYY      07913009
079200         MOVE PV-FP-ID-TEST-PRD      TO PV-FP-ID-RESULT-PRD       07914009
079210     END-IF.                                                      07915009
079220                                                                  07916009
079230     IF  PV-EDIT-SW-OK-SO-FAR                                     07917009
079240     AND PV-INTERNAL-PRCSS-OK                                     07918009
079250         PERFORM 3160-CONVERT-FISCAL-PERIOD                       07919009
079260     END-IF.                                                      07920009
079270                                                                  07921009
079280                                                                  07922009
079290* ************************************************************** *07923009
079300* * FISCAL PERIOD DECREMENT; LOOK BACK.                        * *07924009
079310* * 1) DECREMENT THE INPUT FISCAL YEAR AND PERIOD              * *07925009
079320* *    BY THE NUMBER OF FISCAL PERIODS REQUESTED,              * *07926009
079330* * 2) THEN CONVERT THIS TO 2ND SATURDAY OF THE FISCAL PERIOD. * *07927009
079340* ************************************************************** *07928009
079350 3120-FP-DECR-LOOK-BACK.                                          07929009
079360                                                                  07930009
079370     INITIALIZE PV-FP-ID-TEST-PRD                                 07931009
079380                PV-FISCAL-PRD-REMAINDER.                          07932009
079390                                                                  07933009
079400     COMPUTE PV-FP-ID-TEST-PRD =                                  07934009
079410             DPG52-FISCAL-CC-PRD-PP -                             07935009
079420             DPG51-INCR-DECR-FISCAL-PRD-9.                        07936009
079430                                                                  07937009
079440     IF  PV-FP-ID-TEST-PRD < PC-01                                07938009
079450         COMPUTE PV-FP-ID-NET-PERIODS   =                         07939009
079460                 (DPG51-INCR-DECR-FISCAL-PRD-9 -                  07940009
079470                 DPG52-FISCAL-CC-PRD-PP)                          07941009
079480         DIVIDE  PV-FP-ID-NET-PERIODS                             07942009
079490                 BY        PC-MAX-PRDS-IN-YR                      07943009
079500                 GIVING    PV-FP-ID-YRS                           07944009
079510                 REMAINDER PV-FP-ID-PRD                           07945009
079520         ADD PC-ONE  TO  PV-FP-ID-YRS                             07946009
079530         COMPUTE PV-FP-ID-RESULT-CCYY =                           07947009
079540                 DPG52-FISCAL-CC-PRD-YR -                         07948009
079550                 PV-FP-ID-YRS                                     07949009
079560         COMPUTE PV-FP-ID-RESULT-PRD =                            07950009
079570                 PC-MAX-MONTH        -                            07951009
079580                 PV-FP-ID-PRD                                     07952009
079590         IF PV-FP-ID-RESULT-CCYY < PC-HT-BASELINE-YEAR            07953009
079600             MOVE PC-ERRMSG94-MIN-DATE-RANGE                      07954009
079610                                     TO DPG54-ERROR-MESSAGE       07955009
079620             SET PV-EDIT-SW-ERROR                                 07956009
079630                 DPG54-SEVERE-ERROR  TO TRUE                      07957009
079640         END-IF                                                   07958009
079650     ELSE                                                         07959009
079660         MOVE DPG52-FISCAL-CC-PRD-YR TO PV-FP-ID-RESULT-CCYY      07960009
079670         MOVE PV-FP-ID-TEST-PRD      TO PV-FP-ID-RESULT-PRD       07961009
079680     END-IF.                                                      07962009
079690                                                                  07963009
079700     IF  PV-EDIT-SW-OK-SO-FAR                                     07964009
079710     AND PV-INTERNAL-PRCSS-OK                                     07965009
079720         PERFORM 3160-CONVERT-FISCAL-PERIOD                       07966009
079730     END-IF.                                                      07967009
079740                                                                  07968009
079750                                                                  07969009
079760* ************************************************************** *07970009
079770* * CONVERT THE INCR/DECR FISCAL PERIOD RESULT, TO AN          * *07971009
079780* * "ACTUAL DATE" FOR FURTHER "ACTUAL CALENDAR" PROCESSING.    * *07972009
079790* *                                                            * *07973009
079800* * - VALUE   OF DATE WILL BE: 2ND SATURDAY OF THE FISCAL PERIOD.*07974009
079810* * - FORMATS OF DATE WILL BE:                                 * *07975009
079820* *   1) CREL (DAYS SINCE BASELINE)                            * *07976009
079830* *   2) WORKING FORMAT (CCYYMMDD)                             * *07977009
079840* ************************************************************** *07978009
079850 3160-CONVERT-FISCAL-PERIOD.                                      07979009
079860                                                                  07980009
079870     MOVE PV-FP-ID-RESULT-CCYY     TO  PV-FWFP-INPUT-CCYY-N       07981009
079880                                       PV-FISCAL-PROPOSED-CCYY    07982009
079890                                       PV-CCYYMMDD-CCYY-N         07983009
079900                                       PV-CCYYMMDD-FWFP-CCYY-N    07984009
079910                                       PV-CCYYMMDD-FWFP-ID-CCYY-N.07985009
079920                                                                  07986009
079930     MOVE PV-FP-ID-RESULT-PRD      TO PV-EDIT-FP                  07987009
079940                                      PV-CCYYMMDD-MM-N            07988009
079950                                      PV-CCYYMMDD-FWFP-MM-N       07989009
079960                                      PV-CCYYMMDD-FWFP-ID-MM-N.   07990009
079970                                                                  07991009
079980     IF DPG51-FISCAL-JAN-DEC-F2                                   07992009
079990         IF PV-FP-ID-RESULT-PRD    = PC-1ST-PRD                   07993009
080000             COMPUTE PV-CCYYMMDD-MM-N =                           07994009
080010                  PV-FP-ID-RESULT-PRD    + PC-ONE                 07995009
080020             MOVE PV-CCYYMMDD-MM-N TO PV-CCYYMMDD-FWFP-MM-N       07996009
080030                                      PV-CCYYMMDD-FWFP-ID-MM-N    07997009
080040         ELSE                                                     07998009
080050             MOVE PV-FP-ID-RESULT-PRD                             07999009
080060                                   TO PV-CCYYMMDD-MM-N            08000009
080070                                      PV-CCYYMMDD-FWFP-MM-N       08001009
080080                                      PV-CCYYMMDD-FWFP-ID-MM-N    08002009
080090         END-IF                                                   08003009
080100     ELSE                                                         08004009
080110         IF PV-FP-ID-RESULT-PRD  = PC-MAX-PRDS-IN-YR              08005009
080120            MOVE PC-1ST-PRD        TO PV-CCYYMMDD-MM-N            08006009
080130                                      PV-CCYYMMDD-FWFP-MM-N       08007009
080140                                      PV-CCYYMMDD-FWFP-ID-MM-N    08008009
080150            ADD PC-ONE             TO PV-FISCAL-PROPOSED-CCYY     08009009
080160                                      PV-CCYYMMDD-CCYY-N          08010009
080170                                      PV-CCYYMMDD-FWFP-CCYY-N     08011009
080180                                      PV-CCYYMMDD-FWFP-ID-CCYY-N  08012009
080190         ELSE                                                     08013009
080200            COMPUTE PV-CCYYMMDD-MM-N =                            08014009
080210                PV-FP-ID-RESULT-PRD    + PC-ONE                   08015009
080220            MOVE PV-CCYYMMDD-MM-N  TO PV-CCYYMMDD-FWFP-MM-N       08016009
080230                                      PV-CCYYMMDD-FWFP-ID-MM-N    08017009
080240         END-IF                                                   08018009
080250     END-IF.                                                      08019009
080260                                                                  08020009
080270     MOVE PC-15TH-DAY              TO PV-CCYYMMDD-DD-N            08021009
080280                                      PV-CCYYMMDD-FWFP-DD-N       08022009
080290                                      PV-CCYYMMDD-FWFP-ID-DD-N.   08023009
080300                                                                  08024009
080310     IF  PV-EDIT-SW-OK-SO-FAR                                     08025009
080320     AND PV-INTERNAL-PRCSS-OK                                     08026009
080330         PERFORM 5200-CONVERT-CCYYMMDD-TO-DAYS                    08027009
080340     ELSE                                                         08028009
080350         SET PV-EDIT-SW-ERROR                                     08029009
080360             DPG54-DATE-INVALID    TO TRUE                        08030009
080370     END-IF.                                                      08031009
080380                                                                  08032009
080390     IF  PV-EDIT-SW-OK-SO-FAR                                     08033009
080400     AND PV-INTERNAL-PRCSS-OK                                     08034009
080410         MOVE PV-DAYS-SINCE-BASELINE                              08035009
080420                                   TO PV-CREL-PRIMARY-INCR-DECR   08036009
080430                                      PV-CREL-PROCESS-DATE        08037009
080440     ELSE                                                         08038009
080450         SET PV-EDIT-SW-ERROR                                     08039009
080460             DPG54-DATE-INVALID    TO TRUE                        08040009
080470     END-IF.                                                      08041009
080480                                                                  08042009
