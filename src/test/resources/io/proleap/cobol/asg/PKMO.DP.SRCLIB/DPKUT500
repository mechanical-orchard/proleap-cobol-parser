049080* ************************************************************** *04908009
049090* * VALIDATE DPG52 INPUT PARAMETERS:                           * *04909009
049100* *              PRIMARY INPUT DATE FORMAT SPECIFICATION       * *04910009
049110* *              AND PRIMARY DATE.                             * *04911009
049120* ************************************************************** *04912009
049130 2200-VALIDATE-DPG52-INPUT.                                       04913009
049140                                                                  04914009
049150     PERFORM 2210-CHECK-DATE-LENGTH-AND-FMT.                      04915009
049160                                                                  04916009
049170     IF  PV-EDIT-SW-OK-SO-FAR                                     04917009
049180     AND PV-INTERNAL-PRCSS-OK                                     04918009
049190         PERFORM 2220-ADD-CENTURY-TO-PRIMARY                      04919009
049200     END-IF.                                                      04920009
049210                                                                  04921009
049220     IF  PV-EDIT-SW-OK-SO-FAR                                     04922009
049230     AND PV-INTERNAL-PRCSS-OK                                     04923009
049240         PERFORM 2230-EVALUATE-PRIMARY-INPUT                      04924009
049250     END-IF.                                                      04925009
049260                                                                  04926009
049270     IF  DPG54-DATE-INVALID                                       04927009
049280         SET DPG54-SEVERE-ERROR TO TRUE                           04928009
049290         IF  DPG54-ERROR-MESSAGE = SPACES                         04929009
049300             MOVE PC-ERRMSG08-INVAL-INPT-DATE                     04930009
049310                                    TO DPG54-ERROR-MESSAGE        04931009
049320         END-IF                                                   04932009
049330     END-IF.                                                      04933009
049340                                                                  04934009
049350     IF  PV-EDIT-SW-OK-SO-FAR                                     04935009
049360     AND PV-INTERNAL-PRCSS-OK                                     04936009
049370         CONTINUE                                                 04937009
049380     ELSE                                                         04938009
049390         SET DPG54-SEVERE-ERROR TO TRUE                           04939009
049400         IF DPG54-ERROR-MESSAGE = SPACES                          04940009
049410             MOVE PC-ERRMSG52-52EDIT-ERROR TO DPG54-ERROR-MESSAGE 04941009
049420         END-IF                                                   04942009
049430     END-IF.                                                      04943009
049440                                                                  04944009
049450                                                                  04945009
049460* ************************************************************** *04946009
049470* * PERFORM A QUICK CHECK FOR MISMATCH OF DATES / FORMATS.     * *04947009
049480* ************************************************************** *04948009
049490 2210-CHECK-DATE-LENGTH-AND-FMT.                                  04949009
049500                                                                  04950009
049510     INITIALIZE PV-TALLY-OF-PRIMARY.                              04951009
049520     INSPECT DPG52-LK-DATE-INPUT                                  04952009
049530         TALLYING PV-TALLY-OF-PRIMARY                             04953009
049540         FOR  ALL SPACES                                          04954009
049550                  LOW-VALUES.                                     04955009
049560                                                                  04956009
049570     COMPUTE PV-ACT-LEN-OF-PRIMARY-DT =                           04957009
049580             (LENGTH OF DPG52-LK-DATE-INPUT -                     04958009
049590              PV-TALLY-OF-PRIMARY).                               04959009
049600                                                                  04960009
049610                                                                  04961009
049620     EVALUATE TRUE                                                04962009
049630                                                                  04963009
049640         WHEN DPG52-LK-DTE-ISO-FMT                                04964009
049650*        ******************************************************* *04965009
049660*        *    LENGTH OF FORMAT:                   (CCYY-MM-DD) * *04966009
049670*        ******************************************************* *04967009
049680             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   04968009
049690                     LENGTH OF DPG52-DB2-ISO-DATE-FMT             04969009
049700                                                                  04970009
049710         WHEN DPG52-LK-DTE-ISO                                    04971009
049720*        ******************************************************* *04972009
049730*        *    LENGTH OF FORMAT:                   (CCYYMMDD)   * *04973009
049740*        ******************************************************* *04974009
049750             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   04975009
049760                     LENGTH OF DPG52-DB2-ISO-DATE                 04976009
049770                                                                  04977009
049780         WHEN DPG52-LK-DTE-GREG-CC-FMT                            04978009
049790*        ******************************************************* *04979009
049800*        *    LENGTH OF FORMAT:                   (MM/DD/CCYY) * *04980009
049810*        ******************************************************* *04981009
049820             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   04982009
049830                     LENGTH OF DPG52-GREG-CC-DATE-FMT             04983009
049840                                                                  04984009
049850         WHEN DPG52-LK-DTE-GREG-CC                                04985009
049860*        ******************************************************* *04986009
049870*        *    LENGTH OF FORMAT:                   (MMDDCCYY)   * *04987009
049880*        ******************************************************* *04988009
049890             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   04989009
049900                     LENGTH OF DPG52-GREG-CC-DATE                 04990009
049910                                                                  04991009
049920         WHEN DPG52-LK-DTE-GREG-FMT                               04992009
049930*        ******************************************************* *04993009
049940*        *    LENGTH OF FORMAT:                   (MM/DD/YY)   * *04994009
049950*        ******************************************************* *04995009
049960             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   04996009
049970                     LENGTH OF DPG52-GREG-DATE-FMT                04997009
049980                                                                  04998009
049990         WHEN DPG52-LK-DTE-GREG                                   04999009
050000*        ******************************************************* *05000009
050010*        *    LENGTH OF FORMAT:                   (MMDDYY)     * *05001009
050020*        ******************************************************* *05002009
050030             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05003009
050040                     LENGTH OF DPG52-GREG-DATE                    05004009
050050                                                                  05005009
050060         WHEN DPG52-LK-DTE-JULIAN-CC                              05006009
050070*        ******************************************************* *05007009
050080*        *    LENGTH OF FORMAT:                   (CCYYDDD)    * *05008009
050090*        ******************************************************* *05009009
050100             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05010009
050110                     LENGTH OF DPG52-JULIAN-DATE-CCYYDDD          05011009
050120                                                                  05012009
050130         WHEN DPG52-LK-DTE-JULIAN-FMT                             05013009
050140*        ******************************************************* *05014009
050150*        *    LENGTH OF FORMAT:                   (YY/DDD)     * *05015009
050160*        ******************************************************* *05016009
050170             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05017009
050180                     LENGTH OF DPG52-JULIAN-DATE-FMT              05018009
050190                                                                  05019009
050200         WHEN DPG52-LK-DTE-JULIAN                                 05020009
050210*        ******************************************************* *05021009
050220*        *    LENGTH OF FORMAT:                   (YYDDD)      * *05022009
050230*        ******************************************************* *05023009
050240             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05024009
050250                     LENGTH OF DPG52-JULIAN-DATE                  05025009
050260                                                                  05026009
050270         WHEN DPG52-LK-DTE-FISC-WEEK-CC                           05027009
050280*        ******************************************************* *05028009
050290*        *    LENGTH OF FORMAT:                   (CCYYWW)     * *05029009
050300*        ******************************************************* *05030009
050310             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05031009
050320                     LENGTH OF DPG52-FISCAL-CC-WEEK               05032009
050330                                                                  05033009
050340         WHEN DPG52-LK-DTE-FISC-WEEK                              05034009
050350*        ******************************************************* *05035009
050360*        *    LENGTH OF FORMAT:                   (YYWW  )     * *05036009
050370*        ******************************************************* *05037009
050380             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05038009
050390                     LENGTH OF DPG52-FISCAL-WEEK                  05039009
050400                                                                  05040009
050410         WHEN DPG52-LK-DTE-FISC-PRD-CC                            05041009
050420*        ******************************************************* *05042009
050430*        *    LENGTH OF FORMAT:                   (CCYYPP)     * *05043009
050440*        ******************************************************* *05044009
050450             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05045009
050460                     LENGTH OF DPG52-FISCAL-CC-PRD                05046009
050470                                                                  05047009
050480         WHEN DPG52-LK-DTE-FISC-PRD                               05048009
050490*        ******************************************************* *05049009
050500*        *    LENGTH OF FORMAT:                   (YYPP)       * *05050009
050510*        ******************************************************* *05051009
050520             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT =                   05052009
050530                     LENGTH OF DPG52-FISCAL-PRD                   05053009
050540                                                                  05054009
050550         WHEN OTHER                                               05055009
050560*        ******************************************************* *05056009
050570*        *   PRIMARY INPUT DATE FORMAT IS UNKNOWN (ERROR)      * *05057009
050580*        ******************************************************* *05058009
050590             COMPUTE PV-MAX-LEN-OF-PRIMARY-DT = ZERO              05059009
050600             MOVE PC-ERRMSG09-INVAL-INPT-DT-F                     05060009
050610                                          TO DPG54-ERROR-MESSAGE  05061009
050620             SET  DPG54-DATE-INVALID      TO TRUE                 05062009
050630             SET  DPG54-SEVERE-ERROR      TO TRUE                 05063009
050640                                                                  05064009
050650                                                                  05065009
050660     END-EVALUATE.                                                05066009
050670                                                                  05067009
050680     IF DPG54-SEVERE-ERROR                                        05068009
050690         CONTINUE                                                 05069009
050700     ELSE                                                         05070009
050710         IF  PV-ACT-LEN-OF-PRIMARY-DT = PV-MAX-LEN-OF-PRIMARY-DT  05071009
050720             CONTINUE                                             05072009
050730         ELSE                                                     05073009
050740             IF  PV-ACT-LEN-OF-PRIMARY-DT >                       05074009
050750                                        PV-MAX-LEN-OF-PRIMARY-DT  05075009
050760                     SET PV-GF-EXTRANEOUS-INPUT                   05076009
050770                                              TO TRUE             05077009
050780             ELSE                                                 05078009
050790                 SET DPG54-DATE-INVALID                           05079009
050800                     PV-EDIT-SW-ERROR                             05080009
050810                     DPG54-SEVERE-ERROR       TO TRUE             05081009
050820                 MOVE PC-ERRMSG08-INVAL-INPT-DATE                 05082009
050830                                              TO                  05083009
050840                                             DPG54-ERROR-MESSAGE  05084009
050850             END-IF                                               05085009
050860         END-IF                                                   05086009
050870     END-IF.                                                      05087009
050880                                                                  05088009
050890                                                                  05089009
050900* ************************************************************** *05090009
050910* * THE FUNCTIONS OF THIS PARAGRAPH ARE:                       * *05091009
050920* * 1. CONVERT DATE INPUT AS FORMAT                            * *05092009
050930* *    DPG52-LK-DTE-GREG-FMT        (MM/DD/YY)                 * *05093009
050940* *    OR                                                      * *05094009
050950* *    DPG52-LK-DTE-GREG            (MMDDYY)                   * *05095009
050960* *    TO FORMAT:                                              * *05096009
050970* *    DPG52-LK-DTE-GREG-CC         (MMDDCCYY)                 * *05097009
050980* * 2. CALCULATE CENTURY (CC)       (MMDD19YY OR MMDD20YY)     * *05098009
050990* ************************************************************** *05099009
051000 2220-ADD-CENTURY-TO-PRIMARY.                                     05100009
051010                                                                  05101009
051020     IF DPG52-LK-DTE-GREG-FMT                                     05102009
051030* ************************************************************** *05103009
051040* *     DPG52-LK-DTE-GREG-FMT       (MM/DD/YY)                 * *05104009
051050* ************************************************************** *05105009
051060         IF  DPG52-GREG-DATE-FMT-S1 = PC-SLASH                    05106009
051070         AND DPG52-GREG-DATE-FMT-S2 = PC-SLASH                    05107009
051080         AND DPG52-GREG-DATE-FMT-MO-X NUMERIC                     05108009
051090         AND DPG52-GREG-DATE-FMT-DY-X NUMERIC                     05109009
051100         AND DPG52-GREG-DATE-FMT-YR-X NUMERIC                     05110009
051110             IF  DPG52-GREG-DATE-FMT-MO >  ZERO                   05111009
051120             AND DPG52-GREG-DATE-FMT-MO <= PC-MAX-MONTH           05112009
051130             AND DPG52-GREG-DATE-FMT-DY >  ZERO                   05113009
051140             AND DPG52-GREG-DATE-FMT-DY <= PC-MAX-DAY-IN-ANY-MONTH05114009
051150                 MOVE DPG52-GREG-DATE-FMT-MO TO PV-MMDDCCYY-MM-N  05115009
051160                 MOVE DPG52-GREG-DATE-FMT-DY TO PV-MMDDCCYY-DD-N  05116009
051170                 MOVE DPG52-GREG-DATE-FMT-YR TO PV-CALC-CCYY-N    05117009
051180                 PERFORM 5920-CALC-CC                             05118009
051190                 MOVE PV-MMDDCCYY-MM-N    TO DPG52-GREG-CC-DATE-MO05119009
051200                 MOVE PV-MMDDCCYY-DD-N    TO DPG52-GREG-CC-DATE-DY05120009
051210                 MOVE PV-CALC-CCYY-N      TO DPG52-GREG-CC-DATE-YR05121009
051220                 SET DPG52-LK-DTE-GREG-CC TO TRUE                 05122009
051230                 SET PV-PRIMARY-CC-ADDED  TO TRUE                 05123009
051240             ELSE                                                 05124009
051250                 SET DPG54-DATE-INVALID   TO TRUE                 05125009
051260             END-IF                                               05126009
051270         ELSE                                                     05127009
051280             SET DPG54-DATE-INVALID       TO TRUE                 05128009
051290         END-IF                                                   05129009
051300     END-IF.                                                      05130009
051310                                                                  05131009
051320                                                                  05132009
051330     IF DPG52-LK-DTE-GREG                                         05133009
051340* ************************************************************** *05134009
051350* *     DPG52-LK-DTE-GREG           (MMDDYY)                   * *05135009
051360* ************************************************************** *05136009
051370         IF  DPG52-GREG-DATE-MO-X NUMERIC                         05137009
051380         AND DPG52-GREG-DATE-DY-X NUMERIC                         05138009
051390         AND DPG52-GREG-DATE-YR-X NUMERIC                         05139009
051400             IF  DPG52-GREG-DATE-MO >  ZERO                       05140009
051410             AND DPG52-GREG-DATE-MO <= PC-MAX-MONTH               05141009
051420             AND DPG52-GREG-DATE-DY >  ZERO                       05142009
051430             AND DPG52-GREG-DATE-DY <= PC-MAX-DAY-IN-ANY-MONTH    05143009
051440                 MOVE DPG52-GREG-DATE-MO  TO PV-MMDDCCYY-MM-N     05144009
051450                 MOVE DPG52-GREG-DATE-DY  TO PV-MMDDCCYY-DD-N     05145009
051460                 MOVE DPG52-GREG-DATE-YR  TO PV-CALC-CCYY-N       05146009
051470                 PERFORM 5920-CALC-CC                             05147009
051480                 MOVE PV-MMDDCCYY-MM-N    TO DPG52-GREG-CC-DATE-MO05148009
051490                 MOVE PV-MMDDCCYY-DD-N    TO DPG52-GREG-CC-DATE-DY05149009
051500                 MOVE PV-CALC-CCYY-N      TO DPG52-GREG-CC-DATE-YR05150009
051510                 SET DPG52-LK-DTE-GREG-CC TO TRUE                 05151009
051520                 SET PV-PRIMARY-CC-ADDED  TO TRUE                 05152009
051530             ELSE                                                 05153009
051540                 SET DPG54-DATE-INVALID   TO TRUE                 05154009
051550             END-IF                                               05155009
051560         ELSE                                                     05156009
051570             SET DPG54-DATE-INVALID   TO TRUE                     05157009
051580         END-IF                                                   05158009
051590     END-IF.                                                      05159009
051600                                                                  05160009
051610                                                                  05161009
051620     IF DPG52-LK-DTE-FISC-WEEK                                    05162009
051630* ************************************************************** *05163009
051640* *     DPG52-LK-DTE-FISC-WEEK      (YYWW)                     * *05164009
051650* ************************************************************** *05165009
051660         IF  DPG52-FISCAL-WEEK-YR-X NUMERIC                       05166009
051670         AND DPG52-FISCAL-WEEK-WK-X NUMERIC                       05167009
051680             MOVE DPG52-FISCAL-WEEK-YR TO PV-CALC-CCYY-N          05168009
051690             PERFORM 5920-CALC-CC                                 05169009
051700             MOVE DPG52-FISCAL-WEEK-WK TO DPG52-FISCAL-CC-WEEK-WK 05170009
