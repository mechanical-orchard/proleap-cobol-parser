089280         PERFORM 5300-CONVERT-DAYS-TO-CCYYMMDD                    08928009
089290     END-IF.                                                      08929009
089300                                                                  08930009
089310     IF  PV-EDIT-SW-OK-SO-FAR                                     08931009
089320     AND PV-INTERNAL-PRCSS-OK                                     08932009
089330         PERFORM 5500-CONVERT-TO-JULIAN                           08933009
089340     ELSE                                                         08934009
089350         PERFORM 5501-5500-INITIALIZATIONS                        08935009
089360         PERFORM 5801-5800-INITIALIZATIONS                        08936009
089370     END-IF.                                                      08937009
089380                                                                  08938009
089390     IF  PV-EDIT-SW-OK-SO-FAR                                     08939009
089400     AND PV-INTERNAL-PRCSS-OK                                     08940009
089410         MOVE PV-CCYYMMDD-CCYY-N TO DPG55-1ST-WKDY-DB2ISO-FMT-YR  08941009
089420                                    DPG55-1ST-WKDY-GREG-CC-YR     08942009
089430                                                                  08943009
089440         MOVE PV-CCYYMMDD-YY-N   TO DPG55-1ST-WKDY-GREG-FMT-YR    08944009
089450                                    DPG55-1ST-WKDY-GREG-YR        08945009
089460                                    DPG55-1ST-WKDY-JULIAN-YR      08946009
089470                                                                  08947009
