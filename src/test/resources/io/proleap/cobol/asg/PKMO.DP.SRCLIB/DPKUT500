085360             PC-HT-M-NEW-YEARS-DAY  OR                            08530009
085370             PC-HT-M-FOURTH-OF-JULY OR                            08531009
085380             PC-HT-M-CHRISTMAS-DAY                                08532009
085390             ADD PC-01 TO PV-COUNT-NON-BUSDAY                     08533009
085400                           PV-COUNT-HOL                           08534009
085410                           PV-COUNT-FRI-SAT-CORP-HOL              08535009
085420         ELSE                                                     08536009
085430             ADD PC-01 TO PV-COUNT-BUSDAY                         08537009
085440         END-IF                                                   08538009
085450     ELSE                                                         08539009
