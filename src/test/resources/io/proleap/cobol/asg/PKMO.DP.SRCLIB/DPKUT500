053850                 MOVE PV-CALC-CCYY-N       TO PV-CCYYDDD-CCYY-X   05385009
053860                                                                  05386009
053870                 PERFORM 2240-EDIT-JULIAN-PRIMARY                 05387009
053880                                                                  05388009
053890                 IF  PV-EDIT-SW-OK-SO-FAR                         05389009
053900                 AND PV-INTERNAL-PRCSS-OK                         05390009
053910                     PERFORM 2290-HOLD-VALID-DATE                 05391009
053920                 ELSE                                             05392009
053930                     SET DPG54-DATE-INVALID                       05393009
053940                                           TO TRUE                05394009
053950                 END-IF                                           05395009
053960             ELSE                                                 05396009
053970                 SET DPG54-DATE-INVALID    TO TRUE                05397009
053980             END-IF                                               05398009
053990                                                                  05399009
054000                                                                  05400009
054010         WHEN DPG52-LK-DTE-FISC-WEEK-CC                           05401009
054020*        ******************************************************* *05402009
054030*        *   EDIT PRIMARY INPUT DATE, FORMAT IS:  (CCYYWW)     * *05403009
054040*        ******************************************************* *05404009
054050             IF  DPG52-FISCAL-CC-WEEK-YR-X NUMERIC                05405009
054060             AND DPG52-FISCAL-CC-WEEK-WK-X NUMERIC                05406009
054070                 MOVE DPG52-FISCAL-CC-WEEK-YR-X                   05407009
054080                                             TO PV-EDIT-CCYY-X    05408009
054090                 IF  (PV-EDIT-CCYY-CC-N =   PC-VALID-CC-19  OR    05409009
054100                      PV-EDIT-CCYY-CC-N =   PC-VALID-CC-20  OR    05410009
054110                      PV-EDIT-CCYY-CC-N =   PC-VALID-CC-21)       05411009
054120                     IF  (DPG52-FISCAL-CC-WEEK-WK > ZERO)         05412009
054130                     AND (DPG52-FISCAL-CC-WEEK-WK <=              05413009
054140                                                 PC-53-FISCAL-WKS)05414009
054150                         PERFORM 2250-CONVERT-FISCAL-WEEK         05415009
054160                         IF  PV-EDIT-SW-OK-SO-FAR                 05416009
054170                         AND PV-INTERNAL-PRCSS-OK                 05417009
054180                             SET DPG54-DATE-VALID TO TRUE         05418009
054190                             PERFORM 2290-HOLD-VALID-DATE         05419009
054200                             MOVE PV-CCYYMMDD-FWFP-CCYY-N         05420009
054210                               TO PV-CCYYMMDD-FWFP-P-CCYY-N       05421009
054220                             MOVE PV-CCYYMMDD-FWFP-MM-N           05422009
054230                               TO PV-CCYYMMDD-FWFP-P-MM-N         05423009
054240                             MOVE PV-CCYYMMDD-FWFP-DD-N           05424009
054250                               TO PV-CCYYMMDD-FWFP-P-DD-N         05425009
054260                         ELSE                                     05426009
054270                             SET DPG54-DATE-INVALID               05427009
054280                                                 TO TRUE          05428009
054290                         END-IF                                   05429009
054300                     ELSE                                         05430009
054310                         MOVE PC-ERRMSG10-INVAL-INPT-DT-FW        05431009
054320                                            TO DPG54-ERROR-MESSAGE05432009
054330                         SET DPG54-DATE-INVALID  TO TRUE          05433009
054340                 ELSE                                             05434009
054350                     MOVE PC-ERRMSG08-INVAL-INPT-DATE             05435009
054360                                    TO DPG54-ERROR-MESSAGE        05436009
054370                     SET DPG54-DATE-INVALID  TO TRUE              05437009
054380                 END-IF                                           05438009
054390             END-IF                                               05439009
054400                                                                  05440009
054410                                                                  05441009
054420         WHEN DPG52-LK-DTE-FISC-WEEK                              05442009
