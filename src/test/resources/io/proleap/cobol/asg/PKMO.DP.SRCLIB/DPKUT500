074340                             DPG54-INCR-DECR-INVALID              07428009
074350                                            TO TRUE               07429009
074360                     END-IF                                       07430009
074370             END-EVALUATE                                         07431009
074380         END-IF                                                   07432009
074390     END-IF.                                                      07433009
074400                                                                  07434009
074410                                                                  07435009
074420* ************************************************************** *07436009
074430* * 10) IF AN INCR/DECR FUNCTION ENTERED,                      * *07437009
074440* *     A VALID INCR/DECR TIMEFRAME MUST BE ENTERED            * *07438009
074450* *     (IE- BUSINESS DAYS, CALENDAR DAYS,                     * *07439009
074460* *          FISCAL WEEKS, FISCAL PERIODS)                     * *07440009
074470* ************************************************************** *07441009
074480     IF  PV-EDIT-SW-OK-SO-FAR                                     07442009
074490     AND PV-INTERNAL-PRCSS-OK                                     07443009
074500         IF  DPG51-INCR-DECR-REQUESTED                            07444009
074510             IF  DPG52-LK-DTE-FISC-WEEK-CC                        07445009
074520             OR  DPG52-LK-DTE-FISC-WEEK                           07446009
074530                 IF  PV-FISCLWK-VALID                             07447009
074540                 OR  PV-FISCLWK-ZERO                              07448009
074550                     CONTINUE                                     07449009
074560                 ELSE                                             07450009
074570                     SET PV-EDIT-SW-ERROR                         07451009
074580                         DPG54-INCR-DECR-INVALID                  07452009
074590                                            TO TRUE               07453009
074600                     MOVE PC-ERRMSG-D3-NO-ID-TMFRM                07454009
074610                                            TO DPG54-ERROR-MESSAGE07455009
074620                 END-IF                                           07456009
074630             ELSE                                                 07457009
074640                 IF DPG52-LK-DTE-FISC-PRD-CC                      07458009
074650                 OR DPG52-LK-DTE-FISC-PRD                         07459009
074660                     IF  PV-FISCLPRD-VALID                        07460009
074670                     OR  PV-FISCLPRD-ZERO                         07461009
074680                         CONTINUE                                 07462009
074690                     ELSE                                         07463009
074700                         SET PV-EDIT-SW-ERROR                     07464009
074710                             DPG54-INCR-DECR-INVALID              07465009
074720                                            TO TRUE               07466009
074730                         MOVE PC-ERRMSG-D4-NO-ID-TMFRM            07467009
074740                                            TO DPG54-ERROR-MESSAGE07468009
074750                     END-IF                                       07469009
074760                 ELSE                                             07470009
074770                     IF PV-BUSDAYS-VALID                          07471009
074780                     OR PV-CALDAYS-VALID                          07472009
074790                     OR PV-FISCLWK-VALID                          07473009
074800                     OR PV-FISCLPRD-VALID                         07474009
074810                     OR PV-BUSDAYS-ZERO                           07475009
074820                     OR PV-CALDAYS-ZERO                           07476009
074830                     OR PV-FISCLWK-ZERO                           07477009
074840                     OR PV-FISCLPRD-ZERO                          07478009
074850                         CONTINUE                                 07479009
074860                     ELSE                                         07480009
074870                         IF  PV-NO-BUSDAYS-RQST                   07481009
074880                         OR  PV-NO-CALDAYS-RQST                   07482009
074890                             SET  PV-GF-GRANDFATHERED-ERR         07483009
074900                                            TO TRUE               07484009
074910                         ELSE                                     07485009
074920*                            *********************************** *07486009
074930*                            * NO TIMEFRAME ENTERED            * *07487009
074940*                            *********************************** *07488009
074950                             SET PV-EDIT-SW-ERROR                 07489009
074960                                 DPG54-INCR-DECR-INVALID          07490009
074970                                            TO TRUE               07491009
074980                             MOVE PC-ERRMSG-D2-NO-ID-TMFRM        07492009
074990                                            TO DPG54-ERROR-MESSAGE07493009
075000                         END-IF                                   07494009
075010                     END-IF                                       07495009
075020                 END-IF                                           07496009
075030             END-IF                                               07497009
075040          END-IF                                                  07498009
075050      END-IF.                                                     07499009
075060                                                                  07500009
075070                                                                  07501009
