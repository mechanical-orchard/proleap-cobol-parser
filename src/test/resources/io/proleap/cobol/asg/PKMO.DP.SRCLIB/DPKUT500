084030* ************************************************************** *08397009
084040* * FOR EFFICIENT EXECUTION, DETERMINE THE STARTING POINT IN   * *08398009
084050* * THE HOLIDAY TABLE.                                         * *08399009
084060* *                                                            * *08400009
084070* * PROCESSING WILL PROCEED SEQUENTIALLY FROM THAT POINT.      * *08401009
084080* ************************************************************** *08402009
084090 3710-CALC-STARTING-HI-HOLIDX.                                    08403009
084100                                                                  08404009
084110     COMPUTE PV-HT-STARTING-POINT = ((PV-SET-IDX-CCYY-N           08405009
084120                                  -  PC-HT-BASELINE-YEAR)         08406009
084130                                  *  PC-HT-HOLIDAYS-PER-YR)       08407009
084140                                  +  PC-HT-HOLIDAYS-PER-YR.       08408009
084150                                                                  08409009
084160*    *********************************************************** *08410009
084170*    *  ERROR TRAP TO PREVENT INDEX ERROR                      * *08411009
084180*    *  (FUTURE ENHANCEMENT COULD CAUSE CONSTANTS OUT-OF-SYNC) * *08412009
084190*    *  (WITH HT-TABLE OCCURS CLAUSE OR DATA.                ) * *08413009
084200*    *********************************************************** *08414009
084210     IF  PV-HT-STARTING-POINT < PC-01                             08415009
084220         MOVE PC-01                     TO PV-HT-STARTING-POINT   08416009
084230         SET PV-INTERNAL-PRCSS-ERR      TO TRUE                   08417009
084240         MOVE '3710- A'                 TO PV-ERRMSG99-PARA       08418009
084250         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      08419009
084260                                        TO DPG54-ERROR-MESSAGE    08420009
084270     ELSE                                                         08421009
084280         IF  PV-HT-STARTING-POINT > PC-HT-MAX-HOLIDAY-ENTRIES     08422009
084290             MOVE PC-HT-MAX-HOLIDAY-ENTRIES                       08423009
084300                                        TO PV-HT-STARTING-POINT   08424009
084310             SET PV-INTERNAL-PRCSS-ERR  TO TRUE                   08425009
084320             MOVE '3710- B'             TO PV-ERRMSG99-PARA       08426009
084330             MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                  08427009
084340                                        TO DPG54-ERROR-MESSAGE    08428009
084350         END-IF                                                   08429009
084360     END-IF.                                                      08430009
084370                                                                  08431009
084380     SET HT-INDEX  TO  PV-HT-STARTING-POINT.                      08432009
084390                                                                  08433009
084400     IF PV-CREL-DATE-TO-CHECK < HT-DAYS-SINCE-BASELINE (HT-INDEX) 08434009
084410         PERFORM 3720-ESTABLISH-HOL-TBL-POS                       08435009
084420            VARYING PV-PREDAYS-LOOP-COUNT                         08436009
084430            FROM    PC-01                                         08437009
084440            BY      PC-01                                         08438009
084450            UNTIL   HT-INDEX < PC-01                              08439009
084460               OR  PV-PREDAYS-LOOP-COUNT > PC-HT-HOLIDAYS-PLUS-ONE08440009
084470               OR  PV-STARTING-DATE-FOUND                         08441009
084480     END-IF.                                                      08442009
084490                                                                  08443009
084500                                                                  08444009
