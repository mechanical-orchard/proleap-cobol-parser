045060* ************************************************************** *04506009
045070* *  "STUB" TO RESET INDICATORS TO GRANDFATHERED VALUES. (SIGH)* *04507009
045080* ************************************************************** *04508009
045090*1001-GRANDFATHERED-VALUES.                                       04509009
045100*                                                                 04510009
045110* ************************************************************** *04511009
045120* *  OLD ROUTINE PROVIDED "SPACE" FOR VALID CALENDAR TYPE.     * *04512009
045130* *  NEW ROUTINE PROVIDES "Y"     FOR VALID CALENDAR TYPE.     * *04513009
045140* *                                                            * *04514009
045150* *  THIS CODE RESETS THE INDICATOR FROM "Y" TO "SPACE".       * *04515009
045160* ************************************************************** *04516009
045170*    IF  DPG54-CALENDAR-TYPE-VALID                                04517009
045180*        MOVE SPACE TO DPG54-CALENDAR-TYPE-IND                    04518009
045190*    END-IF.                                                      04519009
045200*                                                                 04520009
045210*                                                                 04521009
045220* ************************************************************** *04522009
045230* *  OLD ROUTINE PROVIDED "I" (INVALID) WHEN OPTIONAL COMPARE  * *04523009
045240* *  DATE FEATURE WAS NOT USED.                                * *04524009
045250* *                                                            * *04525009
045260* *  NEW ROUTINE PROVIDES "SPACE"       WHEN OPTIONAL COMPARE  * *04526009
045270* *  DATE FEATURE WAS NOT USED.                                * *04527009
045280* *                                                            * *04528009
045290* *  THIS CODE RESETS THE INDICATOR FROM "SPACE" TO "I".       * *04529009
045300* ************************************************************** *04530009
045310*    IF  DPG54-CMPR-DATE-NOT-USED                                 04531009
045320*        SET DPG54-CMPR-DATE-INVALID TO TRUE                      04532009
045330*    END-IF.                                                      04533009
045340                                                                  04534009
045350                                                                  04535009
045360* ************************************************************** *04536009
045370* * VALIDATE INPUT PARAMETERS.                                 * *04537009
045380* ************************************************************** *04538009
045390 2000-VALIDATE-INPUT.                                             04539009
045400                                                                  04540009
045410     PERFORM 2100-VALIDATE-DPG51-INPUT.                           04541009
045420                                                                  04542009
045430     IF  PV-EDIT-SW-OK-SO-FAR                                     04543009
045440     AND PV-INTERNAL-PRCSS-OK                                     04544009
045450         SET PV-THIS-PASS-INIT           TO TRUE                  04545009
045460         PERFORM 2200-VALIDATE-DPG52-INPUT                        04546009
045470     END-IF.                                                      04547009
045480                                                                  04548009
045490     IF  PV-EDIT-SW-OK-SO-FAR                                     04549009
045500     AND PV-INTERNAL-PRCSS-OK                                     04550009
045510         SET PV-THIS-PASS-INIT           TO TRUE                  04551009
045520         PERFORM 2300-VALIDATE-DPG53-INPUT                        04552009
045530     END-IF.                                                      04553009
045540                                                                  04554009
045550     IF  PV-EDIT-SW-OK-SO-FAR                                     04555009
045560     AND PV-INTERNAL-PRCSS-OK                                     04556009
045570         PERFORM 2500-CROSS-CHECK-EDITS                           04557009
045580     END-IF.                                                      04558009
045590                                                                  04559009
045600     IF  PV-EDIT-SW-OK-SO-FAR                                     04560009
045610     AND PV-INTERNAL-PRCSS-OK                                     04561009
045620         CONTINUE                                                 04562009
045630     ELSE                                                         04563009
045640         SET DPG54-SEVERE-ERROR          TO TRUE                  04564009
045650         IF DPG54-ERROR-MESSAGE = SPACES                          04565009
045660             MOVE PC-ERRMSG60-EDIT-ERROR TO DPG54-ERROR-MESSAGE   04566009
045670         END-IF                                                   04567009
045680     END-IF.                                                      04568009
045690                                                                  04569009
045700                                                                  04570009
045710**************************************************************** *04571009
045720* VALIDATE DPG51 INPUT PARAMETERS.                             * *04572009
045730**************************************************************** *04573009
045740 2100-VALIDATE-DPG51-INPUT.                                       04574009
045750                                                                  04575009
045760     PERFORM 2110-VALIDATE-CALENDAR-TYPE.                         04576009
045770                                                                  04577009
045780     IF  PV-EDIT-SW-OK-SO-FAR                                     04578009
045790     AND PV-INTERNAL-PRCSS-OK                                     04579009
045800         PERFORM 2120-VALIDATE-INCR-DECR-FLAG                     04580009
045810     END-IF.                                                      04581009
045820                                                                  04582009
045830     IF  PV-EDIT-SW-OK-SO-FAR                                     04583009
045840     AND PV-INTERNAL-PRCSS-OK                                     04584009
045850         PERFORM 2130-VALIDATE-INCR-DECR-BUS                      04585009
045860     END-IF.                                                      04586009
045870                                                                  04587009
045880     IF  PV-EDIT-SW-OK-SO-FAR                                     04588009
045890     AND PV-INTERNAL-PRCSS-OK                                     04589009
045900         PERFORM 2140-VALIDATE-INCR-DECR-CAL                      04590009
045910     END-IF.                                                      04591009
045920                                                                  04592009
045930     IF  PV-EDIT-SW-OK-SO-FAR                                     04593009
045940     AND PV-INTERNAL-PRCSS-OK                                     04594009
045950         PERFORM 2150-VALIDATE-INCR-DECR-FW                       04595009
045960     END-IF.                                                      04596009
045970                                                                  04597009
045980     IF  PV-EDIT-SW-OK-SO-FAR                                     04598009
045990     AND PV-INTERNAL-PRCSS-OK                                     04599009
046000         PERFORM 2160-VALIDATE-INCR-DECR-FP                       04600009
046010     END-IF.                                                      04601009
046020                                                                  04602009
046030     IF  PV-EDIT-SW-OK-SO-FAR                                     04603009
046040     AND PV-INTERNAL-PRCSS-OK                                     04604009
046050         EVALUATE TRUE                                            04605009
046060             WHEN DPG52-LK-DTE-GREG-FMT                           04606009
046070             WHEN DPG52-LK-DTE-GREG                               04607009
046080             WHEN DPG52-LK-DTE-JULIAN-FMT                         04608009
046090             WHEN DPG52-LK-DTE-JULIAN                             04609009
046100             WHEN DPG52-LK-DTE-FISC-WEEK                          04610009
046110             WHEN DPG52-LK-DTE-FISC-PRD                           04611009
046120             WHEN DPG53-LK-CMPR-DTE-GREG-FMT                      04612009
046130             WHEN DPG53-LK-CMPR-DTE-GREG                          04613009
046140             WHEN DPG53-LK-CMPR-DTE-JULIAN-FMT                    04614009
046150             WHEN DPG53-LK-CMPR-DTE-JULIAN                        04615009
046160             WHEN DPG53-LK-CMPR-DTE-FISC-WEEK                     04616009
046170             WHEN DPG53-LK-CMPR-DTE-FISC-PRD                      04617009
046180                 PERFORM 2170-VALIDATE-PIVOT-YR                   04618009
046190         END-EVALUATE                                             04619009
046200     END-IF.                                                      04620009
046210                                                                  04621009
046220     IF  PV-EDIT-SW-OK-SO-FAR                                     04622009
046230     AND PV-INTERNAL-PRCSS-OK                                     04623009
046240         CONTINUE                                                 04624009
046250     ELSE                                                         04625009
046260         SET DPG54-SEVERE-ERROR TO TRUE                           04626009
046270         IF DPG54-ERROR-MESSAGE = SPACES                          04627009
046280             MOVE PC-ERRMSG51-51EDIT-ERROR TO DPG54-ERROR-MESSAGE 04628009
046290         END-IF                                                   04629009
046300     END-IF.                                                      04630009
