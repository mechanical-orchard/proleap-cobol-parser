104920                                                                  10492009
104930     IF  (PV-CCYYMMDD-CCYY-X NUMERIC                              10493009
104940     AND  PV-CCYYMMDD-MM-X   NUMERIC                              10494009
104950     AND  PV-CCYYMMDD-DD-X   NUMERIC)                             10495009
104960         IF  (PV-CCYYMMDD-CC-N      =     PC-INVAL-CC-18  OR      10496009
104970              PV-CCYYMMDD-CC-N      =     PC-VALID-CC-19  OR      10497009
104980              PV-CCYYMMDD-CC-N      =     PC-VALID-CC-20  OR      10498009
104990              PV-CCYYMMDD-CC-N      =     PC-VALID-CC-21)         10499009
105000         AND (PV-CCYYMMDD-YY-N     >=     ZERO AND                10500009
105010              PV-CCYYMMDD-YY-N     <=     PC-MAX-2DIGIT-YR)       10501009
105020         AND (PV-CCYYMMDD-MM-N     >=     PC-1ST-MONTH AND        10502009
105030              PV-CCYYMMDD-MM-N     <=     PC-MAX-MONTH)           10503009
105040         AND (PV-CCYYMMDD-DD-N     >=     PC-1ST-DAY AND          10504009
105050              PV-CCYYMMDD-DD-N     <=     PC-MAX-DAY-IN-ANY-MONTH)10505009
105060             PERFORM 5110-VALIDATE-DAYS-IN-MONTH                  10506009
105070             IF  PV-EDIT-SW-OK-SO-FAR                             10507009
105080             AND PV-INTERNAL-PRCSS-OK                             10508009
105090                 PERFORM 5120-VALIDATION-OF-RANGE                 10509009
105100             END-IF                                               10510009
105110         ELSE                                                     10511009
105120             SET PV-EDIT-SW-ERROR   TO TRUE                       10512009
105130         END-IF                                                   10513009
105140     ELSE                                                         10514009
105150         SET PV-EDIT-SW-ERROR       TO TRUE                       10515009
105160     END-IF.                                                      10516009
105170                                                                  10517009
105180                                                                  10518009
105190* ************************************************************** *10519009
105200* * VALIDATE FORMATTED DATE: CCYYMMDD.                         * *10520009
105210* ************************************************************** *10521009
105220 5110-VALIDATE-DAYS-IN-MONTH.                                     10522009
105230                                                                  10523009
105240     IF   PV-CCYYMMDD-DD-N < PC-MAX-DAY-IN-FEB                    10524009
105250          CONTINUE                                                10525009
105260     ELSE                                                         10526009
105270          IF PV-CCYYMMDD-DD-N = PC-MAX-DAY-IN-ANY-MONTH           10527009
105280             IF (PV-CCYYMMDD-MM-N = PC-SEPT  OR                   10528009
105290                 PV-CCYYMMDD-MM-N = PC-APR   OR                   10529009
105300                 PV-CCYYMMDD-MM-N = PC-JUNE  OR                   10530009
105310                 PV-CCYYMMDD-MM-N = PC-NOV   OR                   10531009
105320                 PV-CCYYMMDD-MM-N = PC-FEB)                       10532009
105330                 SET PV-EDIT-SW-ERROR      TO TRUE                10533009
105340             END-IF                                               10534009
105350          END-IF                                                  10535009
105360          IF PV-CCYYMMDD-DD-N = PC-MAX-DAY-IN-POEM-MONTHS         10536009
105370             IF (PV-CCYYMMDD-MM-N = PC-FEB)                       10537009
105380                 SET PV-EDIT-SW-ERROR      TO TRUE                10538009
105390             END-IF                                               10539009
105400          END-IF                                                  10540009
105410          IF PV-CCYYMMDD-DD-N = PC-MAX-DAY-IN-FEB                 10541009
105420             IF (PV-CCYYMMDD-MM-N = PC-FEB)                       10542009
105430                 IF (PV-CCYYMMDD-YY-N = ZERO)                     10543009
105440                     CONTINUE                                     10544009
105450                 ELSE                                             10545009
105460                     MOVE PV-CCYYMMDD-YY-N TO PV-CHK-LEAPYR-CCYY-N10546009
105470                     PERFORM 5800-CHECK-LEAP-YEAR                 10547009
105480                     IF PV-LEAP-YEAR                              10548009
105490                         CONTINUE                                 10549009
105500                     ELSE                                         10550009
105510                         SET PV-EDIT-SW-ERROR                     10551009
105520                                           TO TRUE                10552009
105530                     END-IF                                       10553009
105540                 END-IF                                           10554009
105550             END-IF                                               10555009
105560         END-IF                                                   10556009
105570     END-IF.                                                      10557009
105580                                                                  10558009
105590                                                                  10559009
105600* ************************************************************** *10560009
105610* * VALIDATE RANGE OF FORMATTED DATE: CCYYMMDD                 * *10561009
105620* * THIS MUST BE WITHIN BOUNDS OF THE HT-HOLIDAY-TABLE-DATA.   * *10562009
105630* ************************************************************** *10563009
105640 5120-VALIDATION-OF-RANGE.                                        10564009
105650                                                                  10565009
105660     IF PV-CCYYMMDD   < PC-HT-BASELINE-DATE                       10566009
105670     OR PV-CCYYMMDD   > PC-HT-MAX-DATE                            10567009
105680         SET PV-EDIT-SW-ERROR        TO TRUE                      10568009
105690         MOVE PC-ERRMSG12-DATE-OUT-OF-RANGE                       10569009
105700                                     TO DPG54-ERROR-MESSAGE       10570009
105710     END-IF.                                                      10571009
105720                                                                  10572009
105730                                                                  10573009
105740* ************************************************************** *10574009
105750* * THIS PARAGRAPH USES A GIVEN DATE (PV-CCYYMMDD) AND         * *10575009
105760* * DETERMINES NUMBER OF DAYS ELAPSED SINCE THE BASELINE DATE. * *10576009
105770* *                                                            * *10577009
105780* * THE BASELINE DATE IS JANUARY 01, 1901;                     * *10578009
105790* * EXAMPLES: JAN 01, 1901 = 0 DAYS SINCE BASELINE.            * *10579009
105800* *           JAN 02, 1901 = 1 DAYS SINCE BASELINE.            * *10580009
105810* *                                                            * *10581009
105820* * THIS IS ALSO REFERRED TO AS "CREL" (COMPARE RELATIVE DATE) * *10582009
105830* * AND IS THE COMMON BASIS FOR ALL DATE MATH.                 * *10583009
105840* *                                                            * *10584009
105850* * THE DATE CALCULATION IS DONE IN THREE STEPS                * *10585009
105860* * AND THE RESULTS OF EACH STEP ARE ACCUMULATED INTO          * *10586009
105870* * PV-DAYS-SINCE-BASELINE.                                    * *10587009
105880* *                                                            * *10588009
105890* * 1) TO ACCOUNT FOR THE "YEAR" FIELD OF THE GIVEN DATE:      * *10589009
105900* *    A. SUBTRACTS BASELINE FROM YEAR, GIVING RELATIVE YEAR.  * *10590009
105910* *    B. MULTIPLIES RELATIVE YEAR BY 365.25, GIVING NUMBER OF * *10591009
105920* *       DAYS IN ALL RELEVANT YEARS PRIOR TO THE GIVEN YEAR.  * *10592009
105930* * 2) TO ACCOUNT FOR THE "DAY" FIELD OF THE GIVEN DATE:       * *10593009
105940* *    ADDS THE GIVEN DAY-OF-MONTH TO GET NUMBER OF DAYS       * *10594009
105950* *    SO FAR IN THE GIVEN MONTH.                              * *10595009
105960* * 3) TO ACCOUNT FOR THE "MONTH" FIELD OF THE GIVEN DATE:     * *10596009
105970* *    PERFORMS A TABLE LOOKUP ON MONTH AND CAPTURES THE       * *10597009
105980* *    NUMBER OF DAYS PRECEEDING THIS MONTH IN A NON-LEAP YEAR.* *10598009
105990* *                                                            * *10599009
106000* ************************************************************** *10600009
106010 5200-CONVERT-CCYYMMDD-TO-DAYS.                                   10601009
106020                                                                  10602009
106030     INITIALIZE PV-DAYS-SINCE-BASELINE.                           10603009
106040     SET PV-INTERNAL-PRCSS-SW-INIT                                10604009
106050         PV-MONTH-MATCH-SW-INITIALIZED  TO TRUE.                  10605009
106060     MOVE PC-01                         TO PV-SUB1.               10606009
106070                                                                  10607009
106080     COMPUTE PV-YEARS-FACTOR =                                    10608009
106090             (PV-CCYYMMDD-CCYY-N                                  10609009
106100              -  PC-HT-BASELINE-YEAR)                             10610009
106110              *  PC-DAYS-PER-YEAR.                                10611009
106120     COMPUTE PV-DAYS-SINCE-BASELINE =                             10612009
106130             (PV-YEARS-FACTOR + PV-CCYYMMDD-DD-N) - PC-ONE.       10613009
106140                                                                  10614009
106150* ************************************************************** *10615009
106160* * - LOOP THRU TABLE OF MONTH DATA SEEKING MATCH ON           * *10616009
106170* *   MONTH NUMBER (01 - 12)                                   * *10617009
106180* * - WHEN THE MATCH IS FOUND, CAPTURE THE OFFSET NUMBER OF    * *10618009
106190* *   DAYS IN A NON-LEAP YEAR (DAYS PRECEEDING THE GIVEN MONTH). *10619009
106200* ************************************************************** *10620009
106210     PERFORM VARYING PV-SUB1                                      10621009
106220         FROM  PC-01                                              10622009
106230         BY    PC-01                                              10623009
106240         UNTIL PV-SUB1 > PC-MAX-MONTH                             10624009
106250         OR    PV-MONTH-MATCH-FOUND                               10625009
106260         IF PV-CCYYMMDD-MM-N  =  PC-ACT-MO-NUMBER (PV-SUB1)       10626009
106270             ADD PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1) TO          10627009
106280                 PV-DAYS-SINCE-BASELINE                           10628009
106290             SET PV-MONTH-MATCH-FOUND TO TRUE                     10629009
106300         END-IF                                                   10630009
106310     END-PERFORM.                                                 10631009
106320                                                                  10632009
106330     IF  PV-MONTH-MATCH-FOUND                                     10633009
106340         SUBTRACT PC-01 FROM PV-SUB1                              10634009
106350         MOVE PV-CCYYMMDD-CCYY-N TO PV-CHK-LEAPYR-CCYY-N          10635009
106360         PERFORM 5800-CHECK-LEAP-YEAR                             10636009
106370         IF  PV-LEAP-YEAR                                         10637009
106380         AND PV-SUB1 > PC-FEB                                     10638009
106390             ADD PC-ONE TO PV-DAYS-SINCE-BASELINE                 10639009
106400         END-IF                                                   10640009
106410     ELSE                                                         10641009
106420         SET PV-INTERNAL-PRCSS-ERR     TO TRUE                    10642009
106430         MOVE PC-99-MO-ERROR           TO PV-CCYYMMDD-MM-N        10643009
106440         MOVE '5200-'                  TO PV-ERRMSG99-PARA        10644009
106450         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      10645009
106460                                       TO DPG54-ERROR-MESSAGE     10646009
106470     END-IF.                                                      10647009
106480                                                                  10648009
106490     IF  PV-EDIT-SW-OK-SO-FAR                                     10649009
106500     AND PV-INTERNAL-PRCSS-OK                                     10650009
106510         PERFORM 5999-VALIDATE-CREL                               10651009
106520     END-IF.                                                      10652009
106530                                                                  10653009
106540                                                                  10654009
106550* ************************************************************** *10655009
106560* * CONVERT PV-DAYS-SINCE-BASELINE TO FORMAT PV-CCYYMMDD,      * *10656009
106570* * PROVIDED "PV-DAYS-SINCE-BASELINE" FALLS WITHIN THE RANGE   * *10657009
106580* * OF VALID VALUES.                                           * *10658009
106590* ************************************************************** *10659009
106600 5300-CONVERT-DAYS-TO-CCYYMMDD.                                   10660009
106610                                                                  10661009
106620     PERFORM 5301-5300-INITIALIZATIONS.                           10662009
106630                                                                  10663009
106640     PERFORM 5999-VALIDATE-CREL.                                  10664009
106650                                                                  10665009
106660     IF  PV-EDIT-SW-OK-SO-FAR                                     10666009
106670     AND PV-INTERNAL-PRCSS-OK                                     10667009
106680         PERFORM 5302-OK-CONVERT-VALID-DAYS                       10668009
106690     END-IF.                                                      10669009
106700                                                                  10670009
106710                                                                  10671009
106720* ************************************************************** *10672009
106730* * INITIALIZE VALUES USED & OUTPUT FROM THE PARAGRAPH TO:     * *10673009
106740* * CONVERT-DAYS-TO-CCYYMMDD.                                  * *10674009
106750* ************************************************************** *10675009
106760 5301-5300-INITIALIZATIONS.                                       10676009
106770                                                                  10677009
106780     INITIALIZE PV-CCYYMMDD                                       10678009
106790                PV-DAYS-FACTOR                                    10679009
106800                PV-YEARS-FACTOR                                   10680009
106810                PV-LEAPDAY.                                       10681009
106820     SET PV-INTERNAL-PRCSS-SW-INIT                                10682009
106830         PV-MONTH-MATCH-SW-INITIALIZED  TO TRUE.                  10683009
106840     MOVE PC-MAX-MONTH                  TO PV-SUB1.               10684009
106850                                                                  10685009
106860                                                                  10686009
106870* ************************************************************** *10687009
106880* * CONVERT PV-DAYS-SINCE-BASELINE TO FORMAT PV-CCYYMMDD.      * *10688009
106890* * PV-DAYS-SINCE-BASELINE ("CREL", THE COMPARE RELATIVE DATE) * *10689009
106900* * IS THE COMMON BASIS USED FOR ALL DATE MATH.                * *10690009
106910* *                                                            * *10691009
106920* * 1)  TO ACCOUNT FOR THE "YEAR" FIELD OF THE GIVEN DATE:     * *10692009
106930* *     A. DIVIDES DAYS BY 365.25, GIVING NUMBER OF YEARS      * *10693009
106940* *        SINCE THE BASELINE.                                 * *10694009
106950* *     B. ADDS THE BASELINE YEAR TO GET THE ACTUAL YEAR.      * *10695009
106960* * 2)  TO ACCOUNT FOR THE "MONTH" & "DAY" OF THE GIVEN DATE:  * *10696009
106970* *     A. MULTIPLIES THE NUMBER OF YEARS SINCE THE BASELINE   * *10697009
106980* *        BY 365.25 TO GET NUMBER OF DAYS IN PRIOR YEARS      * *10698009
106990* *     B. SUBTRACTS NUMBER OF DAYS IN PRIOR YEARS FROM        * *10699009
107000* *        DAYS-SINCE-BASELINE TO GET THE NUMBER OF DAYS       * *10700009
107010* *        OFFSET INTO THIS YEAR.                              * *10701009
107020* *     C. LOOKS AT THE MONTH TABLE TO SEE WHERE THIS OFFSET   * *10702009
107030* *        FALLS.                                              * *10703009
107040* ************************************************************** *10704009
107050 5302-OK-CONVERT-VALID-DAYS.                                      10705009
107060                                                                  10706009
107070     IF  PV-DAYS-SINCE-BASELINE = ZERO                            10707009
107080         MOVE PC-HT-BASELINE-DATE TO PV-CCYYMMDD                  10708009
107090     ELSE                                                         10709009
107100         COMPUTE PV-YEARS-FACTOR =                                10710009
107110             PV-DAYS-SINCE-BASELINE /                             10711009
107120             PC-DAYS-PER-YEAR                                     10712009
107130                                                                  10713009
107140         COMPUTE PV-CCYYMMDD-CCYY-N =                             10714009
107150             PV-YEARS-FACTOR +                                    10715009
107160             PC-HT-BASELINE-YEAR                                  10716009
107170                                                                  10717009
107180         COMPUTE PV-YEARS-FACTOR =                                10718009
107190             PV-YEARS-FACTOR *                                    10719009
107200             PC-DAYS-PER-YEAR                                     10720009
107210                                                                  10721009
107220         COMPUTE PV-DAYS-FACTOR =                                 10722009
107230             PV-DAYS-SINCE-BASELINE -                             10723009
107240             PV-YEARS-FACTOR                                      10724009
107250                                                                  10725009
107260     END-IF.                                                      10726009
107270                                                                  10727009
107280     PERFORM VARYING PV-SUB1                                      10728009
107290         FROM  PC-MAX-MONTH                                       10729009
107300         BY    PC-NEG01                                           10730009
107310         UNTIL PV-SUB1 < PC-01                                    10731009
107320         OR    PV-MONTH-MATCH-FOUND                               10732009
107330             IF  PV-DAYS-FACTOR         >=                        10733009
107340                 PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1)             10734009
107350                 MOVE PC-ACT-MO-NUMBER (PV-SUB1)                  10735009
107360                                        TO PV-CCYYMMDD-MM-N       10736009
107370                 SET PV-MONTH-MATCH-FOUND                         10737009
107380                                        TO TRUE                   10738009
107390                 ADD PC-01              TO PV-SUB1                10739009
107400             END-IF                                               10740009
107410     END-PERFORM.                                                 10741009
107420                                                                  10742009
107430     IF PV-MONTH-MATCH-FOUND                                      10743009
107440         MOVE PV-CCYYMMDD-CCYY-N        TO PV-CHK-LEAPYR-CCYY-N   10744009
107450         PERFORM 5800-CHECK-LEAP-YEAR                             10745009
107460         IF  PV-LEAP-YEAR                                         10746009
107470             IF  PV-DAYS-FACTOR  =  PC-LEAPDAY-OFFSET             10747009
107480                 MOVE PC-MAX-DAY-IN-FEB TO PV-DAYS-FACTOR         10748009
107490                 MOVE PC-FEB            TO PV-CCYYMMDD-MM-N       10749009
107500             ELSE                                                 10750009
107510                 IF  PV-DAYS-FACTOR   >= PC-LEAP-YEAR-DAY         10751009
107520                     COMPUTE PV-DAYS-FACTOR =                     10752009
107530                         (PV-DAYS-FACTOR    -                     10753009
107540                         PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1))    10754009
107550                     IF  PV-DAYS-FACTOR = ZERO                    10755009
107560                         SUBTRACT PC-01  FROM PV-SUB1             10756009
107570                         MOVE PC-ACT-DAYS-THIS-MO (PV-SUB1)       10757009
107580                         TO   PV-DAYS-FACTOR                      10758009
107590                         MOVE PC-ACT-MO-NUMBER (PV-SUB1)          10759009
107600                         TO   PV-CCYYMMDD-MM-N                    10760009
107610                     END-IF                                       10761009
107620                 ELSE                                             10762009
107630                     COMPUTE  PV-DAYS-FACTOR =                    10763009
107640                         (PV-DAYS-FACTOR     -                    10764009
107650                        PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1)) +   10765009
107660                        PC-ONE                                    10766009
107670                 END-IF                                           10767009
107680             END-IF                                               10768009
107690         ELSE                                                     10769009
107700             COMPUTE  PV-DAYS-FACTOR       =                      10770009
107710                     (PV-DAYS-FACTOR       -                      10771009
107720                      PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1)) +     10772009
107730                      PC-ONE                                      10773009
107740         END-IF                                                   10774009
107750                                                                  10775009
107760                                                                  10776009
107770         MOVE PV-DAYS-FACTOR            TO PV-CCYYMMDD-DD-N       10777009
107780                                                                  10778009
107790         IF  PV-CCYYMMDD-DD-N = PC-32ND-DAY                       10779009
107800         AND PV-SUB1 = PC-MAX-MONTH                               10780009
107810             MOVE PC-1ST-DAY            TO PV-CCYYMMDD-DD-N       10781009
107820             MOVE PC-1ST-MONTH          TO PV-CCYYMMDD-MM-N       10782009
107830             ADD  PC-ONE                TO PV-CCYYMMDD-CCYY-N     10783009
107840         END-IF                                                   10784009
107850         IF  PV-CCYYMMDD-DD-N = ZERO                              10785009
107860         AND PV-CCYYMMDD-YY-N = ZERO                              10786009
107870             MOVE PC-1ST-DAY            TO PV-CCYYMMDD-DD-N       10787009
107880         END-IF                                                   10788009
107890     ELSE                                                         10789009
107900         SET PV-INTERNAL-PRCSS-ERR      TO TRUE                   10790009
107910         MOVE PC-99-MO-ERROR            TO PV-CCYYMMDD-MM-N       10791009
107920         MOVE '5302-'                   TO PV-ERRMSG99-PARA       10792009
107930         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      10793009
107940                                        TO DPG54-ERROR-MESSAGE    10794009
107950     END-IF.                                                      10795009
107960                                                                  10796009
107970                                                                  10797009
107980* ************************************************************** *10798009
107990* * THIS PARAGRAPH:                                            * *10799009
108000* * 1) EDITS:    MONTH FOR VALIDITY AS A NUMERIC SUBSCRIPT     * *10800009
108010* * 2) USING:    VALID 2-POSITION NUMERIC MONTH AS A SUBSCRIPT * *10801009
108020* *    CAPTURES: 3-POSITION ALPHA MONTH ABBREVIATION           * *10802009
108030* *              AND                                           * *10803009
108040* *              FULL MONTH NAME                               * *10804009
108050* *              FROM THE TABLE OF MONTH STATISTICS.           * *10805009
108060* ************************************************************** *10806009
108070 5400-GET-MO-ABBRV3-N-NAME.                                       10807009
108080                                                                  10808009
108090     PERFORM 5401-5400-INITIALIZATIONS.                           10809009
108100                                                                  10810009
108110     MOVE PV-CCYYMMDD-MM-N TO PV-MO-NUMERIC.                      10811009
108120                                                                  10812009
108130     IF  PV-MO-NUMERIC NOT NUMERIC                                10813009
108140     OR  PV-MO-NUMERIC < PC-1ST-MONTH                             10814009
108150     OR  PV-MO-NUMERIC > PC-MAX-MONTH                             10815009
108160         SET PV-INTERNAL-PRCSS-ERR  TO TRUE                       10816009
108170         MOVE SPACES                TO PV-MO-ABBRV-3              10817009
108180         MOVE '5400-'               TO PV-ERRMSG99-PARA           10818009
108190         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      10819009
108200                                    TO DPG54-ERROR-MESSAGE        10820009
108210     ELSE                                                         10821009
108220         MOVE PC-ACT-MO-ABBRV-3 (PV-MO-NUMERIC)                   10822009
108230                                    TO PV-MO-ABBRV-3              10823009
108240         MOVE PC-ACT-FULL-MONTH-NAME (PV-MO-NUMERIC)              10824009
108250                                    TO PV-FULL-DATE-MONTH-NAME    10825009
108260         MOVE PC-ACT-DAYS-THIS-MO (PV-MO-NUMERIC)                 10826009
108270                                    TO PV-DAYS-IN-MONTH           10827009
108280         IF  PV-LEAP-YEAR                                         10828009
108290         AND PV-MO-NUMERIC = PC-FEB                               10829009
108300             MOVE PC-MAX-DAY-IN-FEB TO PV-DAYS-IN-MONTH           10830009
108310         END-IF                                                   10831009
108320     END-IF.                                                      10832009
108330                                                                  10833009
108340                                                                  10834009
108350* ************************************************************** *10835009
108360* * INITIALIZE VALUES USED & OUTPUT FROM THE PARAGRAPH TO:     * *10836009
108370* * GET-MO-ABBRV3-N-NAME.                                      * *10837009
108380* ************************************************************** *10838009
108390 5401-5400-INITIALIZATIONS.                                       10839009
108400     INITIALIZE PV-MONTH-REPRESENTATIONS.                         10840009
108410                                                                  10841009
108420                                                                  10842009
108430* ************************************************************** *10843009
108440* * CONVERT GIVEN DATE (PV-CCYYMMDD) TO JULIAN (PV-CCYYDDD).   * *10844009
108450* ************************************************************** *10845009
108460 5500-CONVERT-TO-JULIAN.                                          10846009
108470     PERFORM 5501-5500-INITIALIZATIONS.                           10847009
108480     SET PV-INTERNAL-PRCSS-SW-INIT      TO TRUE.                  10848009
108490                                                                  10849009
108500*    *********************************************************** *10850009
108510*    * YEAR                                                    * *10851009
108520*    *********************************************************** *10852009
108530     MOVE PV-CCYYMMDD-CCYY-N        TO PV-CCYYDDD-CCYY-X.         10853009
108540                                                                  10854009
108550*    *********************************************************** *10855009
108560*    * LEAP YEAR ADJUSTMENT TO APPLY TO DAYS                   * *10856009
108570*    *********************************************************** *10857009
108580     MOVE PV-CCYYMMDD-CCYY-N        TO PV-CHK-LEAPYR-CCYY-N.      10858009
108590     PERFORM 5800-CHECK-LEAP-YEAR.                                10859009
108600     IF  PV-LEAP-YEAR                                             10860009
108610     AND PV-CCYYMMDD-MM-N NOT = PC-1ST-MONTH                      10861009
108620     AND PV-CCYYMMDD-MM-N NOT = PC-FEB                            10862009
108630         MOVE PC-ONE                TO PV-LEAPDAY                 10863009
108640     END-IF.                                                      10864009
108650     PERFORM VARYING PV-SUB1                                      10865009
108660         FROM  PC-01                                              10866009
108670         BY    PC-01                                              10867009
108680         UNTIL PV-SUB1 > PC-MAX-MONTH                             10868009
108690         OR    PV-MONTH-MATCH-FOUND                               10869009
108700         IF PV-CCYYMMDD-MM-N   =  PC-ACT-MO-NUMBER (PV-SUB1)      10870009
108710             SET PV-MONTH-MATCH-FOUND TO TRUE                     10871009
108720*            *************************************************** *10872009
108730*            * CAPTURE NUMBER OF DAYS PRECEEDING THIS MONTH    * *10873009
108740*            *************************************************** *10874009
108750             MOVE PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1)            10875009
108760                                    TO PV-CCYYDDD-DDD-N           10876009
108770*            *************************************************** *10877009
108780*            * INCREMENT DAYS BY DAY-OF-MONTH, LEAP YEAR ADJ   * *10878009
108790*            *************************************************** *10879009
108800             ADD PV-CCYYMMDD-DD-N   TO PV-CCYYDDD-DDD-N           10880009
108810             ADD PV-LEAPDAY         TO PV-CCYYDDD-DDD-N           10881009
108820         END-IF                                                   10882009
108830     END-PERFORM.                                                 10883009
108840                                                                  10884009
108850     IF PV-MONTH-MATCH-FOUND                                      10885009
108860         CONTINUE                                                 10886009
108870     ELSE                                                         10887009
108880         SET PV-INTERNAL-PRCSS-ERR  TO TRUE                       10888009
108890         MOVE '5500-'               TO PV-ERRMSG99-PARA           10889009
108900         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      10890009
108910                                    TO DPG54-ERROR-MESSAGE        10891009
108920     END-IF.                                                      10892009
108930                                                                  10893009
108940                                                                  10894009
108950* ************************************************************** *10895009
108960* * INITIALIZE VALUES USED & OUTPUT FROM THE PARAGRAPH TO:     * *10896009
108970* * CONVERT GIVEN DATE (PV-CCYYMMDD) TO JULIAN (PV-CCYYDDD).   * *10897009
108980* ************************************************************** *10898009
108990 5501-5500-INITIALIZATIONS.                                       10899009
109000     INITIALIZE PV-CCYYDDD-N                                      10900009
109010                PV-LEAPDAY                                        10901009
109020                PV-CHK-LEAPYR-CCYY-N.                             10902009
109030     SET PV-MONTH-MATCH-SW-INITIALIZED  TO TRUE.                  10903009
109040     MOVE PC-01                         TO PV-SUB1.               10904009
109050                                                                  10905009
109060                                                                  10906009
109070* ************************************************************** *10907009
109080* * GET THE NUMERIC MONTH REPRESENTATION.                      * *10908009
109090* * THIS IS DONE WHILE CONVERTING A GIVEN DATE (PV-CCYYDDD)    * *10909009
109100* * TO WORKING FORMAT CCYYMMDD.                                * *10910009
109110* ************************************************************** *10911009
109120 5600-CONVERT-JUL-TO-CCYYMMDD.                                    10912009
109130                                                                  10913009
109140     INITIALIZE PV-CCYYMMDD                                       10914009
109150                PV-DAYS-FACTOR                                    10915009
109160                PV-YEARS-FACTOR                                   10916009
109170                PV-LEAPDAY.                                       10917009
109180     SET PV-INTERNAL-PRCSS-SW-INIT                                10918009
109190         PV-EDIT-SW-INITIALIZED                                   10919009
109200         PV-MONTH-MATCH-SW-INITIALIZED    TO TRUE.                10920009
109210     MOVE PC-MAX-MONTH                    TO PV-SUB1.             10921009
109220                                                                  10922009
109230     IF PV-CCYYDDD-CCYY-X  NOT NUMERIC                            10923009
109240     OR PV-CCYYDDD-DDD-X   NOT NUMERIC                            10924009
109250         SET PV-INTERNAL-PRCSS-ERR        TO TRUE                 10925009
109260         MOVE SPACES                      TO PV-CCYYMMDD          10926009
109270         MOVE '5600- A'                   TO PV-ERRMSG99-PARA     10927009
109280         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      10928009
109290                                          TO DPG54-ERROR-MESSAGE  10929009
109300     ELSE                                                         10930009
109310*        ******************************************************* *10931009
109320*        *  YEAR                                               * *10932009
109330*        ******************************************************* *10933009
109340         MOVE PV-CCYYDDD-CCYY-X           TO PV-CCYYMMDD-CCYY-X   10934009
109350         MOVE PV-CCYYMMDD-CCYY-N          TO PV-CHK-LEAPYR-CCYY-N 10935009
109360         PERFORM  5800-CHECK-LEAP-YEAR                            10936009
109370*        ******************************************************* *10937009
109380*        *  VALIDATE DDD RELATIVE TO LEAPYEAR OR NON-LEAPYEAR  * *10938009
109390*        ******************************************************* *10939009
109400         IF  PV-LEAP-YEAR                                         10940009
109410             IF  PV-VALID-ACTUAL-DAYS-IN-LEAPYR                   10941009
109420                 CONTINUE                                         10942009
109430             ELSE                                                 10943009
109440                 SET PV-EDIT-SW-ERROR     TO TRUE                 10944009
109450             END-IF                                               10945009
109460         ELSE                                                     10946009
109470             IF  PV-VALID-ACTUAL-DAYS-NONLEAPYR                   10947009
109480                 CONTINUE                                         10948009
109490             ELSE                                                 10949009
109500                 SET PV-EDIT-SW-ERROR     TO TRUE                 10950009
109510             END-IF                                               10951009
109520         END-IF                                                   10952009
109530         IF  PV-EDIT-SW-OK-SO-FAR                                 10953009
109540*            *************************************************** *10954009
109550*            *  LEAP YEAR ADJUSTMENT TO APPLY TO MONTH & DAY   * *10955009
109560*            *************************************************** *10956009
109570             IF  PV-LEAP-YEAR                                     10957009
109580             AND PV-CCYYDDD-DDD-N >= PC-LEAP-YEAR-DAY             10958009
109590                 MOVE  PC-ONE             TO PV-LEAPDAY           10959009
109600             END-IF                                               10960009
109610                                                                  10961009
109620             PERFORM VARYING PV-SUB1                              10962009
109630                 FROM PC-MAX-MONTH                                10963009
109640                 BY   PC-NEG01                                    10964009
109650                 UNTIL PV-SUB1 < PC-01                            10965009
109660                 OR PV-MONTH-MATCH-FOUND                          10966009
109670                     MOVE PC-ACT-ACCUM-PRIOR-MO-DAYS (PV-SUB1)    10967009
109680                                          TO PV-DAYS-FACTOR       10968009
109690                     ADD PV-LEAPDAY       TO PV-DAYS-FACTOR       10969009
109700                     IF  PV-CCYYDDD-DDD-N  > PV-DAYS-FACTOR       10970009
109710*                        *************************************** *10971009
109720*                        *     MONTH: ABBRV3, FULL NAME, NBR   * *10972009
109730*                        *************************************** *10973009
109740                         MOVE PC-ACT-MO-NUMBER (PV-SUB1)          10974009
109750                                          TO PV-CCYYMMDD-MM-N     10975009
109760                         MOVE PC-ACT-FULL-MONTH-NAME (PV-SUB1)    10976009
109770                                          TO                      10977009
109780                                           PV-FULL-DATE-MONTH-NAME10978009
109790                         MOVE PV-SUB1     TO PV-MO-NUMERIC        10979009
109800                         SET PV-MONTH-MATCH-FOUND                 10980009
109810                                          TO TRUE                 10981009
109820                     END-IF                                       10982009
109830             END-PERFORM                                          10983009
109840                                                                  10984009
109850             IF PV-MONTH-MATCH-FOUND                              10985009
109860*                *********************************************** *10986009
109870*                * CALCULATE DAY OF THE MONTH                  * *10987009
109880*                *********************************************** *10988009
109890                 MULTIPLY PC-NEG01 BY PV-DAYS-FACTOR              10989009
109900                 ADD PV-CCYYDDD-DDD-N     TO PV-DAYS-FACTOR       10990009
109910                 IF  PV-CCYYMMDD-MM-N = PC-1ST-MONTH              10991009
109920                 OR  PV-CCYYMMDD-MM-N = PC-FEB                    10992009
109930                     ADD PV-LEAPDAY       TO PV-DAYS-FACTOR       10993009
109940                 END-IF                                           10994009
109950                 MOVE PV-DAYS-FACTOR      TO PV-CCYYMMDD-DD-N     10995009
109960             ELSE                                                 10996009
109970                 SET PV-INTERNAL-PRCSS-ERR                        10997009
109980                                          TO TRUE                 10998009
109990                 MOVE SPACES              TO PV-CCYYMMDD          10999009
110000                 MOVE '5600- B'           TO PV-ERRMSG99-PARA     11000009
110010                 MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR              11001009
110020                                          TO DPG54-ERROR-MESSAGE  11002009
110030             END-IF                                               11003009
110040         ELSE                                                     11004009
110050             SET PV-INTERNAL-PRCSS-ERR    TO TRUE                 11005009
110060             MOVE '5600- C'               TO PV-ERRMSG99-PARA     11006009
110070             MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                  11007009
110080                                          TO DPG54-ERROR-MESSAGE  11008009
110090         END-IF.                                                  11009009
