082730         UNTIL    HT-INDEX < PC-01                                08267009
082740            OR    PV-BUS-DAYS-IDENTIFIED.                         08268009
082750                                                                  08269009
082760     MOVE PV-COUNT-BUSDAY         TO  PV-CREL-CMPR-DIFF-BUS.      08270009
082770                                                                  08271009
082780                                                                  08272009
082790* ************************************************************** *08273009
082800* * FOR EFFICIENT EXECUTION, DETERMINE THE STARTING POINT IN   * *08274009
082810* * THE HOLIDAY TABLE.                                         * *08275009
082820* * - SET THE INDEX TO POINT TO THE BEGINNING OF THE YEAR      * *08276009
082830* *   OF THE PRIMARY INPUT DATE.                               * *08277009
082840* * - THEN COMPARE THE GIVEN DATE WITH THE TABLIZED HOLIDAYS,  * *08278009
082850* *   ADVANCING THRU THE TABLE AS REQUIRED, TO REFINE THE      * *08279009
082860* *   STARTING POINT.                                          * *08280009
082870* ************************************************************** *08281009
082880 3610-CALC-STARTING-LO-HOLIDX.                                    08282009
082890                                                                  08283009
082900     IF PV-SET-IDX-CCYY-N = PC-HT-BASELINE-YEAR                   08284009
082910         MOVE PC-01 TO PV-HT-STARTING-POINT                       08285009
082920     ELSE                                                         08286009
082930         COMPUTE PV-HT-STARTING-POINT = (PV-SET-IDX-CCYY-N        08287009
082940                                      -  PC-HT-BASELINE-YEAR)     08288009
082950                                      *  PC-HT-HOLIDAYS-PER-YR    08289009
082960     END-IF.                                                      08290009
082970                                                                  08291009
082980*    *********************************************************** *08292009
082990*    *  ERROR TRAP TO PREVENT INDEX ERROR                      * *08293009
083000*    *  (FUTURE ENHANCEMENT COULD CAUSE CONSTANTS OUT-OF-SYNC) * *08294009
083010*    *  (WITH HT-TABLE OCCURS CLAUSE OR DATA.                ) * *08295009
083020*    *********************************************************** *08296009
083030     IF  PV-HT-STARTING-POINT < PC-01                             08297009
083040         MOVE PC-01                     TO PV-HT-STARTING-POINT   08298009
083050         SET PV-INTERNAL-PRCSS-ERR      TO TRUE                   08299009
083060         MOVE '3610- A'                 TO PV-ERRMSG99-PARA       08300009
083070         MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                      08301009
083080                                        TO DPG54-ERROR-MESSAGE    08302009
083090     ELSE                                                         08303009
083100         IF  PV-HT-STARTING-POINT > PC-HT-MAX-HOLIDAY-ENTRIES     08304009
083110             MOVE PC-HT-MAX-HOLIDAY-ENTRIES                       08305009
083120                                        TO PV-HT-STARTING-POINT   08306009
083130             SET PV-INTERNAL-PRCSS-ERR  TO TRUE                   08307009
083140             MOVE '3610- B'             TO PV-ERRMSG99-PARA       08308009
083150             MOVE PC-ERRMSG99-INTERNAL-PRCSS-ERR                  08309009
083160                                        TO DPG54-ERROR-MESSAGE    08310009
083170         END-IF                                                   08311009
083180     END-IF.                                                      08312009
083190                                                                  08313009
083200     SET HT-INDEX  TO  PV-HT-STARTING-POINT.                      08314009
083210                                                                  08315009
083220     IF PV-CREL-DATE-TO-CHECK > HT-DAYS-SINCE-BASELINE (HT-INDEX) 08316009
083230         PERFORM 3620-ESTABLISH-HOL-TBL-POS                       08317009
083240            VARYING PV-PREDAYS-LOOP-COUNT                         08318009
083250            FROM   PC-01                                          08319009
083260            BY     PC-01                                          08320009
083270            UNTIL  HT-INDEX > PC-HT-MAX-HOLIDAY-ENTRIES           08321009
083280               OR  PV-PREDAYS-LOOP-COUNT > PC-HT-HOLIDAYS-PLUS-ONE08322009
083290               OR  PV-STARTING-DATE-FOUND                         08323009
083300     END-IF.                                                      08324009
083310                                                                  08325009
083320                                                                  08326009
083330* ************************************************************** *08327009
083340* * REFINE THE STARTING POSITION IN THE TABLE.                 * *08328009
083350* * - MOVE PAST ALL PRECEEDING HOLIDAYS FOR THE YEAR.          * *08329009
083360* ************************************************************** *08330009
083370 3620-ESTABLISH-HOL-TBL-POS.                                      08331009
083380                                                                  08332009
083390     IF PV-CREL-DATE-TO-CHECK > HT-DAYS-SINCE-BASELINE (HT-INDEX) 08333009
083400         SET HT-INDEX UP BY PC-01                                 08334009
083410     ELSE                                                         08335009
083420         SET PV-STARTING-DATE-FOUND TO TRUE                       08336009
083430     END-IF.                                                      08337009
083440                                                                  08338009
