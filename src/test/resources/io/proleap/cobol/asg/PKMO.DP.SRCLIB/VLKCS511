125200                    - PV-TOP-ITEM + 1.                            VLKCS511
125300     IF  PV-SUB < +1 OR > PC-ITEMS-PER-PANEL                      VLKCS511
125400         CONTINUE                                                 VLKCS511
125500     ELSE                                                         VLKCS511
125600         MOVE -1                   TO MR-SELECTL (PV-SUB)         VLKCS511
125700         MOVE DP015-REVERSE        TO MR-SELECTH (PV-SUB)         VLKCS511
125800         MOVE DP015-RED            TO MR-SELECTC (PV-SUB)         VLKCS511
125900     END-IF.                                                      VLKCS511
126000                                                                  VLKCS511
126100*----------------------------------------------------------------*VLKCS511
126200*    WHENEVER THE USER SELECTS AN ITEM AS THE BEGINNING RANGE    *VLKCS511
126300*    AND ONE AS THE ENDING RANGE, MARK THESE ITEMS AS WELL AS    *VLKCS511
126400*    THOSE THAT FALL IN THE RANGE AS BEING SELECTED.             *VLKCS511
126500*----------------------------------------------------------------*VLKCS511
126600 3500-MARK-RANGE-AS-SELECTED.                                     VLKCS511
126700                                                                  VLKCS511
126800     MOVE +1                       TO  ASC-BLK-SUB.               VLKCS511
126900     PERFORM 3510-MARK-SELECTED-INDS                              VLKCS511
127000         VARYING ASC-ITEM-SUB                                     VLKCS511
127100         FROM 1 BY 1                                              VLKCS511
127200         UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL.                 VLKCS511
127300                                                                  VLKCS511
127400     ADD +1                        TO ASC-BLK-SUB.                VLKCS511
127500     PERFORM 3510-MARK-SELECTED-INDS                              VLKCS511
127600         VARYING ASC-ITEM-SUB                                     VLKCS511
127700         FROM 1 BY 1                                              VLKCS511
127800         UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL.                 VLKCS511
127900                                                                  VLKCS511
128000     MOVE ASC-LIST-ITEM-NUMBER(1 1)                               VLKCS511
128100                                   TO  PV-LIST-ITEM-NUMBER.       VLKCS511
128200     COMPUTE PV-FIRST-BLOCK-IN-ARRAY =                            VLKCS511
128300         1 + ((PV-LIST-ITEM-NUMBER - 1) /                         VLKCS511
128400               PC-ITEMS-PER-PANEL).                               VLKCS511
128500                                                                  VLKCS511
128600     MOVE ASC-START-OF-SELECTED-RANGE                             VLKCS511
128700                                   TO PV-START-OF-SELECTED-RANGE. VLKCS511
128800     COMPUTE PV-FIRST-BLOCK-IN-RANGE =                            VLKCS511
128900         1 + ((PV-START-OF-SELECTED-RANGE - 1) /                  VLKCS511
129000               PC-ITEMS-PER-PANEL).                               VLKCS511
129100                                                                  VLKCS511
129200     MOVE ASC-END-OF-SELECTED-RANGE                               VLKCS511
129300                                   TO  PV-END-OF-SELECTED-RANGE.  VLKCS511
129400     COMPUTE PV-LAST-BLOCK-IN-RANGE =                             VLKCS511
129500         1 + ((PV-END-OF-SELECTED-RANGE - 1) /                    VLKCS511
129600               PC-ITEMS-PER-PANEL).                               VLKCS511
129700     COMPUTE PV-NEXT-BLOCK-IN-ARRAY =                             VLKCS511
129800         PV-FIRST-BLOCK-IN-ARRAY + 1.                             VLKCS511
129900                                                                  VLKCS511
130000     IF (PV-FIRST-BLOCK-IN-RANGE < PV-FIRST-BLOCK-IN-ARRAY) OR    VLKCS511
130100        (PV-LAST-BLOCK-IN-RANGE  > PV-NEXT-BLOCK-IN-ARRAY)        VLKCS511
130200         MOVE +1                   TO ASC-BLK-SUB                 VLKCS511
130300         PERFORM 5100-WRITE-TEMP-STORAGE                          VLKCS511
130400         COMPUTE PV-PREV-BLOCK-IN-ARRAY =                         VLKCS511
130500             PV-FIRST-BLOCK-IN-ARRAY - 1                          VLKCS511
130600                                                                  VLKCS511
130700         PERFORM                                                  VLKCS511
130800             UNTIL PV-FIRST-BLOCK-IN-RANGE >                      VLKCS511
130900                   PV-PREV-BLOCK-IN-ARRAY                         VLKCS511
131000             MOVE PV-FIRST-BLOCK-IN-RANGE                         VLKCS511
131100                                   TO PV-BLOCK-NUMBER             VLKCS511
131200             PERFORM 4410-READ-TEMP-STORAGE                       VLKCS511
131300             PERFORM 3510-MARK-SELECTED-INDS                      VLKCS511
131400                 VARYING ASC-ITEM-SUB                             VLKCS511
131500                 FROM 1 BY 1                                      VLKCS511
131600                 UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL          VLKCS511
131700             PERFORM 5100-WRITE-TEMP-STORAGE                      VLKCS511
131800             ADD +1                TO PV-FIRST-BLOCK-IN-RANGE     VLKCS511
131900         END-PERFORM                                              VLKCS511
132000                                                                  VLKCS511
132100         COMPUTE PV-NEXT-BLOCK-IN-ARRAY =                         VLKCS511
132200             PV-FIRST-BLOCK-IN-ARRAY + PC-MAX-BLOCKS-IN-ARRAY     VLKCS511
132300         PERFORM                                                  VLKCS511
132400             UNTIL PV-NEXT-BLOCK-IN-ARRAY >                       VLKCS511
132500                   PV-LAST-BLOCK-IN-RANGE                         VLKCS511
132600             MOVE PV-NEXT-BLOCK-IN-ARRAY                          VLKCS511
132700                                   TO PV-BLOCK-NUMBER             VLKCS511
132800             PERFORM 4410-READ-TEMP-STORAGE                       VLKCS511
132900             PERFORM 3510-MARK-SELECTED-INDS                      VLKCS511
133000                 VARYING ASC-ITEM-SUB                             VLKCS511
133100                 FROM 1 BY 1                                      VLKCS511
133200                 UNTIL ASC-ITEM-SUB > PC-ITEMS-PER-PANEL          VLKCS511
133300             PERFORM 5100-WRITE-TEMP-STORAGE                      VLKCS511
133400             ADD +1                TO PV-NEXT-BLOCK-IN-ARRAY      VLKCS511
133500         END-PERFORM                                              VLKCS511
133600                                                                  VLKCS511
133700         MOVE PV-FIRST-BLOCK-IN-ARRAY                             VLKCS511
133800                                   TO PV-BLOCK-NUMBER             VLKCS511
133900         PERFORM 4410-READ-TEMP-STORAGE                           VLKCS511
134000     END-IF.                                                      VLKCS511
134100                                                                  VLKCS511
