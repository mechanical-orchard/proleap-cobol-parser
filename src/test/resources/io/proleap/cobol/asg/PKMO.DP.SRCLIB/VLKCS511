062900*----------------------------------------------------------------*VLKCS511
063000*    IF THE USER HITS THE PAGE BACKWARD KEY, COMPUTE THE TOP     *VLKCS511
063100*    ITEM NUMBER FOR THE NEW PAGE.  IF IT IS ABOVE THE TOP,      *VLKCS511
063200*    SET THE TOP ITEM NUMBER TO +1 AND DISPLAY THE TOP OF LIST   *VLKCS511
063300*    MESSAGE.                                                    *VLKCS511
063400*----------------------------------------------------------------*VLKCS511
063500 2110-BROWSE-BACKWARD.                                            VLKCS511
063600                                                                  VLKCS511
063700     SUBTRACT PC-ITEMS-PER-PANEL FROM ASC-ITEM-AT-TOP-OF-PANEL    VLKCS511
063800         GIVING PV-TOP-ITEM.                                      VLKCS511
063900                                                                  VLKCS511
064000     IF  ((PV-TOP-ITEM < +1)                                      VLKCS511
064100       OR (PV-TOP-ITEM = +1))                                     VLKCS511
064200         MOVE +1                   TO PV-TOP-ITEM                 VLKCS511
064300*        -- TOP OF LIST --                                        VLKCS511
064400         MOVE PC-TSYMSG-00069      TO DP020-MSG-NUMBER            VLKCS511
064500         SET DP020-MSG-INFORMATIONAL TO TRUE                      VLKCS511
064600     END-IF.                                                      VLKCS511
064700                                                                  VLKCS511
064800*----------------------------------------------------------------*VLKCS511
064900*    IF THE USER HITS THE PAGE FORWARD KEY, COMPUTE THE TOP      *VLKCS511
065000*    ITEM NUMBER FOR THE NEW PAGE.  IF THE USER ATTEMPTS TO GO   *VLKCS511
065100*    PAST THE END, ISSUE THE BOTTOM OF LIST MESSAGE.             *VLKCS511
065200*----------------------------------------------------------------*VLKCS511
065300 2120-BROWSE-FORWARD.                                             VLKCS511
065400                                                                  VLKCS511
065500     ADD PC-ITEMS-PER-PANEL ASC-ITEM-AT-TOP-OF-PANEL              VLKCS511
065600         GIVING PV-TOP-ITEM.                                      VLKCS511
065700     IF  PV-TOP-ITEM > ASC-NUMBER-OF-ITEMS-READ                   VLKCS511
065800         MOVE ASC-ITEM-AT-TOP-OF-PANEL                            VLKCS511
065900                                   TO PV-TOP-ITEM                 VLKCS511
066000*        -- BOTTOM OF LIST --                                     VLKCS511
066100         MOVE PC-TSYMSG-00070      TO DP020-MSG-NUMBER            VLKCS511
066200         SET DP020-MSG-INFORMATIONAL                              VLKCS511
066300                                   TO TRUE                        VLKCS511
066400     ELSE                                                         VLKCS511
066500         COMPUTE PV-BOTTOM-ITEM =                                 VLKCS511
066600             PV-TOP-ITEM + PC-ITEMS-PER-PANEL - 1                 VLKCS511
066700         IF  PV-BOTTOM-ITEM > ASC-NUMBER-OF-ITEMS-READ            VLKCS511
066800*            -- BOTTOM OF LIST --                                 VLKCS511
066900             MOVE PC-TSYMSG-00070  TO DP020-MSG-NUMBER            VLKCS511
067000             SET DP020-MSG-INFORMATIONAL                          VLKCS511
067100                                   TO TRUE                        VLKCS511
067200         END-IF                                                   VLKCS511
067300     END-IF.                                                      VLKCS511
067400                                                                  VLKCS511
067500*----------------------------------------------------------------*VLKCS511
067600*    THE USER CAN CHANGE THE CURRENT BEGINNING LIST NUMBER SO    *VLKCS511
067700*    THAT THE TRANSACTION WILL JUMP TO THAT POINT IN THE LIST.   *VLKCS511
067800*    THIS IS A FASTER WAY TO GET TO A CERTAIN POINT IN THE LIST  *VLKCS511
067900*    RATHER THAN HITTING NEXT-PAGE TO GET TO THAT POINT.         *VLKCS511
068000*    THE NEW LIST NUMBER ENTERED IS EDITED TO MAKE SURE THAT     *VLKCS511
068100*    IT IS NUMERIC AND FALLS BETWEEN A SPECIFIED RANGE.          *VLKCS511
068200*----------------------------------------------------------------*VLKCS511
068300 2130-CHECK-FOR-JUMP-BROWSE.                                      VLKCS511
068400                                                                  VLKCS511
068500     SET PS-NO-ERROR               TO TRUE.                       VLKCS511
068600     IF (ASC-START-ITEM-REQUEST = SPACE)                          VLKCS511
068700         MOVE ASC-ITEM-AT-TOP-OF-PANEL                            VLKCS511
068800                                   TO PV-TOP-ITEM                 VLKCS511
068900     ELSE                                                         VLKCS511
069000         MOVE ASC-START-ITEM-REQUEST                              VLKCS511
069100                                   TO DP010I-UNEDITED-FIELD       VLKCS511
069200         MOVE PC-ITM-NUMBER-LENGTH TO DP010I-MAXIMUM-DIGITS       VLKCS511
069300         MOVE +0                   TO DP010I-MAXIMUM-DECIMALS     VLKCS511
069400         SET DP010I-NEGATIVE-NOT-ALLOWED                          VLKCS511
069500                                   TO TRUE                        VLKCS511
263627         CALL DP010I-NUMERIC-EDIT-ROUTINE                                 
263627              USING DP010I-NUMERIC-EDIT-AREA                              
069700                                                                  VLKCS511
069800         IF  NOT DP010I-ERROR-DETECTED                            VLKCS511
069900             MOVE DP010I-NUMERIC-FIELD                            VLKCS511
070000                                   TO  ASC-START-ITEM-REQUEST-N   VLKCS511
070100             IF  ASC-START-ITEM-REQUEST-N > ZERO                  VLKCS511
070200                 IF  ASC-START-ITEM-REQUEST-N >                   VLKCS511
070300                                          ASC-NUMBER-OF-ITEMS-READVLKCS511
070400                     MOVE SPACE TO ASC-START-ITEM-REQUEST         VLKCS511
070500                     COMPUTE PV-TOP-ITEM =                        VLKCS511
070600                             ASC-NUMBER-OF-ITEMS-READ             VLKCS511
070700                             - PC-ITEMS-PER-PANEL + 1             VLKCS511
070800                     IF  PV-TOP-ITEM < +1                         VLKCS511
070900                         MOVE +1   TO PV-TOP-ITEM                 VLKCS511
071000                         MOVE PC-TSYMSG-00070                     VLKCS511
071100                                   TO DP020-MSG-NUMBER            VLKCS511
071200                         SET DP020-MSG-INFORMATIONAL              VLKCS511
071300                                   TO TRUE                        VLKCS511
071400                     END-IF                                       VLKCS511
071500                 ELSE                                             VLKCS511
071600                     MOVE ASC-START-ITEM-REQUEST-N                VLKCS511
071700                                   TO PV-TOP-ITEM                 VLKCS511
071800                     MOVE SPACE    TO ASC-START-ITEM-REQUEST      VLKCS511
071900                     COMPUTE PV-BOTTOM-ITEM =                     VLKCS511
072000                         PV-TOP-ITEM + PC-ITEMS-PER-PANEL - 1     VLKCS511
072100                     IF  PV-BOTTOM-ITEM > ASC-NUMBER-OF-ITEMS-READVLKCS511
072200*                        -- BOTTOM OF LIST --                     VLKCS511
072300                         MOVE PC-TSYMSG-00070                     VLKCS511
072400                                   TO DP020-MSG-NUMBER            VLKCS511
072500                         SET DP020-MSG-INFORMATIONAL              VLKCS511
072600                                   TO TRUE                        VLKCS511
072700                     END-IF                                       VLKCS511
072800                 END-IF                                           VLKCS511
072900             ELSE                                                 VLKCS511
072910                CONTINUE                                                  
073000*                --- VALUE OUT OF RANGE ----                      VLKCS511
073100*                MOVE PC-TSYMSG-00101                             VLKCS511
073200*                                  TO DP020-MSG-NUMBER            VLKCS511
073300*                MOVE -1           TO ASTRTITL                    VLKCS511
073400*                SET DP030-SET-CURSOR-APPL-1                      VLKCS511
073500*                                  TO TRUE                        VLKCS511
073600*                MOVE DP015-REVERSE                               VLKCS511
073700*                                  TO ASTRTITH                    VLKCS511
073800*                MOVE DP015-RED    TO ASTRTITC                    VLKCS511
