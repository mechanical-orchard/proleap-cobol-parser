141700*----------------------------------------------------------------*VLKCS511
141800* PREPARE THE DB KEY BY REPLACING * WITH %                       *VLKCS511
141900*----------------------------------------------------------------*VLKCS511
142000*4050-PREPARE-CURSOR-KEY.                                         VLKCS511
142100*                                                                 VLKCS511
142200*    MOVE ASC-KEY-VEND-NAME    TO DK-VEND-NAME.                   VLKCS511
142300*    MOVE ASC-KEY-STATUS-CODE  TO DK-STATUS-CODE.                 VLKCS511
142400*    IF DK-VEND-NAME > SPACES                                     VLKCS511
142500*        IF DK-VEND-NAME-BYTE (1) = '*'                           VLKCS511
142600*            MOVE PC-PERCENT   TO DK-VEND-NAME-BYTE (1)           VLKCS511
142700*            INSPECT DK-VEND-NAME                                 VLKCS511
142800*                REPLACING FIRST '*' BY PC-PERCENT                VLKCS511
142900*        END-IF                                                   VLKCS511
143000*        SET PS-NOT-DONE TO TRUE                                  VLKCS511
143100*        PERFORM VARYING PV-SUB                                   VLKCS511
143200*            FROM LENGTH OF DK-VEND-NAME BY -1                    VLKCS511
143300*            UNTIL PV-SUB = 1                                     VLKCS511
143400*               OR PS-DONE                                        VLKCS511
143500*                IF DK-VEND-NAME-BYTE (PV-SUB) = '*'              VLKCS511
143600*                    MOVE PC-PERCENT TO DK-VEND-NAME-BYTE (PV-SUB)VLKCS511
143700*                    SET PS-DONE TO TRUE                          VLKCS511
143800*                END-IF                                           VLKCS511
143900**               IF DK-VEND-NAME-BYTE (PV-SUB) > SPACES           VLKCS511
144000*                    SET PS-DONE TO TRUE                          VLKCS511
144100*                ELSE                                             VLKCS511
144200*                    MOVE PC-PERCENT TO DK-VEND-NAME-BYTE (PV-SUB)VLKCS511
144300*                END-IF                                           VLKCS511
144400*        END-PERFORM                                              VLKCS511
144500*    ELSE                                                         VLKCS511
144600*        MOVE ALL '%' TO DK-VEND-NAME.                            VLKCS511
144700                                                                  VLKCS511
144800*----------------------------------------------------------------*VLKCS511
144900* OPEN  THE VENDOR   CURSOR.                                     *VLKCS511
145000*----------------------------------------------------------------*VLKCS511
145100 4010-OPEN-CURSOR.                                                VLKCS511
145200                                                                  VLKCS511
145300     PERFORM                                                      VLKCS511
145400         WITH TEST AFTER                                          VLKCS511
145500         VARYING PV-RETRY-COUNTER                                 VLKCS511
145600         FROM 1 BY 1                                              VLKCS511
145700         UNTIL ((PV-RETRY-COUNTER > PC-MAX-RETRIES)               VLKCS511
145800                   OR (SQLCODE = ZERO))                           VLKCS511
145900             MOVE 'CURSOR:  RSNDSC_CSR'                                 88
146000                               TO DP013-MESSAGE-TEXT (2)          VLKCS511
146100             EXEC SQL                                             VLKCS511
146200                  OPEN RSNDSC_CSR                                       88
146300             END-EXEC                                             VLKCS511
146400                                                                  VLKCS511
146500         EVALUATE TRUE                                            VLKCS511
146600             WHEN SQLCODE = ZERO                                  VLKCS511
146700             WHEN SQLCODE = -904                                  VLKCS511
146800             WHEN SQLCODE = -913                                  VLKCS511
146900                 CONTINUE                                         VLKCS511
147000             WHEN SQLWARN0 NOT = SPACES                           VLKCS511
147100             WHEN SQLCODE < ZERO                                  VLKCS511
147200             WHEN SQLCODE > ZERO                                  VLKCS511
147300                 MOVE '4010-OPEN-CURSOR'                          VLKCS511
147400                                   TO DP013-PARAGRAPH             VLKCS511
147500                 MOVE 'OPEN VENDOR CURSOR - '                     VLKCS511
147600                                   TO DP013-MESSAGE-TEXT (1)      VLKCS511
147700                 MOVE SQLCA        TO DP013-SQLCA                 VLKCS511
147800                 MOVE 'TSYMSG  '   TO DP013-DB2-TABLE-NAME (1)    VLKCS511
147900                 SET DP013-DB2-ABEND                              VLKCS511
148000                     DP013-XCTL-DISPLAY-RESTART TO TRUE           VLKCS511
148100                 PERFORM DP013-0000-PROCESS-ABEND                 VLKCS511
148200         END-EVALUATE                                             VLKCS511
148300     END-PERFORM.                                                 VLKCS511
148400                                                                  VLKCS511
148500     IF  PV-RETRY-COUNTER > PC-MAX-RETRIES                        VLKCS511
148600         MOVE '4010-OPEN-CURSOR'   TO DP013-PARAGRAPH             VLKCS511
148700         MOVE 'MAXIMUM RETRIES EXCEEDED ATTEMPTING TO OPEN CSR'   VLKCS511
148800                                   TO DP013-MESSAGE-TEXT (1)      VLKCS511
148900         MOVE SQLCA                TO DP013-SQLCA                 VLKCS511
149000         MOVE 'TSYMSG  '           TO DP013-DB2-TABLE-NAME (1)    VLKCS511
149100         SET DP013-DB2-ABEND                                      VLKCS511
149200             DP013-XCTL-DISPLAY-RESTART                           VLKCS511
149300                                   TO TRUE                        VLKCS511
149400         PERFORM DP013-0000-PROCESS-ABEND                         VLKCS511
149500     END-IF.                                                      VLKCS511
