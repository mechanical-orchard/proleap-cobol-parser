212400*----------------------------------------------------------------*VLKCS511
212500*    INCREMENT THE TSQ ITEM NUMBER AND READ THE LIST QUEUE.      *VLKCS511
212600*    LOOP THROUGH THE LIST QUEUE TO OBTAIN THE LINE ITEM NUMBER  *VLKCS511
212700*    TO BE USED TO UPDATE (DESELECT) THE CORRECT LINE ITEM IN    *VLKCS511
212800*    THE TEMP QUEUE.                                             *VLKCS511
212900*----------------------------------------------------------------*VLKCS511
213000 6100-UPDATE-TSQ.                                                 VLKCS511
213100                                                                  VLKCS511
213200     ADD 1                     TO  PV-SEL-QUEUE-ITEM.             VLKCS511
213300     PERFORM 6105-READ-SEL-QUEUE.                                 VLKCS511
213400     PERFORM                                                      VLKCS511
213500         VARYING VL006-IDX                                        VLKCS511
213600         FROM 1 BY 1                                              VLKCS511
213700         UNTIL ((PV-ITEMS-PROCESSED >                             VLKCS511
213800                              PV-HOLD-SELECTED-ITEMS)             VLKCS511
213900         OR (VL006-IDX GREATER THAN VL006-MAX-ITEMS))             VLKCS511
214000         IF  VL006-ACTION-SW (VL006-IDX) > SPACE                  VLKCS511
214100             PERFORM 6110-PROCESS-ITEMS                           VLKCS511
214200         END-IF                                                   VLKCS511
214300         ADD +1                TO PV-ITEMS-PROCESSED              VLKCS511
214400     END-PERFORM.                                                 VLKCS511
214500                                                                  VLKCS511
214600     PERFORM 6111-WRITE-LIST-QUEUE.                               VLKCS511
214700                                                                  VLKCS511
214800*----------------------------------------------------------------*VLKCS511
214900*    READ THE LIST QUEUE RECORD.                                 *VLKCS511
215000*----------------------------------------------------------------*VLKCS511
215100 6105-READ-SEL-QUEUE.                                             VLKCS511
215200                                                                  VLKCS511
215300     EXEC CICS                                                    VLKCS511
215400          READQ TS                                                VLKCS511
215500              QUEUE (ASC-SEL-QUEUE-NAME)                          VLKCS511
215600              INTO  (VL006-LINE-SEL-REC)                                84
215700              ITEM  (PV-SEL-QUEUE-ITEM)                           VLKCS511
215800              RESP  (PV-CICS-RESP)                                VLKCS511
215900     END-EXEC.                                                    VLKCS511
216000                                                                  VLKCS511
216100     IF  PV-CICS-RESP = DFHRESP (NORMAL)                          VLKCS511
216200         CONTINUE                                                 VLKCS511
216300     ELSE                                                         VLKCS511
216400         SET DP013-CICS-ABEND      TO  TRUE                       VLKCS511
216500         SET DP013-NO-ROLLBACK     TO  TRUE                       VLKCS511
216600         MOVE '6105-READ-SEL-QUEUE'                               VLKCS511
216700                                   TO  DP013-PARAGRAPH            VLKCS511
216800         MOVE 'ATTEMPTING TO READ SELECTED ITEMS QUEUE'           VLKCS511
216900                                   TO  DP013-MESSAGE-TEXT(1)      VLKCS511
217000         PERFORM DP013-0000-PROCESS-ABEND                         VLKCS511
217100     END-IF.                                                      VLKCS511
217200                                                                  VLKCS511
217300*----------------------------------------------------------------*VLKCS511
217400*    CALCULATE THE TEMP QUEUE ITEM THAT MUST BE READ AND READ IT.*VLKCS511
217500*    REWRITE THE CURRENT TEMP QUEUE ITEM IF NECESSARY.  CALCULATE*VLKCS511
217600*    THE LINE ITEM OCCURENCE AND UPDATE (DESELECT) IT.           *VLKCS511
217700*----------------------------------------------------------------*VLKCS511
217800 6110-PROCESS-ITEMS.                                              VLKCS511
217900                                                                  VLKCS511
218000     COMPUTE PV-ITEM = 1 + ((VL006-ITEM (VL006-IDX) - 1)          VLKCS511
218100                     / PC-ITEMS-PER-PANEL).                       VLKCS511
218200                                                                  VLKCS511
218300     IF  PV-ITEM NOT EQUAL PV-SAVE-ITEM                           VLKCS511
218400         IF  PV-SAVE-ITEM NOT EQUAL ZERO                          VLKCS511
218500             PERFORM 6111-WRITE-LIST-QUEUE                        VLKCS511
218600         END-IF                                                   VLKCS511
218700         MOVE PV-ITEM          TO PV-SAVE-ITEM                    VLKCS511
218800                                                                  VLKCS511
218900         PERFORM 6112-READ-LIST-QUEUE                             VLKCS511
219000         IF  PV-CICS-RESP NOT EQUAL DFHRESP(NORMAL)               VLKCS511
219100             CONTINUE                                             VLKCS511
219200         END-IF                                                   VLKCS511
219300     END-IF.                                                      VLKCS511
219400                                                                  VLKCS511
219500     COMPUTE PV-SUB = (VL006-ITEM (VL006-IDX)                     VLKCS511
219600                    - LQW-LIST-ITEM-NUMBER (1)) + 1.              VLKCS511
219700     SET LQW-DESELECT (PV-SUB) TO    TRUE                         VLKCS511
219800     MOVE LQW-SELECTED-SW (PV-SUB)                                VLKCS511
219900                               TO LQW-TEMP-SELECTED-SW (PV-SUB).  VLKCS511
220000     EVALUATE TRUE                                                VLKCS511
220100         WHEN VL006-INQ (VL006-IDX)                               VLKCS511
220200              MOVE PC-INQ      TO LQW-ACTION (PV-SUB)             VLKCS511
220300                                                                  VLKCS511
220400         WHEN VL006-UPD (VL006-IDX)                               VLKCS511
220500              MOVE PC-UPD      TO LQW-ACTION (PV-SUB)             VLKCS511
220600                                                                  VLKCS511
220700         WHEN VL006-DEL (VL006-IDX)                               VLKCS511
220800              MOVE PC-DEL      TO LQW-ACTION (PV-SUB)             VLKCS511
220900                                                                  VLKCS511
221000         WHEN VL006-ERR (VL006-IDX)                               VLKCS511
221100              MOVE PC-ERR      TO LQW-ACTION (PV-SUB)             VLKCS511
221200     END-EVALUATE.                                                VLKCS511
221300                                                                  VLKCS511
221400     SUBTRACT 1 FROM ASC-NUMBER-OF-SELECTED-ITEMS.                VLKCS511
221500                                                                  VLKCS511
