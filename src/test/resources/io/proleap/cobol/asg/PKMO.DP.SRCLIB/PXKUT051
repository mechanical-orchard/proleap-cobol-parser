023800*--------------------------------------------------------------*  02380000
023900* 2000-PROCESS-PRCHGS.                                         *  02390000
024000*  PROCESS UPCOMING PRICE CHANGES.                             *  02400000
024100*--------------------------------------------------------------*  02410000
024200 2000-PROCESS-PRCHGS.                                             02420000
024300                                                                  02430000
024900                                                                  02490000
CL3132     PERFORM 2100-OPEN-CURSOR.                                    02491016
CL3132                                                                  02492016
CL3132     PERFORM 2200-FETCH-CURSOR                                    02494016
CL3132       UNTIL SQLCODE = +100.                                      02495016
CL3132                                                                  02495116
CL3132     PERFORM 2300-CLOSE-CURSOR.                                   02496016
CL3132                                                                  02497016
CL3132*--------------------------------------------------------------*  02661016
CL3132* 2100-OPEN-INPUT-CURSOR.                                      *  02662016
CL3132*--------------------------------------------------------------*  02663016
CL3132 2100-OPEN-CURSOR.                                                02664016
CL3132     EXEC SQL                                                     02665016
CL3132          OPEN ONHAND-CSR                                         02666016
CL3132     END-EXEC.                                                    02667016
CL3132                                                                  02668016
CL3132     EVALUATE SQLCODE                                             02669016
CL3132         WHEN 0                                                   02669116
CL3132            CONTINUE                                              02669216
CL3132         WHEN OTHER                                               02669316
CL3132            MOVE '2100-CLOSE-CURSOR    '                          02669416
CL3132                TO PV-PARAGRAPH-NAME                              02669516
CL3132            MOVE 'ERROR ON OPEN OF ONHAND CURSOR '                02669616
CL3132                TO PV-DB2-OPERATION                               02669716
CL3132            DISPLAY 'ERROR IN OPENING CURSOR :' SQLCODE           02669816
CL3132            PERFORM 9900-SQL-ABEND                                02669916
CL3132     END-EVALUATE.                                                02670016
CL3132                                                                  02670116
CL3132*--------------------------------------------------------------*  03301016
CL3132* 2200-FETCH-CURSOR                                            *  03302016
CL3132*  SELECT THE STORE ONHAND QUANTITY AND INTRANSIT QUANTITY     *  03303016
CL3132*--------------------------------------------------------------*  03304016
CL3132 2200-FETCH-CURSOR.                                               03305016
CL3132                                                                  03306016
CL3132     EXEC SQL                                                     03307016
CL3132          FETCH NEXT ROWSET FROM ONHAND-CSR                       03308016
CL3132          FOR :PV-ROWS ROWS                                       03309016
CL3132          INTO :TL-LOC-NBR                                        03309118
CL3132              ,:TL-INTR-UPC-ID                                    03309218
CL3132              ,:TL-STR-EXPTD-QTY                                  03309318
CL3132              ,:TL-ONHAND                                         03309418
CL3132              ,:TL-INTRANST-QTY                                   03309518
CL3132     END-EXEC.                                                    03309616
CL3132                                                                  03309716
CL3132     EVALUATE SQLCODE                                             03309816
CL3132          WHEN 0                                                  03309916
CL3132          WHEN +100                                               03310016
CL3132               CONTINUE                                           03310118
CL3132          WHEN OTHER                                              03310516
CL3132               MOVE '2200-FETCH-ONHAND-CSR'                       03310616
CL3132                   TO PV-PARAGRAPH-NAME                           03310716
CL3132               MOVE 'ERROR ON SELECT OF ONHAND QTY  '             03310816
CL3132                   TO PV-DB2-OPERATION                            03310916
CL3132               PERFORM 9900-SQL-ABEND                             03311016
CL3132     END-EVALUATE.                                                03311116
CL3132                                                                  03311216
CL3132*    SQLERRD (3) - CONTAINS THE COUNT OF RECORDS FETCHED          03311318
CL3132     ADD SQLERRD(3) TO PV-SELECT-QTY-CNT                          03311418
CL3132     PERFORM 2500-BUILD-AND-WRITE-RECORD.                         03311518
CL3132                                                                  03311618
CL3132******************************************************************03311716
CL3132 2300-CLOSE-CURSOR.                                               03311816
CL3132******************************************************************03311916
CL3132     EXEC SQL                                                     03312016
CL3132          CLOSE ONHAND-CSR                                        03312116
CL3132     END-EXEC.                                                    03312216
CL3132                                                                  03312316
CL3132     EVALUATE SQLCODE                                             03312416
CL3132         WHEN 0                                                   03312516
CL3132            CONTINUE                                              03312616
CL3132         WHEN OTHER                                               03312716
CL3132               MOVE '2300-CLOSE-CURSOR    '                       03312816
CL3132                   TO PV-PARAGRAPH-NAME                           03312916
CL3132               MOVE 'ERROR ON CLOSE OF ONHAND CURSOR '            03313016
CL3132                   TO PV-DB2-OPERATION                            03313116
CL3132            DISPLAY 'ERROR IN CLOSING CURSOR :' SQLCODE           03313216
CL3132            PERFORM 9900-SQL-ABEND                                03313316
CL3132     END-EVALUATE.                                                03313416
CL3132                                                                  03313516
CL3132******************************************************************03313616
CL3132*  BUILD RECORD FOR OUTPUT FILE.                                * 03313716
CL3132******************************************************************03313816
CL3132 2500-BUILD-AND-WRITE-RECORD.                                     03313916
CL3132                                                                  03314016
CL3132     PERFORM VARYING PV-SUB FROM 1 BY 1                           03314116
CL3132       UNTIL PV-SUB > SQLERRD(3)                                  03314216
CL3132             MOVE TL-LOC-NBR (PV-SUB)                             03314318
CL3132               TO PXRD003-STR-NBR                                 03314416
CL3132             MOVE TL-INTR-UPC-ID (PV-SUB)                         03314518
CL3132               TO PXRD003-INTR-UPC-ID                             03314616
CL3132             MOVE TL-STR-EXPTD-QTY (PV-SUB)                       03314718
CL3132               TO PXRD003-STR-EXPCTD-QTY                          03314816
CL3132             MOVE TL-ONHAND (PV-SUB)                              03314918
CL3132               TO PXRD003-FSHN-ONHAND-QTY                         03315016
CL3132             MOVE TL-INTRANST-QTY (PV-SUB)                        03315118
CL3132               TO PXRD003-INTRANSIT-QTY                           03315216
CL3132             MOVE DF-FORMAT-DATE                                  03315316
CL3132               TO PXRD003-DATE                                    03315416
CL3132                                                                  03315516
CL3132        WRITE OUTPUT-STR-QTY-RECORD                               03315616
CL3132         FROM PXRD003-PX-COMMON-FORMAT                            03315716
CL3132                                                                  03315816
CL3132        ADD +1         TO PV-OUTPUT-REC-CNT                       03315916
CL3132        INITIALIZE PXRD003-PX-COMMON-FORMAT                       03316016
CL3132     END-PERFORM.                                                 03316116
CL3132                                                                  03317016
