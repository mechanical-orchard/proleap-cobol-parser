CL3132*--------------------------------------------------------------*  01801016
CL3132* CURSOR TO PULL ONHANDS AND INTRANSIT DATA                       01802016
CL3132*--------------------------------------------------------------*  01803016
CL3132     EXEC SQL                                                     01804016
CL3132       DECLARE ONHAND-CSR CURSOR WITH ROWSET POSITIONING FOR      01805016
CL3132          SELECT  A.STR_NBR                                       01806016
CL3132                 ,A.INTR_UPC_ID                                   01807016
CL3132                 ,(COALESCE(B.ONHAND_QTY,0) -                     01808016
CL3132                  COALESCE(C.TRN_CLSN_QTY,0)) AS STR_QTY          01809016
CL3132                 ,COALESCE (B.ONHAND_QTY,0)   AS ONHAND           01809116
CL3132                 ,COALESCE (C.TRN_CLSN_QTY,0) AS TRANSIT_QTY      01809216
CL3132            FROM  PXTW_SKU_STR A                                  01809316
CL3132            LEFT OUTER JOIN TSKST  B                              01809416
CL3132                                                                  01809516
CL3132            ON  A.ITM_NBR =  B.ITEM_NMBR                          01809616
CL3132            AND A.STR_NBR =  B.LOC_NBR                            01809716
CL3132                                                                  01809816
CL3132            LEFT OUTER JOIN PCT_TRN_CLSN_BAL C                    01809916
CL3132            ON  C.INTR_UPC_ID  = A.INTR_UPC_ID                    01810016
CL3132            AND C.LOC_NBR      = A.STR_NBR                        01810116
CL3132            AND C.TRN_CLSN_CDE = 'SE'                             01810216
CL3132            AND C.INTR_UPC_ID  > 1                                01810316
CL3132          FOR READ ONLY                                           01810416
CL3132          WITH UR                                                 01810516
CL3132     END-EXEC.                                                    01810616
CL3132                                                                  01810716
