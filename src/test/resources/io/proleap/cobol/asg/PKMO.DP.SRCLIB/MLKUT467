      ******************************************************************02220000
       Z210-OPEN-CURSOR-TWKDPTS.                                        02230004
      ******************************************************************02240000
      *                                                                 02250000
           EXEC SQL                                                     02260000
               OPEN TWKDPTS_CURSOR                                      02270004
           END-EXEC.                                                    02280000
                                                                        02290000
           EVALUATE TRUE                                                02300000
               WHEN SQLCODE < 0                                         02310000
                   MOVE 'Z210-OPEN-CURSOR-TWKDPTS'                      02320004
                                       TO AA-PARAGRAPH-NAME             02330000
                   MOVE 'TWKDPTS TABLE ERROR'                           02340004
                                       TO AA-MESSAGE                    02350000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02360000
                   PERFORM Z998-SQL-ERROR                               02370000
               WHEN SQLCODE > 0                                         02380000
               WHEN SQLWARN0 NOT = SPACES                               02390000
                   MOVE 'Z210-OPEN-CURSOR-TWKDPTS'                      02400004
                                       TO AA-PARAGRAPH-NAME             02410000
                   MOVE 'TWKDPTS TABLE WARNING'                         02420004
                                       TO AA-MESSAGE                    02430000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02440000
                   PERFORM Z998-SQL-ERROR                               02450000
           END-EVALUATE.                                                02460000
                                                                        02470000
       EJECT                                                            02480000
                                                                        02490000
      ******************************************************************02500000
       Z220-FETCH-TWKDPTS.                                              02510004
      ******************************************************************02520000
      *                                                                 02530000
W31475*    EXEC SQL                                                     02540008
  |   *        FETCH TWKDPTS_CURSOR                                     02550008
  |   *         INTO :WKDPTS-DEPT-NBR                                   02560008
  |   *             ,:WKDPTS-STR-NBR                                    02580008
  |   *    END-EXEC.                                                    02590008
  |        EXEC SQL                                                     02591008
  |            FETCH NEXT ROWSET FROM TWKDPTS_CURSOR FOR 100 ROWS       02592008
  |             INTO :HV-WKDPTS-DEPT-NBR                                02593008
  |                 ,:HV-WKDPTS-STR-NBR                                 02594008
W31475     END-EXEC.                                                    02595008
                                                                        02600000
           EVALUATE TRUE                                                02610000
               WHEN SQLCODE = 0                                         02620000
W31475*            ADD +1              TO PA-TWKDPTS-ROWS-FETCHED       02630008
W31475             ADD +100            TO PA-TWKDPTS-ROWS-FETCHED       02631008
               WHEN SQLCODE = +100                                      02640000
                   MOVE 'Y'            TO PS-TWKDPTS-DONE               02650004
W31475             ADD SQLERRD (3)     TO PA-TWKDPTS-ROWS-FETCHED       02651008
  |                PERFORM VARYING ROW-ID FROM 1 BY 1                   02652008
  |                UNTIL ROW-ID > SQLERRD (3)                           02653008
  |                     PERFORM C100-WRITE-EXTRACT                      02654008
W31475             END-PERFORM                                          02655008
               WHEN SQLCODE < 0                                         02660000
                   MOVE 'Z220-FETCH-TWKDPTS'                            02670004
                                       TO AA-PARAGRAPH-NAME             02680000
                   MOVE 'TWKDPTS TABLE ERROR'                           02690004
                                       TO AA-MESSAGE                    02700000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02710000
                   PERFORM Z998-SQL-ERROR                               02720000
               WHEN ((SQLCODE > 0) AND (SQLCODE NOT = +100))            02730000
               WHEN SQLWARN0 NOT = SPACES                               02740000
                   MOVE 'Z220-FETCH-TWKDPTS'                            02750004
                                       TO AA-PARAGRAPH-NAME             02760000
                   MOVE 'TWKDPTS TABLE WARNING'                         02770004
                                       TO AA-MESSAGE                    02780000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              02790000
                   PERFORM Z998-SQL-ERROR                               02800000
           END-EVALUATE.                                                02810000
                                                                        02820000
       EJECT                                                            02830000
                                                                        02840000
      ******************************************************************02850000
       Z230-CLOSE-CURSOR-TWKDPTS.                                       02860004
      ******************************************************************02870000
