      ******************************************************************01250000
      *  DB2 TWKDPTS CURSOR                                             01260004
      ******************************************************************01270000
           EXEC SQL                                                     01280000
W31475         DECLARE TWKDPTS_CURSOR CURSOR                            01290008
W31475         WITH ROWSET POSITIONING FOR                              01291008
               SELECT DISTINCT DEPT_NBR                                 01300000
                              ,STR_NBR                                  01311000
               FROM TWKDPTS                                             01320004
                   ,TMLCNTL                                             01331000
               WHERE (FSCL_WK_END_DTE BETWEEN                           01340006
                                   OPN_FSCL_WK_DTE                      01350006
                               AND MXPSTG_FSCL_WK_DTE) OR               01360006
                    ((FSCL_WK_END_DTE BETWEEN                           01361006
                      :DTATTR-PFM-END-DTE AND                           01362000
                      MXPSTG_FSCL_MN_DTE) AND                           01363000
                       (END_INV_RTL_AMT  <> 0  OR                       01363100
                        END_INV_COST_AMT <> 0))                         01364000
           END-EXEC.                                                    01380000
                                                                        01390000
       EJECT                                                            01400000
                                                                        01410000
