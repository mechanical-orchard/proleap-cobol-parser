017900                                                                  01780000
018000                                                                  01790000
018100******************************************************************01800000
018200* PREFORMS TASK INITIATION                                        01810000
018300******************************************************************01820000
018400 A100-INITIALIZE.                                                 01830000
018500                                                                  01840000
018600     OPEN INPUT UPDATE-FILE.                                      01850000
018700                                                                  01860000
018800     MOVE FUNCTION CURRENT-DATE TO SYS-DATE-TIME.                 01870000
018900                                                                  01880000
019000     MOVE SYS-TIME (1:2) TO ST-BEG-HR.                            01890000
019100     MOVE SYS-TIME (3:2) TO ST-BEG-MN.                            01900000
019200                                                                  01910000
019300     MOVE SYS-DATE (1:2) TO SD-CENT.                              01920000
019400     MOVE SYS-DATE (3:2) TO SD-YEAR.                              01930000
019500     MOVE SYS-DATE (5:2) TO SD-MONTH.                             01940000
019600     MOVE SYS-DATE (7:2) TO SD-DAY.                               01950000
019700                                                                  01960000
019800     PERFORM C100-ISSUE-OPEN-REQUEST.                             01970000
019900                                                                  01980000
020000     PERFORM C200-READ-UPDATE-INPUT-FILE.                         01990000
020100                                                                  02000000
020200     IF PS-INPUT-UPDATE-EOF                                       02010000
020200        DISPLAY '**** EMPTY UPDATE FILE ****'                     02020000
020800     END-IF.                                                      02030000
020900                                                                  02040000
021000                                                                  02050000
021100******************************************************************02060000
021200* PROCESS THE UPDATE FILE. IF A RECORD IS READ, GATHER WHATEVER   02070000
021300* OTHER DATA IS NECESSARY TO TAKE CHECKPOINTS BASED ON THE TYPE OF02080000
021400* CHECKPOINTS BEING TAKEN BY SVKUP012 AND ISSUE A WRITE REQUEST   02090001
021500* TO THE TRANSACTION PREPARATION PROGRAM.                         02100000
021600******************************************************************02110000
021700 B100-PROCESS-UPDATE-INPUT-FILE.                                  02120000
021800                                                                  02130000
021900     IF FIRST-TRANS-READ                                          02140000
022000         MOVE SV024-CRD-NBR       TO PV-HOLD-CRD-NBR              02150001
022100         MOVE 'N'                 TO PS-FIRST-TRANS-READ-SW       02160000
022200     END-IF.                                                      02170000
022300                                                                  02180000
022400     IF CONTROL-BREAK                                             02190000
022500         PERFORM C300-CKPT-BY-CNTL-BREAK                          02200000
022600     ELSE                                                         02210000
022700         IF LOGICAL-TRANSACTION                                   02220000
022800             PERFORM C400-CKPT-BY-LOGICAL-TRANS                   02230000
022900         ELSE                                                     02240000
023000             PERFORM C500-CKPT-BY-TIME-OR-REQUEST                 02250000
023100         END-IF                                                   02260000
023200     END-IF.                                                      02270000
023300                                                                  02280000
023400     PERFORM C200-READ-UPDATE-INPUT-FILE.                         02290000
023500                                                                  02300000
023600                                                                  02310000
023700******************************************************************02320000
023800* ISSUE AN 'OPEN' REQUEST TO THE TRANSACTION PREPARATION MODULE   02330000
023900******************************************************************02340000
024000 C100-ISSUE-OPEN-REQUEST.                                         02350000
024100                                                                  02360000
024200     MOVE PC-TRAN-PREP-FUNC-OPEN TO DP000-FUNC-CODE.              02370000
024300     MOVE PC-JOB-NAME            TO DP000-JOB-NAME.               02380000
024400     MOVE PC-PLAN-NAME           TO DP000-PLAN-NAME.              02390000
024500                                                                  02400000
024600     PERFORM Z100-CALL-TRANS-PREP-MODULE.                         02410000
024700                                                                  02420000
024800     MOVE DP000-CKPT-TYPE-CODE   TO PV-CHECKPOINT-TYPE.           02430000
024900                                                                  02440000
025000                                                                  02450000
025100******************************************************************02460000
025200* READ THE UPDATE INPUT FILE                                      02470000
025300******************************************************************02480000
025400 C200-READ-UPDATE-INPUT-FILE.                                     02490000
025500                                                                  02500000
025600     READ UPDATE-FILE INTO SV024-CARD-UPDT-RECORD                 02510001
025700       AT END SET PS-INPUT-UPDATE-EOF TO TRUE.                    02520000
025800                                                                  02530000
025900     IF PS-INPUT-UPDATE-EOF                                       02540000
026000         CONTINUE                                                 02550000
026100     ELSE                                                         02560000
026200         ADD +1 TO PA-READ                                        02570000
026300     END-IF.                                                      02580000
026400                                                                  02590000
026500                                                                  02600000
026600******************************************************************02610000
026700* THIS ROUTINE SETS THE CHECKPOINT INDICATOR ON THE TRANSACATION  02620000
026800* RECORD "ON" IF THERE IS A CHANGE IN THE STORE NUMBER. AFTER     02630000
026900* THE TRANSACTION RECORD HAS BEEN FORMATTED, A WRITE REQUEST IS   02640000
027000* ISSUED TO THE TRANSACTION PREPARATION MODULE.                   02650000
027100*                                                                 02660000
027200* NOTE: AT THIS TIME CONTROL/LOGIC BREAK PROCESSING IS IDENTICAL. 02670000
027300******************************************************************02680000
027400 C300-CKPT-BY-CNTL-BREAK.                                         02690000
027500                                                                  02700000
027600     IF  SV024-CRD-NBR = PV-HOLD-CRD-NBR                          02710001
027700         MOVE 'N' TO DP000-CHECKPOINT-IND                         02720000
027800     ELSE                                                         02730000
027900         MOVE 'Y' TO DP000-CHECKPOINT-IND                         02740000
028000         MOVE SV024-CRD-NBR       TO PV-HOLD-CRD-NBR              02750001
028100     END-IF.                                                      02760000
028200                                                                  02770000
028300     MOVE PC-TRANS-RECORD-LENGTH  TO DP000-TRANS-REC-LENGTH.      02780000
028400     MOVE SV024-CARD-UPDT-RECORD  TO DP000-TRANS-REC.             02790001
028500     MOVE PC-TRAN-PREP-FUNC-WRITE TO DP000-FUNC-CODE.             02800000
028600     MOVE PC-JOB-NAME             TO DP000-JOB-NAME.              02810000
028700     MOVE PC-PLAN-NAME            TO DP000-PLAN-NAME.             02820000
028800                                                                  02830000
028900     PERFORM Z100-CALL-TRANS-PREP-MODULE.                         02840000
029000                                                                  02850000
029100     ADD +1 TO PA-WRITTEN.                                        02860000
029200                                                                  02870000
029300                                                                  02880000
029400******************************************************************02890000
029500* THIS ROUTINE SETS THE CHECKPOINT INDICATOR ON THE TRANSACTION   02900000
029600* RECORD "ON".  THE UPDATE PROGRAM WILL CHECK THE CHECKPOINT      02910000
029700* FREQUENCY TO KNOW WHETHER A CHECKPOINT SHOULD BE TAKEN. AFTER   02920000
029800* THE TRANSACTION RECORD HAS BEEN FORMATTED, A WRITE REQUEST IS   02930000
029900* ISSUED TO THE TRANSACTION PREPARATION MODULE.                   02940000
030000******************************************************************02950000
030100 C400-CKPT-BY-LOGICAL-TRANS.                                      02960000
030200                                                                  02970000
027600     IF  SV024-CRD-NBR = PV-HOLD-CRD-NBR                          02980001
027700         MOVE 'N' TO DP000-CHECKPOINT-IND                         02990000
027800     ELSE                                                         03000000
027900         MOVE 'Y' TO DP000-CHECKPOINT-IND                         03010000
028000         MOVE SV024-CRD-NBR       TO PV-HOLD-CRD-NBR              03020001
028100     END-IF.                                                      03030000
030900                                                                  03040000
028300     MOVE PC-TRANS-RECORD-LENGTH  TO DP000-TRANS-REC-LENGTH.      03050000
028400     MOVE SV024-CARD-UPDT-RECORD  TO DP000-TRANS-REC.             03060001
028500     MOVE PC-TRAN-PREP-FUNC-WRITE TO DP000-FUNC-CODE.             03070000
028600     MOVE PC-JOB-NAME             TO DP000-JOB-NAME.              03080000
028700     MOVE PC-PLAN-NAME            TO DP000-PLAN-NAME.             03090000
031500                                                                  03100000
031600     PERFORM Z100-CALL-TRANS-PREP-MODULE.                         03110000
031700                                                                  03120000
031800     ADD +1 TO PA-WRITTEN.                                        03130000
031900                                                                  03140000
032000                                                                  03150000
032100******************************************************************03160000
032200* THIS ROUTINE BUILDS THE TRANSACTION RECORD AND ISSUES A WRITE   03170000
032300* REQUEST TO THE TRANSACTION PREPARATION MODULE IF CHECKPOINTS ARE03180000
032400* TAKEN BY TIME INTERVAL (SECONDS) OR REQUESTED BY THE MAINTENANCE03190000
032500* PROGRAM.                                                        03200000
032600******************************************************************03210000
032700 C500-CKPT-BY-TIME-OR-REQUEST.                                    03220000
032800                                                                  03230000
032900     MOVE 'N'                     TO DP000-CHECKPOINT-IND.        03240000
033000                                                                  03250000
033100     MOVE PC-TRANS-RECORD-LENGTH  TO DP000-TRANS-REC-LENGTH.      03260000
