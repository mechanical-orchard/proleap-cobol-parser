002500 WORKING-STORAGE SECTION.                                         00250000
002600 01  WS-INDICATORS.                                               00260000
002700     05 WI-PORT            PIC 9(4) VALUE 2980.                   00270001
002710     05 WI-PORT2           PIC 9(4) VALUE 3980.                   00271001
002711     05 WI-PORT3           PIC 9(4) VALUE 2992.                   00271103
002712     05 WI-PORT4           PIC 9(4) VALUE 3992.                   00271202
002720******************************************************************00272000
002730 PROCEDURE DIVISION.                                              00273000
002740******************************************************************00274000
002750 0000-MAIN-LINE.                                                  00275000
002760*    EXEC CICS START                                              00276006
002770*              TRANSID('Z300')                                    00277006
002780*              FROM(WI-PORT)                                      00278006
002790*    END-EXEC.                                                    00279006
002800*    EXEC CICS START                                              00280006
002900*              TRANSID('Z300')                                    00290006
003000*              FROM(WI-PORT2)                                     00300006
003100*    END-EXEC.                                                    00310006
003110     EXEC CICS START                                              00311005
003120               TRANSID('T230')                                    00312005
003140     END-EXEC.                                                    00314005
003200*    EXEC CICS START                                              00320004
003300*              TRANSID('Z300')                                    00330004
003400*              FROM(WI-PORT3)                                     00340004
003500*    END-EXEC.                                                    00350004
003510*    EXEC CICS START                                              00351004
003520*              TRANSID('Z300')                                    00352004
003521*              FROM(WI-PORT4)                                     00352104
003522*    END-EXEC.                                                    00352204
003530     EXEC CICS RETURN                                             00353000
003540     END-EXEC.                                                    00354000
