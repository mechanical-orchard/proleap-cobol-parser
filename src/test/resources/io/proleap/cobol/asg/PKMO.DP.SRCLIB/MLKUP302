             SELECT  CKPT_STAT_CODE                                     05570000
                    ,CKPT_FRQNCY_QTY                                    05580000
               INTO  :TCKRSTCNTL-CKPT-STAT-CODE                         05590000
                    ,:TCKRSTCNTL-CKPT-FRQNCY-QTY                        05600000
