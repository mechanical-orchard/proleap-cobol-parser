      ******************************************************************01990000
      * OPEN INPUT FILE , INITIALIZE COUNTERS , READ FIRST INPUT RECORD 02000000
      * PREPARE CHECKPOINT RESTART TABLES FOR PROCESSING / RESTART      02010000
      ******************************************************************02020000
      *                                                                 02030000
       1000-INITIALIZATION.                                             02040000
      *                                                                 02050000
           OPEN INPUT CONDENSED-FILE.                                   02060000
                                                                        02070000
           PERFORM 7400-INIT-CHECKPOINT-SELECT.                         02080000
                                                                        02090000
      * CKPT-STAT-CODE = '9' INDICATES A RESTART CONDITION              02100000
           IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                           02110000
               PERFORM 7500-SELECT-RESTART-INFO                         02120000
               MOVE TCKRSTINF-WORK-STRG-DESC-TEXT TO                    02130000
                                            CR-CHECKPOINT-RESTART-VAR   02140000
               MOVE CR-PREV-DTL-KEY          TO CK-SAVE-PREV-KEY        02150000
               DISPLAY 'CHECKPOINT RESTART INFORMATION:'                02160000
               DISPLAY 'MONTH NBR       ' CR-FISCAL-NBR                 02170000
               DISPLAY 'MONTH END DATE  ' CR-FISCAL-MN-END-DTE          02180000
               DISPLAY 'DEPARTMENT NBR  ' CR-DEPT-NBR                   02190000
               DISPLAY 'MAJOR CLASS NBR ' CR-MAJ-CL-NBR                 02200000
               DISPLAY 'INPUT RECS READ ' CR-INPUT-READ-COUNT           02210000
           ELSE                                                         02220000
               SET PS-FIRST-COMMIT TO TRUE                              02230000
               INITIALIZE CR-CHECKPOINT-RESTART-VAR                     02240000
           END-IF.                                                      02250000
      *                                                                 02260000
           PERFORM 4000-READ-CONDENSED-FILE UNTIL                       02270000
               PA-INPUT-COUNT > CR-INPUT-READ-COUNT                     02280000
            OR PS-EOF-CONDENSED-FILE.                                   02290000
      *                                                                 02300000
           IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                           02310000
               DISPLAY 'INPUT FILE RESTARTED ON RECORD '                02320000
                        PA-INPUT-COUNT                                  02330000
               DISPLAY 'FISCAL MONTH NBR      ' ML105-FSCL-MN-NBR       02340000
               DISPLAY 'FISCAL MONTH END DATE ' ML105-FSCL-MN-END-DTE   02350000
               DISPLAY 'DEPARTMENT NBR        ' ML105-DEPT-NBR          02360000
               DISPLAY 'MAJOR CLASS NBR       ' ML105-MAJ-CL-NBR        02370000
           END-IF.                                                      02380000
                                                                        02390000
           IF PS-EOF-CONDENSED-FILE                                     02400000
               IF TCKRSTCNTL-CKPT-STAT-CODE = '9'                       02410000
                   CONTINUE                                             02420000
               ELSE                                                     02430000
                   DISPLAY '** NO RECORDS ON INPUT FILE **'             02440000
           ELSE                                                         02450000
               PERFORM 7300-GET-COMMIT-TIMESTAMP                        02460000
               DISPLAY 'COMMITS WILL BE TAKEN EVERY ',                  02470000
                    TCKRSTCNTL-CKPT-FRQNCY-QTY, ' SECONDS.'             02480000
           END-IF.                                                      02490000
                                                                        02500000
      ******************************************************************02510000
      * PROCESS THE SORTED/CONDENSED FILE - UPDATE OR INSERT AS NEEDED  02520000
      ******************************************************************02530000
       2000-PROCESS-INPUT.                                              02540000
      *                                                                 02550000
           MOVE 'N' TO PS-DEADLOCK-RESTART-SW.                          02560000
      *                                                                 02570000
           PERFORM 7000-SELECT-DB-ROW.                                  02580000
      *                                                                 02590000
           IF PS-PROGRAM-DEADLOCKED                                     02600000
               CONTINUE                                                 02610000
           ELSE                                                         02620000
               EVALUATE TRUE                                            02630000
                   WHEN PI-UPDATE-MNMCL-ROW                             02640000
                       PERFORM 7100-UPDATE-MNMCL-ROW                    02650000
                   WHEN PI-INSERT-MNMCL-ROW                             02660000
                       PERFORM 7200-INSERT-MNMCL-ROW                    02670000
                   WHEN OTHER                                           02680000
                       MOVE '7000-SELECT-DB-ROW' TO PV-PARAGRAPH-NAME   02690000
                       MOVE PE-MSG-01 TO PV-OPERATION-ATTEMPTED         02700000
                       PERFORM 9999-SQL-ABEND                           02710000
               END-EVALUATE                                             02720000
           END-IF.                                                      02730000
      *                                                                 02740000
           IF PS-PROGRAM-DEADLOCKED                                     02750000
               CONTINUE                                                 02760000
           ELSE                                                         02770000
               PERFORM 7700-COMMIT-PROCESSING                           02780000
               PERFORM 4000-READ-CONDENSED-FILE                         02790000
           END-IF.                                                      02800000
      *                                                                 02810000
      *                                                                 02820000
      ******************************************************************02830000
      * CALCULATE FIELDS FOR UPDATE OF DATABASE                         02840000
      ******************************************************************02850000
       2100-CALCULATE-UPDATES.                                          02860000
      * ADD DB2 TABLE ROW VALUES TO CURRENT INPUT RECORD ROW VALUES     02870000
      *                                                                 02880000
           COMPUTE SV-SLS-RTL-AMT       =                               02890000
                   MNMCL-SLS-RTL-AMT       + ML105-SLS-RTL-AMT.         02900000
           COMPUTE SV-MKDN-RTL-AMT      =                               02910000
                   MNMCL-MKDN-RTL-AMT      + ML105-MKDN-RTL-AMT.        02920000
           COMPUTE SV-MKUP-RTL-AMT      =                               02930000
                   MNMCL-MKUP-RTL-AMT      + ML105-MKUP-RTL-AMT.        02940000
           COMPUTE SV-XFER-IN-RTL-AMT   =                               02950000
