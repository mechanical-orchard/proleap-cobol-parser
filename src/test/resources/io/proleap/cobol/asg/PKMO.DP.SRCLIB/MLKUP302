      *                                                                 03090000
      ******************************************************************03100000
      * READS THE CONDENSED FILE INTO THE TMNMCL LAYOUT                 03110000
      ******************************************************************03120000
       4000-READ-CONDENSED-FILE.                                        03130000
           READ CONDENSED-FILE INTO ML105-UPDATE-REC                    03140000
              AT END                                                    03150000
                MOVE 'Y' TO PS-EOF-CONDENSED-FILE-SW.                   03160000
     *                                                                  03170000
           IF PS-EOF-CONDENSED-FILE                                     03180000
               MOVE ML105-FSCL-MN-NBR         TO CR-FISCAL-NBR          03190000
               MOVE ML105-FSCL-MN-END-DTE     TO CR-FISCAL-MN-END-DTE   03200000
               MOVE ML105-DEPT-NBR            TO CR-DEPT-NBR            03210000
               MOVE ML105-MAJ-CL-NBR          TO CR-MAJ-CL-NBR          03220000
               MOVE PA-INPUT-COUNT            TO CR-INPUT-READ-COUNT    03230000
      *        MOVE COMMIT INFO TO WORKING STORAGE ROW                  03240000
               MOVE CR-CHECKPOINT-RESTART-AREA TO                       03250000
                                             TCKRSTINF-WORK-STRG-DESC   03260000
               EXEC SQL                                                 03270000
                 UPDATE TCKRSTINF                                       03280000
                    SET WORK_STRG_DESC = :TCKRSTINF-WORK-STRG-DESC      03290000
                  WHERE JOB_NAME   = :PC-JOB-NAME                       03300000
                    AND PLAN_NAME  = :PC-PROGRAM-NAME                   03310000
               END-EXEC                                                 03320000
               IF SQLCODE = 0                                           03330000
                   CONTINUE                                             03340000
               ELSE                                                     03350000
                   MOVE PE-MSG-05          TO PV-OPERATION-ATTEMPTED    03360000
                   MOVE '* 4000-READ-CONDENSED *' TO PV-PARAGRAPH-NAME  03370000
                   PERFORM 9999-SQL-ABEND                               03380000
               END-IF                                                   03390000
            ELSE                                                        03400000
                ADD 1                        TO PA-INPUT-COUNT          03410000
            END-IF.                                                     03420000
      *                                                                 03430000
      ******************************************************************03440000
      * SELECT A ROW FROM THE MONTHLY MCL  TABLE THAT MATCHES INPUT KEY 03450000
      *   SQLCODE FROM 'SELECT' IS EVALUATED IN 2000-PROCESS-INPUT      03460000
      ******************************************************************03470000
       7000-SELECT-DB-ROW.                                              03480000
      *                                                                 03490000
           EXEC SQL                                                     03500000
               SELECT SLS_RTL_AMT                                       03510000
                     ,MKDN_RTL_AMT                                      03520000
                     ,MKUP_RTL_AMT                                      03530000
                     ,XFER_IN_RTL_AMT                                   03540000
                     ,XFER_OUT_RTL_AMT                                  03550000
                     ,RCPT_RTL_AMT                                      03560000
                     ,RCPT_NET_COST_AMT                                 03570000
                     ,PADJ_RTL_AMT                                      03580000
                     ,PADJ_NET_COST_AMT                                 03590000
                     ,INV_ADJ_RTL_AMT                                   03600000
                 INTO :MNMCL-SLS-RTL-AMT                                03610000
                     ,:MNMCL-MKDN-RTL-AMT                               03620000
                     ,:MNMCL-MKUP-RTL-AMT                               03630000
                     ,:MNMCL-XFER-IN-RTL-AMT                            03640000
                     ,:MNMCL-XFER-OUT-RTL-AMT                           03650000
                     ,:MNMCL-RCPT-RTL-AMT                               03660000
                     ,:MNMCL-RCPT-NET-COST-AMT                          03670000
                     ,:MNMCL-PADJ-RTL-AMT                               03680000
                     ,:MNMCL-PADJ-NET-COST-AMT                          03690000
                     ,:MNMCL-INV-ADJ-RTL-AMT                            03700000
               FROM TMNMCL                                              03710000
              WHERE   FSCL_MN_NBR         = :ML105-FSCL-MN-NBR          03720000
                AND   FSCL_MN_END_DTE     = :ML105-FSCL-MN-END-DTE      03730000
                AND   DEPT_NBR            = :ML105-DEPT-NBR             03740000
                AND   MAJ_CL_NBR          = :ML105-MAJ-CL-NBR           03750000
W26603        WITH UR                                                   03760000
           END-EXEC.                                                    03770000
      *                                                                 03780000
           EVALUATE SQLCODE                                             03790000
               WHEN 0                                                   03800000
                   SET PI-UPDATE-MNMCL-ROW        TO TRUE               03810000
               WHEN +100                                                03820000
                   SET PI-INSERT-MNMCL-ROW        TO TRUE               03830000
               WHEN -911                                                03840000
                   DISPLAY '*** -911 OCCURRED ON SELECT ***'            03850000
                   ADD +1             TO PV-DEADLOCK-COUNT              03860000
                   IF PV-DEADLOCK-COUNT > PC-MAX-DEADLOCKS              03870000
                       MOVE '7000-SELECT-DB-ROW' TO                     03880000
                                                    PV-PARAGRAPH-NAME   03890000
                       PERFORM 9000-DEADLOCK-ERROR                      03900000
                   ELSE                                                 03910000
                       PERFORM 7999-RESTART-PROCESS                     03920000
                   END-IF                                               03930000
               WHEN OTHER                                               03940000
                   MOVE '7000-SELECT-DB-ROW'   TO PV-PARAGRAPH-NAME     03950000
                   MOVE PE-MSG-01 TO PV-OPERATION-ATTEMPTED             03960000
                   PERFORM 9999-SQL-ABEND                               03970000
           END-EVALUATE.                                                03980000
      *                                                                 03990000
      *                                                                 04000000
      ***************************************************************** 04010000
      * THE INPUT RECORD FOUND A MATCHING ROW BASED ON FSCL INFO, DEPT  04020000
      * AND MAJOR CLASS. CURRENT TABLE VALUES UPDATED TO INCLUDE VALUES 04030000
      * FROM INPUT REC MLRD105                                          04040000
      ***************************************************************** 04050000
       7100-UPDATE-MNMCL-ROW.                                           04060000
      *                                                                 04070000
           PERFORM 2100-CALCULATE-UPDATES.                              04080000
      *                                                                 04090000
           EXEC SQL                                                     04100000
               UPDATE TMNMCL                                            04110000
                   SET RCPT_RTL_AMT        = :SV-RCPT-RTL-AMT           04120000
                     , RCPT_NET_COST_AMT   = :SV-RCPT-NET-COST          04130000
                     , PADJ_RTL_AMT        = :SV-PADJ-RTL-AMT           04140000
                     , PADJ_NET_COST_AMT   = :SV-PADJ-NET-COST          04150000
                     , SLS_RTL_AMT         = :SV-SLS-RTL-AMT            04160000
                     , MKDN_RTL_AMT        = :SV-MKDN-RTL-AMT           04170000
                     , MKUP_RTL_AMT        = :SV-MKUP-RTL-AMT           04180000
                     , XFER_IN_RTL_AMT     = :SV-XFER-IN-RTL-AMT        04190000
                     , XFER_OUT_RTL_AMT    = :SV-XFER-OUT-RTL           04200000
                     , INV_ADJ_RTL_AMT     = :SV-INV-ADJ-RTL-AMT        04210000
              WHERE   FSCL_MN_NBR          = :ML105-FSCL-MN-NBR         04220000
                AND   FSCL_MN_END_DTE      = :ML105-FSCL-MN-END-DTE     04230000
                AND   DEPT_NBR             = :ML105-DEPT-NBR            04240000
                AND   MAJ_CL_NBR           = :ML105-MAJ-CL-NBR          04250000
           END-EXEC                                                     04260000
      *                                                                 04270000
           EVALUATE SQLCODE                                             04280000
               WHEN 0                                                   04290000
                   CONTINUE                                             04300000
               WHEN -911                                                04310000
                   ADD +1             TO PV-DEADLOCK-COUNT              04320000
                   DISPLAY '*** -911 OCCURRED ON UPDATE ***'            04330000
                   IF PV-DEADLOCK-COUNT > PC-MAX-DEADLOCKS              04340000
                       MOVE '7100-UPDATE-MNMCL-ROW' TO                  04350000
                                                    PV-PARAGRAPH-NAME   04360000
                       PERFORM 9000-DEADLOCK-ERROR                      04370000
                   ELSE                                                 04380000
                       PERFORM 7999-RESTART-PROCESS                     04390000
                   END-IF                                               04400000
               WHEN OTHER                                               04410000
                   MOVE '7100-UPDATE-MNMCL-ROW' TO PV-PARAGRAPH-NAME    04420000
                   MOVE PE-MSG-02              TO PV-OPERATION-ATTEMPTED04430000
                   PERFORM 9999-SQL-ABEND                               04440000
           END-EVALUATE.                                                04450000
      *                                                                 04460000
           ADD 1 TO PA-UPDATE-COUNT.                                    04470000
      *                                                                 04480000
      ***************************************************************** 04490000
      * THERE WAS NO MATCHING ROW ON THE MONTHLY MCL  TABLE. INITIAL  * 04500000
      * - ENTRY FOR THIS ENTITY IS INSERTED AS A 'NEW' ROW            * 04510000
      * - CALCULATED FIELDS ARE INITIALLY SET TO 0 AND UPDATED IN RECALC04520000
