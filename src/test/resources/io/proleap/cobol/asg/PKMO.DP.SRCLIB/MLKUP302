             UPDATE TCKRSTINF                                           06540000
                SET WORK_STRG_DESC = :TCKRSTINF-WORK-STRG-DESC          06550000
              WHERE JOB_NAME       = :PC-JOB-NAME                       06560000
                AND PLAN_NAME      = :PC-PROGRAM-NAME                   06570000
           END-EXEC.                                                    06580000
                                                                        06590000
           IF SQLCODE = 0                                               06600000
               CONTINUE                                                 06610000
           ELSE                                                         06620000
               MOVE PE-MSG-05                  TO PV-OPERATION-ATTEMPTED06630000
               MOVE  '* 7800-COMMIT-CHANGES *' TO PV-PARAGRAPH-NAME     06640000
               PERFORM 9999-SQL-ABEND                                   06650000
           END-IF.                                                      06660000
                                                                        06670000
           EXEC SQL                                                     06680000
               COMMIT                                                   06690000
           END-EXEC.                                                    06700000
                                                                        06710000
           IF SQLCODE = 0                                               06720000
               CONTINUE                                                 06730000
