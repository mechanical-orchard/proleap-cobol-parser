053220     ELSE                                                         05322011
053230        COMPUTE PV-TMP-VAR-RTN-TRN-TYVSLY-HLD =                   05323011
053240                ((CC-CORP-RTN-TRN-QTY (CC-CORP-INDEX) -           05324011
053250                CC-CORP-LY-RTN-TRN-QTY (CC-CORP-INDEX)) /         05325011
053260                CC-CORP-LY-RTN-TRN-QTY (CC-CORP-INDEX)) * 100     05326011
053270        COMPUTE PV-TMP-VAR-RTN-TRNS-TY-VS-LY ROUNDED =            05327011
053280                PV-TMP-VAR-RTN-TRN-TYVSLY-HLD                     05328011
053290     END-IF.                                                      05329011
053300     IF CC-CORP-LY-RTN-ITM-QTY (CC-CORP-INDEX) = 0 THEN           05330011
053310        MOVE 0 TO PV-TMP-VAR-RTN-ITMS-TY-VS-LY                    05331011
053320     ELSE                                                         05332011
053330        COMPUTE PV-TMP-VAR-RTN-ITM-TYVSLY-HLD =                   05333011
053340                ((CC-CORP-RTN-ITM-QTY (CC-CORP-INDEX) -           05334011
053350                CC-CORP-LY-RTN-ITM-QTY (CC-CORP-INDEX)) /         05335011
053360                CC-CORP-LY-RTN-ITM-QTY (CC-CORP-INDEX)) * 100     05336011
053370        COMPUTE PV-TMP-VAR-RTN-ITMS-TY-VS-LY ROUNDED =            05337011
053380                PV-TMP-VAR-RTN-ITM-TYVSLY-HLD                     05338011
053390     END-IF.                                                      05339011
