028350        SET CT-CORP-INDEX    TO 1                                 02835011
028360        SET DT-DIST-IDX      TO 1                                 02836011
028370        SET TT-STR-IDX       TO 1                                 02837011
028380        SET TT-STRNR-IDX  UP BY 1                                 02838011
028390        MOVE EPT00004-STR-NBR       TO PV-HOLD-STR-NBR            02839011
028400        MOVE XST00002-DIST-NBR      TO PV-HOLD-DISTRICT-NBR       02840011
028410        SET DT-DISTNR-IDX           TO PV-HOLD-DISTRICT-NBR       02841011
028420        SET DC-DISTNR-IDX           TO PV-HOLD-DISTRICT-NBR       02842011
028430        SET DN-DISTNR-IDX           TO PV-HOLD-DISTRICT-NBR       02843011
028440        SET DX-DISTNR-IDX           TO PV-HOLD-DISTRICT-NBR       02844011
028450        MOVE EPPART-SLS-DTE         TO PV-HOLD-SLS-DTE            02845011
028460                                                                  02846011
028470        IF EPT00004-SUM-CTG-CDE = 'RET'                           02847011
028480           MOVE EPT00004-CTG-TRN-QTY    TO PV-HOLD-RTN-TRN-QTY    02848011
028490           MOVE EPT00004-CTG-ITM-QTY    TO PV-HOLD-RTN-ITM-QTY    02849011
028500           MOVE PV-SLS-RET-AMT          TO PV-HOLD-RTN-AMT        02850011
028510        ELSE                                                      02851011
028520           IF EPT00004-SUM-CTG-CDE = 'SAL'                        02852011
028530              MOVE EPT00004-CTG-TRN-QTY TO PV-HOLD-SLS-TRN-QTY    02853011
028540              MOVE EPT00004-CTG-ITM-QTY TO PV-HOLD-SLS-ITM-QTY    02854011
028550              MOVE PV-SLS-RET-AMT       TO PV-HOLD-SLS-AMT        02855011
028560           END-IF                                                 02856011
028570        END-IF                                                    02857011
028580     END-IF.                                                      02858011
028590******************************************************************02859011
028600* 3050-PROCESS-LAST-RECORD.                                       02860011
028610******************************************************************02861011
028620 3050-PROCESS-LAST-RECORD.                                        02862011
028630                                                                  02863011
028640     PERFORM 3100-GET-DIST-NBR.                                   02864011
028650     PERFORM 3150-GET-OPN-CLSE-DATE.                              02865011
028660     MOVE XST00002-DIST-NBR     TO PV-HOLD-DISTRICT-NBR.          02866011
028670     SET DT-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR           02867011
028680     SET DC-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR           02868011
028690     SET DN-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR           02869011
028700     SET DX-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR           02870011
028710     MOVE XST00001-OPN-DTE      TO PV-HOLD-OPENED-DATE.           02871011
028720     MOVE XST00001-CLO-DTE      TO PV-HOLD-CLOSED-DATE.           02872011
028730     PERFORM 3200-FILL-STORE-TABLE-CURR.                          02873011
028740     PERFORM 3300-FILL-CORP-TABLE-CURR.                           02874011
028750     PERFORM 3600-FILL-DIST-TABLE-CURR.                           02875011
028760     PERFORM 5075-DETERMINE-COMP-NCOMP.                           02876011
028770     IF PS-COMP-STORE                                             02877011
028780        PERFORM 3400-FILL-CORP-COMP-TBL-CUR                       02878011
028790        PERFORM 3500-FILL-DIST-COMP-TBL-CUR                       02879011
028800     ELSE                                                         02880011
028810        IF PS-NCOMP-STORE                                         02881011
028820           PERFORM 3410-FILL-CORP-NCOMP-TBL-CUR                   02882011
028830           PERFORM 3510-FILL-DIST-NCOMP-TBL-CUR                   02883011
028840        ELSE                                                      02884011
028850           IF PS-NEW-STORE                                        02885011
028860              PERFORM 3420-FILL-CORP-NEW-TBL-CUR                  02886011
028870              PERFORM 3520-FILL-DIST-NEW-TBL-CUR                  02887011
028880           END-IF                                                 02888011
028890        END-IF                                                    02889011
028900     END-IF.                                                      02890011
028910     SET PV-MAX-STRNR-IDX     TO TT-STRNR-IDX.                    02891011
028920     SET PV-MAX-DT-DISTNR-IDX TO DT-DISTNR-IDX.                   02892011
028930     SET PV-NUM-DAYS          TO CT-CORP-INDEX.                   02893011
028940******************************************************************02894011
028950* 3100-GET-DIST-NBR.                                              02895011
028960*    SELECTS DISTRICT NUMBER FOR THE CURRENT STORE.               02896011
028970******************************************************************02897011
028980 3100-GET-DIST-NBR.                                               02898011
028990                                                                  02899011
029000     EXEC SQL                                                     02900011
029010         SELECT DIST_NBR                                          02901011
029020         INTO   :XST00002-DIST-NBR                                02902011
029030         FROM   XST_LOC_DIST                                      02903011
029040         WHERE  LOC_NBR         = :PV-HOLD-STR-NBR                02904011
029050           AND  BEG_DTE        <= :CC-DATE                        02905011
029060           AND (END_DTE        >= :CC-DATE                        02906011
029070            OR  END_DTE        IS NULL)                           02907011
029080         WITH UR                                                  02908011
029090     END-EXEC.                                                    02909011
029100                                                                  02910011
029110     EVALUATE TRUE                                                02911011
029120        WHEN SQLCODE = ZERO                                       02912011
029130          CONTINUE                                                02913011
029140        WHEN SQLCODE NOT EQUAL ZERO                               02914011
029150            MOVE '3100-GET-DIST-NBR'                              02915011
029160                                    TO PV-PARAGRAPH-NAME          02916011
029170            MOVE SQLCODE            TO PV-DB2-OPERATION           02917011
029180            PERFORM 9100-SQL-ABEND                                02918011
029190     END-EVALUATE.                                                02919011
029200                                                                  02920011
029210******************************************************************02921011
029220* 3150-GET-OPN-CLSE-DATE.                                         02922011
029230*    DISTRICT NUMBER, OPEN AND CLOSE DATE FOR THE CURRENT STORE   02923011
029240*    IS RETURNED FROM THIS PARAGRAPH.                             02924011
029250******************************************************************02925011
029260 3150-GET-OPN-CLSE-DATE.                                          02926011
029270                                                                  02927011
029280     EXEC SQL                                                     02928011
029290         SELECT OPN_DTE                                           02929011
029300              , CLO_DTE                                           02930011
029310         INTO   :XST00001-OPN-DTE                                 02931011
029320              , :XST00001-CLO-DTE                                 02932011
029330         FROM   XST_LOC                                           02933011
029340         WHERE  LOC_NBR         = :PV-HOLD-STR-NBR                02934011
029350         WITH UR                                                  02935011
029360     END-EXEC.                                                    02936011
029370                                                                  02937011
029380     EVALUATE TRUE                                                02938011
029390        WHEN SQLCODE = ZERO                                       02939011
029400          CONTINUE                                                02940011
029410        WHEN SQLCODE NOT EQUAL ZERO                               02941011
029420            MOVE '3150-GET-OPN-CLSE-DATE'                         02942011
029430                                    TO PV-PARAGRAPH-NAME          02943011
029440            MOVE SQLCODE            TO PV-DB2-OPERATION           02944011
029450            PERFORM 9100-SQL-ABEND                                02945011
029460     END-EVALUATE.                                                02946011
029470                                                                  02947011
029480******************************************************************02948011
029490* 3200-FILL-STORE-TABLE-CURR.                                     02949011
029500*    ADDS THIS YEARS DATA TO THE STORE TOTALS ARRAY.              02950011
029510******************************************************************02951011
029520 3200-FILL-STORE-TABLE-CURR.                                      02952011
029530                                                                  02953011
029540     MOVE TT-DATE (TT-STRNR-IDX, TT-STR-IDX) TO PV-TEMP-DATE.     02954011
029550     PERFORM 3275-VERIFY-STR-TABLE-DATE                           02955011
029560        UNTIL PV-HOLD-SLS-DTE = PV-TEMP-DATE.                     02956011
029570     MOVE SPACES  TO PV-TEMP-DATE.                                02957011
029580                                                                  02958011
029590     MOVE PV-HOLD-STR-NBR           TO                            02959011
029600             TT-STR-NBR (TT-STRNR-IDX, TT-STR-IDX).               02960011
029610     MOVE PV-HOLD-DISTRICT-NBR      TO                            02961011
029620             TT-DISTRICT-NBR (TT-STRNR-IDX, TT-STR-IDX).          02962011
029630     MOVE PV-HOLD-SLS-AMT           TO                            02963011
029640             TT-STORE-SLS-AMT (TT-STRNR-IDX, TT-STR-IDX).         02964011
029650     MOVE PV-HOLD-RTN-AMT           TO                            02965011
029660             TT-STORE-RTN-AMT (TT-STRNR-IDX, TT-STR-IDX).         02966011
029670     MOVE PV-HOLD-SLS-TRN-QTY       TO                            02967011
029680             TT-STORE-SLS-TRN-QTY (TT-STRNR-IDX, TT-STR-IDX).     02968011
029690     MOVE PV-HOLD-RTN-TRN-QTY       TO                            02969011
029700             TT-STORE-RTN-TRN-QTY (TT-STRNR-IDX, TT-STR-IDX).     02970011
029710     MOVE PV-HOLD-SLS-ITM-QTY       TO                            02971011
029720             TT-STORE-SLS-ITM-QTY (TT-STRNR-IDX, TT-STR-IDX).     02972011
029730     MOVE PV-HOLD-RTN-ITM-QTY       TO                            02973011
029740             TT-STORE-RTN-ITM-QTY (TT-STRNR-IDX, TT-STR-IDX).     02974011
029750                                                                  02975011
029760                                                                  02976011
029770******************************************************************02977011
029780* 3225-FILL-STORE-TABLE-HIST.                                     02978011
029790*    ADDS LAST YEARS DATA TO THE STORE TOTALS ARRAY.              02979011
029800******************************************************************02980011
029810 3225-FILL-STORE-TABLE-HIST.                                      02981011
029820                                                                  02982011
029830     MOVE TT-STR-NBR (TT-STRNR-IDX, TT-STR-IDX)                   02983011
029840                                             TO PV-TEMP-STR-NBR.  02984011
029850     SET PS-STR-NBR-FOUND TO TRUE.                                02985011
029860     SET PV-TEMP-STR-IDX-VAL TO TT-STRNR-IDX.                     02986011
029870     PERFORM 3280-VERIFY-STR-NBR                                  02987011
029880        UNTIL (PV-HOLD-STR-NBR = PV-TEMP-STR-NBR) OR              02988011
029890              PS-STR-NBR-NOT-FOUND.                               02989011
029900                                                                  02990011
029910     IF PV-HOLD-STR-NBR = PV-TEMP-STR-NBR                         02991011
029920        SET PS-STR-NBR-FOUND TO TRUE                              02992011
029930     END-IF.                                                      02993011
029940                                                                  02994011
029950     IF PS-STR-NBR-FOUND                                          02995011
029960        SET PS-VERIFY-LY-DATE-L TO TRUE                           02996011
029970        MOVE TT-DATE (TT-STRNR-IDX, TT-STR-IDX) TO PV-TEMP-DATE   02997011
029980        SUBTRACT 1 FROM PV-TEMP-DATE-CCYY                         02998011
029990        PERFORM 3275-VERIFY-STR-TABLE-DATE                        02999011
030000           UNTIL PV-HOLD-SLS-DTE = PV-TEMP-DATE                   03000011
030010        MOVE SPACES  TO PV-TEMP-DATE                              03001011
030020                                                                  03002011
030030        MOVE PV-HOLD-LY-SLS-AMT           TO                      03003011
030040                TT-STORE-LY-SLS-AMT (TT-STRNR-IDX, TT-STR-IDX)    03004011
030050        MOVE PV-HOLD-LY-RTN-AMT           TO                      03005011
030060                TT-STORE-LY-RTN-AMT (TT-STRNR-IDX, TT-STR-IDX)    03006011
030070        MOVE PV-HOLD-LY-SLS-TRN-QTY       TO                      03007011
030080               TT-STORE-LY-SLS-TRN-QTY (TT-STRNR-IDX, TT-STR-IDX) 03008011
030090        MOVE PV-HOLD-LY-RTN-TRN-QTY       TO                      03009011
030100               TT-STORE-LY-RTN-TRN-QTY (TT-STRNR-IDX, TT-STR-IDX) 03010011
030110        MOVE PV-HOLD-LY-SLS-ITM-QTY       TO                      03011011
030120               TT-STORE-LY-SLS-ITM-QTY (TT-STRNR-IDX, TT-STR-IDX) 03012011
030130        MOVE PV-HOLD-LY-RTN-ITM-QTY       TO                      03013011
030140               TT-STORE-LY-RTN-ITM-QTY (TT-STRNR-IDX, TT-STR-IDX) 03014011
030150     END-IF.                                                      03015011
030160                                                                  03016011
030170******************************************************************03017011
030180* 3275-VERIFY-STR-TABLE-DATE.                                     03018011
030190*    BEFORE ADDING DATA TO THE STORE TOTALS ARRAY,                03019011
030200*    THE DATE IS VERIFIED AGAINST THE DATA IN THE ARRAY TO MAKE   03020011
030210*    SURE THE DATA IS BEING ADDED TO THE CORRECT DAY.             03021011
030220******************************************************************03022011
030230 3275-VERIFY-STR-TABLE-DATE.                                      03023011
030240     IF PV-HOLD-SLS-DTE NOT = PV-TEMP-DATE                        03024011
030250        MOVE PV-HOLD-STR-NBR                                      03025011
030260                   TO TT-STR-NBR (TT-STRNR-IDX, TT-STR-IDX)       03026011
030270        IF PV-HOLD-SLS-DTE > PV-TEMP-DATE                         03027011
030280           SET TT-STR-IDX UP BY +1                                03028011
030290        ELSE                                                      03029011
030300           IF PV-HOLD-SLS-DTE < PV-TEMP-DATE                      03030011
030310              SET TT-STR-IDX DOWN BY 1                            03031011
030320           END-IF                                                 03032011
030330        END-IF                                                    03033011
030340     END-IF.                                                      03034011
030350                                                                  03035011
030360     MOVE TT-DATE (TT-STRNR-IDX, TT-STR-IDX) TO PV-TEMP-DATE.     03036011
030370                                                                  03037011
030380     IF PS-VERIFY-LY-DATE-L                                       03038011
030390        SUBTRACT 1 FROM PV-TEMP-DATE-CCYY                         03039011
030400     END-IF.                                                      03040011
030410                                                                  03041011
030420******************************************************************03042011
030430* 3280-VERIFY-STR-NBR.                                            03043011
030440*    BEFORE ADDING DATA TO THE STORE TOTALS ARRAY, THE STORE      03044011
030450*    NUMBER IS VERIFIED AGAINST THE DATA IN THE TABLE TO MAKE     03045011
030460*    SURE THE DATA IS BEING ADDED TO THE CORRECT STORE TOTALS.    03046011
030470******************************************************************03047011
030480 3280-VERIFY-STR-NBR.                                             03048011
030490                                                                  03049011
030500                                                                  03050011
030510     IF PV-HOLD-STR-NBR NOT = PV-TEMP-STR-NBR                     03051011
030520        IF PV-HOLD-STR-NBR > PV-TEMP-STR-NBR AND                  03052011
030530           PS-STR-NBR-SEARCH NOT = 'H'                            03053011
030540           SET TT-STRNR-IDX UP BY +1                              03054011
030550           SET PS-STR-NBR-SEARCH-LOW TO TRUE                      03055011
030560        ELSE                                                      03056011
030570           IF PV-HOLD-STR-NBR < PV-TEMP-STR-NBR AND               03057011
030580              PS-STR-NBR-SEARCH NOT = 'L'                         03058011
030590              SET TT-STRNR-IDX DOWN BY 1                          03059011
030600              SET PS-STR-NBR-SEARCH-HIGH TO TRUE                  03060011
030610           ELSE                                                   03061011
030620              SET PS-STR-NBR-NOT-FOUND TO TRUE                    03062011
030630           END-IF                                                 03063011
030640        END-IF                                                    03064011
030650     END-IF.                                                      03065011
030660                                                                  03066011
030670     IF TT-STRNR-IDX > PV-MAX-STRNR-IDX                           03067011
030680         DISPLAY 'PV-HOLD-STR-NBR ' PV-HOLD-STR-NBR               03068011
030690         DISPLAY 'PV-TEMP-STR-NBR ' PV-TEMP-STR-NBR               03069011
030700         SET PS-STR-NBR-NOT-FOUND TO TRUE                         03070011
030710         SET TT-STRNR-IDX TO PV-TEMP-STR-IDX-VAL                  03071011
030720     ELSE                                                         03072011
030730         MOVE TT-STR-NBR (TT-STRNR-IDX, TT-STR-IDX)               03073011
030740                                   TO PV-TEMP-STR-NBR             03074011
030750     END-IF.                                                      03075011
030760                                                                  03076011
030770******************************************************************03077011
030780* 3300-FILL-CORP-TABLE-CURR.                                      03078011
030790*    ADDS THIS YEARS DATA TO THE CORPORATE TOTALS TABLE.          03079011
030800******************************************************************03080011
030810 3300-FILL-CORP-TABLE-CURR.                                       03081011
030820                                                                  03082011
030830                                                                  03083011
030840     MOVE CT-DATE (CT-CORP-INDEX) TO PV-TEMP-DATE.                03084011
030850     PERFORM 3375-VERIFY-TABLE-DATE                               03085011
030860        UNTIL PV-HOLD-SLS-DTE = PV-TEMP-DATE.                     03086011
030870     MOVE SPACES  TO PV-TEMP-DATE.                                03087011
030880                                                                  03088011
030890     IF PS-FIRST-STR-Y                                            03089011
030900        MOVE PV-HOLD-SLS-AMT           TO                         03090011
030910                CT-CORP-SLS-AMT (CT-CORP-INDEX)                   03091011
030920        MOVE PV-HOLD-RTN-AMT           TO                         03092011
030930                CT-CORP-RTN-AMT (CT-CORP-INDEX)                   03093011
030940        MOVE PV-HOLD-SLS-TRN-QTY       TO                         03094011
030950                CT-CORP-SLS-TRN-QTY (CT-CORP-INDEX)               03095011
030960        MOVE PV-HOLD-RTN-TRN-QTY       TO                         03096011
030970                CT-CORP-RTN-TRN-QTY (CT-CORP-INDEX)               03097011
030980        MOVE PV-HOLD-SLS-ITM-QTY       TO                         03098011
030990                CT-CORP-SLS-ITM-QTY (CT-CORP-INDEX)               03099011
031000        MOVE PV-HOLD-RTN-ITM-QTY       TO                         03100011
031010                CT-CORP-RTN-ITM-QTY (CT-CORP-INDEX)               03101011
031020     ELSE                                                         03102011
031030        ADD  PV-HOLD-SLS-AMT           TO                         03103011
031040                CT-CORP-SLS-AMT (CT-CORP-INDEX)                   03104011
031050        ADD  PV-HOLD-RTN-AMT           TO                         03105011
031060                CT-CORP-RTN-AMT (CT-CORP-INDEX)                   03106011
031070        ADD  PV-HOLD-SLS-TRN-QTY       TO                         03107011
031080                CT-CORP-SLS-TRN-QTY (CT-CORP-INDEX)               03108011
031090        ADD  PV-HOLD-RTN-TRN-QTY       TO                         03109011
031100                CT-CORP-RTN-TRN-QTY (CT-CORP-INDEX)               03110011
031110        ADD  PV-HOLD-SLS-ITM-QTY       TO                         03111011
031120                CT-CORP-SLS-ITM-QTY (CT-CORP-INDEX)               03112011
031130        ADD  PV-HOLD-RTN-ITM-QTY       TO                         03113011
031140                CT-CORP-RTN-ITM-QTY (CT-CORP-INDEX)               03114011
031150     END-IF.                                                      03115011
031160                                                                  03116011
031170                                                                  03117011
