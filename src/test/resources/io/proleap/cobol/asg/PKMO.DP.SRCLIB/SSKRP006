038670* 3680-VERIFY-DIST-NBR.                                           03867011
038680*    BEFORE ADDING HISTORICAL DATA TO DISTRICT ARRAY,             03868011
038690*    THE DISTRICT NUMBER IS VERIFIED HERE TO MAKE SURE IT IS      03869011
038700*    BEING ADDED TO THE CORRECT DISTRICT.                         03870011
038710******************************************************************03871011
038720 3680-VERIFY-DIST-NBR.                                            03872011
038730                                                                  03873011
038740     IF PV-HOLD-DISTRICT-NBR NOT = PV-TEMP-DIST-NBR               03874011
038750        IF PV-HOLD-DISTRICT-NBR > PV-TEMP-DIST-NBR AND            03875011
038760           PS-DIST-NBR-SEARCH NOT = 'H'                           03876011
038770           SET DT-DISTNR-IDX UP BY +1                             03877011
038780           SET PS-DIST-NBR-SEARCH-LOW TO TRUE                     03878011
038790        ELSE                                                      03879011
038800           IF PV-HOLD-DISTRICT-NBR < PV-TEMP-DIST-NBR AND         03880011
038810              PS-DIST-NBR-SEARCH NOT = 'L'                        03881011
038820              SET DT-DISTNR-IDX DOWN BY 1                         03882011
038830              SET PS-DIST-NBR-SEARCH-HIGH TO TRUE                 03883011
038840           ELSE                                                   03884011
038850              SET PS-DIST-NBR-NOT-FOUND TO TRUE                   03885011
038860           END-IF                                                 03886011
038870        END-IF                                                    03887011
038880     END-IF.                                                      03888011
038890                                                                  03889011
