039840        SET DT-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        03984011
039850        SET DC-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        03985011
039860        SET DN-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        03986011
039870        SET DX-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        03987011
039880        MOVE XST00001-OPN-DTE      TO PV-HOLD-OPENED-DATE         03988011
039890        MOVE XST00001-CLO-DTE      TO PV-HOLD-CLOSED-DATE         03989011
039900     END-IF.                                                      03990011
039910                                                                  03991011
039920      IF EPT00004-STR-NBR   = PV-HOLD-STR-NBR                     03992011
039930        IF EPPART-SLS-DTE  = PV-HOLD-SLS-DTE                      03993011
039940           IF EPT00004-SUM-CTG-CDE = 'RET'                        03994011
039950             MOVE EPT00004-CTG-TRN-QTY  TO PV-HOLD-LY-RTN-TRN-QTY 03995011
039960             MOVE EPT00004-CTG-ITM-QTY  TO PV-HOLD-LY-RTN-ITM-QTY 03996011
039970             MOVE PV-SLS-RET-AMT        TO PV-HOLD-LY-RTN-AMT     03997011
039980           ELSE                                                   03998011
