035240******************************************************************03524011
035250* 3505-FILL-DIST-COMP-TBL-HIST.                                   03525011
035260*    ADDS LAST YEARS DATA TO THE COMP DISTRICT TOTALS ARRAY.      03526011
035270******************************************************************03527011
035280 3505-FILL-DIST-COMP-TBL-HIST.                                    03528011
035290                                                                  03529011
035300     SET PV-TEMP-DIST-NBR TO DC-DISTNR-IDX.                       03530011
035310                                                                  03531011
035320     PERFORM 3685-VERIFY-DIST-NBR                                 03532011
035330        UNTIL (PV-HOLD-DISTRICT-NBR = PV-TEMP-DIST-NBR) OR        03533011
035340              PS-DIST-NBR-NOT-FOUND.                              03534011
035350                                                                  03535011
035360     IF PV-HOLD-DISTRICT-NBR = PV-TEMP-DIST-NBR                   03536011
035370        SET PS-DIST-NBR-FOUND TO TRUE                             03537011
035380     END-IF.                                                      03538011
035390                                                                  03539011
035400     IF PS-DIST-NBR-FOUND                                         03540011
035410        SET PS-VERIFY-LY-DATE-L TO TRUE                           03541011
035420        MOVE DC-DATE (DC-DISTNR-IDX, DC-DIST-IDX) TO PV-TEMP-DATE 03542011
035430        SUBTRACT 1 FROM PV-TEMP-DATE-CCYY                         03543011
035440        PERFORM 3575-VERIFY-TABLE-DATE                            03544011
035450           UNTIL PV-HOLD-SLS-DTE = PV-TEMP-DATE                   03545011
035460        MOVE SPACES  TO PV-TEMP-DATE                              03546011
