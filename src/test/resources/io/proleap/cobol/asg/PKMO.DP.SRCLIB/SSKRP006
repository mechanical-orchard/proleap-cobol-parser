068160                                                                  06816011
068170     MOVE 0  TO  PV-WEEK-TOT-SLS-AMT                              06817011
068180                 PV-WEEK-TOT-RTN-AMT                              06818011
068190                 PV-WEEK-TOT-SLS-TRN-QTY                          06819011
068200                 PV-WEEK-TOT-RTN-TRN-QTY                          06820011
068210                 PV-WEEK-TOT-SLS-ITM-QTY                          06821011
068220                 PV-WEEK-TOT-RTN-ITM-QTY                          06822011
068230                 PV-WEEK-TOT-LY-SLS-AMT                           06823011
068240                 PV-WEEK-TOT-LY-RTN-AMT                           06824011
068250                 PV-WEEK-TOT-LY-SLS-TRN-QTY                       06825011
068260                 PV-WEEK-TOT-LY-RTN-TRN-QTY                       06826011
068270                 PV-WEEK-TOT-LY-SLS-ITM-QTY                       06827011
068280                 PV-WEEK-TOT-LY-RTN-ITM-QTY.                      06828011
068290                                                                  06829011
068300     IF PV-WEEKDAY-NUM = 1 AND PV-DAYS-REMAINING > 0              06830011
068310        MOVE PV-WEEKDAY-NUM                TO H5-DAY1             06831011
068320        MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE        06832011
068330        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY       06833011
068340        MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM         06834011
068350        MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD         06835011
068360        PERFORM 6675-MAKE-DIST-CALCULATIONS                       06836011
068370        PERFORM 8010-RPTVAL-TO-REPORT-DAY1                        06837011
068380                                                                  06838011
068390        SUBTRACT 1 FROM PV-DAYS-REMAINING                         06839011
068400        SET DC-DIST-IDX UP BY +1                                  06840011
068410        ADD +1 TO PV-WEEKDAY-NUM                                  06841011
068420     ELSE                                                         06842011
068430        IF PV-WEEKDAY-NUM > 1 AND PV-DAYS-REMAINING > 0           06843011
068440           MOVE 1                             TO H5-DAY1          06844011
068450           MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE     06845011
068460           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY    06846011
068470           MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM      06847011
068480           MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD      06848011
068490           PERFORM 8015-ZERO-TO-REPORT-DAY1                       06849011
068500        END-IF                                                    06850011
068510     END-IF.                                                      06851011
068520                                                                  06852011
068530     IF PV-WEEKDAY-NUM = 2 AND PV-DAYS-REMAINING > 0              06853011
068540        PERFORM 5050-CONVERT-INPUT-DATE                           06854011
068550        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE2-CCYY     06855011
068560        MOVE PV-TEMP-DATE-MM                 TO H4-DATE2-MM       06856011
068570        MOVE PV-TEMP-DATE-DD                 TO H4-DATE2-DD       06857011
068580        MOVE PV-WEEKDAY-NUM                  TO H5-DAY2           06858011
068590        PERFORM 6675-MAKE-DIST-CALCULATIONS                       06859011
068600        PERFORM 8020-RPTVAL-TO-REPORT-DAY2                        06860011
068610                                                                  06861011
