053490     ELSE                                                         05349011
053500        COMPUTE PV-TMP-AVG-RTN-TRN-VALUE-TY ROUNDED =             05350011
053510                CC-CORP-RTN-AMT (CC-CORP-INDEX) /                 05351011
053520                CC-CORP-RTN-TRN-QTY (CC-CORP-INDEX)               05352011
053530     END-IF.                                                      05353011
053540     COMPUTE PV-TMP-AVG-RTN-TRN-VALUE-VAR ROUNDED =               05354011
053550                (PV-TMP-AVG-RTN-TRN-VALUE-TY -                    05355011
053560                PV-TMP-AVG-RTN-TRN-VALUE-LY) * -1.                05356011
053570     IF PV-TMP-AVG-RTN-TRN-VALUE-LY = 0                           05357011
053580        MOVE 0 TO PV-TMP-AVG-RTN-TRN-VAL-TYVSLY                   05358011
053590     ELSE                                                         05359011
053600        COMPUTE PV-TMP-AVG-RTNTRN-VAL-TYLY-HLD =                  05360011
053610                ((PV-TMP-AVG-RTN-TRN-VALUE-TY -                   05361011
053620                PV-TMP-AVG-RTN-TRN-VALUE-LY) /                    05362011
053630                PV-TMP-AVG-RTN-TRN-VALUE-LY) * 100                05363011
053640        COMPUTE PV-TMP-AVG-RTN-TRN-VAL-TYVSLY ROUNDED =           05364011
053650                PV-TMP-AVG-RTNTRN-VAL-TYLY-HLD                    05365011
053660     END-IF.                                                      05366011
053670     IF CC-CORP-LY-RTN-ITM-QTY (CC-CORP-INDEX) = 0 THEN           05367011
