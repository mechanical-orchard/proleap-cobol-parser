045640     END-IF.                                                      04564011
045650                                                                  04565011
045660******************************************************************04566011
045670* 6100-PROCESS-RPT-DAYS.                                          04567011
045680*     PROCESS CORPORATE TOTALS REPORT.                            04568011
045690******************************************************************04569011
045700 6100-PROCESS-RPT-DAYS.                                           04570011
045710                                                                  04571011
045720      SET PV-TEMP-INDEX TO CT-CORP-INDEX                          04572011
045730      COMPUTE PV-DAYS-REMAINING =                                 04573011
045740              PV-NUM-DAYS - PV-TEMP-INDEX + 1.                    04574011
045750      IF PV-DAYS-REMAINING > 0 THEN                               04575011
045760         ADD +1 TO PV-WEEK-CNT                                    04576011
045770         PERFORM 6150-PROCESS-WEEK                                04577011
045780      ELSE                                                        04578011
045790         SET PS-CORP-COMPLETED-Y TO TRUE                          04579011
045800      END-IF.                                                     04580011
045810                                                                  04581011
045820******************************************************************04582011
045830* 6150-PROCESS-WEEK.                                              04583011
045840*     PROCESS CORPORATE TOTALS REPORT WEEK.                       04584011
045850******************************************************************04585011
045860 6150-PROCESS-WEEK.                                               04586011
045870                                                                  04587011
045880     MOVE CT-DATE (CT-CORP-INDEX) TO PV-TEMP-DATE.                04588011
045890     MOVE PV-WEEK-CNT             TO H4-WEEKNUM                   04589011
045900                                     H4-ACCUM-WEEKNUM.            04590011
045910                                                                  04591011
045920     PERFORM 5000-CONVERT-INPUT-DATE.                             04592011
045930                                                                  04593011
045940     MOVE 0  TO  PV-WEEK-TOT-SLS-AMT                              04594011
045950                 PV-WEEK-TOT-RTN-AMT                              04595011
045960                 PV-WEEK-TOT-SLS-TRN-QTY                          04596011
045970                 PV-WEEK-TOT-RTN-TRN-QTY                          04597011
045980                 PV-WEEK-TOT-SLS-ITM-QTY                          04598011
045990                 PV-WEEK-TOT-RTN-ITM-QTY                          04599011
046000                 PV-WEEK-TOT-LY-SLS-AMT                           04600011
046010                 PV-WEEK-TOT-LY-RTN-AMT                           04601011
046020                 PV-WEEK-TOT-LY-SLS-TRN-QTY                       04602011
046030                 PV-WEEK-TOT-LY-RTN-TRN-QTY                       04603011
046040                 PV-WEEK-TOT-LY-SLS-ITM-QTY                       04604011
046050                 PV-WEEK-TOT-LY-RTN-ITM-QTY.                      04605011
046060                                                                  04606011
046070     IF PV-WEEKDAY-NUM = 1 AND PV-DAYS-REMAINING > 0              04607011
046080        MOVE PV-WEEKDAY-NUM                TO H5-DAY1             04608011
046090        MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE        04609011
046100        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY       04610011
046110        MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM         04611011
046120        MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD         04612011
046130        PERFORM 6175-MAKE-CORP-CALCULATIONS                       04613011
046140        PERFORM 8010-RPTVAL-TO-REPORT-DAY1                        04614011
046150                                                                  04615011
046160        SUBTRACT 1 FROM PV-DAYS-REMAINING                         04616011
046170        SET CT-CORP-INDEX UP BY +1                                04617011
046180        ADD +1 TO PV-WEEKDAY-NUM                                  04618011
046190     ELSE                                                         04619011
046200        IF PV-WEEKDAY-NUM > 1 AND PV-DAYS-REMAINING > 0           04620011
046210           MOVE 1                             TO H5-DAY1          04621011
046220           MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE     04622011
046230           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY    04623011
046240           MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM      04624011
046250           MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD      04625011
046260           PERFORM 8015-ZERO-TO-REPORT-DAY1                       04626011
046270        END-IF                                                    04627011
046280     END-IF.                                                      04628011
046290                                                                  04629011
046300     IF PV-WEEKDAY-NUM = 2 AND PV-DAYS-REMAINING > 0              04630011
046310        PERFORM 5050-CONVERT-INPUT-DATE                           04631011
046320        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE2-CCYY     04632011
046330        MOVE PV-TEMP-DATE-MM                 TO H4-DATE2-MM       04633011
046340        MOVE PV-TEMP-DATE-DD                 TO H4-DATE2-DD       04634011
046350        MOVE PV-WEEKDAY-NUM                  TO H5-DAY2           04635011
046360        PERFORM 6175-MAKE-CORP-CALCULATIONS                       04636011
046370        PERFORM 8020-RPTVAL-TO-REPORT-DAY2                        04637011
046380                                                                  04638011
046390        SUBTRACT 1 FROM PV-DAYS-REMAINING                         04639011
046400        SET CT-CORP-INDEX UP BY +1                                04640011
046410        ADD +1 TO PV-WEEKDAY-NUM                                  04641011
046420     ELSE                                                         04642011
046430        IF (PV-WEEKDAY-NUM = 2 AND PV-DAYS-REMAINING <= 0) OR     04643011
046440           (PV-WEEKDAY-NUM > 2 AND PV-DAYS-REMAINING > 0)         04644011
046450           MOVE 2                          TO H5-DAY2             04645011
046460           PERFORM 5050-CONVERT-INPUT-DATE                        04646011
046470           MOVE PV-TEMP-DATE-CCYY          TO H4-DATE2-CCYY       04647011
046480           MOVE PV-TEMP-DATE-MM            TO H4-DATE2-MM         04648011
046490           MOVE PV-TEMP-DATE-DD            TO H4-DATE2-DD         04649011
046500           IF PV-WEEKDAY-NUM = 2                                  04650011
046510              ADD +1 TO PV-WEEKDAY-NUM                            04651011
046520           END-IF                                                 04652011
046530           PERFORM 8025-ZERO-TO-REPORT-DAY2                       04653011
046540        END-IF                                                    04654011
046550     END-IF.                                                      04655011
046560                                                                  04656011
046570     IF PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING > 0              04657011
046580        PERFORM 5050-CONVERT-INPUT-DATE                           04658011
046590        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE3-CCYY     04659011
046600        MOVE PV-TEMP-DATE-MM                 TO H4-DATE3-MM       04660011
046610        MOVE PV-TEMP-DATE-DD                 TO H4-DATE3-DD       04661011
046620        MOVE PV-WEEKDAY-NUM                  TO H5-DAY3           04662011
046630        PERFORM 6175-MAKE-CORP-CALCULATIONS                       04663011
046640        PERFORM 8030-RPTVAL-TO-REPORT-DAY3                        04664011
046650                                                                  04665011
046660        SUBTRACT 1 FROM PV-DAYS-REMAINING                         04666011
046670        SET CT-CORP-INDEX UP BY +1                                04667011
046680        ADD +1 TO PV-WEEKDAY-NUM                                  04668011
046690     ELSE                                                         04669011
046700        IF (PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING <= 0) OR     04670011
046710           (PV-WEEKDAY-NUM > 3 AND PV-DAYS-REMAINING > 0)         04671011
046720           MOVE 3                          TO H5-DAY3             04672011
046730           PERFORM 5050-CONVERT-INPUT-DATE                        04673011
046740           MOVE PV-TEMP-DATE-CCYY          TO H4-DATE3-CCYY       04674011
046750           MOVE PV-TEMP-DATE-MM            TO H4-DATE3-MM         04675011
046760           MOVE PV-TEMP-DATE-DD            TO H4-DATE3-DD         04676011
046770           IF PV-WEEKDAY-NUM = 3                                  04677011
046780              ADD +1 TO PV-WEEKDAY-NUM                            04678011
046790           END-IF                                                 04679011
046800           PERFORM 8035-ZERO-TO-REPORT-DAY3                       04680011
046810        END-IF                                                    04681011
