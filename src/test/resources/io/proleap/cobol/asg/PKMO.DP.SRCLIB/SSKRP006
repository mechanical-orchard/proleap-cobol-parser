066090                                      PV-WEEK-TOT-LY-RTN-TRN-QTY  06609011
066100                                      PV-ACCUM-TOT-LY-RTN-TRN-QTY.06610011
066110     ADD DT-DIST-LY-SLS-ITM-QTY (DT-DISTNR-IDX, DT-DIST-IDX) TO   06611011
066120                                      PV-WEEK-TOT-LY-SLS-ITM-QTY  06612011
066130                                      PV-ACCUM-TOT-LY-SLS-ITM-QTY.06613011
066140     ADD DT-DIST-LY-RTN-ITM-QTY (DT-DISTNR-IDX, DT-DIST-IDX) TO   06614011
066150                                      PV-WEEK-TOT-LY-RTN-ITM-QTY  06615011
066160                                      PV-ACCUM-TOT-LY-RTN-ITM-QTY.06616011
066170                                                                  06617011
066180     IF DT-DIST-LY-SLS-AMT (DT-DISTNR-IDX, DT-DIST-IDX) = 0       06618011
066190        MOVE 0 TO PV-TMP-RETN-DLRS-PCNT-LY                        06619011
066200     ELSE                                                         06620011
066210        COMPUTE PV-TMP-RETN-DLRS-PCNT-LY-HLD =                    06621011
066220            ((DT-DIST-LY-RTN-AMT (DT-DISTNR-IDX, DT-DIST-IDX) /   06622011
066230            DT-DIST-LY-SLS-AMT (DT-DISTNR-IDX, DT-DIST-IDX))      06623011
066240            * 100) * -1                                           06624011
066250        COMPUTE PV-TMP-RETN-DLRS-PCNT-LY ROUNDED =                06625011
066260            PV-TMP-RETN-DLRS-PCNT-LY-HLD                          06626011
066270     END-IF.                                                      06627011
