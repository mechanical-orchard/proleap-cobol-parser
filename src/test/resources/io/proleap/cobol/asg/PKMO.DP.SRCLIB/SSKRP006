068730           IF PV-WEEKDAY-NUM = 2                                  06873011
068740              ADD +1 TO PV-WEEKDAY-NUM                            06874011
068750           END-IF                                                 06875011
068760           PERFORM 8025-ZERO-TO-REPORT-DAY2                       06876011
068770        END-IF                                                    06877011
068780     END-IF.                                                      06878011
068790                                                                  06879011
068800     IF PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING > 0              06880011
068810        PERFORM 5050-CONVERT-INPUT-DATE                           06881011
068820        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE3-CCYY     06882011
068830        MOVE PV-TEMP-DATE-MM                 TO H4-DATE3-MM       06883011
068840        MOVE PV-TEMP-DATE-DD                 TO H4-DATE3-DD       06884011
068850        MOVE PV-WEEKDAY-NUM                  TO H5-DAY3           06885011
068860        PERFORM 6675-MAKE-DIST-CALCULATIONS                       06886011
068870        PERFORM 8030-RPTVAL-TO-REPORT-DAY3                        06887011
068880                                                                  06888011
068890        SUBTRACT 1 FROM PV-DAYS-REMAINING                         06889011
068900        SET DC-DIST-IDX UP BY +1                                  06890011
068910        ADD +1 TO PV-WEEKDAY-NUM                                  06891011
068920     ELSE                                                         06892011
068930        IF (PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING <= 0) OR     06893011
068940           (PV-WEEKDAY-NUM > 3 AND PV-DAYS-REMAINING > 0)         06894011
068950           MOVE 3                          TO H5-DAY3             06895011
068960           PERFORM 5050-CONVERT-INPUT-DATE                        06896011
068970           MOVE PV-TEMP-DATE-CCYY          TO H4-DATE3-CCYY       06897011
068980           MOVE PV-TEMP-DATE-MM            TO H4-DATE3-MM         06898011
