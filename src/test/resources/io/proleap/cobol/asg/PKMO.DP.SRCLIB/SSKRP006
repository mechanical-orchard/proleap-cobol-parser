062740                ((PV-TMP-AVG-RTN-ITM-VALUE-TY -                   06274011
062750                PV-TMP-AVG-RTN-ITM-VALUE-LY) /                    06275011
062760                PV-TMP-AVG-RTN-ITM-VALUE-LY) * 100                06276011
062770        COMPUTE PV-TMP-AVG-RTN-ITM-VAL-TYVSLY ROUNDED =           06277011
062780                PV-TMP-AVG-RTNITM-VAL-TYLY-HLD                    06278011
062790     END-IF.                                                      06279011
062800     COMPUTE PV-TMP-RET-ITM-TOT-TY ROUNDED =                      06280011
062810                CX-CORP-RTN-ITM-QTY (CX-CORP-INDEX).              06281011
062820     COMPUTE PV-TMP-RET-ITM-TOT-LY ROUNDED =                      06282011
062830                CX-CORP-LY-RTN-ITM-QTY (CX-CORP-INDEX).           06283011
062840     COMPUTE PV-TMP-RET-ITM-TOT-VAR ROUNDED =                     06284011
062850                PV-TMP-RET-ITM-TOT-TY - PV-TMP-RET-ITM-TOT-LY.    06285011
062860     IF CX-CORP-LY-RTN-ITM-QTY (CX-CORP-INDEX) = 0                06286011
062870        MOVE 0 TO PV-TMP-RET-ITM-TOT-TYVSLY                       06287011
062880     ELSE                                                         06288011
062890        COMPUTE PV-TMP-RET-ITM-TOT-TYVSLY-HLD =                   06289011
062900                ((CX-CORP-RTN-ITM-QTY (CX-CORP-INDEX) -           06290011
062910                CX-CORP-LY-RTN-ITM-QTY (CX-CORP-INDEX)) /         06291011
062920                CX-CORP-LY-RTN-ITM-QTY (CX-CORP-INDEX)) * 100     06292011
062930        COMPUTE PV-TMP-RET-ITM-TOT-TYVSLY ROUNDED =               06293011
062940                PV-TMP-RET-ITM-TOT-TYVSLY-HLD                     06294011
062950     END-IF.                                                      06295011
062960     IF CX-CORP-LY-RTN-TRN-QTY (CX-CORP-INDEX) = 0 AND            06296011
062970        CX-CORP-LY-SLS-TRN-QTY (CX-CORP-INDEX) = 0                06297011
062980        MOVE 0 TO PV-TMP-RET-TRNS-PCNT-TOT-LY                     06298011
062990     ELSE                                                         06299011
063000        COMPUTE PV-TMP-RET-TRN-PCNT-TOT-LY-HLD =                  06300011
063010                (CX-CORP-LY-RTN-TRN-QTY (CX-CORP-INDEX) /         06301011
063020                (CX-CORP-LY-RTN-TRN-QTY (CX-CORP-INDEX) +         06302011
063030                CX-CORP-LY-SLS-TRN-QTY (CX-CORP-INDEX))) * 100    06303011
063040        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-LY ROUNDED =             06304011
063050                PV-TMP-RET-TRN-PCNT-TOT-LY-HLD                    06305011
063060     END-IF.                                                      06306011
063070     IF CX-CORP-RTN-TRN-QTY (CX-CORP-INDEX) = 0 AND               06307011
063080        CX-CORP-SLS-TRN-QTY (CX-CORP-INDEX) = 0                   06308011
063090        MOVE 0 TO PV-TMP-RET-TRNS-PCNT-TOT-TY                     06309011
063100     ELSE                                                         06310011
063110        COMPUTE PV-TMP-RET-TRN-PCNT-TOT-TY-HLD =                  06311011
063120                (CX-CORP-RTN-TRN-QTY (CX-CORP-INDEX) /            06312011
063130                (CX-CORP-RTN-TRN-QTY (CX-CORP-INDEX) +            06313011
063140                CX-CORP-SLS-TRN-QTY (CX-CORP-INDEX))) * 100       06314011
063150        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-TY ROUNDED =             06315011
063160                PV-TMP-RET-TRN-PCNT-TOT-TY-HLD                    06316011
063170     END-IF.                                                      06317011
063180     COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-VAR ROUNDED =               06318011
063190                PV-TMP-RET-TRNS-PCNT-TOT-TY -                     06319011
063200                PV-TMP-RET-TRNS-PCNT-TOT-LY.                      06320011
063210     COMPUTE PV-TMP-TOT-TRNS-LY ROUNDED =                         06321011
063220                CX-CORP-LY-RTN-TRN-QTY (CX-CORP-INDEX) +          06322011
063230                CX-CORP-LY-SLS-TRN-QTY (CX-CORP-INDEX).           06323011
063240     COMPUTE PV-TMP-TOT-TRNS-TY ROUNDED =                         06324011
063250                CX-CORP-RTN-TRN-QTY (CX-CORP-INDEX) +             06325011
063260                CX-CORP-SLS-TRN-QTY (CX-CORP-INDEX).              06326011
063270     IF PV-TMP-TOT-TRNS-LY = 0                                    06327011
063280        MOVE 0 TO PV-TMP-TOT-TRNS-VAR                             06328011
063290     ELSE                                                         06329011
063300        COMPUTE PV-TMP-TOT-TRNS-VAR-HLD =                         06330011
063310                ((PV-TMP-TOT-TRNS-TY - PV-TMP-TOT-TRNS-LY) /      06331011
063320                PV-TMP-TOT-TRNS-LY) * 100                         06332011
063330        COMPUTE PV-TMP-TOT-TRNS-VAR ROUNDED =                     06333011
