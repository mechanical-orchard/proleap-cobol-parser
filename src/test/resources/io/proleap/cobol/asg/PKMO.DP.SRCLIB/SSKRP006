056450           END-IF                                                 05645011
056460           PERFORM 8065-ZERO-TO-REPORT-DAY6                       05646011
056470        END-IF                                                    05647011
056480     END-IF.                                                      05648011
056490                                                                  05649011
056500     IF PV-WEEKDAY-NUM = 7 AND PV-DAYS-REMAINING > 0              05650011
056510        PERFORM 5050-CONVERT-INPUT-DATE                           05651011
056520        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE7-CCYY       05652011
056530        MOVE PV-TEMP-DATE-MM               TO H4-DATE7-MM         05653011
056540        MOVE PV-TEMP-DATE-DD               TO H4-DATE7-DD         05654011
056550        MOVE PV-WEEKDAY-NUM                TO H5-DAY7             05655011
056560        PERFORM 6375-MAKE-CORP-CALCULATIONS                       05656011
056570        PERFORM 8070-RPTVAL-TO-REPORT-DAY7                        05657011
056580                                                                  05658011
056590        SUBTRACT 1 FROM PV-DAYS-REMAINING                         05659011
056600        SET CN-CORP-INDEX UP BY +1                                05660011
056610        ADD +1 TO PV-WEEKDAY-NUM                                  05661011
056620     ELSE                                                         05662011
056630        IF PV-WEEKDAY-NUM = 7 AND PV-DAYS-REMAINING <= 0          05663011
056640           MOVE 7                             TO H5-DAY7          05664011
056650           PERFORM 5050-CONVERT-INPUT-DATE                        05665011
056660           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE7-CCYY    05666011
056670           MOVE PV-TEMP-DATE-MM               TO H4-DATE7-MM      05667011
056680           MOVE PV-TEMP-DATE-DD               TO H4-DATE7-DD      05668011
056690           IF PV-WEEKDAY-NUM = 7                                  05669011
056700              ADD +1 TO PV-WEEKDAY-NUM                            05670011
056710           END-IF                                                 05671011
056720           PERFORM 8075-ZERO-TO-REPORT-DAY7                       05672011
056730        END-IF                                                    05673011
056740     END-IF.                                                      05674011
056750                                                                  05675011
056760     MOVE 'NON COMP'                    TO H3-STR-NBR.            05676011
056770     MOVE 'ALL'                         TO H3-DISTRICT-NBR.       05677011
056780                                                                  05678011
