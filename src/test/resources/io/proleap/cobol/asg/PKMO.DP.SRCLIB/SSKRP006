040730        PERFORM 3325-FILL-CORP-TABLE-HIST                         04073011
040740        PERFORM 3625-FILL-DIST-TABLE-HIST                         04074011
040750        PERFORM 5075-DETERMINE-COMP-NCOMP                         04075011
040760        IF PS-COMP-STORE                                          04076011
040770           PERFORM 3405-FILL-CORP-COMP-TBL-HIST                   04077011
040780           SET PS-FIRST-CC-STR-N     TO TRUE                      04078011
040790           SET CC-CORP-INDEX         TO 1                         04079011
040800           PERFORM 3505-FILL-DIST-COMP-TBL-HIST                   04080011
040810           SET PS-FIRST-DC-STR-N     TO TRUE                      04081011
040820           SET DC-DIST-IDX           TO 1                         04082011
040830        ELSE                                                      04083011
040840           IF PS-NCOMP-STORE                                      04084011
040850              PERFORM 3415-FILL-CORP-NCOMP-TBL-HIST               04085011
040860              SET PS-FIRST-CN-STR-N     TO TRUE                   04086011
040870              SET CN-CORP-INDEX         TO 1                      04087011
040880              PERFORM 3515-FILL-DIST-NCOMP-TBL-HIST               04088011
040890              SET PS-FIRST-DN-STR-N     TO TRUE                   04089011
040900              SET DN-DIST-IDX           TO 1                      04090011
040910           ELSE                                                   04091011
040920              IF PS-NEW-STORE                                     04092011
040930                 PERFORM 3425-FILL-CORP-NEW-TBL-HIST              04093011
040940                 SET PS-FIRST-CX-STR-N     TO TRUE                04094011
040950                 SET CX-CORP-INDEX         TO 1                   04095011
040960                 PERFORM 3525-FILL-DIST-NEW-TBL-HIST              04096011
040970                 SET PS-FIRST-DX-STR-N     TO TRUE                04097011
040980                 SET DX-DIST-IDX           TO 1                   04098011
040990              END-IF                                              04099011
041000           END-IF                                                 04100011
041010        END-IF                                                    04101011
041020                                                                  04102011
041030        SET PS-FIRST-STR-N          TO TRUE                       04103011
041040        SET CT-CORP-INDEX    TO 1                                 04104011
041050        SET DT-DIST-IDX      TO 1                                 04105011
041060        SET TT-STR-IDX       TO 1                                 04106011
