058950* 6400-PROCESS-RPT-DAYS.                                          05895011
058960*     PROCESS NEW STORE CORPORATE TOTALS REPORT.                  05896011
058970******************************************************************05897011
058980 6400-PROCESS-RPT-DAYS.                                           05898011
058990                                                                  05899011
059000      SET PV-TEMP-INDEX TO CX-CORP-INDEX                          05900011
059010      COMPUTE PV-DAYS-REMAINING =                                 05901011
059020              PV-NUM-DAYS - PV-TEMP-INDEX + 1.                    05902011
059030      IF PV-DAYS-REMAINING > 0 THEN                               05903011
059040         ADD +1 TO PV-WEEK-CNT                                    05904011
059050         PERFORM 6450-PROCESS-WEEK                                05905011
059060      ELSE                                                        05906011
059070         SET PS-CORP-NEW-COMPLETED-Y TO TRUE                      05907011
059080      END-IF.                                                     05908011
059090                                                                  05909011
059100******************************************************************05910011
059110* 6450-PROCESS-WEEK.                                              05911011
059120*     PROCESS NEW STORE CORPORATE TOTALS REPORT WEEK.             05912011
059130******************************************************************05913011
059140 6450-PROCESS-WEEK.                                               05914011
059150                                                                  05915011
059160     MOVE CX-CORP-DATE (CX-CORP-INDEX) TO PV-TEMP-DATE.           05916011
059170     MOVE PV-WEEK-CNT             TO H4-WEEKNUM                   05917011
059180                                     H4-ACCUM-WEEKNUM.            05918011
059190                                                                  05919011
059200     PERFORM 5000-CONVERT-INPUT-DATE.                             05920011
059210                                                                  05921011
059220     MOVE 0  TO  PV-WEEK-TOT-SLS-AMT                              05922011
059230                 PV-WEEK-TOT-RTN-AMT                              05923011
059240                 PV-WEEK-TOT-SLS-TRN-QTY                          05924011
059250                 PV-WEEK-TOT-RTN-TRN-QTY                          05925011
059260                 PV-WEEK-TOT-SLS-ITM-QTY                          05926011
059270                 PV-WEEK-TOT-RTN-ITM-QTY                          05927011
059280                 PV-WEEK-TOT-LY-SLS-AMT                           05928011
059290                 PV-WEEK-TOT-LY-RTN-AMT                           05929011
059300                 PV-WEEK-TOT-LY-SLS-TRN-QTY                       05930011
059310                 PV-WEEK-TOT-LY-RTN-TRN-QTY                       05931011
059320                 PV-WEEK-TOT-LY-SLS-ITM-QTY                       05932011
059330                 PV-WEEK-TOT-LY-RTN-ITM-QTY.                      05933011
059340                                                                  05934011
059350     IF PV-WEEKDAY-NUM = 1 AND PV-DAYS-REMAINING > 0              05935011
059360        MOVE PV-WEEKDAY-NUM                TO H5-DAY1             05936011
059370        MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE        05937011
059380        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY       05938011
059390        MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM         05939011
059400        MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD         05940011
059410        PERFORM 6475-MAKE-CORP-CALCULATIONS                       05941011
059420        PERFORM 8010-RPTVAL-TO-REPORT-DAY1                        05942011
059430                                                                  05943011
059440        SUBTRACT 1 FROM PV-DAYS-REMAINING                         05944011
059450        SET CX-CORP-INDEX UP BY +1                                05945011
059460        ADD +1 TO PV-WEEKDAY-NUM                                  05946011
059470     ELSE                                                         05947011
059480        IF PV-WEEKDAY-NUM > 1 AND PV-DAYS-REMAINING > 0           05948011
059490           MOVE 1                             TO H5-DAY1          05949011
059500           MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE     05950011
059510           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY    05951011
