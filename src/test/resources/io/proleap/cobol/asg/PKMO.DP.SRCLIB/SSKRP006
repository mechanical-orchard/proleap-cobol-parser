038990        IF PV-HOLD-DISTRICT-NBR > PV-TEMP-DIST-NBR AND            03899011
039000           PS-DIST-NBR-SEARCH NOT = 'H'                           03900011
039010           SET DC-DISTNR-IDX UP BY +1                             03901011
039020           SET PS-DIST-NBR-SEARCH-LOW TO TRUE                     03902011
039030        ELSE                                                      03903011
039040           IF PV-HOLD-DISTRICT-NBR < PV-TEMP-DIST-NBR AND         03904011
039050              PS-DIST-NBR-SEARCH NOT = 'L'                        03905011
039060              SET DC-DISTNR-IDX DOWN BY 1                         03906011
039070              SET PS-DIST-NBR-SEARCH-HIGH TO TRUE                 03907011
039080           ELSE                                                   03908011
039090              SET PS-DIST-NBR-NOT-FOUND TO TRUE                   03909011
039100           END-IF                                                 03910011
039110        END-IF                                                    03911011
039120     END-IF.                                                      03912011
039130                                                                  03913011
039140******************************************************************03914011
039150* 3690-VERIFY-DIST-NBR.                                           03915011
039160*    BEFORE ADDING HISTORICAL DATA TO NON COMP DISTRICT ARRAY,    03916011
039170*    THE DISTRICT NUMBER IS VERIFIED HERE TO MAKE SURE IT IS      03917011
039180*    BEING ADDED TO THE CORRECT DISTRICT.                         03918011
039190******************************************************************03919011
039200 3690-VERIFY-DIST-NBR.                                            03920011
039210                                                                  03921011
039220     IF PV-HOLD-DISTRICT-NBR NOT = PV-TEMP-DIST-NBR               03922011
039230        IF PV-HOLD-DISTRICT-NBR > PV-TEMP-DIST-NBR AND            03923011
039240           PS-DIST-NBR-SEARCH NOT = 'H'                           03924011
039250           SET DN-DISTNR-IDX UP BY +1                             03925011
039260           SET PS-DIST-NBR-SEARCH-LOW TO TRUE                     03926011
039270        ELSE                                                      03927011
039280           IF PV-HOLD-DISTRICT-NBR < PV-TEMP-DIST-NBR AND         03928011
039290              PS-DIST-NBR-SEARCH NOT = 'L'                        03929011
039300              SET DN-DISTNR-IDX DOWN BY 1                         03930011
039310              SET PS-DIST-NBR-SEARCH-HIGH TO TRUE                 03931011
039320           ELSE                                                   03932011
039330              SET PS-DIST-NBR-NOT-FOUND TO TRUE                   03933011
039340           END-IF                                                 03934011
039350        END-IF                                                    03935011
039360     END-IF.                                                      03936011
039370                                                                  03937011
