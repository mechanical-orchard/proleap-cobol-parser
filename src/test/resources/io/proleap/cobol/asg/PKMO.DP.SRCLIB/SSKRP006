034540                                                                  03454011
034550     IF PV-HOLD-SLS-DTE NOT = PV-TEMP-DATE                        03455011
034560        IF PV-HOLD-SLS-DTE > PV-TEMP-DATE                         03456011
034570           SET CN-CORP-INDEX UP BY +1                             03457011
034580        ELSE                                                      03458011
034590           IF PV-HOLD-SLS-DTE < PV-TEMP-DATE                      03459011
034600              SET CN-CORP-INDEX DOWN BY 1                         03460011
034610           END-IF                                                 03461011
034620        END-IF                                                    03462011
034630     END-IF.                                                      03463011
034640                                                                  03464011
034650     MOVE CN-CORP-DATE (CN-CORP-INDEX) TO PV-TEMP-DATE.           03465011
034660                                                                  03466011
034670     IF PS-VERIFY-LY-DATE-L                                       03467011
034680        SUBTRACT 1 FROM PV-TEMP-DATE-CCYY                         03468011
034690     END-IF.                                                      03469011
034700                                                                  03470011
