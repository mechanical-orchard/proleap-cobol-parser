072060        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-LY ROUNDED =             07206011
072070                PV-TMP-RET-TRN-PCNT-TOT-LY-HLD                    07207011
072080     END-IF.                                                      07208011
072090     IF DC-DIST-RTN-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) = 0 AND  07209011
072100        DC-DIST-SLS-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) = 0      07210011
072110        MOVE 0 TO PV-TMP-RET-TRNS-PCNT-TOT-TY                     07211011
072120     ELSE                                                         07212011
072130        COMPUTE PV-TMP-RET-TRN-PCNT-TOT-TY-HLD =                  07213011
072140          (DC-DIST-RTN-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) /     07214011
072150          (DC-DIST-RTN-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) +     07215011
072160          DC-DIST-SLS-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX))) * 10007216011
072170        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-TY ROUNDED =             07217011
072180            PV-TMP-RET-TRN-PCNT-TOT-TY-HLD                        07218011
072190     END-IF.                                                      07219011
072200     COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-VAR ROUNDED =               07220011
072210            PV-TMP-RET-TRNS-PCNT-TOT-TY -                         07221011
072220            PV-TMP-RET-TRNS-PCNT-TOT-LY.                          07222011
072230     COMPUTE PV-TMP-TOT-TRNS-LY ROUNDED =                         07223011
072240            DC-DIST-LY-RTN-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) + 07224011
072250            DC-DIST-LY-SLS-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX).  07225011
072260     COMPUTE PV-TMP-TOT-TRNS-TY ROUNDED =                         07226011
072270            DC-DIST-RTN-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX) +    07227011
072280            DC-DIST-SLS-TRN-QTY (DC-DISTNR-IDX, DC-DIST-IDX).     07228011
072290     IF PV-TMP-TOT-TRNS-LY = 0                                    07229011
072300        MOVE 0 TO PV-TMP-TOT-TRNS-VAR                             07230011
072310     ELSE                                                         07231011
072320        COMPUTE PV-TMP-TOT-TRNS-VAR-HLD =                         07232011
072330                ((PV-TMP-TOT-TRNS-TY - PV-TMP-TOT-TRNS-LY) /      07233011
072340                PV-TMP-TOT-TRNS-LY) * 100                         07234011
072350        COMPUTE PV-TMP-TOT-TRNS-VAR ROUNDED =                     07235011
072360                PV-TMP-TOT-TRNS-VAR-HLD                           07236011
072370     END-IF.                                                      07237011
072380                                                                  07238011
072390******************************************************************07239011
