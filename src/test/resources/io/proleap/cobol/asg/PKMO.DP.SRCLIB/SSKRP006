075330            ((DN-DIST-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX) /      07533011
075340            DN-DIST-SLS-AMT (DN-DISTNR-IDX, DN-DIST-IDX))         07534011
075350            * 100) * -1                                           07535011
075360        COMPUTE PV-TMP-RETN-DLRS-PCNT-TY ROUNDED =                07536011
075370            PV-TMP-RETN-DLRS-PCNT-TY-HLD                          07537011
075380     END-IF.                                                      07538011
075390     COMPUTE PV-TMP-RETN-DLRS-PCNT-VAR ROUNDED =                  07539011
075400            PV-TMP-RETN-DLRS-PCNT-TY - PV-TMP-RETN-DLRS-PCNT-LY.  07540011
075410     COMPUTE PV-TMP-GROSS-SLS-TY-RTN-TRANS ROUNDED =              07541011
075420            DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX).     07542011
075430     COMPUTE PV-TMP-GROSS-SLS-TY-RTN-DLRS ROUNDED =               07543011
075440            DN-DIST-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX).         07544011
075450     COMPUTE PV-TMP-GROSS-SLS-TY-SLS-DLRS ROUNDED =               07545011
075460            DN-DIST-SLS-AMT (DN-DISTNR-IDX, DN-DIST-IDX).         07546011
075470     COMPUTE PV-TMP-GROSS-SLS-LY-RTN-TRANS ROUNDED =              07547011
075480            DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX).  07548011
075490     COMPUTE PV-TMP-GROSS-SLS-LY-RTN-DLRS ROUNDED =               07549011
075500            DN-DIST-LY-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX).      07550011
075510     COMPUTE PV-TMP-GROSS-SLS-LY-SLS-DLRS ROUNDED =               07551011
075520            DN-DIST-LY-SLS-AMT (DN-DISTNR-IDX, DN-DIST-IDX).      07552011
075530     IF DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07553011
075540        MOVE 0 TO PV-TMP-VAR-RTN-TRNS-TY-VS-LY                    07554011
075550     ELSE                                                         07555011
075560        COMPUTE PV-TMP-VAR-RTN-TRN-TYVSLY-HLD =                   07556011
075570            ((DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) -  07557011
075580            DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX)) /07558011
075590            DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX))  07559011
075600            * 100                                                 07560011
075610        COMPUTE PV-TMP-VAR-RTN-TRNS-TY-VS-LY ROUNDED =            07561011
075620                PV-TMP-VAR-RTN-TRN-TYVSLY-HLD                     07562011
075630     END-IF.                                                      07563011
075640     IF DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07564011
075650        MOVE 0 TO PV-TMP-VAR-RTN-ITMS-TY-VS-LY                    07565011
075660     ELSE                                                         07566011
075670        COMPUTE PV-TMP-VAR-RTN-ITM-TYVSLY-HLD =                   07567011
075680            ((DN-DIST-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) -  07568011
075690            DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX)) /07569011
075700            DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX))  07570011
075710            * 100                                                 07571011
075720        COMPUTE PV-TMP-VAR-RTN-ITMS-TY-VS-LY ROUNDED =            07572011
075730                PV-TMP-VAR-RTN-ITM-TYVSLY-HLD                     07573011
075740     END-IF.                                                      07574011
075750     IF DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07575011
075760        MOVE 0 TO PV-TMP-AVG-RTN-TRN-VALUE-LY                     07576011
075770     ELSE                                                         07577011
075780        COMPUTE PV-TMP-AVG-RTN-TRN-VALUE-LY ROUNDED =             07578011
075790            DN-DIST-LY-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX) /     07579011
075800            DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX)   07580011
075810     END-IF.                                                      07581011
075820     IF DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0      07582011
075830        MOVE 0 TO PV-TMP-AVG-RTN-TRN-VALUE-TY                     07583011
075840     ELSE                                                         07584011
075850        COMPUTE PV-TMP-AVG-RTN-TRN-VALUE-TY ROUNDED =             07585011
075860                DN-DIST-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX) /    07586011
075870                DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX)  07587011
075880     END-IF.                                                      07588011
075890     COMPUTE PV-TMP-AVG-RTN-TRN-VALUE-VAR ROUNDED =               07589011
075900                (PV-TMP-AVG-RTN-TRN-VALUE-TY -                    07590011
075910                PV-TMP-AVG-RTN-TRN-VALUE-LY) * -1.                07591011
075920     IF PV-TMP-AVG-RTN-TRN-VALUE-LY = 0                           07592011
075930        MOVE 0 TO PV-TMP-AVG-RTN-TRN-VAL-TYVSLY                   07593011
075940     ELSE                                                         07594011
075950        COMPUTE PV-TMP-AVG-RTNTRN-VAL-TYLY-HLD =                  07595011
075960                ((PV-TMP-AVG-RTN-TRN-VALUE-TY -                   07596011
075970                PV-TMP-AVG-RTN-TRN-VALUE-LY) /                    07597011
075980                PV-TMP-AVG-RTN-TRN-VALUE-LY) * 100                07598011
075990        COMPUTE PV-TMP-AVG-RTN-TRN-VAL-TYVSLY ROUNDED =           07599011
076000                PV-TMP-AVG-RTNTRN-VAL-TYLY-HLD                    07600011
076010     END-IF.                                                      07601011
076020     IF DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07602011
076030        MOVE 0 TO PV-TMP-AVG-RTN-ITM-VALUE-LY                     07603011
076040     ELSE                                                         07604011
076050        COMPUTE PV-TMP-AVG-RTN-ITM-VALUE-LY ROUNDED =             07605011
076060            DN-DIST-LY-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX) /     07606011
076070            DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX)   07607011
076080     END-IF.                                                      07608011
076090     IF DN-DIST-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0      07609011
076100        MOVE 0 TO PV-TMP-AVG-RTN-ITM-VALUE-TY                     07610011
076110     ELSE                                                         07611011
076120        COMPUTE PV-TMP-AVG-RTN-ITM-VALUE-TY ROUNDED =             07612011
076130                DN-DIST-RTN-AMT (DN-DISTNR-IDX, DN-DIST-IDX) /    07613011
076140                DN-DIST-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX)  07614011
076150     END-IF.                                                      07615011
076160     COMPUTE PV-TMP-AVG-RTN-ITM-VALUE-VAR ROUNDED =               07616011
076170                PV-TMP-AVG-RTN-ITM-VALUE-TY -                     07617011
076180                PV-TMP-AVG-RTN-ITM-VALUE-LY.                      07618011
076190     IF PV-TMP-AVG-RTN-ITM-VALUE-LY = 0 THEN                      07619011
076200        MOVE 0 TO PV-TMP-AVG-RTN-ITM-VAL-TYVSLY                   07620011
076210     ELSE                                                         07621011
076220        COMPUTE PV-TMP-AVG-RTNITM-VAL-TYLY-HLD =                  07622011
076230                ((PV-TMP-AVG-RTN-ITM-VALUE-TY -                   07623011
076240                PV-TMP-AVG-RTN-ITM-VALUE-LY) /                    07624011
076250                PV-TMP-AVG-RTN-ITM-VALUE-LY) * 100                07625011
076260        COMPUTE PV-TMP-AVG-RTN-ITM-VAL-TYVSLY ROUNDED =           07626011
076270                PV-TMP-AVG-RTNITM-VAL-TYLY-HLD                    07627011
076280     END-IF.                                                      07628011
076290     COMPUTE PV-TMP-RET-ITM-TOT-TY ROUNDED =                      07629011
076300                DN-DIST-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX). 07630011
076310     COMPUTE PV-TMP-RET-ITM-TOT-LY ROUNDED =                      07631011
076320           DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX).   07632011
076330     COMPUTE PV-TMP-RET-ITM-TOT-VAR ROUNDED =                     07633011
076340                PV-TMP-RET-ITM-TOT-TY - PV-TMP-RET-ITM-TOT-LY.    07634011
076350     IF DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07635011
076360        MOVE 0 TO PV-TMP-RET-ITM-TOT-TYVSLY                       07636011
076370     ELSE                                                         07637011
076380        COMPUTE PV-TMP-RET-ITM-TOT-TYVSLY-HLD =                   07638011
076390            ((DN-DIST-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX) -  07639011
076400            DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX)) /07640011
076410            DN-DIST-LY-RTN-ITM-QTY (DN-DISTNR-IDX, DN-DIST-IDX))  07641011
076420            * 100                                                 07642011
076430        COMPUTE PV-TMP-RET-ITM-TOT-TYVSLY ROUNDED =               07643011
076440                PV-TMP-RET-ITM-TOT-TYVSLY-HLD                     07644011
076450     END-IF.                                                      07645011
076460     IF DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07646011
076470        AND                                                       07647011
076480        DN-DIST-LY-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0   07648011
076490        MOVE 0 TO PV-TMP-RET-TRNS-PCNT-TOT-LY                     07649011
076500     ELSE                                                         07650011
076510        COMPUTE PV-TMP-RET-TRN-PCNT-TOT-LY-HLD =                  07651011
076520            (DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) /07652011
076530            (DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) +07653011
076540            DN-DIST-LY-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX))) 07654011
076550            * 100                                                 07655011
076560        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-LY ROUNDED =             07656011
076570                PV-TMP-RET-TRN-PCNT-TOT-LY-HLD                    07657011
076580     END-IF.                                                      07658011
076590     IF DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0 AND  07659011
076600        DN-DIST-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) = 0      07660011
076610        MOVE 0 TO PV-TMP-RET-TRNS-PCNT-TOT-TY                     07661011
076620     ELSE                                                         07662011
076630        COMPUTE PV-TMP-RET-TRN-PCNT-TOT-TY-HLD =                  07663011
076640          (DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) /     07664011
076650          (DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) +     07665011
076660          DN-DIST-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX))) * 10007666011
076670        COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-TY ROUNDED =             07667011
076680            PV-TMP-RET-TRN-PCNT-TOT-TY-HLD                        07668011
076690     END-IF.                                                      07669011
076700     COMPUTE PV-TMP-RET-TRNS-PCNT-TOT-VAR ROUNDED =               07670011
076710            PV-TMP-RET-TRNS-PCNT-TOT-TY -                         07671011
076720            PV-TMP-RET-TRNS-PCNT-TOT-LY.                          07672011
076730     COMPUTE PV-TMP-TOT-TRNS-LY ROUNDED =                         07673011
076740            DN-DIST-LY-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) + 07674011
076750            DN-DIST-LY-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX).  07675011
076760     COMPUTE PV-TMP-TOT-TRNS-TY ROUNDED =                         07676011
076770            DN-DIST-RTN-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX) +    07677011
076780            DN-DIST-SLS-TRN-QTY (DN-DISTNR-IDX, DN-DIST-IDX).     07678011
076790     IF PV-TMP-TOT-TRNS-LY = 0                                    07679011
076800        MOVE 0 TO PV-TMP-TOT-TRNS-VAR                             07680011
076810     ELSE                                                         07681011
076820        COMPUTE PV-TMP-TOT-TRNS-VAR-HLD =                         07682011
076830                ((PV-TMP-TOT-TRNS-TY - PV-TMP-TOT-TRNS-LY) /      07683011
076840                PV-TMP-TOT-TRNS-LY) * 100                         07684011
076850        COMPUTE PV-TMP-TOT-TRNS-VAR ROUNDED =                     07685011
076860                PV-TMP-TOT-TRNS-VAR-HLD                           07686011
076870     END-IF.                                                      07687011
076880                                                                  07688011
076890******************************************************************07689011
076900* 6800-PROCESS-RPT-DIST.                                          07690011
076910*    PROCESSES DISTRICT UNTIL ALL DAYS/WEEKS ARE PRINTED TO THE.  07691011
076920*    NEW STORE DISTRICT TOTALS REPORT.                            07692011
076930******************************************************************07693011
076940 6800-PROCESS-RPT-DIST.                                           07694011
076950                                                                  07695011
076960      SET PV-TEMP-INDEX TO DX-DIST-IDX                            07696011
076970      COMPUTE PV-DAYS-REMAINING =                                 07697011
076980              PV-NUM-DAYS - PV-TEMP-INDEX + 1.                    07698011
076990      IF PV-DAYS-REMAINING > 0 THEN                               07699011
077000         ADD +1 TO PV-WEEK-CNT                                    07700011
077010         PERFORM 6850-PROCESS-WEEK                                07701011
077020      ELSE                                                        07702011
077030         SET PS-DIST-NEW-COMPLETED-Y TO TRUE                      07703011
077040      END-IF.                                                     07704011
077050                                                                  07705011
077060******************************************************************07706011
077070* 6850-PROCESS-WEEK.                                              07707011
077080*     PROCESS NEW STORE DISTRICT TOTALS REPORT WEEK.              07708011
077090******************************************************************07709011
077100 6850-PROCESS-WEEK.                                               07710011
077110                                                                  07711011
077120     MOVE DX-DATE (DX-DISTNR-IDX, DX-DIST-IDX) TO PV-TEMP-DATE.   07712011
077130     MOVE PV-WEEK-CNT             TO H4-WEEKNUM                   07713011
077140                                     H4-ACCUM-WEEKNUM.            07714011
077150                                                                  07715011
077160     PERFORM 5000-CONVERT-INPUT-DATE.                             07716011
077170                                                                  07717011
077180     MOVE 0  TO  PV-WEEK-TOT-SLS-AMT                              07718011
077190                 PV-WEEK-TOT-RTN-AMT                              07719011
077200                 PV-WEEK-TOT-SLS-TRN-QTY                          07720011
077210                 PV-WEEK-TOT-RTN-TRN-QTY                          07721011
077220                 PV-WEEK-TOT-SLS-ITM-QTY                          07722011
077230                 PV-WEEK-TOT-RTN-ITM-QTY                          07723011
077240                 PV-WEEK-TOT-LY-SLS-AMT                           07724011
077250                 PV-WEEK-TOT-LY-RTN-AMT                           07725011
077260                 PV-WEEK-TOT-LY-SLS-TRN-QTY                       07726011
077270                 PV-WEEK-TOT-LY-RTN-TRN-QTY                       07727011
077280                 PV-WEEK-TOT-LY-SLS-ITM-QTY                       07728011
077290                 PV-WEEK-TOT-LY-RTN-ITM-QTY.                      07729011
077300                                                                  07730011
077310     IF PV-WEEKDAY-NUM = 1 AND PV-DAYS-REMAINING > 0              07731011
077320        MOVE PV-WEEKDAY-NUM                TO H5-DAY1             07732011
077330        MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE        07733011
077340        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY       07734011
077350        MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM         07735011
077360        MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD         07736011
077370        PERFORM 6875-MAKE-DIST-CALCULATIONS                       07737011
077380        PERFORM 8010-RPTVAL-TO-REPORT-DAY1                        07738011
077390                                                                  07739011
077400        SUBTRACT 1 FROM PV-DAYS-REMAINING                         07740011
077410        SET DX-DIST-IDX UP BY +1                                  07741011
077420        ADD +1 TO PV-WEEKDAY-NUM                                  07742011
077430     ELSE                                                         07743011
077440        IF PV-WEEKDAY-NUM > 1 AND PV-DAYS-REMAINING > 0           07744011
077450           MOVE 1                             TO H5-DAY1          07745011
077460           MOVE PV-BEGIN-CCYY-MM-DD           TO PV-TEMP-DATE     07746011
077470           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE1-CCYY    07747011
077480           MOVE PV-TEMP-DATE-MM               TO H4-DATE1-MM      07748011
077490           MOVE PV-TEMP-DATE-DD               TO H4-DATE1-DD      07749011
077500           PERFORM 8015-ZERO-TO-REPORT-DAY1                       07750011
077510        END-IF                                                    07751011
077520     END-IF.                                                      07752011
077530                                                                  07753011
077540     IF PV-WEEKDAY-NUM = 2 AND PV-DAYS-REMAINING > 0              07754011
077550        PERFORM 5050-CONVERT-INPUT-DATE                           07755011
077560        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE2-CCYY     07756011
077570        MOVE PV-TEMP-DATE-MM                 TO H4-DATE2-MM       07757011
077580        MOVE PV-TEMP-DATE-DD                 TO H4-DATE2-DD       07758011
077590        MOVE PV-WEEKDAY-NUM                  TO H5-DAY2           07759011
077600        PERFORM 6875-MAKE-DIST-CALCULATIONS                       07760011
077610        PERFORM 8020-RPTVAL-TO-REPORT-DAY2                        07761011
077620                                                                  07762011
077630        SUBTRACT 1 FROM PV-DAYS-REMAINING                         07763011
077640        SET DX-DIST-IDX UP BY +1                                  07764011
077650        ADD +1 TO PV-WEEKDAY-NUM                                  07765011
077660     ELSE                                                         07766011
077670        IF (PV-WEEKDAY-NUM = 2 AND PV-DAYS-REMAINING <= 0) OR     07767011
077680           (PV-WEEKDAY-NUM > 2 AND PV-DAYS-REMAINING > 0)         07768011
077690           MOVE 2                          TO H5-DAY2             07769011
077700           PERFORM 5050-CONVERT-INPUT-DATE                        07770011
077710           MOVE PV-TEMP-DATE-CCYY          TO H4-DATE2-CCYY       07771011
077720           MOVE PV-TEMP-DATE-MM            TO H4-DATE2-MM         07772011
077730           MOVE PV-TEMP-DATE-DD            TO H4-DATE2-DD         07773011
077740           IF PV-WEEKDAY-NUM = 2                                  07774011
077750              ADD +1 TO PV-WEEKDAY-NUM                            07775011
077760           END-IF                                                 07776011
077770           PERFORM 8025-ZERO-TO-REPORT-DAY2                       07777011
077780        END-IF                                                    07778011
077790     END-IF.                                                      07779011
077800                                                                  07780011
077810     IF PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING > 0              07781011
077820        PERFORM 5050-CONVERT-INPUT-DATE                           07782011
077830        MOVE PV-TEMP-DATE-CCYY               TO H4-DATE3-CCYY     07783011
077840        MOVE PV-TEMP-DATE-MM                 TO H4-DATE3-MM       07784011
077850        MOVE PV-TEMP-DATE-DD                 TO H4-DATE3-DD       07785011
077860        MOVE PV-WEEKDAY-NUM                  TO H5-DAY3           07786011
077870        PERFORM 6875-MAKE-DIST-CALCULATIONS                       07787011
077880        PERFORM 8030-RPTVAL-TO-REPORT-DAY3                        07788011
077890                                                                  07789011
077900        SUBTRACT 1 FROM PV-DAYS-REMAINING                         07790011
077910        SET DX-DIST-IDX UP BY +1                                  07791011
077920        ADD +1 TO PV-WEEKDAY-NUM                                  07792011
077930     ELSE                                                         07793011
077940        IF (PV-WEEKDAY-NUM = 3 AND PV-DAYS-REMAINING <= 0) OR     07794011
077950           (PV-WEEKDAY-NUM > 3 AND PV-DAYS-REMAINING > 0)         07795011
077960           MOVE 3                          TO H5-DAY3             07796011
077970           PERFORM 5050-CONVERT-INPUT-DATE                        07797011
077980           MOVE PV-TEMP-DATE-CCYY          TO H4-DATE3-CCYY       07798011
077990           MOVE PV-TEMP-DATE-MM            TO H4-DATE3-MM         07799011
078000           MOVE PV-TEMP-DATE-DD            TO H4-DATE3-DD         07800011
078010           IF PV-WEEKDAY-NUM = 3                                  07801011
078020              ADD +1 TO PV-WEEKDAY-NUM                            07802011
078030           END-IF                                                 07803011
078040           PERFORM 8035-ZERO-TO-REPORT-DAY3                       07804011
078050        END-IF                                                    07805011
078060     END-IF.                                                      07806011
078070                                                                  07807011
078080     IF PV-WEEKDAY-NUM = 4 AND PV-DAYS-REMAINING > 0              07808011
078090        PERFORM 5050-CONVERT-INPUT-DATE                           07809011
078100        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE4-CCYY       07810011
078110        MOVE PV-TEMP-DATE-MM               TO H4-DATE4-MM         07811011
078120        MOVE PV-TEMP-DATE-DD               TO H4-DATE4-DD         07812011
078130        MOVE PV-WEEKDAY-NUM                TO H5-DAY4             07813011
078140        PERFORM 6875-MAKE-DIST-CALCULATIONS                       07814011
078150        PERFORM 8040-RPTVAL-TO-REPORT-DAY4                        07815011
078160                                                                  07816011
078170        SUBTRACT 1 FROM PV-DAYS-REMAINING                         07817011
078180        SET DX-DIST-IDX UP BY +1                                  07818011
078190        ADD +1 TO PV-WEEKDAY-NUM                                  07819011
078200     ELSE                                                         07820011
078210        IF (PV-WEEKDAY-NUM = 4 AND PV-DAYS-REMAINING <= 0) OR     07821011
078220           (PV-WEEKDAY-NUM > 4 AND PV-DAYS-REMAINING > 0)         07822011
078230           MOVE 4                             TO H5-DAY4          07823011
078240           PERFORM 5050-CONVERT-INPUT-DATE                        07824011
078250           MOVE PV-TEMP-DATE-CCYY             TO H4-DATE4-CCYY    07825011
078260           MOVE PV-TEMP-DATE-MM               TO H4-DATE4-MM      07826011
078270           MOVE PV-TEMP-DATE-DD               TO H4-DATE4-DD      07827011
078280           IF PV-WEEKDAY-NUM = 4                                  07828011
078290              ADD +1 TO PV-WEEKDAY-NUM                            07829011
078300           END-IF                                                 07830011
078310           PERFORM 8045-ZERO-TO-REPORT-DAY4                       07831011
078320        END-IF                                                    07832011
078330     END-IF.                                                      07833011
078340                                                                  07834011
078350     IF PV-WEEKDAY-NUM = 5 AND PV-DAYS-REMAINING > 0              07835011
078360        PERFORM 5050-CONVERT-INPUT-DATE                           07836011
078370        MOVE PV-TEMP-DATE-CCYY             TO H4-DATE5-CCYY       07837011
078380        MOVE PV-TEMP-DATE-MM               TO H4-DATE5-MM         07838011
078390        MOVE PV-TEMP-DATE-DD               TO H4-DATE5-DD         07839011
078400        MOVE PV-WEEKDAY-NUM                TO H5-DAY5             07840011
078410        PERFORM 6875-MAKE-DIST-CALCULATIONS                       07841011
078420        PERFORM 8050-RPTVAL-TO-REPORT-DAY5                        07842011
078430                                                                  07843011
078440        SUBTRACT 1 FROM PV-DAYS-REMAINING                         07844011
