049900                PV-TMP-RET-TRNS-PCNT-TOT-TY -                     04990011
049910                PV-TMP-RET-TRNS-PCNT-TOT-LY.                      04991011
049920     COMPUTE PV-TMP-TOT-TRNS-LY ROUNDED =                         04992011
049930                CT-CORP-LY-RTN-TRN-QTY (CT-CORP-INDEX) +          04993011
049940                CT-CORP-LY-SLS-TRN-QTY (CT-CORP-INDEX).           04994011
049950     COMPUTE PV-TMP-TOT-TRNS-TY ROUNDED =                         04995011
049960                CT-CORP-RTN-TRN-QTY (CT-CORP-INDEX) +             04996011
049970                CT-CORP-SLS-TRN-QTY (CT-CORP-INDEX).              04997011
049980     IF PV-TMP-TOT-TRNS-LY = 0                                    04998011
049990        MOVE 0 TO PV-TMP-TOT-TRNS-VAR                             04999011
050000     ELSE                                                         05000011
050010        COMPUTE PV-TMP-TOT-TRNS-VAR-HLD =                         05001011
050020                ((PV-TMP-TOT-TRNS-TY - PV-TMP-TOT-TRNS-LY) /      05002011
050030                PV-TMP-TOT-TRNS-LY) * 100                         05003011
050040        COMPUTE PV-TMP-TOT-TRNS-VAR ROUNDED =                     05004011
050050                PV-TMP-TOT-TRNS-VAR-HLD                           05005011
050060     END-IF.                                                      05006011
050070                                                                  05007011
050080******************************************************************05008011
