027010* 3000-PROCESS-MAIN-CURSOR.                                       02701011
027020******************************************************************02702011
027030 3000-PROCESS-MAIN-CURSOR.                                        02703011
027040                                                                  02704011
027050     IF PS-FIRST-RUN-Y                                            02705011
027060        MOVE EPT00004-STR-NBR  TO PV-HOLD-STR-NBR                 02706011
027070        MOVE EPPART-SLS-DTE    TO PV-HOLD-SLS-DTE                 02707011
027080        SET PS-FIRST-RUN-N     TO TRUE                            02708011
027090        SET CT-CORP-INDEX      TO 1                               02709011
027100        SET CC-CORP-INDEX      TO 1                               02710011
027110        SET CN-CORP-INDEX      TO 1                               02711011
027120        SET CX-CORP-INDEX      TO 1                               02712011
027130        SET DT-DIST-IDX        TO 1                               02713011
027140        SET DC-DIST-IDX        TO 1                               02714011
027150        SET DN-DIST-IDX        TO 1                               02715011
027160        SET DX-DIST-IDX        TO 1                               02716011
027170        SET TT-STR-IDX         TO 1                               02717011
027180        SET TT-STRNR-IDX       TO 1                               02718011
027190        PERFORM 3100-GET-DIST-NBR                                 02719011
027200        PERFORM 3150-GET-OPN-CLSE-DATE                            02720011
027210        MOVE XST00002-DIST-NBR TO PV-HOLD-DISTRICT-NBR            02721011
027220        SET DT-DISTNR-IDX      TO PV-HOLD-DISTRICT-NBR            02722011
027230        SET DC-DISTNR-IDX      TO PV-HOLD-DISTRICT-NBR            02723011
027240        SET DN-DISTNR-IDX      TO PV-HOLD-DISTRICT-NBR            02724011
027250        SET DX-DISTNR-IDX      TO PV-HOLD-DISTRICT-NBR            02725011
027260        MOVE XST00001-OPN-DTE  TO PV-HOLD-OPENED-DATE             02726011
027270        MOVE XST00001-CLO-DTE  TO PV-HOLD-CLOSED-DATE             02727011
027280     END-IF.                                                      02728011
027290                                                                  02729011
027300     IF EPT00004-STR-NBR   = PV-HOLD-STR-NBR                      02730011
027310        IF EPPART-SLS-DTE  = PV-HOLD-SLS-DTE                      02731011
027320           IF EPT00004-SUM-CTG-CDE = 'RET'                        02732011
027330              MOVE EPT00004-CTG-TRN-QTY    TO PV-HOLD-RTN-TRN-QTY 02733011
027340              MOVE EPT00004-CTG-ITM-QTY    TO PV-HOLD-RTN-ITM-QTY 02734011
027350              MOVE PV-SLS-RET-AMT          TO PV-HOLD-RTN-AMT     02735011
027360           ELSE                                                   02736011
027370              IF EPT00004-SUM-CTG-CDE = 'SAL'                     02737011
027380                 MOVE EPT00004-CTG-TRN-QTY TO PV-HOLD-SLS-TRN-QTY 02738011
027390                 MOVE EPT00004-CTG-ITM-QTY TO PV-HOLD-SLS-ITM-QTY 02739011
027400                 MOVE PV-SLS-RET-AMT       TO PV-HOLD-SLS-AMT     02740011
027410              END-IF                                              02741011
027420           END-IF                                                 02742011
027430        ELSE                                                      02743011
027440           PERFORM 3100-GET-DIST-NBR                              02744011
027450           PERFORM 3150-GET-OPN-CLSE-DATE                         02745011
027460           MOVE XST00002-DIST-NBR     TO PV-HOLD-DISTRICT-NBR     02746011
027470           SET DT-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR     02747011
027480           SET DC-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR     02748011
027490           SET DN-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR     02749011
027500           SET DX-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR     02750011
027510           MOVE XST00001-OPN-DTE      TO PV-HOLD-OPENED-DATE      02751011
027520           MOVE XST00001-CLO-DTE      TO PV-HOLD-CLOSED-DATE      02752011
027530           PERFORM 3200-FILL-STORE-TABLE-CURR                     02753011
027540           PERFORM 3300-FILL-CORP-TABLE-CURR                      02754011
027550           PERFORM 3600-FILL-DIST-TABLE-CURR                      02755011
027560           PERFORM 5075-DETERMINE-COMP-NCOMP                      02756011
027570           IF PS-COMP-STORE                                       02757011
027580              PERFORM 3400-FILL-CORP-COMP-TBL-CUR                 02758011
027590              SET CC-CORP-INDEX UP BY 1                           02759011
027600              PERFORM 3500-FILL-DIST-COMP-TBL-CUR                 02760011
027610              SET DC-DIST-IDX   UP BY 1                           02761011
027620           ELSE                                                   02762011
027630              IF PS-NCOMP-STORE                                   02763011
027640                 PERFORM 3410-FILL-CORP-NCOMP-TBL-CUR             02764011
027650                 SET CN-CORP-INDEX UP BY 1                        02765011
027660                 PERFORM 3510-FILL-DIST-NCOMP-TBL-CUR             02766011
027670                 SET DN-DIST-IDX   UP BY 1                        02767011
027680              ELSE                                                02768011
027690                 IF PS-NEW-STORE                                  02769011
027700                    PERFORM 3420-FILL-CORP-NEW-TBL-CUR            02770011
027710                    SET CX-CORP-INDEX UP BY 1                     02771011
027720                    PERFORM 3520-FILL-DIST-NEW-TBL-CUR            02772011
027730                    SET DX-DIST-IDX   UP BY 1                     02773011
027740                 END-IF                                           02774011
027750              END-IF                                              02775011
027760           END-IF                                                 02776011
027770           SET CT-CORP-INDEX UP BY 1                              02777011
027780           SET DT-DIST-IDX   UP BY 1                              02778011
027790           SET TT-STR-IDX    UP BY 1                              02779011
027800           MOVE EPPART-SLS-DTE TO PV-HOLD-SLS-DTE                 02780011
027810           IF EPT00004-SUM-CTG-CDE = 'RET'                        02781011
027820              MOVE EPT00004-CTG-TRN-QTY    TO PV-HOLD-RTN-TRN-QTY 02782011
027830              MOVE EPT00004-CTG-ITM-QTY    TO PV-HOLD-RTN-ITM-QTY 02783011
027840              MOVE PV-SLS-RET-AMT          TO PV-HOLD-RTN-AMT     02784011
027850           ELSE                                                   02785011
027860              IF EPT00004-SUM-CTG-CDE = 'SAL'                     02786011
027870                 MOVE EPT00004-CTG-TRN-QTY TO PV-HOLD-SLS-TRN-QTY 02787011
027880                 MOVE EPT00004-CTG-ITM-QTY TO PV-HOLD-SLS-ITM-QTY 02788011
027890                 MOVE PV-SLS-RET-AMT       TO PV-HOLD-SLS-AMT     02789011
027900              END-IF                                              02790011
027910           END-IF                                                 02791011
027920        END-IF                                                    02792011
027930     ELSE                                                         02793011
027940        PERFORM 3100-GET-DIST-NBR                                 02794011
027950        PERFORM 3150-GET-OPN-CLSE-DATE                            02795011
027960        MOVE XST00002-DIST-NBR     TO PV-HOLD-DISTRICT-NBR        02796011
027970        SET DT-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        02797011
027980        SET DC-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        02798011
027990        SET DN-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        02799011
028000        SET DX-DISTNR-IDX          TO PV-HOLD-DISTRICT-NBR        02800011
028010        MOVE XST00001-OPN-DTE      TO PV-HOLD-OPENED-DATE         02801011
028020        MOVE XST00001-CLO-DTE      TO PV-HOLD-CLOSED-DATE         02802011
028030        PERFORM 3200-FILL-STORE-TABLE-CURR                        02803011
028040        PERFORM 3300-FILL-CORP-TABLE-CURR                         02804011
028050        PERFORM 3600-FILL-DIST-TABLE-CURR                         02805011
028060        PERFORM 5075-DETERMINE-COMP-NCOMP                         02806011
028070        IF PS-COMP-STORE                                          02807011
028080           PERFORM 3400-FILL-CORP-COMP-TBL-CUR                    02808011
028090           SET PS-FIRST-CC-STR-N     TO TRUE                      02809011
028100           SET CC-CORP-INDEX         TO 1                         02810011
028110           PERFORM 3500-FILL-DIST-COMP-TBL-CUR                    02811011
028120           SET PS-FIRST-DC-STR-N     TO TRUE                      02812011
028130           SET DC-DIST-IDX           TO 1                         02813011
028140        ELSE                                                      02814011
028150           IF PS-NCOMP-STORE                                      02815011
028160              PERFORM 3410-FILL-CORP-NCOMP-TBL-CUR                02816011
028170              SET PS-FIRST-CN-STR-N     TO TRUE                   02817011
028180              SET CN-CORP-INDEX         TO 1                      02818011
028190              PERFORM 3510-FILL-DIST-NCOMP-TBL-CUR                02819011
028200              SET PS-FIRST-DN-STR-N     TO TRUE                   02820011
028210              SET DN-DIST-IDX           TO 1                      02821011
028220           ELSE                                                   02822011
028230              IF PS-NEW-STORE                                     02823011
028240                 PERFORM 3420-FILL-CORP-NEW-TBL-CUR               02824011
028250                 SET PS-FIRST-CX-STR-N     TO TRUE                02825011
028260                 SET CX-CORP-INDEX         TO 1                   02826011
028270                 PERFORM 3520-FILL-DIST-NEW-TBL-CUR               02827011
