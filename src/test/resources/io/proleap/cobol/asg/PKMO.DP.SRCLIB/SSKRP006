039700        SET PS-FIRST-RUN-N     TO TRUE                            03970011
039710        SET CT-CORP-INDEX      TO 1                               03971011
039720        SET CC-CORP-INDEX      TO 1                               03972011
039730        SET CN-CORP-INDEX      TO 1                               03973011
039740        SET CX-CORP-INDEX      TO 1                               03974011
039750        SET DT-DIST-IDX        TO 1                               03975011
039760        SET DC-DIST-IDX        TO 1                               03976011
039770        SET DN-DIST-IDX        TO 1                               03977011
039780        SET DX-DIST-IDX        TO 1                               03978011
039790        SET TT-STR-IDX         TO 1                               03979011
