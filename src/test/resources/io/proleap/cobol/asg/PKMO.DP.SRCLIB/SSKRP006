040150           PERFORM 3225-FILL-STORE-TABLE-HIST                     04015011
040160           PERFORM 3325-FILL-CORP-TABLE-HIST                      04016011
040170           PERFORM 3625-FILL-DIST-TABLE-HIST                      04017011
040180           PERFORM 5075-DETERMINE-COMP-NCOMP                      04018011
040190           IF PS-COMP-STORE                                       04019011
040200              PERFORM 3405-FILL-CORP-COMP-TBL-HIST                04020011
040210              SET CC-CORP-INDEX UP BY 1                           04021011
040220              PERFORM 3505-FILL-DIST-COMP-TBL-HIST                04022011
040230              SET DC-DIST-IDX   UP BY 1                           04023011
040240           ELSE                                                   04024011
040250              IF PS-NCOMP-STORE                                   04025011
040260                 PERFORM 3415-FILL-CORP-NCOMP-TBL-HIST            04026011
040270                 SET CN-CORP-INDEX UP BY 1                        04027011
040280                 PERFORM 3515-FILL-DIST-NCOMP-TBL-HIST            04028011
040290                 SET DN-DIST-IDX   UP BY 1                        04029011
040300              ELSE                                                04030011
040310                 IF PS-NEW-STORE                                  04031011
040320                    PERFORM 3425-FILL-CORP-NEW-TBL-HIST           04032011
040330                    SET CX-CORP-INDEX UP BY 1                     04033011
040340                    PERFORM 3525-FILL-DIST-NEW-TBL-HIST           04034011
040350                    SET DX-DIST-IDX   UP BY 1                     04035011
040360                 END-IF                                           04036011
040370              END-IF                                              04037011
040380           END-IF                                                 04038011
040390           MOVE 0       TO  PV-HOLD-LY-SLS-AMT                    04039011
040400                            PV-HOLD-LY-RTN-AMT                    04040011
040410                            PV-HOLD-LY-SLS-TRN-QTY                04041011
040420                            PV-HOLD-LY-RTN-TRN-QTY                04042011
040430                            PV-HOLD-LY-SLS-ITM-QTY                04043011
040440                            PV-HOLD-LY-RTN-ITM-QTY                04044011
040450                                                                  04045011
040460           SET CT-CORP-INDEX UP  BY 1                             04046011
040470           SET DT-DIST-IDX   UP  BY 1                             04047011
040480           SET TT-STR-IDX    UP BY 1                              04048011
040490           MOVE EPPART-SLS-DTE TO PV-HOLD-SLS-DTE                 04049011
040500           IF EPT00004-SUM-CTG-CDE = 'RET'                        04050011
040510             MOVE EPT00004-CTG-TRN-QTY  TO PV-HOLD-LY-RTN-TRN-QTY 04051011
040520             MOVE EPT00004-CTG-ITM-QTY  TO PV-HOLD-LY-RTN-ITM-QTY 04052011
040530             MOVE PV-SLS-RET-AMT        TO PV-HOLD-LY-RTN-AMT     04053011
040540           ELSE                                                   04054011
040550             IF EPT00004-SUM-CTG-CDE = 'SAL'                      04055011
040560               MOVE EPT00004-CTG-TRN-QTY TO PV-HOLD-LY-SLS-TRN-QTY04056011
040570               MOVE EPT00004-CTG-ITM-QTY TO PV-HOLD-LY-SLS-ITM-QTY04057011
040580               MOVE PV-SLS-RET-AMT       TO PV-HOLD-LY-SLS-AMT    04058011
040590             END-IF                                               04059011
040600           END-IF                                                 04060011
040610        END-IF                                                    04061011
