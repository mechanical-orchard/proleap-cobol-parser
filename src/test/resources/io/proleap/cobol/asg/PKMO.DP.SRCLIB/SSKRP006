022240******************************************************************02224011
022250* MAIN CURSOR                                                     02225011
022260******************************************************************02226011
022270     EXEC SQL                                                     02227011
022280       DECLARE MAIN-CURSOR CURSOR FOR                             02228011
022290        SELECT SUM.STR_NBR                                        02229011
022300             , PRT.SLS_DTE                                        02230011
022310             , SUM.SUM_CTG_CDE                                    02231011
022320             , SUM(SUM.CTG_TRN_QTY)                               02232011
022330             , SUM(SUM.CTG_ITM_QTY)                               02233011
022340             , SUM(SUM.CTG_AMT)                                   02234011
022350        FROM   EPT_TRN_CTG_SUM SUM                                02235011
022360             , TEPPART         PRT                                02236011
022370        WHERE  PRT.SLS_DTE   BETWEEN :CC-BEGIN-DATE               02237011
022380                                 AND :CC-DATE                     02238011
101056           AND SUM.STR_NBR     <> 6666                            02239011
022390           AND SUM.PARTN_NBR    = PRT.PARTN_NBR                   02239111
022400           AND SUM.SUM_TYP_CDE IN ('D','L','N')                   02240011
022410           AND SUM.SUM_CTG_CDE IN ('SAL', 'RET')                  02241011
022420           AND STR_GP_CDE = 'A'                                   02242011
022430        GROUP BY SUM.STR_NBR,PRT.SLS_DTE,SUM.SUM_CTG_CDE          02243011
022440        ORDER BY SUM.STR_NBR,PRT.SLS_DTE                          02244011
022450        WITH UR                                                   02245011
022460     END-EXEC.                                                    02246011
022470                                                                  02247011
022480******************************************************************02248011
022490* MAIN CURSOR2 - READ THE OPERATIONAL CURSOR WITH LAST YEARS DATE 02249011
022500******************************************************************02250011
022510     EXEC SQL                                                     02251011
022520       DECLARE MAIN-CURSOR2 CURSOR FOR                            02252011
022530        SELECT SUM.STR_NBR                                        02253011
022540             , PRT.SLS_DTE                                        02254011
022550             , SUM.SUM_CTG_CDE                                    02255011
022560             , SUM(SUM.CTG_TRN_QTY)                               02256011
022570             , SUM(SUM.CTG_ITM_QTY)                               02257011
022580             , SUM(SUM.CTG_AMT)                                   02258011
022590        FROM   EPT_TRN_CTG_SUM SUM                                02259011
022600             , TEPPART         PRT                                02260011
022610        WHERE  PRT.SLS_DTE   BETWEEN :PV-BEGIN-LY-DATE            02261011
022620                                 AND :PV-END-LY-DATE              02262011
101056           AND SUM.STR_NBR     <> 6666                            02262111
022630           AND SUM.PARTN_NBR    = PRT.PARTN_NBR                   02263011
022640           AND SUM.SUM_TYP_CDE IN ('D','L','N')                   02264011
022650           AND SUM.SUM_CTG_CDE IN ('SAL', 'RET')                  02265011
022660           AND STR_GP_CDE = 'A'                                   02266011
022670        GROUP BY SUM.STR_NBR,PRT.SLS_DTE,SUM.SUM_CTG_CDE          02267011
022680        ORDER BY SUM.STR_NBR,PRT.SLS_DTE                          02268011
022690        WITH UR                                                   02269011
022700     END-EXEC.                                                    02270011
022710                                                                  02271011
022720******************************************************************02272011
022730 PROCEDURE DIVISION.                                              02273011
022740******************************************************************02274011
022750                                                                  02275011
022760     PERFORM 1000-INITIALIZATION.                                 02276011
022770                                                                  02277011
022780     PERFORM 2000-PROCESS-RECORDS.                                02278011
022790                                                                  02279011
022800     PERFORM 5100-WRITE-CORP-RPTS.                                02280011
022810                                                                  02281011
022820     SET TT-STRNR-IDX TO 1.                                       02282011
022830     PERFORM 5900-WRITE-STORE-RPT                                 02283011
022840        UNTIL PS-NO-MORE-STORES-Y.                                02284011
022850                                                                  02285011
022860     MOVE 0 TO PV-PAGE-CNT.                                       02286011
022870     SET DT-DISTNR-IDX TO 1.                                      02287011
022880     PERFORM 5500-WRITE-DIST-RPT                                  02288011
022890        UNTIL PS-NO-MORE-DISTRICTS-Y.                             02289011
022900                                                                  02290011
022910     MOVE 0 TO PV-PAGE-CNT.                                       02291011
022920     SET DC-DISTNR-IDX TO 1.                                      02292011
022930     PERFORM 5600-WRITE-DIST-RPT                                  02293011
022940        UNTIL PS-NO-MORE-DC-DISTRICTS-Y.                          02294011
022950                                                                  02295011
022960     MOVE 0 TO PV-PAGE-CNT.                                       02296011
022970     SET DN-DISTNR-IDX TO 1.                                      02297011
022980     PERFORM 5700-WRITE-DIST-RPT                                  02298011
022990        UNTIL PS-NO-MORE-DN-DISTRICTS-Y.                          02299011
023000                                                                  02300011
023010     MOVE 0 TO PV-PAGE-CNT.                                       02301011
023020     SET DX-DISTNR-IDX TO 1.                                      02302011
023030     PERFORM 5800-WRITE-DIST-RPT                                  02303011
023040        UNTIL PS-NO-MORE-DX-DISTRICTS-Y.                          02304011
023050                                                                  02305011
023060     PERFORM 8999-CLOSE-FILES.                                    02306011
023070                                                                  02307011
023080     STOP RUN.                                                    02308011
023090                                                                  02309011
023100******************************************************************02310011
023110*    INITIALIZATION ROUTINE. OPEN THE CONTROL CARD FILES AND THE *02311011
023120*    AND OUTPUT REPORTS. PERFORM THE ROUTINE TO OPEN THE REPORT  *02312011
023130*    CURSOR. INITIALIZE THE TOTALING FIELDS.                     *02313011
023140*                                                                 02314011
023150******************************************************************02315011
023160                                                                  02316011
023170 1000-INITIALIZATION.                                             02317011
023180                                                                  02318011
023190     OPEN  INPUT DATE-CARD-FILE.                                  02319011
023200                                                                  02320011
023210     PERFORM 1100-PROCESS-CONTROL-CARD.                           02321011
023220                                                                  02322011
023230     OPEN OUTPUT TOTAL-STORE-REPORT                               02323011
023240                 TOTAL-DISTRICT-REPORT                            02324011
023250                 TOTAL-CORP-REPORT                                02325011
023260                 COMP-DISTRICT-REPORT                             02326011
023270                 COMP-CORP-REPORT                                 02327011
023280                 NONCOMP-DISTRICT-REPORT                          02328011
023290                 NONCOMP-CORP-REPORT                              02329011
023300                 NEW-DISTRICT-REPORT                              02330011
023310                 NEW-CORP-REPORT                                  02331011
023320                 TOTAL-CORP-EMAIL                                 02332011
023330                 COMP-CORP-EMAIL.                                 02333011
023340                                                                  02334011
023350     MOVE CC-BEGIN-DATE TO PV-TEMP-DATE                           02335011
023360     PERFORM 1400-FILL-TABLE-DATES                                02336011
023370         UNTIL PV-TEMP-DATE > CC-END-DATE.                        02337011
023380                                                                  02338011
023390     PERFORM 1200-FORMAT-HEADINGS.                                02339011
023400                                                                  02340011
023410******************************************************************02341011
023420* 1100-PROCESS-CONTROL-CARD                                       02342011
023430*     READ CONTROL CARD TO GET DATE.                              02343011
023440******************************************************************02344011
023450 1100-PROCESS-CONTROL-CARD.                                       02345011
023460                                                                  02346011
023470     PERFORM 1110-READ-DATE-CARD-FILE.                            02347011
023480                                                                  02348011
023490     CLOSE DATE-CARD-FILE.                                        02349011
023500                                                                  02350011
023510     IF PS-END-OF-DATE-CARD-FILE OR                               02351011
023520         CC-DATE = SPACES                                         02352011
023530         MOVE '1100-PROCESS-CONTROL-CARD'  TO PV-PARAGRAPH-NAME   02353011
023540         MOVE 'NO DATE INFO TO PROCESS' TO PV-OPERATION-MSG-1     02354011
023550         SET AC-CONTROL-CARD-ERROR TO TRUE                        02355011
023560         PERFORM 9999-ABEND                                       02356011
023570     END-IF.                                                      02357011
023580                                                                  02358011
023590     MOVE FUNCTION CURRENT-DATE (1:8)                             02359011
023600                                   TO PV-CURR-DATE-CCYYMMDD.      02360011
023610     DISPLAY 'CONTROL DATE = ' CC-DATE                            02361011
023620     DISPLAY 'CURRENT DATE = ' PV-CURR-DATE-CCYYMMDD.             02362011
023630     MOVE ALL '-'           TO PV-CURRENT-DATE2.                  02363011
023640     MOVE PV-CURR-DATE-CCYY TO PV-CURRENT-DATE2-YY.               02364011
023650     MOVE PV-CURR-DATE-MM   TO PV-CURRENT-DATE2-MM.               02365011
023660     MOVE PV-CURR-DATE-DD   TO PV-CURRENT-DATE2-DD.               02366011
023670     DISPLAY 'PV-CURRENT-DATE2 = ' PV-CURRENT-DATE2.              02367011
023680                                                                  02368011
023690     IF CC-DATE > PV-CURRENT-DATE2                                02369011
023700         DISPLAY 'CONTROL DATE > TODAY"S DATE, USE TODAY"S DATE'  02370011
023710         MOVE PV-CURRENT-DATE2 TO CC-DATE                         02371011
023720         DISPLAY 'OVERRIDE TO ' CC-DATE                           02372011
023730     END-IF.                                                      02373011
023740                                                                  02374011
023750     PERFORM 1510-DETERMINE-WEEK-INFO.                            02375011
023760                                                                  02376011
023770******************************************************************02377011
023780* 1110-READ-DATE-CARD-FILE                                        02378011
023790*     READ DATE CONTROL CARD.                                     02379011
023800******************************************************************02380011
023810 1110-READ-DATE-CARD-FILE.                                        02381011
023820                                                                  02382011
023830     READ DATE-CARD-FILE INTO CC-DATE-CONTROL-CARD                02383011
023840       AT END                                                     02384011
023850          SET PS-END-OF-DATE-CARD-FILE TO TRUE.                   02385011
023860                                                                  02386011
023870***************************************************************** 02387011
023880* 1200-FORMAT-HEADINGS                                            02388011
023890*     - GET CURRENT DATE AND TIME FROM SYSTEM.                    02389011
023900*     - FORMAT REPORT FIELDS IN THE TOP PAGE HEADING LINE.        02390011
023910*     - FORMAT HEADING LINE 2 WITH CONVERTED POS DATE.            02391011
023920***************************************************************** 02392011
023930 1200-FORMAT-HEADINGS.                                            02393011
023940                                                                  02394011
023950     ACCEPT PV-CURR-TIME FROM TIME.                               02395011
023960                                                                  02396011
023970     MOVE PV-CURR-DATE-MM             TO DP200-RUN-MONTH.         02397011
023980     MOVE PV-CURR-DATE-DD             TO DP200-RUN-DAY.           02398011
023990     MOVE PV-CURR-DATE-CCYY           TO DP200-RUN-YEAR.          02399011
024000                                                                  02400011
024010     MOVE PV-CURR-TIME-HH             TO DP200-RUN-HOUR.          02401011
024020     MOVE PV-CURR-TIME-MM             TO DP200-RUN-MINUTE.        02402011
024030     MOVE PC-CURRENT-PROGRAM-NAME     TO DP200-PROGRAM-NAME.      02403011
024040                                                                  02404011
024050     MOVE CC-BEGIN-DATE-MM            TO H2-FROM-MM.              02405011
024060     MOVE CC-BEGIN-DATE-DD            TO H2-FROM-DD.              02406011
024070     MOVE CC-BEGIN-DATE-CCYY          TO H2-FROM-CCYY.            02407011
024080     MOVE CC-END-DATE-MM              TO H2-TO-MM.                02408011
024090     MOVE CC-END-DATE-DD              TO H2-TO-DD.                02409011
024100     MOVE CC-END-DATE-CCYY            TO H2-TO-CCYY.              02410011
024110                                                                  02411011
024120******************************************************************02412011
024130* 1400-FILL-TABLE-DATES.                                          02413011
024140*     THIS PARAGRAPH PRE-FILLS THE DATES INTO THE TABLES/ARRAYS.  02414011
024150*     THIS IS NEEDED SINCE SOME STORES ARE NOT OPEN EVERY DAY.    02415011
024160*     PRE FILLING THE DATES IS USED TO VERIFY DATES BEFORE ADDING 02416011
024170*     DATA.                                                       02417011
024180******************************************************************02418011
024190 1400-FILL-TABLE-DATES.                                           02419011
024200                                                                  02420011
024210     MOVE PV-TEMP-DATE    TO CT-DATE (CT-CORP-INDEX)              02421011
024220                             CC-CORP-DATE (CC-CORP-INDEX)         02422011
024230                             CN-CORP-DATE (CN-CORP-INDEX)         02423011
024240                             CX-CORP-DATE (CX-CORP-INDEX)         02424011
024250     PERFORM VARYING DT-DISTNR-IDX                                02425011
024260        FROM +1 BY +1                                             02426011
024270        UNTIL DT-DISTNR-IDX > PV-MAX-DISTRICTS                    02427011
024280           MOVE PV-TEMP-DATE                                      02428011
024290                          TO DT-DATE (DT-DISTNR-IDX, DT-DIST-IDX) 02429011
024300     END-PERFORM.                                                 02430011
024310                                                                  02431011
024320     PERFORM VARYING DC-DISTNR-IDX                                02432011
024330        FROM +1 BY +1                                             02433011
024340        UNTIL DC-DISTNR-IDX > PV-MAX-DISTRICTS                    02434011
024350           MOVE PV-TEMP-DATE                                      02435011
024360                          TO DC-DATE (DC-DISTNR-IDX, DC-DIST-IDX) 02436011
024370     END-PERFORM.                                                 02437011
024380                                                                  02438011
024390     PERFORM VARYING DN-DISTNR-IDX                                02439011
024400        FROM +1 BY +1                                             02440011
024410        UNTIL DN-DISTNR-IDX > PV-MAX-DISTRICTS                    02441011
024420           MOVE PV-TEMP-DATE                                      02442011
024430                          TO DN-DATE (DN-DISTNR-IDX, DN-DIST-IDX) 02443011
024440     END-PERFORM.                                                 02444011
024450                                                                  02445011
024460     PERFORM VARYING DX-DISTNR-IDX                                02446011
024470        FROM +1 BY +1                                             02447011
024480        UNTIL DX-DISTNR-IDX > PV-MAX-DISTRICTS                    02448011
024490           MOVE PV-TEMP-DATE                                      02449011
024500                          TO DX-DATE (DX-DISTNR-IDX, DX-DIST-IDX) 02450011
024510     END-PERFORM.                                                 02451011
024520                                                                  02452011
024530     PERFORM VARYING TT-STRNR-IDX                                 02453011
024540        FROM +1 BY +1                                             02454011
024550        UNTIL TT-STRNR-IDX > PV-MAX-STORES                        02455011
024560           MOVE PV-TEMP-DATE TO TT-DATE (TT-STRNR-IDX, TT-STR-IDX)02456011
024570     END-PERFORM.                                                 02457011
024580                                                                  02458011
024590     PERFORM 5050-CONVERT-INPUT-DATE.                             02459011
024600     SET CT-CORP-INDEX UP BY +1.                                  02460011
024610     SET CC-CORP-INDEX UP BY +1.                                  02461011
024620     SET CN-CORP-INDEX UP BY +1.                                  02462011
024630     SET CX-CORP-INDEX UP BY +1.                                  02463011
024640     SET DT-DIST-IDX   UP BY +1.                                  02464011
024650     SET DC-DIST-IDX   UP BY +1.                                  02465011
024660     SET DN-DIST-IDX   UP BY +1.                                  02466011
024670     SET DX-DIST-IDX   UP BY +1.                                  02467011
024680     SET TT-STR-IDX    UP BY +1.                                  02468011
024690                                                                  02469011
024700*-----------------------------------------------------------------02470011
024710*1510-DETERMINE-WEEK-INFO.                                        02471011
024720*    *** FIND BEGIN/END WEEK RANGE DATES                          02472011
024730*    *** FIND DAY OF WEEK                                         02473011
024740*-----------------------------------------------------------------02474011
024750 1510-DETERMINE-WEEK-INFO.                                        02475011
024760                                                                  02476011
024770     INITIALIZE DPG51 DPG52 DPG53 DPG54 DPG55 DPG56.              02477011
024780     SET  DPG52-LK-DTE-ISO-FMT          TO TRUE.                  02478011
024790     SET  DPG51-FISCAL-454-CALENDAR     TO TRUE.                  02479011
024800     SET  DPG51-DO-NOT-INCR-DECR-DATE   TO TRUE.                  02480011
024810     MOVE CC-DATE                    TO DPG52-DB2-ISO-DATE-FMT.   02481011
024820                                                                  02482011
024830     CALL DP500-KOHLS-CALENDAR-ROUTINE USING DPG51 DPG52 DPG53    02483011
024840                                             DPG54 DPG55 DPG56.   02484011
024850                                                                  02485011
024860     EVALUATE TRUE                                                02486011
024870        WHEN DPG54-NO-ERROR                                       02487011
024880           MOVE DPG55-NUMERIC-WEEK-DAY     TO PV-WEEKDAY-RUN-DAY  02488011
024890           MOVE DPG55-ALPHA-WEEK-DAY       TO PV-WEEK-DAY         02489011
024900           MOVE CC-DATE(9:2)               TO PV-DATE-TODAY       02490011
024910           MOVE DPG55-ALPHA-MONTH          TO PV-MONTH            02491011
024920           DISPLAY 'PV-WEEKDAY-RUN-DAY = ' PV-WEEKDAY-RUN-DAY     02492011
024930           DISPLAY 'MONTH     = ' DPG55-ALPHA-MONTH               02493011
024940           DISPLAY 'DAY       = ' CC-DATE(9:2)                    02494011
024950           DISPLAY 'DAY IN WK = ' DPG55-ALPHA-WEEK-DAY            02495011
024960           DISPLAY DPG55-1ST-WKDY-DB2ISO-FMT                      02496011
024970           DISPLAY DPG55-LAST-WKDY-DB2ISO-FMT                     02497011
024980           MOVE DPG55-1ST-WKDY-DB2ISO-FMT  TO CC-BEGIN-DATE       02498011
024990           MOVE DPG55-LAST-WKDY-DB2ISO-FMT TO CC-END-DATE         02499011
025000        WHEN OTHER                                                02500011
025010             MOVE '1510-DETERMINE-WEEK-INFO'                      02501011
025020                                           TO PV-PARAGRAPH-NAME   02502011
025030             MOVE 'ERROR GETTING WEEK IN MONTH'                   02503011
025040                                           TO PV-OPERATION-MSG-1  02504011
025050             SET AC-CALENDAR-ROUTINE-ERROR TO TRUE                02505011
025060             MOVE DPG54-ERROR-MESSAGE      TO PV-OPERATION-MSG-2  02506011
025070             PERFORM 9999-ABEND                                   02507011
025080                                                                  02508011
025090     END-EVALUATE.                                                02509011
025100                                                                  02510011
025110******************************************************************02511011
025120* 2000-PROCESS-RECORDS.                                           02512011
025130******************************************************************02513011
025140 2000-PROCESS-RECORDS.                                            02514011
025150                                                                  02515011
025160     PERFORM 2100-OPEN-MAIN-CURSOR.                               02516011
025170*    DISPLAY '## ' CC-BEGIN-DATE ' ' CC-END-DATE                  02517011
025180                                                                  02518011
025190     PERFORM 2200-FETCH-MAIN-CURSOR                               02519011
025200          UNTIL PS-END-OF-MAIN-CURSOR.                            02520011
025210                                                                  02521011
025220     PERFORM 2300-CLOSE-MAIN-CURSOR.                              02522011
025230                                                                  02523011
025240     PERFORM 3050-PROCESS-LAST-RECORD.                            02524011
025250                                                                  02525011
025260     SET PS-FIRST-RUN-Y TO TRUE.                                  02526011
025270     MOVE CC-BEGIN-DATE TO PV-BEGIN-LY-DATE.                      02527011
025280     MOVE CC-END-DATE   TO PV-END-LY-DATE.                        02528011
025290     SUBTRACT 1 FROM PV-BEGIN-LY-DATE-CCYY.                       02529011
025300     SUBTRACT 1 FROM PV-END-LY-DATE-CCYY.                         02530011
025310                                                                  02531011
025320**     PROCESS FROM THE OPERATIONAL TABLES & ADD TO HISTORY TABLE 02532011
025330     MOVE 'N' TO PS-END-OF-MAIN-CURSOR2-SW.                       02533011
025340                                                                  02534011
025350     PERFORM 2150-OPEN-MAIN-CURSOR2.                              02535011
025360                                                                  02536011
025370     PERFORM UNTIL PS-END-OF-MAIN-CURSOR2                         02537011
025380        PERFORM 2250-FETCH-MAIN-CURSOR2                           02538011
025390        IF PS-END-OF-MAIN-CURSOR2                                 02539011
025400           PERFORM 2350-CLOSE-MAIN-CURSOR2                        02540011
025410        ELSE                                                      02541011
025420           PERFORM 4000-PROCESS-LY-CURSOR                         02542011
025430        END-IF                                                    02543011
025440     END-PERFORM.                                                 02544011
025450                                                                  02545011
025460     IF PS-FIRST-RUN-N                                            02546011
025470         PERFORM 4050-PROCESS-FILL-TBL                            02547011
025480     END-IF.                                                      02548011
025490                                                                  02549011
025500******************************************************************02550011
025510* 2100-OPEN-MAIN-CURSOR.                                          02551011
025520******************************************************************02552011
025530 2100-OPEN-MAIN-CURSOR.                                           02553011
025540                                                                  02554011
025550     EXEC SQL                                                     02555011
025560          OPEN MAIN-CURSOR                                        02556011
025570     END-EXEC.                                                    02557011
025580                                                                  02558011
025590     EVALUATE TRUE                                                02559011
025600         WHEN SQLCODE = ZEROES                                    02560011
025610              CONTINUE                                            02561011
025620         WHEN SQLWARN0 NOT EQUAL SPACES                           02562011
025630         WHEN SQLCODE NOT EQUAL ZEROES                            02563011
025640             MOVE '2100-OPEN-MAIN-CURSOR'                         02564011
025650                                 TO PV-PARAGRAPH-NAME             02565011
025660             MOVE 'ERROR ON OPEN OF MAIN-CURSOR'                  02566011
025670                                 TO PV-OPERATION-MSG-1            02567011
025680             MOVE SQLCODE        TO PV-DB2-OPERATION              02568011
025690             PERFORM 9100-SQL-ABEND                               02569011
025700     END-EVALUATE.                                                02570011
