013700 PROCEDURE DIVISION.                                              01370002
013710*----------------------------------------------------------------*01371002
013900                                                                  01390002
013910*----------------------------------------------------------------*01391002
014100 0000-MAINLINE.                                                   01410002
014110*----------------------------------------------------------------*01411002
014300     DISPLAY '************************'.                          01430002
014310     DISPLAY '***     PCKUT124     ***'.                          01431002
014320     DISPLAY '************************'.                          01432002
014321     DISPLAY ' '.                                                 01432102
014330                                                                  01433002
014400     PERFORM 1000-INITIALIZE.                                     01440002
014500                                                                  01450002
014600     PERFORM 2000-PROCESS-INPUT-FILE                              01460002
014700         UNTIL PS-EOF-INPUT-FILE.                                 01470002
014800                                                                  01480002
014810                                                                  01481013
014820     DISPLAY ' '.                                                 01482013
014900     MOVE PV-READ-COUNT TO PV-DISP-CNT.                           01490004
015000     DISPLAY 'TOTAL RECORDS READ    '          PV-DISP-CNT.       01500004
015001                                                                  01500113
015010     MOVE PV-WRITE-COUNT TO PV-DISP-CNT.                          01501013
015020     DISPLAY 'TOTAL OUTPUT RECORDS  '          PV-DISP-CNT.       01502013
015030     DISPLAY ' '.                                                 01503013
015031                                                                  01503119
015032     MOVE PV-ERROR-COUNT TO PV-DISP-CNT.                          01503219
015033     DISPLAY 'TOTAL ERROR RECORDS   '          PV-DISP-CNT.       01503319
015034     DISPLAY ' '.                                                 01503419
015040     DISPLAY '************************'.                          01504013
015100                                                                  01510002
015200     CLOSE PICKMADE-EXTRACT-IN                                    01520002
015210           PICKMADE-EXTRACT-OUT                                   01521019
015220           PICKMADE-EXTRACT-ERROR.                                01522019
015300                                                                  01530002
015400     GOBACK.                                                      01540002
015500                                                                  01550002
015600                                                                  01560002
015700*----------------------------------------------------------------*01570004
015800 1000-INITIALIZE.                                                 01580002
015810*----------------------------------------------------------------*01581004
016000     SET PS-NOT-EOF-INPUT-FILE TO TRUE.                           01600002
016100                                                                  01610002
016200     OPEN INPUT  PICKMADE-EXTRACT-IN                              01620002
016210          OUTPUT PICKMADE-EXTRACT-OUT                             01621002
016220                 PICKMADE-EXTRACT-ERROR.                          01622019
016300                                                                  01630002
016400     MOVE +90 TO DK-OPER-UNT-ID.                                  01640002
016500     PERFORM 7000-SELECT-TCOCNTL.                                 01650002
016600                                                                  01660002
016700     PERFORM 7100-READ-INPUT-FILE.                                01670006
016800                                                                  01680002
016900     IF PS-EOF-INPUT-FILE                                         01690002
017000         DISPLAY '** INPUT FILE IS EMPTY **'                      01700002
017100     END-IF.                                                      01710002
017200                                                                  01720002
017300                                                                  01730002
017310*----------------------------------------------------------------*01731004
017500 2000-PROCESS-INPUT-FILE.                                         01750002
017510*----------------------------------------------------------------*01751004
017700     MOVE '2000-PROCESS-INPUT-FILE ' TO PV-ABEND-PARAGRAPH.       01770002
017800                                                                  01780002
017900     INITIALIZE RC033-FIELDS.                                     01790002
018000                                                                  01800002
018010     MOVE PC086-OPER-UNT-ID     TO RC033-OPER-UNT-ID              01801007
018020                                   PV-DISP-OPER.                  01802007
018030                                                                  01803007
018040     MOVE +1                    TO RC033-FCLTY-ID.                01804007
018100     MOVE PC086-ORD-NBR         TO RC033-PO-NBR                   01810002
018200                                   PV-DISP-PO.                    01820002
018300                                                                  01830002
018400     MOVE PC086-RCVR-SEQ-NBR    TO RC033-REC-SEQ-NBR              01840002
018500                                   PV-DISP-RCVR.                  01850004
018600                                                                  01860002
018700     MOVE PC086-ORD-LNE-NBR     TO RC033-PO-LNE-NBR               01870002
018800                                   PV-DISP-LINE.                  01880004
018900                                                                  01890002
019000     MOVE PC086-STORE-NBR       TO PV-DISP-STORE.                 01900007
019200                                                                  01920007
019500                                                                  01950002
019600     PERFORM 2100-MODULE-PROCESSING.                              01960002
019700                                                                  01970002
019701     IF  PS-ERROR                                                 01970121
019702         CONTINUE                                                 01970221
019703     ELSE                                                         01970321
019710         PERFORM 8000-WRITE-EXTRACT-REC                           01971021
019711     END-IF,                                                      01971121
019720                                                                  01972005
019800     PERFORM 7100-READ-INPUT-FILE.                                01980002
019900                                                                  01990002
019910*----------------------------------------------------------------*01991004
019920*  CALLS RCKUT061 (WORK ORDER MODULE TWICE)                       01992017
019921*  FIRST CALL CHECK IF WORK ORDER EXISTS                          01992117
019922*  SECOND CALL GETS THE ACTIVE WORK ORDER INSTRUCTION             01992217
019930*----------------------------------------------------------------*01993004
020000 2100-MODULE-PROCESSING.                                          02000002
020100                                                                  02010002
020200     SET RC033-CHECK-EXIST        TO TRUE.                        02020002
020300                                                                  02030002
020400     CALL PC-WKORIN-MODULE                                        02040002
020500        USING RC033-FIELDS.                                       02050002
020600                                                                  02060002
020700     IF  RC033-CALL-SUCCESSFUL                                    02070002
021410                                                                  02141002
021500         SET RC033-BYPASS-CHECK-EXIST TO TRUE                     02150002
021510                                                                  02151007
021600         IF  RC033-WO-EXISTS                                      02160002
021700             SET RC033-GET-WO-INST TO TRUE                        02170002
021800             CALL PC-WKORIN-MODULE    USING RC033-FIELDS          02180002
021810                                                                  02181017
021900             IF  RC033-CALL-SUCCESSFUL                            02190002
022200                 MOVE RC033-RETURN-P2L-AREA                       02220017
022201                   TO PC086-PRCG-AREA-CDE                         02220117
022800             ELSE                                                 02280002
022801                 SET PS-ERROR  TO  TRUE                           02280121
022802                 PERFORM 8010-WRITE-EXTRACT-ERROR                 02280221
022810                 MOVE 'ERROR-WO INST DOES NOT EXIST'              02281007
022900                   TO PV-ABEND-OPERATION1                         02290007
023060                 MOVE '2100-MODULE-PROCESSING - B '               02306010
023070                                      TO PV-ABEND-PARAGRAPH       02307007
023600                 PERFORM 9000-ABEND                               02360007
023700         ELSE                                                     02370002
023900                 SET PS-ERROR  TO  TRUE                           02390021
023901                 PERFORM 8010-WRITE-EXTRACT-ERROR                 02390121
023910                 MOVE 'RCKUT061 CHECK EXIST CALL ERROR'           02391021
024000                   TO PV-ABEND-OPERATION1                         02400007
024620                 MOVE '**WORK ORDER INSTRUCTION DOES NOT EXIST**' 02462023
024630                   TO PV-ABEND-OPERATION2                         02463007
024660                 MOVE '2100-MODULE-PROCESSING - C '               02466010
024670                                      TO PV-ABEND-PARAGRAPH       02467007
024671                 MOVE PV-READ-COUNT   TO  PV-DISP-CNT             02467119
024672                                                                  02467219
024673                 DISPLAY '*                          '            02467319
024674                 DISPLAY '***************************'            02467419
024675                 DISPLAY '*                          '            02467519
024676                                                                  02467619
024677                 DISPLAY ' RCVR:  ' PV-DISP-OPER                  02467719
024678                         '-'        PV-DISP-PO                    02467819
024679                         '-'        PV-DISP-RCVR                  02467919
024680                         '-'        PV-DISP-LINE                  02468019
024681                         '     STORE: ' PV-DISP-STORE             02468119
024682                         '    RECORD: ' PV-DISP-CNT               02468219
024683                 DISPLAY '  PRCS AREA:'                           02468319
024684                         '  FCSE:'  RC033-RETURN-FCSE-AREA        02468419
024685                         '  P2L:'   RC033-RETURN-P2L-AREA         02468519
024686                         '  LAST:'  RC033-RETURN-LAST-AREA        02468619
024687                         '  GP-CDE:'  RC033-RETURN-GP-CDE         02468719
024688                                                                  02468819
024689                 DISPLAY '*                          '            02468919
024692                                                                  02469219
024693                 DISPLAY 'PROGRAM:  :         '                   02469319
024694                                        PV-ABEND-PROGRAM          02469419
024695                 DISPLAY 'PARAGRAPH :         '                   02469519
024696                                        PV-ABEND-PARAGRAPH        02469619
024697                 DISPLAY '                    '                   02469719
024698                 DISPLAY 'OPERATION 1:        '                   02469819
024699                                        PV-ABEND-OPERATION1       02469919
024700                 DISPLAY 'OPERATION 2:        '                   02470019
024701                                        PV-ABEND-OPERATION2       02470119
024702                 DISPLAY 'OPERATION 3:        '                   02470219
024703                                        PV-ABEND-OPERATION3       02470319
024705                 DISPLAY '*                          '            02470519
024708                 PERFORM 2200-DISPLAY-RETURN-DATA                 02470819
024709                 DISPLAY '*                          '            02470919
024710                                                                  02471019
024720         END-IF                                                   02472019
024800     ELSE                                                         02480002
024810         SET  PS-ERROR  TO  TRUE                                  02481021
024900         PERFORM 8010-WRITE-EXTRACT-ERROR                         02490019
025180         MOVE 'RCKUT061 CHECK EXIST CALL ERROR'                   02518019
025190           TO PV-ABEND-OPERATION1                                 02519019
025191         MOVE 'RCKUT061 MODULE ERROR'                             02519119
025192           TO PV-ABEND-OPERATION2                                 02519219
025200         MOVE '2100-MODULE-PROCESSING - D '                       02520019
025300                                      TO PV-ABEND-PARAGRAPH       02530007
025310                                                                  02531019
025311         MOVE PV-READ-COUNT   TO  PV-DISP-CNT                     02531119
025320                                                                  02532019
025330         DISPLAY '*                          '                    02533019
025340         DISPLAY '***************************'                    02534022
025350         DISPLAY '*                          '                    02535022
025360                                                                  02536019
025370         DISPLAY ' RCVR:  ' PV-DISP-OPER                          02537019
025380                 '-'        PV-DISP-PO                            02538019
025390                 '-'        PV-DISP-RCVR                          02539019
025391                 '-'        PV-DISP-LINE                          02539119
025392                 '     STORE: ' PV-DISP-STORE                     02539219
