028300*-----------------------------------------------------------------02830002
028400*  SELECT COMPANY CONTROL                                         02840002
028500*-----------------------------------------------------------------02850002
028600 7000-SELECT-TCOCNTL.                                             02860002
028700                                                                  02870002
028800     EXEC SQL                                                     02880002
028900          SELECT   OPER_UNT_ID                                    02890002
029000            INTO  :COCNTL-OPER-UNT-ID                             02900002
029100            FROM TCOCNTL                                          02910002
029200           WHERE OPER_UNT_ID = :DK-OPER-UNT-ID                    02920002
029300            WITH UR                                               02930002
029400     END-EXEC.                                                    02940002
029500*                                                                 02950002
029600     EVALUATE TRUE                                                02960002
029700         WHEN SQLCODE = ZERO                                      02970002
029800              ADD +1 TO PV-TCOCNTL-CNT                            02980002
029900         WHEN SQLCODE = +100                                      02990002
030000              ADD +1 TO PV-TCOCNTL-CNT                            03000002
030100         WHEN OTHER                                               03010002
030200              DISPLAY 'OTHER SQL CODE FROM '                      03020002
030300                      '7000-SELECT-TCOCNTL'                       03030002
030400     END-EVALUATE.                                                03040002
030500                                                                  03050002
030600*-----------------------------------------------------------------03060002
030700 7100-READ-INPUT-FILE.                                            03070002
030800*-----------------------------------------------------------------03080002
030900     SET  PS-ERROR-NO  TO  TRUE.                                  03090021
030901                                                                  03090121
030902     READ PICKMADE-EXTRACT-IN                                     03090221
030910          INTO  PC086-EXPORT-RECORD-LAYOUT                        03091002
031000        AT END                                                    03100002
031100           SET PS-EOF-INPUT-FILE TO TRUE                          03110002
031200        NOT AT END                                                03120002
031300           ADD +1 TO PV-READ-COUNT                                03130004
031400           MOVE PV-READ-COUNT TO PV-DISP-REC-CNT                  03140004
031800     END-READ.                                                    03180002
031900                                                                  03190002
