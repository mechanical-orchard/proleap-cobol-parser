000100***************************************************************** 00010000
000200 IDENTIFICATION DIVISION.                                         00020000
000300***************************************************************** 00030000
000400 PROGRAM-ID.    XLKUP999.                                         00040000
000500 AUTHOR.        DAN HINTZ.                                        00050000
000600 INSTALLATION.  KOHLS.                                            00060000
000700 DATE-WRITTEN   JUN 2003.                                         00070000
000800 DATE-COMPILED.                                                   00080000
000900***************************************************************** 00090000
001000*  THIS BATCH DB2 PROGRAM LOADS XLT_PK_UPC FROM THE PK UPC ERROR  00100000
001000*  FILE.  ONLY THOSE RECORDS THAT STATE 'NOT ON DOMAIN PACK UPC'  00110000
001000*  WILL BE INSERTED INTO THE TABLE.  THIS IS BEING DONE FOR DATA  00111000
001000*  CLEANUP PURPOSES THAT CAN BE DONE WHILE THE TABLES ARE ACTIVE. 00112000
001400***************************************************************** 00113000
001500 ENVIRONMENT DIVISION.                                            00114000
001600 INPUT-OUTPUT SECTION.                                            00115000
001700                                                                  00116000
001800 FILE-CONTROL.                                                    00117000
001900                                                                  00118000
002000     SELECT XLT-PK-UPC-IN                                         00119000
002010         ASSIGN TO INP01.                                         00120000
002100                                                                  00130000
002200***************************************************************** 00140000
002300 DATA DIVISION.                                                   00150000
002400***************************************************************** 00160000
002500 FILE SECTION.                                                    00170000
002600                                                                  00180000
002700 FD  XLT-PK-UPC-IN                                                00190000
002800     RECORDING MODE IS F                                          00200000
002810     BLOCK CONTAINS 0 RECORDS                                     00210000
002900     LABEL RECORDS ARE STANDARD.                                  00220000
003000                                                                  00230000
003300 01  XLT-PK-UPC-IN-RECORD             PIC X(144).                 00240000
003700                                                                  00250000
003800                                                                  00260000
003900***************************************************************** 00270000
004000 WORKING-STORAGE SECTION.                                         00280000
004100                                                                  00290000
       01  PA-DB2-COMMIT-COUNT              PIC S9(09) VALUE +0         00300000
                                                          COMP-3.       00310000
004200 01  PS-PROGRAM-SWITCHES.                                         00320000
004300     05  PS-EOF-INPUT-SW              PIC X(01)  VALUE 'N'.       00330000
004400         88  PS-EOF-INPUT-YES                    VALUE 'Y'.       00340000
004500         88  PS-EOF-INPUT-NO                     VALUE 'N'.       00350000
004600                                                                  00360000
004700 01  PC-PROGRAM-CONSTANTS.                                        00370000
004800     05  PC-PROGRAM-NAME              PIC  X(08) VALUE 'XLKUP999'.00380000
005000                                                                  00390000
005100 01  PV-PROGRAM-VARIABLES.                                        00400000
005900     05  PV-RECORDS-READ              PIC  9(09) VALUE  0.        00410000
006000     05  PV-RECORDS-INSERTED          PIC  9(09) VALUE  0.        00420000
006000     05  PV-RECORDS-SKIP              PIC  9(09) VALUE  0.        00430000
006400     05  PV-DISP-UPC-ID               PIC  9(09) VALUE  0.        00440000
           05  PV-DISP-UPC-NBR              PIC  9(15) VALUE  0.        00450000
006700     05  PV-SQLCODE                   PIC  S9(9) VALUE +0.        00460000
006800     05  PV-DISP-SQLCODE              PIC ZZZZZZ9-.               00470000
006900                                                                  00480000
007000                                                                  00490000
007100 01  PV-ABEND-FIELDS.                                             00500000
007200     05  PV-ABEND-CODE                PIC  S9(4) VALUE +0         00510000
007300                                                 COMP SYNC.       00520000
007400     05  PV-ABEND-PROGRAM             PIC  X(08) VALUE 'XLKUP999'.00530000
007500     05  PV-ABEND-PARAGRAPH           PIC  X(50) VALUE SPACES.    00540000
007600     05  PV-ABEND-OPERATION           PIC  X(50) VALUE SPACES.    00550000
007700     05  PV-ABEND-TABLE               PIC  X(12) VALUE SPACES.    00560000
007900     05  PV-DB2-OPERATION-MESSAGE-1   PIC  X(79) VALUE SPACES.    00570000
008000     05  PV-DB2-OPERATION-MESSAGE-2   PIC  X(79) VALUE SPACES.    00580000
008100     05  PV-DB2-OPERATION-MESSAGE-3   PIC  X(79) VALUE SPACES.    00590000
008200                                                                  00600000
008300                                                                  00610000
008400***********************************************************       00620000
008500*  COPYBOOK USED FOR THE UPCEAN INPUT FILE              *         00630000
008600***********************************************************       00640000
008700     COPY XLRD031.                                                00650000
008800                                                                  00660000
008900***********************************************************       00670000
009000*  USED FOR DB2 ERROR MESSAGE BUILDING                    *       00680000
009100***********************************************************       00690000
009200     COPY DPWS004.                                                00700000
009300                                                                  00710000
009400***********************************************************       00720000
009500*  DB2 SQL COMMUNICATION AREA                             *       00730000
009600***********************************************************       00740000
009700     EXEC SQL                                                     00750000
009800          INCLUDE SQLCA                                           00760000
009900     END-EXEC.                                                    00770000
010000                                                                  00780000
010100***********************************************************       00790000
010200*  XLT_PK_UPC TABLE                                       *       00800000
010300***********************************************************       00810000
010400     EXEC SQL                                                     00820000
010500          INCLUDE XLT00009                                        00830000
010600     END-EXEC.                                                    00840000
010700                                                                  00850000
010800***************************************************************** 00860000
010900 PROCEDURE DIVISION.                                              00870000
011000***************************************************************** 00880000
011100     DISPLAY '**** BEGINNING XLKUP999 ****'.                      00890000
011200     DISPLAY ' '.                                                 00900000
011300                                                                  00910000
011400     OPEN INPUT  XLT-PK-UPC-IN.                                   00920000
011500                                                                  00930000
011600     PERFORM 1000-READ-INPUT-FILE.                                00940000
011700                                                                  00950000
012100     PERFORM 2000-PROCESS-INPUT-FILE                              00960000
012200       UNTIL PS-EOF-INPUT-YES.                                    00970000
012300                                                                  00980000
012400     CLOSE XLT-PK-UPC-IN.                                         00990000
012500                                                                  01000000
012510     PERFORM 3000-TABLE-COMMIT.                                   01010000
012520                                                                  01020000
012600     DISPLAY '  INPUT UPC RECS READ      = ' PV-RECORDS-READ.     01030000
012700     DISPLAY '  XLT_PK_UPC ROWS INSERTED = ' PV-RECORDS-INSERTED. 01040000
012700     DISPLAY '  XLT_PK_UPC ROWS SKIPPED  = ' PV-RECORDS-SKIP.     01050000
012800     DISPLAY ' '.                                                 01060000
012900     DISPLAY '****  END OF  XLKUP999  ****'.                      01070000
013000                                                                  01080000
013100     STOP RUN.                                                    01090000
013200                                                                  01100000
013300                                                                  01110000
013400***************************************************************** 01120000
013500*  READ A PACKHEAD EXTRACT RECORD THAT WILL BE USED TO INSERT    *01130000
013510*  A ROW OF DATA INTO THE XLT_PK TABLE.                        *  01140000
013600***************************************************************** 01150000
013700 1000-READ-INPUT-FILE.                                            01160000
013800                                                                  01170000
013900     MOVE '1000-READ-INPUT-FILE    ' TO PV-ABEND-PARAGRAPH.       01180000
014000                                                                  01190000
           INITIALIZE PA-DB2-COMMIT-COUNT.                              01200000
014020                                                                  01210000
014100     READ XLT-PK-UPC-IN                                           01220000
014200          INTO XL031-PACK-UPC-DATA                                01230000
014300            AT END                                                01240000
014400               SET PS-EOF-INPUT-YES TO TRUE                       01250000
014500                                                                  01260000
014600            NOT AT END                                            01270000
014700               SET PS-EOF-INPUT-NO  TO TRUE                       01280000
014800               ADD +1 TO PV-RECORDS-READ                          01290000
014900     END-READ.                                                    01300000
015000                                                                  01310000
015100                                                                  01320000
015200***************************************************************** 01330000
015300*  LOAD THE INFORMATION FROM THE UPCEAN EXTRACT FILE INTO THE *   01340000
015310*  HOST VARIABLES AND PERFORM A PARAGRAPH TO INSERT THE NEW ROW * 01350000
015320*  INTO THE XLT_PK_UPC TABLE.                                   * 01360000
015400***************************************************************** 01370000
015500 2000-PROCESS-INPUT-FILE.                                         01380000
015600                                                                  01390000
015700     MOVE '2000-PROCESS-INPUT-FILE ' TO PV-ABEND-PARAGRAPH.       01400000
015800                                                                  01410000
016361     MOVE XL031-UPC-PK-SKU-NBR       TO XLT00009-PK-SKU-NBR       01420000
016362     MOVE XL031-UPC-UPC-NBR          TO XLT00009-UPC-NBR          01430000
016363     MOVE XL031-UPC-EDI-ORD-IND      TO XLT00009-EDI-ORD-IND      01440000
016380                                                                  01450000
016400     IF  XL031-UPC-ERROR-MESSAGE = 'NOT ON DOMAIN PACK UPC'       01460000
016400         PERFORM 2100-INSERT-XLT-PK-UPC                           01470000
016400     END-IF.                                                      01480000
017700                                                                  01490000
017800     PERFORM 1000-READ-INPUT-FILE.                                01500000
018500                                                                  01510000
018600                                                                  01520000
021600***************************************************************** 01530000
021700*  INSERT A ROW TO THE XLT_PK_UPC TABLE                        *  01540000
021800***************************************************************** 01550000
021900 2100-INSERT-XLT-PK-UPC.                                          01560000
022000                                                                  01570000
022100     EXEC SQL                                                     01580000
022200          INSERT                                                  01590000
022300          INTO  XLT_PK_UPC                                        01600000
023093              ( PK_SKU_NBR                                        01610000
023094               ,UPC_NBR                                           01620000
023095               ,EDI_ORD_IND                                       01630002
                     ,LAST_UPD_TMST)                                    01631002
023200          VALUES                                                  01640000
023993             (  :XLT00009-PK-SKU-NBR                              01650000
023994               ,:XLT00009-UPC-NBR                                 01660000
023995               ,:XLT00009-EDI-ORD-IND                             01670002
                     ,CURRENT TIMESTAMP)                                01671002
024010     END-EXEC.                                                    01680000
024100                                                                  01690000
024200     EVALUATE TRUE                                                01700000
024300         WHEN SQLCODE = ZERO                                      01710000
024400              ADD +1                 TO PV-RECORDS-INSERTED       01720000
                    ADD +1                 TO PA-DB2-COMMIT-COUNT       01730000
                    IF PA-DB2-COMMIT-COUNT > 4500                       01740000
                        PERFORM 3000-TABLE-COMMIT                       01750000
                        MOVE 0 TO PA-DB2-COMMIT-COUNT                   01760000
                    END-IF                                              01770000
               WHEN SQLCODE = -803                                      01780000
               WHEN SQLCODE = -530                                      01790000
024400              ADD +1                 TO PV-RECORDS-SKIP           01800000
024500         WHEN SQLCODE NOT = +0                                    01810000
024600              MOVE '2100-INSERT-XLT-PK-UPC'                       01820000
024700                                     TO PV-ABEND-PARAGRAPH        01830000
024800              MOVE 'INSERT XLT_PK_UPC'                            01840000
024810                                     TO PV-ABEND-OPERATION        01850000
025500              STRING 'SKU NBR: '          DELIMITED BY SIZE       01860000
025600                     XL031-UPC-PK-SKU-NBR DELIMITED BY SIZE       01870000
026700                                   INTO PV-DB2-OPERATION-MESSAGE-101880000
026800                                                                  01890000
026900              MOVE 'XLT_PK_UPC'      TO PV-ABEND-TABLE            01900000
027000              PERFORM Z999-SQL-ABEND                              01910000
027100     END-EVALUATE.                                                01920000
027200                                                                  01930000
027300                                                                  01940000
027400***************************************************************** 01950000
027500* COMMIT THE PENDING TABLE CHANGES.                             * 01960000
027600***************************************************************** 01970000
027700 3000-TABLE-COMMIT.                                               01980000
027800                                                                  01990000
027900     EXEC SQL                                                     02000000
028000          COMMIT                                                  02010000
028100     END-EXEC.                                                    02020000
028200                                                                  02030000
028300                                                                  02040000
028400***************************************************************** 02050000
028500*  DISPLAY DB2 ERROR INFORMATION, ROLLBACK ANY PENDING CHANGES, * 02060000
028510*  AND ABEND THE PROGRAM.                                       * 02070000
028600***************************************************************** 02080000
028700 Z999-SQL-ABEND.                                                  02090000
028800                                                                  02100000
028900     MOVE SQLCODE            TO PV-SQLCODE.                       02110000
029000     MOVE PV-SQLCODE         TO PV-DISP-SQLCODE.                  02120000
029100     DISPLAY '*** DB2 ERROR '.                                    02130000
029200     DISPLAY '*** SQLCODE   = ' PV-DISP-SQLCODE.                  02140000
029300     DISPLAY '*** PROGRAM   = ' PV-ABEND-PROGRAM.                 02150000
029400     DISPLAY '*** PARAGRAPH = ' PV-ABEND-PARAGRAPH.               02160000
029500     DISPLAY '*** OPERATION = ' PV-ABEND-OPERATION.               02170000
029600     DISPLAY '*** MESSAGE   = ' PV-DB2-OPERATION-MESSAGE-1.       02180000
029700     DISPLAY '***           = ' PV-DB2-OPERATION-MESSAGE-2.       02190000
029800     DISPLAY '***           = ' PV-DB2-OPERATION-MESSAGE-3.       02200000
029900     DISPLAY '*** TABLE 1   = ' PV-ABEND-TABLE.                   02210000
030000                                                                  02220000
030100     COPY DPPD004.                                                02230000
030200                                                                  02240000
030300     MOVE +4013         TO PV-ABEND-CODE.                         02250000
030400     CALL 'ILBOABN0' USING PV-ABEND-CODE.                         02260000
