024861******************************************************************02484600
024862* USE THE LOGISTICS DOMAIN MODULE (XLKUT001) TO FETCH SKU AND    *02484700
024863* UPC INFORMATION.                                               *02484800
024864******************************************************************02484900
024870 4200-SELECT-SKU-UPC-INFO.                                        02485000
024894     INITIALIZE XL001-ITEM-COMMAREA.                              02486000
024895                                                                  02487000
024896     SET XL001-BY-INTR-UPC-ID                                     02488000
024897         XL001-SELECT-SKU                                         02489000
024898         XL001-SELECT-VND-STYL                                    02489100
024899         XL001-SELECT-SKU-STR       TO TRUE.                      02489200
024900                                                                  02489300
024901     MOVE PCT00014-INTR-UPC-ID      TO XL001-KEY-INTR-UPC-ID      02489400
024902                                       PV-DISPLAY-INTR-UPC-ID.    02489500
024903                                                                  02489600
024904     CALL XL001-VSM-PGM USING XL001-ITEM-COMMAREA.                02489700
024905                                                                  02489800
024906     EVALUATE TRUE                                                02489900
024907        WHEN XL001-SUCCESSFUL                                     02490000
024908            CONTINUE                                              02490100
024909        WHEN XL001-SQL-ERROR                                      02490200
024910            EVALUATE TRUE                                         02490300
024911               WHEN XL001-SKU-SQLCODE NOT = 0                     02490400
024912                   IF  XL001-SKU-NOT-FOUND                        02490500
024913                       MOVE ZERO     TO XL001-SKU-NBR             02490600
024914                       MOVE SPACES   TO XL001-SKU-DESC            02490700
024915                                        XL001-SIZE-DESC           02490800
024916                                        XL001-SHRT-CLOR-DESC      02490900
024919                       DISPLAY 'SKU NOT FOUND FOR '               02491000
024920                               PV-DISPLAY-INTR-UPC-ID             02492000
024921                   ELSE                                           02492100
024922                       MOVE '4200-SELECT-SKU-UPC-INFO'            02492200
024923                                     TO PV-PARAGRAPH-NAME         02492300
024924                       MOVE 'READ SKU UPC INFO -- XLT_SKU'        02492400
024925                                     TO PV-DB2-OPERATION-ATTEMPTED02492500
024928                       MOVE PV-DISPLAY-INTR-UPC-ID                02492600
024929                                     TO PV-FIELD-VALUE            02492700
024930                       MOVE XL001-SQLCA                           02492800
024931                                     TO SQLCA                     02492900
024932                       PERFORM 9200-SQL-ABEND                     02493000
024933                   END-IF                                         02493100
024934               WHEN XL001-VND-STYL-SQLCODE NOT = 0                02493200
024935                   IF  XL001-VND-STYL-NOT-FOUND                   02493300
024936                       MOVE ZERO     TO XL001-DEPT-NBR            02493400
024937                       MOVE SPACES   TO XL001-VS-NBR              02493500
024940                       DISPLAY 'VND STYL NOT FOUND FOR '          02493600
024941                                PV-DISPLAY-INTR-UPC-ID            02493700
024942                   ELSE                                           02493800
024943                       MOVE '4200-SELECT-SKU-UPC-INFO'            02493900
024944                                     TO PV-PARAGRAPH-NAME         02494000
024945                       MOVE 'READ SKU UPC INFO -- XLT_VND_STYL'   02494100
024946                                     TO PV-DB2-OPERATION-ATTEMPTED02494200
024949                       MOVE PV-DISPLAY-INTR-UPC-ID                02494300
024950                                     TO PV-FIELD-VALUE            02494400
024951                       MOVE XL001-SQLCA                           02494500
024952                                     TO SQLCA                     02494600
024953                       PERFORM 9200-SQL-ABEND                     02494700
024954                   END-IF                                         02494800
024955               WHEN XL001-SKU-STR-SQLCODE NOT = 0                 02494900
024956                   IF  XL001-SKU-STR-NOT-FOUND                    02495000
024957                       MOVE ZERO     TO XL001-UNT-RTL-AMT         02495100
024960                       DISPLAY 'SKU STR NOT FOUND FOR '           02495200
024961                                PV-DISPLAY-INTR-UPC-ID            02495300
024962                   ELSE                                           02495400
024963                       MOVE '4200-SELECT-SKU-UPC-INFO'            02495500
024964                                     TO PV-PARAGRAPH-NAME         02495600
024965                       MOVE 'READ SKU UPC INFO -- XLV_SKU_STR'    02495700
024966                                     TO PV-DB2-OPERATION-ATTEMPTED02495800
024969                       MOVE PV-DISPLAY-INTR-UPC-ID                02495900
024970                                     TO PV-FIELD-VALUE            02496000
024971                       MOVE XL001-SQLCA                           02497000
024972                                     TO SQLCA                     02497100
024973                       PERFORM 9200-SQL-ABEND                     02497200
024974                   END-IF                                         02497300
024975            END-EVALUATE                                          02497400
024976        WHEN XL001-INVALID-ACCESS                                 02497500
024977            MOVE '4200-SELECT-SKU-UPC-INFO'                       02497600
024978                                     TO PV-PARAGRAPH-NAME         02497700
024979            MOVE 'READ SKU UPC INFO' TO PV-DB2-OPERATION-ATTEMPTED02497800
024981            MOVE PV-DISPLAY-INTR-UPC-ID                           02497900
024982                                     TO PV-FIELD-VALUE            02498000
024983            MOVE XL001-SQLCA         TO SQLCA                     02498100
024984            PERFORM 9200-SQL-ABEND                                02498200
024985        WHEN OTHER                                                02498300
024986            DISPLAY '** ABEND **'                                 02498400
024987            DISPLAY '** PROGRAM = WSKUT035 **'                    02498500
024988            DISPLAY '** PARAGRAPH = 4200-SELECT-SKU-UPC-INFO'     02498600
024989            DISPLAY '** ERROR WITH XLKUT001'                      02498700
024990            DISPLAY '** RETURN CODE = ' XL001-RETURN-CODE         02498800
024991            DISPLAY '** ATTEMPT TO READ SKU UPC INFO FOR ITEM '   02498900
024992                     PV-DISPLAY-INTR-UPC-ID                       02499000
024993            PERFORM 9300-NON-SQL-ABEND                            02499100
024994     END-EVALUATE.                                                02499200
024995                                                                  02499300
025049******************************************************************02499400
025050*  OPEN THE SKU_CSR                                               02499500
025100******************************************************************02499600
025200 5000-OPEN-SKU-CSR.                                               02499700
025300                                                                  02499800
025400     EXEC SQL                                                     02499900
025500          OPEN SKU_CSR                                            02500000
025600     END-EXEC.                                                    02510000
025700                                                                  02520000
025800     EVALUATE TRUE                                                02530000
025900         WHEN SQLCODE = ZERO                                      02540000
026000              CONTINUE                                            02550000
