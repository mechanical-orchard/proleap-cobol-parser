012300******************************************************************01000000
012400*  CURSOR USED TO RETRIEVE ITEMS AND QUANTITY FOR WEB LOCATIONS.  01010000
      * CLT 38972 REMOVED HARDCODED WHERE CLAUSE OF '873'               01020000
Q25069* Q25069  INCLUDE LOC NBR IN WHERE CLAUSE                         01030000
012500******************************************************************01040000
012700     EXEC SQL                                                     01050000
012800          DECLARE SKU_CSR CURSOR FOR                              01060000
012900           SELECT A.ITEM_NMBR                                     01070000
013000                , A.ONHAND_QTY                                    01080000
013100                , A.LOC_NBR                                       01090000
013200             FROM TSKST A                                         01100000
013400            WHERE A.ONHAND_QTY <> 0                               01110000
Q25069              AND A.LOC_NBR = :PV-LOC-NBR                         01120000
013700              WITH UR                                             01130000
013800     END-EXEC.                                                    01140000
013900                                                                  01150000
013910******************************************************************01160000
013920*  CURSOR USED TO RETRIEVE STORE EXPECTED UNITS FOR THE WEB       01170000
013921*  LOCATION                                                       01180000
Q25069* Q25069  INCLUDE LOC NBR IN WHERE CLAUSE                         01190000
013930******************************************************************01200000
013944     EXEC SQL                                                     01210000
013950          DECLARE STORE_EXP_CSR CURSOR FOR                        01220000
013952         SELECT  A.INTR_UPC_ID                                    01230000
013953              ,  A.LOC_NBR                                        01240000
013954              ,  A.TRN_CLSN_QTY                                   01250000
013955           FROM  PCT_TRN_CLSN_BAL  A                              01260000
013956              ,  XLT_SKU  B                                       01270000
013959          WHERE  A.INTR_UPC_ID   = B.INTR_UPC_ID                  01280000
013961            AND  A.TRN_CLSN_CDE  = 'SE'                           01290000
Q25069            AND A.LOC_NBR = :PV-LOC-NBR                           01300000
013963            AND  A.TRN_CLSN_QTY <> 0                              01310000
013970           WITH  UR                                               01320000
013994     END-EXEC.                                                    01330000
013995                                                                  01340000
