046100******************************************************************04610004
046200 PROCEDURE DIVISION.                                              04620004
046300******************************************************************04630004
046400                                                                  04640004
046500******************************************************************04650004
046600*  CONTROLS THE MAIN PROCESSING OF THE PROGRAM.                   04660004
046700******************************************************************04670004
046800 0000-PROGRAM-MAINLINE.                                           04680004
046810     DISPLAY '**'.                                                04681099
046820     DISPLAY '** PROGRAM:  CNKUP908'.                             04682099
046830     DISPLAY '**'.                                                04683099
046840                                                                  04684099
046900     OPEN INPUT SHIFT-FILE-IN.                                    04690099
047000*                                                                 04700004
047010     PERFORM 5000-READ-INPUT.                                     04701099
047020     PERFORM 2000-PROCESS-SHIFT                                   04702099
047030         UNTIL PS-END-OF-INPUT-YES                                04703099
047040*                                                                 04704099
047050     PERFORM 9000-DISPLAY-COUNTS                                  04705099
047500*                                                                 04750004
047510     CLOSE SHIFT-FILE-IN.                                         04751099
047520*                                                                 04752064
047600     STOP RUN.                                                    04760004
047700*                                                                 04770004
049620******************************************************************04962004
049700 2000-PROCESS-SHIFT.                                              04970099
058600*                                                                 05860004
058700     PERFORM 7100-SELECT-SHIFT                                    05870099
058800     IF PS-SHIFT-FOUND-NO                                         05880099
059200         PERFORM 7000-INSERT-SHIFT                                05920099
059300     ELSE                                                         05930099
059310         MOVE PCT00004-OPER-UNT-ID TO PV-OPER-UNT-ID              05931099
059311         MOVE PCT00004-SHFT-NBR    TO PV-SHIFT-NBR                05931199
059320         DISPLAY '*** SHIFT ALREADY ON PCT_SHFT: '                05932099
059330                  PV-OPER-UNT-ID ' ' PV-SHIFT-NBR                 05933099
059400     END-IF.                                                      05940099
059800*                                                                 05980004
061910     PERFORM 5000-READ-INPUT.                                     06191067
062000*                                                                 06200004
062001 5000-READ-INPUT.                                                 06200166
062010     READ SHIFT-FILE-IN                                           06201099
062021          INTO DCLPCT00004                                        06202199
062030          AT END                                                  06203065
062040              SET PS-END-OF-INPUT-YES TO TRUE                     06204073
062050          NOT AT END                                              06205067
062060              ADD +1 TO PV-INPUT-CNT.                             06206067
111900*                                                                 11190004
112000******************************************************************11200004
112100*  INSERT COMPANY CONTROL ROW                                     11210099
112200******************************************************************11220004
112300 7000-INSERT-SHIFT.                                               11230099
112301*                                                                 11230168
112302     EXEC SQL                                                     11230269
112310     INSERT INTO PCT_SHFT                                         11231099
112320               (OPER_UNT_ID                                       11232099
112330               ,SHFT_NBR                                          11233099
112340               ,SHFT_BEG_TM                                       11234099
112350               ,SHFT_END_TM)                                      11235099
112460       VALUES (:PCT00004-OPER-UNT-ID                              11246099
112470              ,:PCT00004-SHFT-NBR                                 11247099
112480              ,:PCT00004-SHFT-BEG-TM                              11248099
112490              ,:PCT00004-SHFT-END-TM)                             11249099
112721     END-EXEC.                                                    11272169
112730*                                                                 11273004
112800     EVALUATE TRUE                                                11280004
112900         WHEN SQLCODE = ZERO                                      11290004
112910              ADD +1 TO PV-SHIFT-CNT                              11291099
112920              PERFORM 8000-COMMIT                                 11292099
113100         WHEN OTHER                                               11310004
113101              PERFORM 9000-DISPLAY-COUNTS                         11310199
113102              MOVE PCT00004-OPER-UNT-ID   TO PV-OPER-UNT-ID       11310299
113103              MOVE PCT00004-SHFT-NBR      TO PV-SHIFT-NBR         11310399
113104              DISPLAY '*** OPER_UNT_ID = ' PV-OPER-UNT-ID         11310499
113105                                       ' ' PV-SHIFT-NBR           11310599
113200              MOVE '7000-INSERT-SHIFT'                            11320099
113300                                 TO PV-ABEND-PARAGRAPH            11330022
113400              MOVE 'INSERT'      TO PV-DB2-OPERATION-ATTEMPTED    11340068
113500              PERFORM 9200-SQL-ABEND                              11350004
113600     END-EVALUATE.                                                11360004
143400*                                                                 14340004
143410******************************************************************14341099
143420*  SELECT COMPANY CONTROL                                         14342099
143430******************************************************************14343099
143440 7100-SELECT-SHIFT.                                               14344099
143450                                                                  14345099
143460     EXEC SQL                                                     14346099
143470         SELECT COUNT(*)                                          14347099
143551         INTO  :PV-COUNT                                          14355199
143614          FROM PCT_SHFT                                           14361499
143615         WHERE OPER_UNT_ID = :PCT00004-OPER-UNT-ID                14361599
143616           AND SHFT_NBR    = :PCT00004-SHFT-NBR                   14361699
143617         WITH UR                                                  14361799
143618     END-EXEC.                                                    14361899
143619*                                                                 14361999
143620     EVALUATE TRUE                                                14362099
143621         WHEN SQLCODE = ZERO                                      14362199
143622              IF PV-COUNT > ZERO                                  14362299
143623                  SET PS-SHIFT-FOUND-YES                          14362399
143624                                TO TRUE                           14362499
143625              ELSE                                                14362599
143626                  SET PS-SHIFT-FOUND-NO                           14362699
143627                                TO TRUE                           14362799
143628              END-IF                                              14362899
143629         WHEN OTHER                                               14362999
143630              MOVE PCT00004-OPER-UNT-ID   TO PV-OPER-UNT-ID       14363099
143631              MOVE PCT00004-SHFT-NBR      TO PV-SHIFT-NBR         14363199
143640              DISPLAY '*** ' PV-OPER-UNT-ID                       14364099
143641                         ' ' PV-SHIFT-NBR                         14364199
143644              MOVE '7000-SELECT-SHIFT'                            14364499
143645                                 TO PV-ABEND-PARAGRAPH            14364599
143646              MOVE 'SELECT'      TO PV-DB2-OPERATION-ATTEMPTED    14364699
143647              PERFORM 9200-SQL-ABEND                              14364799
143648     END-EVALUATE.                                                14364899
143649 8000-COMMIT.                                                     14364998
143650     EXEC SQL                                                     14365098
143651          COMMIT                                                  14365198
143652     END-EXEC                                                     14365298
143653     IF SQLCODE = ZERO                                            14365399
143654         CONTINUE                                                 14365499
143655     ELSE                                                         14365598
143656         PERFORM 9000-DISPLAY-COUNTS                              14365699
143660         MOVE PCT00004-OPER-UNT-ID                                14366099
143700                           TO PV-OPER-UNT-ID                      14370099
143710         MOVE PCT00004-SHFT-NBR                                   14371099
143720                           TO PV-SHIFT-NBR                        14372099
143800         DISPLAY PV-OPER-UNT-ID ' ' PV-SHIFT-NBR                  14380099
144300         MOVE '8000-COMMIT' TO PV-ABEND-PARAGRAPH                 14430099
144500         MOVE 'COMMIT'      TO PV-DB2-OPERATION-ATTEMPTED         14450098
144600         PERFORM 9200-SQL-ABEND                                   14460098
144610     END-IF.                                                      14461099
144700******************************************************************14470004
