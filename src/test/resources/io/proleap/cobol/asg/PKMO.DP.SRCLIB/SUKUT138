025000******************************************************************02500002
025100 PROCEDURE DIVISION.                                              02510002
025200******************************************************************02520002
025300******************************************************************02530002
025400*  0000-MAINLINE - CONTROLS THE MAIN PROCESSING OF THE PROGRAM.  *02540002
025500******************************************************************02550002
025600 0000-PROGRAM-MAINLINE.                                           02560002
025700                                                                  02570002
025710     OPEN INPUT  KOHLS-INPUT-FILE                                 02571002
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02571124
025720*                DEMATIC-INPUT-FILE                               02572024
025720                 SRTDRCT-INPUT-FILE                               02572124
003300* JAK WR39275 - ADD INPUT CONTROL FILE                            02572224
025720                 SURD085-CONTROL-FILE                             02572324
025730          OUTPUT ERRORS-FILE.                                     02573002
025740                                                                  02574002
003300* JAK WR39275 - ADD INPUT CONTROL FILE                            02575024
026000     PERFORM 3000-READ-CONTROL-FILE.                              02575125
025740                                                                  02576024
026000     PERFORM 1000-PROCESS-INPUT                                   02600003
026010        THRU 1000-PROCESS-INPUT-EXIT                              02601003
026100            UNTIL ( PS-INP01-EOF ) AND ( PS-INP02-EOF ).          02610002
026200                                                                  02620002
026210     PERFORM 2000-WRITE-TOTALS                                    02621003
026211        THRU 2000-WRITE-TOTALS-EXIT.                              02621103
026220                                                                  02622003
026232     IF ( PA-INP01-WRITTEN    > +0 OR                             02623223
026233          PA-INP02-WRITTEN    > +0 OR                             02623322
026234          PA-KEYMATCH-WRITTEN > +0 OR                             02623422
026235          PA-INVALID-WRITTEN  > +0 )                              02623522
026240         MOVE 0001 TO RETURN-CODE                                 02624008
026250     END-IF.                                                      02625008
026260                                                                  02626008
026270     DISPLAY 'RETURN CODE = ' RETURN-CODE.                        02627022
026280                                                                  02628022
026300     CLOSE KOHLS-INPUT-FILE                                       02630003
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02630124
026310*          DEMATIC-INPUT-FILE                                     02631024
026310           SRTDRCT-INPUT-FILE                                     02631124
003300* JAK WR39275 - ADD INPUT CONTROL FILE                            02631224
025720           SURD085-CONTROL-FILE                                   02631324
026320           ERRORS-FILE.                                           02632003
026330                                                                  02633003
026400     STOP RUN.                                                    02640002
026500/                                                                 02650002
028900******************************************************************02890002
029000* 1000-PROCESS-INPUT-FILES                                       *02900002
029100******************************************************************02910002
029200 1000-PROCESS-INPUT.                                              02920004
029300                                                                  02930002
029400     IF ( PS-FIRST-TIME )                                         02940002
029500         MOVE 'N' TO PS-FIRST-TIME-SW                             02950002
029600         PERFORM 6000-READ-KOHLS-INPUT                            02960003
029610            THRU 6000-READ-KOHLS-INPUT-EXIT                       02961003
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02962024
029700*        PERFORM 6010-READ-DEMATIC-INPUT                          02970024
029701*           THRU 6010-READ-DEMATIC-INPUT-EXIT                     02970124
029700         PERFORM 6010-READ-SRTDRCT-INPUT                          02970224
029701            THRU 6010-READ-SRTDRCT-INPUT-EXIT                     02970324
029801     END-IF.                                                      02980103
029802                                                                  02980203
029810     IF ( SU050-KEY-FIELDS  > SU05D-KEY-FIELDS )                  02981003
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02981124
029812*        IF ( SU05D-DEMATIC-INVALID-REC )                         02981224
029812         IF ( SU05D-SRTDRCT-INVALID-REC )                         02981324
029813             ADD +1   TO PA-INVALID-WRITTEN                       02981424
029814         ELSE                                                     02981524
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02981624
029820*            SET SU05D-DEMATIC-NO-MATCH TO TRUE                   02982024
029820             SET SU05D-SRTDRCT-NO-MATCH TO TRUE                   02982124
029822             ADD +1   TO PA-INP02-WRITTEN                         02982213
029823         END-IF                                                   02982313
029824         WRITE ERRORS-RECORD FROM SU05D-DIRECTIV-TRACKING-DATA    02982413
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02982524
029830*        PERFORM 6010-READ-DEMATIC-INPUT                          02983024
029831*           THRU 6010-READ-DEMATIC-INPUT-EXIT                     02983124
029830         PERFORM 6010-READ-SRTDRCT-INPUT                          02983224
029831            THRU 6010-READ-SRTDRCT-INPUT-EXIT                     02983324
029832         GO TO 1000-PROCESS-INPUT-EXIT                            02983417
029850     END-IF.                                                      02985003
029851                                                                  02985103
029852     IF ( SU050-KEY-FIELDS  < SU05D-KEY-FIELDS )                  02985203
029854         SET SU050-KOHLS-NO-MATCH TO TRUE                         02985421
029855         WRITE ERRORS-RECORD FROM SU050-DIRECTIV-TRACKING-DATA    02985503
029856         ADD +1   TO PA-INP01-WRITTEN                             02985603
029857         PERFORM 6000-READ-KOHLS-INPUT                            02985703
029858            THRU 6000-READ-KOHLS-INPUT-EXIT                       02985803
029859         GO TO 1000-PROCESS-INPUT-EXIT                            02985917
029860     END-IF.                                                      02986003
029861                                                                  02986103
029862     IF ( PS-INP01-EOF ) AND                                      02986203
029863        ( PS-INP02-EOF )                                          02986317
029864         GO TO 1000-PROCESS-INPUT-EXIT                            02986403
029865     END-IF.                                                      02986503
029866                                                                  02986603
029869     IF ( SU050-COMPARE-FIELDS NOT = SU05D-COMPARE-FIELDS )       02986903
029870         SET SU050-KOHLS-DATA-DIFF   TO TRUE                      02987014
029871         WRITE ERRORS-RECORD FROM SU050-DIRECTIV-TRACKING-DATA    02987103
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02987224
029872*        SET SU05D-DEMATIC-DATA-DIFF TO TRUE                      02987324
029872         SET SU05D-SRTDRCT-DATA-DIFF TO TRUE                      02987424
029873         WRITE ERRORS-RECORD FROM SU05D-DIRECTIV-TRACKING-DATA    02987524
029874         ADD +1   TO PA-KEYMATCH-WRITTEN                          02987624
029875     END-IF.                                                      02987724
029876                                                                  02987824
029877     PERFORM 6000-READ-KOHLS-INPUT                                02987924
029878        THRU 6000-READ-KOHLS-INPUT-EXIT.                          02988024
003300* JAK WR39275 - REPLACE DEMATIC WITH SORT DIRECTOR                02988124
029879*    PERFORM 6010-READ-DEMATIC-INPUT                              02988224
029880*       THRU 6010-READ-DEMATIC-INPUT-EXIT.                        02988324
029879     PERFORM 6010-READ-SRTDRCT-INPUT                              02988424
029880        THRU 6010-READ-SRTDRCT-INPUT-EXIT.                        02988524
029881                                                                  02988624
029882 1000-PROCESS-INPUT-EXIT.                                         02988724
029890     EXIT.                                                        02989003
