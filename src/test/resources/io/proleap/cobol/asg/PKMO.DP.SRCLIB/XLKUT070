018200                                                                  01820000
018300* COMPARE TO SEARCH FOR MODIFICATIONS                             01830051
018400     IF XI001-EMPLOYEE-USER-ID = XL045-USR-ID                     01840077
018500         IF XI001-LAST-NAME =  XL045-USR-LAST-NM                  01850077
018600           AND XI001-FIRST-NAME   = XL045-USR-FRST-NM             01860077
018700           AND XI001-MIDDLE-INIT  = XL045-USR-MID-INIT            01870077
018800           AND XI001-USR-RVKE-IND = XL045-USR-RVKE-IND            01880077
018900           AND XI001-DFLT-PRTR-ID = XL045-DFLT-PRTR-ID            01890083
019000           AND XI001-EMP-ID       = XL045-EMP-ID                  01900077
019100             ADD  1  TO PA-TOT-MATCH-NO-CHG                       01910052
019200             PERFORM 8000-READ-EXT-FILE                           01920052
019300             PERFORM 8010-READ-USR-FILE                           01930052
019400         ELSE                                                     01940000
019500             INITIALIZE XL038-TRANS-REC                           01950051
019600             ADD  1  TO PA-TOT-MATCH-WITH-CHG                     01960052
019700             SET XL038-MODIFY                                     01970051
019800                 XL038-USR                                        01980051
019900                 XL038-USR-UPD  TO TRUE                           01990051
020000             MOVE XI001-EMPLOYEE-USER-ID   TO XL038-U-USR-ID      02000077
020100             MOVE XI001-LAST-NAME          TO XL038-U-LAST-NM     02010077
020200             MOVE XI001-FIRST-NAME         TO XL038-U-FRST-NM     02020077
020300             MOVE XI001-MIDDLE-INIT        TO XL038-U-MID-INIT    02030077
020400             MOVE XI001-USR-RVKE-IND       TO XL038-U-RVKE-IND    02040077
020500             MOVE XI001-DFLT-PRTR-ID       TO XL038-U-DFLT-PRTR-ID02050083
020600             MOVE XI001-EMP-ID             TO XL038-U-EMP-ID      02060077
020700                                                                  02070000
