016400*                                                                 RCK00185
016500*  CURSOR FOR ALL DISTRIBUTION HEADER ROWS WITH A VERIFIED DATE   RCK00185
016600*  OLDER THAN A SPECIFIED NUMBER OF DAYS.                         RCK00185
016700*                                                                 RCK00185
016800                                                                  RCK00185
016801     EXEC SQL                                                     RCK00185
016802       DECLARE DISTRB_HDR_CURSOR CURSOR FOR                       RCK00121
016803         SELECT                                                   RCL*01*8
016804               ORD_NBR                                                  85
016805              ,RCVR_SEQ_NBR                                             85
016806              ,ORD_LNE_NBR                                              85
016807              ,WRK_ORD_SEQ_NBR                                          85
016808              ,OPER_UNT_ID                                              85
016809              ,DISTRB_PRC_AREA_ID                                       85
016810              ,PRCG_STAT_CDE                                            85
016811              ,CRE_TMST                                                 85
016812              ,RSLTS_RCVD_TMST                                          85
016813              ,VRFD_TMST                                                85
016814              ,PRC_AREA_MODU_ID                                         85
016815              ,CRE_USR_ID                                                 
016816              ,TOT_CART_CNT                                               
016817              ,CART_DWNLD_DTE                                             
016818              ,CART_DWNLD_TM                                              
016819              ,CART_DWNLD_USR_ID                                          
016820              ,FCSE_CART_CNT                                              
016821              ,FCSE_LBL_TYP_CDE                                           
016822              ,P2L_CART_CNT                                               
016823              ,INPK_CART_CNT                                              
016824              ,INPK_UNT_QTY                                               
016825              ,INPK_CNVYB_IND                                             
016826              ,OTPK_UNT_QTY                                               
016827              ,SYS_CRE_CDE                                                
016828              ,SPC_INSTR_DESC                                             
016829              ,FCSE_CNFRMN_TMST                                           
016830              ,FCSE_CNFRMN_USR_ID                                         
016831              ,FCSE_CNFRMN_PGM_ID                                         
                    ,ACTV_IND                                                   
                    ,DLT_REQ_SRC_ID                                             
                    ,DLT_REQ_TMST                                               
                    ,DLT_RSLT_CDE                                               
                    ,P2L_SCN_CART_CNT                                           
                    ,CRE_LBL_OPT_CDE                                            
                    ,FCSE_FCLTY_AREA_ID                                         
                    ,ODD_CART_UNT_QTY                                           
                    ,PREPK_LNE_PRC_CDE                                          
                    ,UCC128_DAT_USG_CDE                                         
                    ,CAL_UNT_PRCD_QTY                                           
                    ,PHY_LBL_PRC_CNT                                            
                    ,VRTL_LBL_PRC_CNT                                           
016832          FROM TDISTHD                                                  *3
016833         WHERE ORD_NBR      = :RECEIV-ORD-NBR                           *5
016840           AND RCVR_SEQ_NBR = :RECEIV-REC-SEQ-NBR                       *5
016900     END-EXEC.                                                          85
022300*                                                                 RCK00185
022424*  CURSOR FOR ALL DISTRIBUTION DETAIL ROWS FOR A SPECIFIED LINE   RCK00185
022430*  OF A RECEIVER.                                                 RCK00185
022440*                                                                 RCK00185
022450                                                                  RCK00185
022460     EXEC SQL                                                     RCK00185
022470       DECLARE DISTRB_DTL_CURSOR CURSOR FOR                             21
022480         SELECT                                                    CL*  *8
022491               ORD_NBR                                                    
022492              ,RCVR_SEQ_NBR                                               
022493              ,ORD_LNE_NBR                                                
022494              ,WRK_ORD_SEQ_NBR                                            
022495              ,STR_NBR                                                    
022496              ,EXPTED_ALLOC_QTY                                           
022497              ,ACTL_ALLOC_QTY                                             
022500              ,RCVG_PSTD_STAT_CDE                                         
022501              ,P2L_SPLT_UNT_QTY                                           
022502              ,FCSE_SPLT_UNT_QTY                                          
022503              ,INPK_SPLT_UNT_QTY                                          
022504              ,FCSE_ACTL_UNT_QTY                                          
                    ,ODD_CART_IND                                               
                    ,PREPK_ODD_UNT_QTY                                          
022505          FROM TDISTDT                                                  *3
022506         WHERE ORD_NBR      = :DISTHD-ORD-NBR                           *5
022507           AND RCVR_SEQ_NBR = :DISTHD-RCVR-SEQ-NBR                      *5
022508           AND ORD_LNE_NBR  = :DISTHD-ORD-LNE-NBR                       *5
022509     END-EXEC.                                                    RCK00185
