085600******************************************************************08500079
085700 PROCEDURE DIVISION.                                              08510079
085800******************************************************************08520079
085900*                                                                 08530079
086000*----------------------------------------------------------------*08540079
086100* 0000-PROGRAM-MAINLINE - CONTROLS THE MAIN PROCESSING OF THE     08550079
086200*      PROGRAM.                                                   08560079
086300*----------------------------------------------------------------*08570079
086400 0000-PROGRAM-MAINLINE.                                           08580079
086500                                                                  08590079
086600     PERFORM 1000-INITIALIZE.                                     08600079
086700*                                                                 08610079
086800     PERFORM 2000-PROCESS-REPORT-DATA                             08620079
086900       UNTIL PS-EOF-ERROR-FILE.                                   08630079
087000     PERFORM 9000-EOJ-ROUTINE.                                    08640079
087100*                                                                 08650079
087200     STOP RUN.                                                    08660079
087300*                                                                 08670079
087400*0000-EXIT.                                                       08680079
087500*                                                                 08690079
087600******************************************************************08700079
087700*  DO FIRST TIME PROGRAM INITIALIZATION TASKS.                    08710079
087800*    -- OPEN FILES                                                08720079
087900*    -- SET UP AND PRINT REPORT HEADINGS                          08730079
088000******************************************************************08740079
088100 1000-INITIALIZE.                                                 08750079
088200                                                                  08760079
088300     OPEN INPUT  ERROR-FILE                                       08770079
088400          OUTPUT ERROR-REPORT-FILE.                               08780079
088500*                                                                 08790079
088600     ACCEPT SD-SYSTEM-DATE FROM DATE.                             08800079
088700     ACCEPT ST-SYSTEM-TIME FROM TIME.                             08810079
088800     MOVE SD-SYSTEM-DATE-YY      TO DP132O-RUN-YEAR.              08820079
088900     MOVE SD-SYSTEM-DATE-MM      TO DP132O-RUN-MONTH.             08830079
089000     MOVE SD-SYSTEM-DATE-DD      TO DP132O-RUN-DAY.               08840079
089100     MOVE ST-SYSTEM-HOUR         TO DP132O-RUN-HOUR.              08850079
089200     MOVE ST-SYSTEM-MINUTE       TO DP132O-RUN-MINUTE.            08860079
089300     MOVE PC-CURRENT-PROGRAM-NAME                                 08870079
089400                                 TO DP132O-PROGRAM-NAME.          08880079
089500     MOVE 1                      TO DP132O-REPORT-NUMBER.         08890079
090600*                                                                 09000079
090700     PERFORM 7000-READ-ERROR-FILE UNTIL                           09010079
090800             PS-EOF-ERROR-FILE OR                                 09020079
090900            (TR211-RPT-TYP-DETAIL AND                             09030079
091000             NOT TR211-HDR-REC-TYP-CDE-HDR)                       09040079
091100                                                                  09050079
091200     IF NOT PS-EOF-ERROR-FILE                                     09060079
091300          MOVE TR211-CONTNR-ID    TO PV-CONTAINER-NBR             09070079
091400                                     DL01-CONTAINER-NBR           09080079
091401          MOVE TR211-ACTL-DPRTR-DTE-MOM   TO PV-ACTL-DPRTR-DTE-MOM        
091402          MOVE TR211-ACTL-DPRTR-DTE-MOM (1:4)  TO                         
091403                  DL01-ATD-DTE-CCYY                                       
091404          MOVE TR211-ACTL-DPRTR-DTE-MOM (5:2)  TO                         
091405                  DL01-ATD-DTE-MM                                         
091410          MOVE TR211-ACTL-DPRTR-DTE-MOM (7:2)  TO                         
091500                  DL01-ATD-DTE-DD                                         
091700        IF TR211-ERROR-TYPE NOT = 'EMT' AND                       09110079
091800          TR211-ERROR-TYPE NOT = 'IEA'                            09120079
091900            PERFORM  8000-WRITE-PAGE-HEADINGS                     09130079
092000        END-IF                                                    09140079
092100     ELSE                                                         09150083
092101          PERFORM  8000-WRITE-PAGE-HEADINGS                       09150183
092102     END-IF.                                                      09151083
092200                                                                  09160079
092300*                                                                 09170079
092400     IF TR211-RPT-TYP-DETAIL                                      09180079
092500        PERFORM 2025-BUILD-ERROR-RPT-DATA                         09190079
092600     END-IF.                                                      09200079
092700*                                                                 09210079
092800     IF PS-EOF-ERROR-FILE OR                                      09220079
092900        TR211-ERROR-TYPE = 'EMT' OR                               09230079
093000        TR211-ERROR-TYPE = 'IEA'                                  09240079
093100        PERFORM 9100-EOJ-ROUTINE                                  09250079
093200        STOP RUN                                                  09260079
093300     END-IF.                                                      09270079
093400*                                                                 09280079
093500*1000-EXIT.                                                       09290079
093600*                                                                 09300079
093700* **************************************************************  09310079
093800*  PROCESSING FOR ERROR RECORDS.                                  09320079
093900******************************************************************09330079
094000 2000-PROCESS-REPORT-DATA.                                        09340079
094100                                                                  09350079
094200     MOVE SPACES TO  TR211-RPT-TYP.                               09360079
094300                                                                  09370079
094400     PERFORM 7000-READ-ERROR-FILE UNTIL                           09380079
094500             PS-EOF-ERROR-FILE OR                                 09390079
094600            (TR211-RPT-TYP-DETAIL AND                             09400079
094700             NOT TR211-HDR-REC-TYP-CDE-HDR)                       09410079
094800                                                                  09420079
094900     IF TR211-RPT-TYP-DETAIL                                      09430079
095000        PERFORM 2025-BUILD-ERROR-RPT-DATA                         09440079
095100     END-IF.                                                      09450079
095200*                                                                 09460079
095300******************************************************************09470079
095400*  BUILD THE LINES FOR THE REPORT.                                09480079
095500******************************************************************09490079
095600 2025-BUILD-ERROR-RPT-DATA.                                       09500079
                                                                                
           IF TR211-CNCART-REC-TYP-CD-CNCART                                    
              CONTINUE                                                          
           ELSE                                                                 
              IF PV-SSCC-CNT > 0                                                
                 PERFORM 8800-WRITE-CNCART-RECORD                               
              END-IF                                                            
              SET PS-CNCART-HL-WRITEN-NO TO TRUE                                
           END-IF.                                                              
095700                                                                  09510079
095701     EVALUATE TRUE                                                        
095702                                                                          
095703         WHEN TR211-CVYA-REC-TYP-CDE-CVYA                                 
095704         WHEN TR211-HDR-REC-TYP-CDE-HDR                                   
095705             PERFORM 3100-MOVE-CVYA-TO-DETAIL                             
095706                                                                          
095707         WHEN TR211-CVYB-REC-TYP-CDE-CVYB                                 
095708             PERFORM 3150-MOVE-CVYB-TO-DETAIL                             
095709                                                                          
095710         WHEN TR211-CONA-REC-TYP-CDE-CONA                                 
095711             PERFORM 3200-MOVE-CONA-TO-DETAIL                             
095712                                                                          
095713         WHEN TR211-CONB-REC-TYP-CDE-CONB                                 
095714             PERFORM 3250-MOVE-CONB-TO-DETAIL                             
095736                                                                          
