001100*----------------------------------------------------------------*00110079
001200*  THIS PROGRAM CREATES A DETAIL REPORT OF DATA THAT WAS          00120079
001300*  LOADED TO THE DB2 TABLES IN PDKUP001.                          00130079
001400*----------------------------------------------------------------*00140079
001500                                                                  00150079
001600******************************************************************00160079
001700 ENVIRONMENT DIVISION.                                            00170079
001800******************************************************************00180079
001900                                                                  00190079
002000 CONFIGURATION SECTION.                                           00200079
002100                                                                  00210079
002200 SOURCE-COMPUTER.        IBM-3090.                                00220079
002300 OBJECT-COMPUTER.        IBM-3090.                                00230079
002400                                                                  00240079
002500 INPUT-OUTPUT SECTION.                                            00250079
002600                                                                  00260079
002700 FILE-CONTROL.                                                    00270079
002800                                                                  00280079
002900     SELECT ERROR-FILE                                            00290079
003000         ASSIGN TO INP01.                                         00300079
003100                                                                  00310079
003200     SELECT ERROR-REPORT-FILE                                     00320079
003300         ASSIGN TO RPT01.                                         00330079
003400                                                                  00340079
003500******************************************************************00350079
003600 DATA DIVISION.                                                   00360079
003700******************************************************************00370079
003800                                                                  00380079
003900 FILE SECTION.                                                    00390079
004000                                                                  00400079
004100 FD  ERROR-FILE                                                   00410079
004200     RECORDING MODE IS F                                          00420079
004300     LABEL RECORDS ARE STANDARD                                   00430079
004400     BLOCK CONTAINS 0 RECORDS.                                    00440079
004500                                                                  00450079
004600 01  ERROR-RECORD                     PIC  X(508).                00460079
004700                                                                  00470079
004800                                                                  00480079
004900 FD  ERROR-REPORT-FILE                                            00490079
005000     RECORDING MODE IS F                                          00500079
005100     LABEL RECORDS ARE STANDARD                                   00510079
005200     BLOCK CONTAINS 0 RECORDS.                                    00520079
005300                                                                  00530079
005400 01  ERROR-REPORT-RECORD              PIC  X(132).                00540079
005500*                                                                 00550079
005600*                                                                 00560079
005700*----------------------------------------------------------------*00570079
005800 WORKING-STORAGE SECTION.                                         00580079
005900*----------------------------------------------------------------*00590079
006000*                                                                 00600079
006100*----------------------------------------------------------------*00610079
006200*   PURCHASE ORDER HEADER TABLE.                                  00620079
006300*----------------------------------------------------------------*00630079
006400                                                                  00640079
006500     EXEC SQL                                                     00650079
006600         INCLUDE TPURCHS                                          00660079
006700     END-EXEC.                                                    00670079
006800                                                                  00680079
006900*----------------------------------------------------------------*00690079
007000*   VENDOR NAME HEADER TABLE.                                     00700079
007100*----------------------------------------------------------------*00710079
007200                                                                  00720079
007300     EXEC SQL                                                     00730079
007400         INCLUDE TENTNME                                          00740079
007500     END-EXEC.                                                    00750079
007600                                                                  00760079
