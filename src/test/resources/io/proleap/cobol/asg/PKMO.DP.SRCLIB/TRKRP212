159110******************************************************************10560079
159120* SELECT VENDOR NAME FROM TPURCHS.                                10570079
159130******************************************************************10580079
159140                                                                  10590079
159150 7050-SELECT-VENDOR-NAME.                                         10600079
159160                                                                  10610079
159170     MOVE TR211-CONPOA-PO-NBR  TO  PURCHS-PO-NBR.                 10620079
159180     MOVE PC-A                 TO  PURCHS-VER-STATUS-CDE.         10630079
159190                                                                  10640079
159191     EXEC SQL                                                     10650079
159192           SELECT DISTINCT                                        10660079
159193                  A.ENT_NAME_DESC                                 10670079
159194           INTO   :ENTNME-ENT-NAME-DESC                           10680079
159195           FROM   TENTNME A,                                      10690079
159196                  TPURCHS B                                       10700079
159197           WHERE  B.PO_NBR       = :PURCHS-PO-NBR                 10710079
159198           AND    A.ENT_ID       = B.ENT_ID                       10720079
159199           AND    VER_STATUS_CDE = :PURCHS-VER-STATUS-CDE         10730079
159200           AND    A.TYPE_CDE    = :PC-01                          10740079
159201                                                                  10750079
159202     END-EXEC.                                                    10760079
159203                                                                  10770079
159204     EVALUATE TRUE                                                10780079
159205         WHEN SQLCODE = 0                                         10790079
159206             MOVE ENTNME-ENT-NAME-DESC   TO DL06-VENDOR-NAME      10800079
159207         WHEN SQLCODE = +100                                      10810079
159208             MOVE SPACES                 TO DL06-VENDOR-NAME      10820079
159209         WHEN SQLCODE = -904                                      10830079
159210         WHEN SQLCODE = -911                                      10840079
159211             MOVE SPACES                 TO DL06-VENDOR-NAME      10850079
159212             CONTINUE                                             10860079
159213     END-EVALUATE.                                                10870079
159214                                                                  10880079
