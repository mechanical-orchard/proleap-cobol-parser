023700                                                                  02370000
023800                                                                  02380007
023900*---------------------------------------------------------------* 02390000
024000*    DEFINE CURSOR FOR TMIITEM :                                * 02400000
024100*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02410000
024200*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02420000
024300*---------------------------------------------------------------* 02430000
024400     EXEC SQL                                                     02440000
024500          DECLARE TABLE-CURSOR CURSOR FOR                         02450000
024600           SELECT PAYT_REQ_SEQ_NBR                                02460000
024700                 ,PAYT_REQ_LNE_NBR                                02470000
024800                 ,UNT_COST_SEL_CDE                                02480000
024900                 ,PO_NBR                                          02490000
025000                 ,VER_NBR                                         02500000
025100                 ,PO_LNE_NBR                                      02510000
025200********** FROM   TKMA235.TMIITEM                                 02520012
025300           FROM   TMIITEM                                         02530000
025400          WHERE  (PAYT_REQ_SEQ_NBR = :PV-PAYT-REQ-SEQ-NBR-COMP)   02540000
025500*      ORDER BY                                                   02550000
025600            FOR  FETCH ONLY                                       02560000
025700           WITH  UR                                               02570000
025800     END-EXEC.                                                    02580000
025900                                                                  02590000
026000                                                                  02600000
026100 PROCEDURE DIVISION.                                              02610000
026200********************                                              02620000
026300                                                                  02630000
026400 0000-PROGRAM-MAINLINE.                                           02640000
026500                                                                  02650000
026600     PERFORM 1000-INITIALIZE.                                     02660000
026700                                                                  02670000
026800     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02680000
026900     PERFORM 2000-PROCESS-DRIVER-FILE                             02690000
027000         UNTIL PS-IN-DRIVER-EOF.                                  02700000
027100                                                                  02710000
027200     PERFORM 6000-EOJ-ROUTINE.                                    02720000
027300     STOP RUN.                                                    02730000
027400                                                                  02740000
027500                                                                  02750000
027600******************************************************************02760012
027700*   INITIALIZATIONS                                              *02770012
027800******************************************************************02780012
027900 1000-INITIALIZE.                                                 02790000
028000                                                                  02800000
028100     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02810000
028200     DISPLAY DM-DISPLAY-MESSAGE.                                  02820000
028300                                                                  02830000
028400     MOVE PC-BEGINNING            TO DP-STATUS-DESCRIPTION.       02840001
028500     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02850000
028600                                                                  02860000
028700     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02870000
028800     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02880000
028900                                                                  02890000
029000                                                                  02900000
029100******************************************************************02910012
029200*   1) GET NEXT RECORD FROM DRIVER FILE, AND                     *02920012
029300*      1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR          *02930012
029400*      1B- OPEN TABLE CURSOR                                     *02940012
029500*      1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL                 *02950012
029600*          ROWS FOR THIS INPUT DRIVER RECORD                     *02960012
029700*      2) FOR EACH ROW,                                          *02970012
029800*         2A- INCREMENT COUNTS                                   *02980012
029900*         2B- WRITE EXTRACT RECORD                               *02990012
030000*      1D- CLOSE TABLE CURSOR                                    *03000012
030100******************************************************************03010012
030200 2000-PROCESS-DRIVER-FILE.                                        03020000
030300                                                                  03030000
030400     PERFORM 9050-READ-IN-DRIVER-FILE.                            03040000
030500     IF PS-IN-DRIVER-EOF                                          03050000
030600         CONTINUE                                                 03060000
030700     ELSE                                                         03070000
030800         MOVE AH001-B-PAYT-REQ-SEQ-NBR TO                         03080010
030900                                      PV-PAYT-REQ-SEQ-NBR-COMP    03090000
031000                                      PV-PAYT-REQ-SEQ-NBR-NUM     03100000
031100         PERFORM 2100-CHECK-PGM-PROGRESS                          03110000
031200         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     03120000
031300         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     03130000
031400         PERFORM 9100-OPEN-TABLE-CURSOR                           03140000
031500         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     03150000
031600             UNTIL PS-TABLE-LOOP-DONE                             03160000
031700         PERFORM 9190-CLOSE-TABLE-CURSOR                          03170000
031800     END-IF.                                                      03180000
031900                                                                  03190000
032000                                                                  03200000
032100******************************************************************03210012
032200*      EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT       *03220012
032300*      RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST     *03230012
032400*      WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.     *03240012
032500******************************************************************03250012
032600 2100-CHECK-PGM-PROGRESS.                                         03260000
032700     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03270000
032800         BY        PC-HOW-OFTEN-TO-DISPLAY                        03280001
032900         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03290001
033000         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03300001
033100                                                                  03310000
033200     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03320000
033300         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03330000
033400         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03340000
033500     END-IF.                                                      03350000
033600                                                                  03360000
033700                                                                  03370000
033800******************************************************************03380012
033900*   EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD       *03390012
034000******************************************************************03400012
034100 3000-EXTRACT-ROWS-FROM-TABLE.                                    03410000
034200     INITIALIZE DCLTMIITEM.                                       03420000
034300     PERFORM 9150-FETCH-TABLE-CURSOR.                             03430000
034400     IF PS-TABLE-LOOP-DONE                                        03440000
034500         CONTINUE                                                 03450000
034600     ELSE                                                         03460000
034700         PERFORM 3010-POPULATE-EXTRACT-RECORD                     03470007
034800         PERFORM 9750-WRITE-EXTRACT-RECORD                        03480007
034900     END-IF.                                                      03490000
035000                                                                  03500000
035100                                                                  03510000
035200******************************************************************03520012
035300*   MOVE COLUMNS FROM TABLE TO FIELDS IN EXTRACT FILE RECORD.    *03530012
035400******************************************************************03540012
035500 3010-POPULATE-EXTRACT-RECORD.                                    03550007
035600                                                                  03560007
035700     INITIALIZE AH005-TMIITEM.                                    03570010
035800                                                                  03580007
035900     MOVE MIITEM-PAYT-REQ-SEQ-NBR   TO  AH005-PAYT-REQ-SEQ-NBR.   03590010
036000     MOVE MIITEM-PAYT-REQ-LNE-NBR   TO  AH005-PAYT-REQ-LNE-NBR.   03600010
036100     MOVE MIITEM-UNT-COST-SEL-CDE   TO  AH005-UNT-COST-SEL-CDE.   03610010
036200     MOVE MIITEM-PO-NBR             TO  AH005-PO-NBR.             03620010
036300                                                                  03630007
036400*    IF PV-NULL-IND-VER-NBR = PC-NOT-NULL                         03640008
036500*        MOVE MIITEM-VER-NBR        TO  AH005-VER-NBR             03650010
036600*        SET AH005-VER-NBR-NOTNULL TO TRUE                        03660010
036700*    ELSE                                                         03670008
036800*        SET AH005-VER-NBR-IS-NULL TO TRUE                        03680010
036900*    END-IF.                                                      03690008
037000*                                                                 03700008
037100*    IF PV-NULL-IND-PO-LNE-NBR = PC-NOT-NULL                      03710008
037200*        MOVE MIITEM-PO-LNE-NBR        TO  AH005-PO-LNE-NBR       03720010
037300*        SET AH005-PO-LNE-NBR-NOTNULL TO TRUE                     03730010
037400*    ELSE                                                         03740008
037500*        SET AH005-PO-LNE-NBR-IS-NULL TO TRUE                     03750010
037600*    END-IF.                                                      03760008
037700                                                                  03770007
037800     IF PV-NULL-IND-VER-NBR = PC-NULL                             03780008
037900         SET AH005-VER-NBR-IS-NULL TO TRUE                        03790010
038000     ELSE                                                         03800008
038100         MOVE MIITEM-VER-NBR       TO  AH005-VER-NBR              03810010
038200         IF PV-NULL-IND-VER-NBR = PC-NOT-NULL                     03820009
038300             SET AH005-VER-NBR-NOTNULL TO TRUE                    03830010
038400         ELSE                                                     03840009
038500             SET AH005-VER-NBR-UNKNOWN TO TRUE                    03850010
038600         END-IF                                                   03860009
038700     END-IF.                                                      03870008
038800                                                                  03880008
038900     IF PV-NULL-IND-PO-LNE-NBR = PC-NULL                          03890008
039000         SET AH005-PO-LNE-NBR-IS-NULL TO TRUE                     03900010
039100     ELSE                                                         03910008
039200         MOVE MIITEM-PO-LNE-NBR       TO  AH005-PO-LNE-NBR        03920010
039300         IF PV-NULL-IND-PO-LNE-NBR = PC-NOT-NULL                  03930009
039400             SET AH005-PO-LNE-NBR-NOTNULL TO TRUE                 03940010
039500         ELSE                                                     03950009
039600             SET AH005-PO-LNE-NBR-UNKNOWN TO TRUE                 03960010
