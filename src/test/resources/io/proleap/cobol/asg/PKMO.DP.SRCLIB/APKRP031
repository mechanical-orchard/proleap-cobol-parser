023200                                                                  02320001
023300     GOBACK.                                                      02330001
023400 EJECT                                                            02340001
023500*----------------------------------------------------------------*02350006
023600*    INITIALIZATION PROCESSING                                   *02360001
023700*----------------------------------------------------------------*02370006
023800 B100-INITIALIZATION.                                             02380001
023900                                                                  02390001
024000     OPEN INPUT  AGED-UNJRNLD-FILE                                02400006
024100          OUTPUT REPORT-FILE.                                     02410001
024200                                                                  02420001
024300     INITIALIZE PV-ACCUMULATORS.                                  02430001
024400                                                                  02440001
024500     MOVE FUNCTION CURRENT-DATE(1:4)  TO DP132O-RUN-YEAR.         02450001
024600     MOVE FUNCTION CURRENT-DATE(5:2)  TO DP132O-RUN-MONTH.        02460001
024700     MOVE FUNCTION CURRENT-DATE(7:2)  TO DP132O-RUN-DAY.          02470001
024800                                                                  02480001
024900     MOVE FUNCTION CURRENT-DATE(9:2)  TO DP132O-RUN-HOUR.         02490001
025000     MOVE FUNCTION CURRENT-DATE(11:2) TO DP132O-RUN-MINUTE.       02500001
025100     MOVE PC-PROGRAM-NAME             TO DP132O-PROGRAM-NAME.     02510001
025200     MOVE 1                           TO DP132O-REPORT-NUMBER.    02520001
025300                                                                  02530001
025400     PERFORM S100-SELECT-BATCH-PROCESS-DATE.                      02540001
025500                                                                  02550001
025600     PERFORM R100-READ-AGED-UNJRNLD-FILE.                         02560006
025700                                                                  02570001
025800     PERFORM W100-WRITE-HEADERS.                                  02580001
025900                                                                  02590001
026000 EJECT                                                            02600001
026100                                                                  02610001
026200*----------------------------------------------------------------*02620006
026300*  ACCUMULATES THE GROSS COST AND RETAIL AMOUNT FOR EACH         *02630001
026400*  RECORD ON THE INPUT FILE WHERE THE DUE DATE IS IN THE PAST.   *02640006
026500*  THESE GO INTO ONE OF 2 BUCKETS, 0-8 WEEKS PAST AND OVER 8     *02650006
026600*  8 WEEKS PAST.  ONCE A RECORD IS FOUND THAT IS NOT PAST DUE,   *02660006
026700*  THE 2 PAST DUE LINES ARE WRITTEN ON THE REPORT, AND THE REST  *02670006
026800*  OF THE INPUT FILE IS PROCESSED, CREATING ONE REPORT LINE FOR  *02680006
026900*  EACH RECORD, AND ACCUMULATING ALL RECORDS FOR GRAND TOTALS.   *02690006
027000*----------------------------------------------------------------*02700006
027100 B200-PROCESS-TRANSACTIONS.                                       02710001
027200                                                                  02720001
027300     PERFORM UNTIL AP003-CALC-OTP-DTE > APCNTL-BTCH-PRC-DTE       02730001
027400                OR END-OF-AGED-UNJRNLD                            02740006
027500         IF AP003-CALC-OTP-DTE < WEEK-8-CUTOFF-DATE               02750001
027600            ADD AP003-COST-AMT TO OVER-8-WEEK-COST                02760001
027700            ADD AP003-RTL-AMT  TO OVER-8-WEEK-RTL-AMT             02770001
027800         ELSE                                                     02780001
027900            ADD AP003-COST-AMT TO DUE-1-8-WEEK-COST               02790001
028000            ADD AP003-RTL-AMT  TO DUE-1-8-WEEK-RTL-AMT            02800001
028100         END-IF                                                   02810001
028200         ADD AP003-COST-AMT TO GRAND-TOTAL-COST                   02820001
028300         ADD AP003-RTL-AMT  TO GRAND-TOTAL-RTL-AMT                02830001
028400         PERFORM R100-READ-AGED-UNJRNLD-FILE                      02840006
028500     END-PERFORM.                                                 02850001
028600                                                                  02860001
028700     MOVE 'OVER 8 WEEKS PAST DUE' TO DL-OTP-DATE.                 02870001
028800     MOVE OVER-8-WEEK-COST        TO DL-GRO-COST-AMT.             02880001
028900     MOVE OVER-8-WEEK-RTL-AMT     TO DL-RETL-AMT.                 02890001
029000     WRITE REPORT-REC FROM REPORT-DETAIL-LINE AFTER 2.            02900001
029100                                                                  02910001
029200     MOVE '1 TO 8 WEEKS PAST DUE' TO DL-OTP-DATE.                 02920001
029300     MOVE DUE-1-8-WEEK-COST       TO DL-GRO-COST-AMT.             02930001
029400     MOVE DUE-1-8-WEEK-RTL-AMT    TO DL-RETL-AMT.                 02940001
029500     WRITE REPORT-REC FROM REPORT-DETAIL-LINE AFTER 1.            02950001
029600                                                                  02960001
029700     PERFORM UNTIL END-OF-AGED-UNJRNLD                            02970006
029800         ADD AP003-COST-AMT TO GRAND-TOTAL-COST                   02980001
029900         ADD AP003-RTL-AMT  TO GRAND-TOTAL-RTL-AMT                02990001
030000         MOVE AP003-CALC-OTP-DTE      TO DL-OTP-DATE              03000001
030100         MOVE AP003-COST-AMT          TO DL-GRO-COST-AMT          03010001
030200         MOVE AP003-RTL-AMT           TO DL-RETL-AMT              03020001
030300         WRITE REPORT-REC FROM REPORT-DETAIL-LINE AFTER 1         03030001
030400         PERFORM R100-READ-AGED-UNJRNLD-FILE                      03040006
030500     END-PERFORM.                                                 03050001
030600 EJECT                                                            03060001
030700                                                                  03070001
030800*----------------------------------------------------------------*03080006
030900*  PRINT FINAL TOTALS AND CLOSE FILES.                           *03090006
031000*----------------------------------------------------------------*03100006
031100 B300-END-PROGRAM.                                                03110001
031200                                                                  03120001
031300     MOVE GRAND-TOTAL-COST        TO TL-GRO-COST-AMT.             03130001
031400     MOVE GRAND-TOTAL-RTL-AMT     TO TL-RETL-AMT.                 03140001
031500     WRITE REPORT-REC FROM REPORT-TOTAL-LINE AFTER 2.             03150001
031600                                                                  03160001
031700     WRITE REPORT-REC FROM REPORT-END-OF-REPORT-LINE AFTER 3.     03170001
031800                                                                  03180001
031900     DISPLAY '**********************************************'     03190001
032000     DISPLAY 'PROGRAM NAME = APKRP031'                            03200001
032100     DISPLAY 'TRAN RECORDS IN = ' PC-TRAN-IN.                     03210001
032200     DISPLAY '**********************************************'     03220001
032300                                                                  03230001
032400     CLOSE AGED-UNJRNLD-FILE                                      03240006
032500           REPORT-FILE.                                           03250001
032600 EJECT                                                            03260001
032700                                                                  03270001
032800*----------------------------------------------------------------*03280006
032900*  READ UNJOURNALIZED AGED FILE (APRD003)                        *03290006
033000*----------------------------------------------------------------*03300006
033100 R100-READ-AGED-UNJRNLD-FILE.                                     03310006
033200                                                                  03320001
033300     READ AGED-UNJRNLD-FILE INTO AP003-UNJRNLD-DUEDATE-RECORD     03330007
033400        AT END                                                    03340001
033500            SET END-OF-AGED-UNJRNLD TO TRUE                       03350006
033600        NOT AT END                                                03360001
033700            ADD 1 TO PC-TRAN-IN                                   03370001
033800     END-READ.                                                    03380001
033900                                                                  03390001
034000 EJECT                                                            03400001
034100*----------------------------------------------------------------*03410006
034200*  SELECT BATCH PROCESS DATE FROM TAPCNTL TABLE                  *03420001
034300*----------------------------------------------------------------*03430006
034400 S100-SELECT-BATCH-PROCESS-DATE.                                  03440001
034500     EXEC SQL                                                     03450001
034600         SELECT  BTCH_PRC_DTE                                     03460001
034700              ,  BTCH_PRC_DTE - 56 DAYS                           03470001
034800           INTO :APCNTL-BTCH-PRC-DTE                              03480001
034900              , :WEEK-8-CUTOFF-DATE                               03490001
035000           FROM TAPCNTL                                           03500001
035100     END-EXEC.                                                    03510001
035200                                                                  03520001
035300     EVALUATE TRUE                                                03530001
035400         WHEN SQLCODE = ZERO                                      03540001
035500              CONTINUE                                            03550001
035600         WHEN OTHER                                               03560001
035700             SET AC-UNSUCCESSFUL-SELECT TO TRUE                   03570001
035800             MOVE 'S100-SELECT-BATCH-PROCESS-DATE'                03580001
035900                                        TO AA-PARAGRAPH-NAME      03590001
036000             MOVE 'UNSUCCESSFUL SELECT' TO AA-DB2-OPERATION       03600001
036100             MOVE 'TAPCNTL'             TO AA-DB2-TABLE-1         03610001
036200             MOVE SPACES                TO AA-DB2-TABLE-2         03620001
036300             MOVE SPACES                TO AA-DB2-TABLE-3         03630001
036400             PERFORM Z998-DB2-ABEND                               03640001
036500     END-EVALUATE.                                                03650001
036600 EJECT                                                            03660001
036700                                                                  03670001
036800*----------------------------------------------------------------*03680006
036900*  WRITES THE HEADINGS LINE FOR THE AGED UNJOURNALIZED REPORT    *03690006
037000*----------------------------------------------------------------*03700006
037100 W100-WRITE-HEADERS.                                              03710001
037200     MOVE 1 TO DP132O-PAGE-NUMBER.                                03720001
037300     WRITE REPORT-REC FROM DP132O-STANDRD-HEADER-132-COLS         03730001
037400           AFTER PAGE.                                            03740001
037500                                                                  03750001
037600     WRITE REPORT-REC FROM REPORT-HEADER1     AFTER 1.            03760001
037700     WRITE REPORT-REC FROM REPORT-HEADER2     AFTER 1.            03770001
037800     WRITE REPORT-REC FROM REPORT-COLUMN-HDR1 AFTER 2.            03780001
037900                                                                  03790001
038000 EJECT                                                            03800001
038100*----------------------------------------------------------------*03810006
038200*     DB2 ABEND ROUTINE                                           03820001
038300*----------------------------------------------------------------*03830006
038400 Z998-DB2-ABEND.                                                  03840001
038500     DISPLAY '**** APKRP031 ABENDING ****'                        03850001
038600                                                                  03860001
038700     CLOSE AGED-UNJRNLD-FILE                                      03870006
038800           REPORT-FILE.                                           03880001
038900                                                                  03890001
039000     DISPLAY AA-ABEND-LIT.                                        03900001
039100     DISPLAY AA-DB2-ERROR-LIT.                                    03910001
039200     DISPLAY AA-PROGRAM-LIT.                                      03920001
039300     DISPLAY AA-PARAGRAPH-LIT.                                    03930001
039400     DISPLAY AA-DB2-OPERATION-LIT.                                03940001
039500     DISPLAY AA-DB2-TABLE-1.                                      03950001
039600     DISPLAY AA-DB2-TABLE-2.                                      03960001
039700     DISPLAY AA-DB2-TABLE-3.                                      03970001
039800     SET AC-DB2-ERROR TO TRUE.                                    03980001
039900                                                                  03990001
040000     COPY DPPD004.                                                04000001
040100                                                                  04010001
040200     CALL 'ILBOABN0' USING ABEND-CODE.                            04020001
040300 EJECT                                                            04030001
