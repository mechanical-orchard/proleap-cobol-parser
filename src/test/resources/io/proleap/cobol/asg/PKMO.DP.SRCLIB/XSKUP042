030700******************************************************************03070000
030800**SELECT LOC ROW FOR UPDATED INFO                                 03080002
030900******************************************************************03090000
031000 2400-SELECT-LOC.                                                 03100002
031100                                                                  03110000
031200     EXEC SQL                                                     03120000
031300         SELECT LOC_5_ABBR_NM                                     03130002
031400               ,FINCL_CLO_DTE                                     03140002
031500               ,MN_SLS_INCL_DTE                                   03150002
031600               ,YR_SLS_INCL_DTE                                   03160002
031700               ,OCPNC_DTE                                         03170002
031800               ,DST_IND                                           03180002
031900               ,E_BUS_IND                                         03190002
032000               ,STD_TM_ZN_CDE                                     03200002
032100               ,SLS_INCL_DTE_CDE                                  03210002
032200               ,REPLT_STR_NBR                                     03220002
032210               ,QTR_SLS_INCL_DTE                                  03221020
032220               ,SEA_SLS_INCL_DTE                                  03222020
032300           INTO :PV-LOC-5-ABBR-NM                                 03230002
032400               ,:PV-FINCL-CLO-DTE    :NI-LOC-1                    03240007
032500               ,:PV-MN-SLS-INCL-DTE  :NI-LOC-2                    03250007
032600               ,:PV-YR-SLS-INCL-DTE  :NI-LOC-3                    03260007
032700               ,:PV-OCPNC-DTE        :NI-LOC-4                    03270007
032800               ,:PV-DST-IND          :NI-LOC-5                    03280007
032900               ,:PV-E-BUS-IND        :NI-LOC-6                    03290007
033000               ,:PV-STD-TM-ZN-CDE    :NI-LOC-7                    03300007
033100               ,:PV-SLS-INCL-DTE-CDE :NI-LOC-8                    03310007
033200               ,:PV-REPLT-STR-NBR    :NI-LOC-9                    03320007
033210               ,:PV-QTR-SLS-INCL-DTE :NI-LOC-10                   03321020
033220               ,:PV-SEA-SLS-INCL-DTE :NI-LOC-11                   03322020
033300           FROM XST_LOC                                           03330002
033400          WHERE  LOC_NBR = :XST00001-LOC-NBR                      03340002
033500     END-EXEC                                                     03350000
033600                                                                  03360000
033700     EVALUATE TRUE                                                03370000
033800        WHEN SQLCODE = 0                                          03380000
033900            CONTINUE                                              03390002
034000        WHEN OTHER                                                03400000
034100            SET PS-PROCESS-COMPLETE     TO TRUE                   03410002
034200     END-EVALUATE.                                                03420000
034300                                                                  03430000
034400******************************************************************03440000
034500**SELECT LOC_ATTR ROW                                             03450002
034600******************************************************************03460000
034700 2500-SELECT-LOC-ATTR.                                            03470002
034800                                                                  03480000
034900     PERFORM                                                      03490000
035000         WITH TEST AFTER                                          03500000
035100         VARYING PV-RETRY-COUNTER                                 03510000
035200         FROM 1 BY 1                                              03520000
035300         UNTIL PV-RETRY-COUNTER > PC-RETRY-MAX                    03530000
035400            OR SQLCODE = 0                                        03540000
035500            OR SQLCODE = +100                                     03550000
035600            OR PS-PROCESS-COMPLETE                                03560000
035700                                                                  03570000
035800         EXEC SQL                                                 03580000
035900           SELECT LS_LVL_ID                                       03590017
036000                 ,POS_CAP_LVL_ID                                  03600002
036100             INTO :PV-LS-LVL-ID        :NI-LOC-ATTR-1             03610017
036200                 ,:PV-POS-CAP-LVL-ID   :NI-LOC-ATTR-2             03620017
036300             FROM XST_LOC_ATTR                                    03630002
036400            WHERE  LOC_NBR = :XST00025-LOC-NBR                    03640002
036500         END-EXEC                                                 03650000
036600                                                                  03660000
036700         EVALUATE TRUE                                            03670000
036800             WHEN SQLCODE = -911                                  03680000
036900             WHEN SQLCODE = -913                                  03690000
037000             WHEN SQLCODE = 0                                     03700000
037100                 CONTINUE                                         03710000
037200             WHEN SQLWARN0 NOT = SPACE                            03720000
037300             WHEN SQLCODE NOT =  ZERO                             03730000
037400                 SET PS-PROCESS-COMPLETE TO TRUE                  03740000
037500         END-EVALUATE                                             03750000
037600     END-PERFORM.                                                 03760000
037700                                                                  03770000
037800     EVALUATE TRUE                                                03780000
037900        WHEN SQLCODE = 0                                          03790000
038000            CONTINUE                                              03800000
038100        WHEN OTHER                                                03810000
038200             SET PS-PROCESS-COMPLETE     TO TRUE                  03820000
038300     END-EVALUATE.                                                03830000
038400                                                                  03840000
038500******************************************************************03850008
038600**UPDATE XST_LOC WITH LOC-5-ABBR-NM                               03860008
038700******************************************************************03870008
038800 3100-UPDATE-LOC-5-ABBR-NM.                                       03880008
038900                                                                  03890008
039000     PERFORM                                                      03900008
039100         WITH TEST AFTER                                          03910008
039200         VARYING PV-RETRY-COUNTER                                 03920008
039300         FROM 1 BY 1                                              03930008
039400         UNTIL PV-RETRY-COUNTER > PC-RETRY-MAX                    03940008
