               SELECT PO_NBR                                            02950064
                  INTO :PURITM-PO-NBR                                   02960064
                  FROM TPURITM                                          02970064
105700           WHERE PO_NBR       = :WS-PO-NBR                        02980064
105700             AND PO_LNE_NBR   = :WS-PO-LNE-NBR                    02990064
                   AND STATUS_CDE IN ('AC', 'AP', 'CM', 'CB', 'RV')     03000064
105800             AND VER_STATUS_CDE = 'A'                             03010064
             UNION                                                      03011064
               SELECT PO_NBR                                            03012064
                  INTO :PURITM-PO-NBR                                   03013064
                  FROM TPURITM A                                        03014064
                      ,TRECITM B                                        03014164
                 WHERE PO_NBR           = :WS-PO-NBR                    03015064
                   AND PO_LNE_NBR       = :WS-PO-LNE-NBR                03016064
                   AND PO_NBR           = ORD_NBR                       03016164
                   AND PO_LNE_NBR       = ORD_LNE_NBR                   03016264
                   AND A.STATUS_CDE       = 'CA'                        03017064
                   AND A.VER_STATUS_CDE = 'A'                           03018064
                   AND B.VER_STATUS_CDE = 'A'                           03019064
106100     END-EXEC.                                                    03021064
106200*                                                                 03030064
