075100****************************************************************  01950064
075200* A100-INITIALIZATION OPENS ALL FILES                          *  01960064
075400****************************************************************  01970064
075500 A100-INITIALIZATION.                                             01980064
075600                                                                  01990064
           OPEN INPUT  DATE-FILE                                        02000064
                       PO-FILE                                          02010064
                OUTPUT NEW-STORE-PO-FILE.                               02020064
                                                                        02030064
077400     READ DATE-FILE           INTO DATE-RECORD                    02040064
077500          AT END                                                  02050064
077600             MOVE PC-Y          TO  PS-END-OF-DATE-REC.           02060064
077700                                                                  02070064
077700                                                                  02090064
075400****************************************************************  02100064
       B000-PROCESS-INPUT.                                              02110064
                                                                        02120064
           READ PO-FILE                                                 02130064
             AT END                                                     02140064
              MOVE PC-Y          TO  PS-END-OF-PO-REC.                  02150064
                                                                        02160064
           IF END-OF-PO-REC                                             02170064
               CONTINUE                                                 02180064
           ELSE                                                         02190064
               MOVE PO-REC TO WS-PO-REC                                 02200064
               IF WS-PO-NBR = PV-LAST-PO-NBR                            02210064
                  IF NEW-STORE-PO                                       02220064
                    IF WS-PO-LNE-NBR = PV-LAST-PO-LNE-NBR               02230064
                       IF ACTIVE-PO-LNE                                 02240064
                           PERFORM B100-WRITE-OUTPUT                    02250064
                       END-IF                                           02260064
                    ELSE                                                02270064
                       MOVE WS-PO-LNE-NBR TO PV-LAST-PO-LNE-NBR         02280064
                       PERFORM B030-VERIFY-LINE-STATUS                  02290064
                       IF ACTIVE-PO-LNE                                 02300064
                          PERFORM B100-WRITE-OUTPUT                     02310064
                       END-IF                                           02320064
                    END-IF                                              02330064
                  END-IF                                                02340064
               ELSE                                                     02350064
                 MOVE WS-PO-NBR TO PV-LAST-PO-NBR                       02360064
                 MOVE WS-PO-LNE-NBR TO PV-LAST-PO-LNE-NBR               02370064
                 PERFORM B020-QUALIFY-PO                                02380064
                 IF (NEW-STORE-PO AND                                   02381064
                     ACTIVE-PO-LNE)                                     02382064
                   PERFORM B100-WRITE-OUTPUT                            02383064
                 ELSE                                                   02384064
                   DISPLAY WS-PO-NBR ' '                                02385064
                           WS-STR-NBR ' '                               02386064
                           WS-PO-LNE-NBR ' '                            02387064
                           WS-STR-DISTRO-QTY ' '                        02388064
                           WS-CHAR5-ABBR-NAME ' '                       02389064
                           'BYPASSED'                                   02390064
                 END-IF                                                 02400064
           END-IF.                                                      02410064
074400                                                                  02420064
081900***************************************************************** 02430064
082000* B020-QUALIFY-PO                                               * 02440064
082300***************************************************************** 02450064
082400*                                                                 02460064
082500 B020-QUALIFY-PO.                                                 02470064
                                                                        02480064
082700     MOVE 'B020-QUALIFY-PO'                                       02490064
082800                                 TO PV-ABEND-PARAGRAPH.           02500064
105200     EXEC SQL                                                     02510064
               SELECT B.PO_NBR                                          02520064
                     ,B.DONT_SHIP_BEF_DTE                               02530064
                     ,B.CAN_IFNOT_SHIP_DTE                              02540064
                     ,B.EXT_CAN_DTE                                     02541070
                  INTO :PLNSHP-PO-NBR                                   02550064
                      ,:PLNSHP-DONT-SHIP-BEF-DTE                        02560064
                      ,:PLNSHP-CAN-IFNOT-SHIP-DTE                       02570064
                      ,:PLNSHP-EXT-CAN-DTE                              02571070
                  FROM TPURCHS A                                        02580064
                      ,TPLNSHP B                                        02590064
105700           WHERE A.PO_NBR       = :WS-PO-NBR                      02600064
105700             AND A.PO_NBR       = B.PO_NBR                        02610064
                   AND A.STATUS_CDE IN ('AC', 'AP', 'CM')               02620064
105800             AND A.VER_STATUS_CDE = 'A'                           02630064
105800             AND B.VER_STATUS_CDE = 'A'                           02640064
C88759             AND DONT_SHIP_BEF_DTE BETWEEN :WS-START-DATE         02650076
C88759                      AND :WS-END-DATE                            02660076
               UNION                                                    02670064
               SELECT B.PO_NBR                                          02680064
                     ,B.DONT_SHIP_BEF_DTE                               02690064
                     ,B.CAN_IFNOT_SHIP_DTE                              02700064
                     ,B.EXT_CAN_DTE                                     02701070
                  INTO :PLNSHP-PO-NBR                                   02710064
                      ,:PLNSHP-DONT-SHIP-BEF-DTE                        02720064
                      ,:PLNSHP-CAN-IFNOT-SHIP-DTE                       02730064
                      ,:PLNSHP-EXT-CAN-DTE                              02730170
                  FROM TPURCHS A                                        02731064
                      ,TPLNSHP B                                        02732064
                      ,TRECITM C                                        02733064
105700           WHERE A.PO_NBR       = :WS-PO-NBR                      02734064
105700             AND A.PO_NBR       = B.PO_NBR                        02735064
105700             AND A.PO_NBR       = C.ORD_NBR                       02736064
                   AND A.STATUS_CDE IN ('CA')                           02737064
105800             AND A.VER_STATUS_CDE = 'A'                           02738064
105800             AND B.VER_STATUS_CDE = 'A'                           02739064
105800             AND C.VER_STATUS_CDE = 'A'                           02739164
C88759             AND DONT_SHIP_BEF_DTE BETWEEN :WS-START-DATE         02739276
C88759                      AND :WS-END-DATE                            02739376
106100     END-EXEC.                                                    02739564
106200*                                                                 02739664
085100     EVALUATE TRUE                                                02739764
085200       WHEN SQLCODE = ZEROS                                       02739864
085800         MOVE PC-Y TO  PS-NEW-STORE-PO-SW                         02739964
               PERFORM B030-VERIFY-LINE-STATUS                          02740064
085600       WHEN SQLCODE = PC-ROW-NOT-FOUND                            02741064
085800         MOVE PC-N TO  PS-NEW-STORE-PO-SW                         02750064
