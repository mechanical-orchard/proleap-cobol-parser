042500* ************************************************************** *04250000
042600 9150-FETCH-TABLE-CURSOR.                                         04260000
042700     EXEC SQL                                                     04270000
042800         FETCH TABLE-CURSOR                                       04280000
042900          INTO  :MESTDS-PO-NBR                                    04290000
043000               ,:MESTDS-PO-LNE-NBR                                04300000
043100               ,:MESTDS-PLAN-NBR                                  04310000
043200               ,:MESTDS-STR-NBR                                   04320000
043300               ,:MESTDS-STR-DISTRO-QTY                            04330000
043400               ,:MESTDS-ALLOC-SEQ-CDE                             04340000
043500               ,:MESTDS-RNK-SEQ-NBR                               04350000
                     ,:MESTDS-STR-CNSTR-CDE                             04351009
                     ,:MESTDS-STR-CNSTR-QTY                             04352009
043600     END-EXEC.                                                    04360000
043700                                                                  04370000
043800     EVALUATE TRUE                                                04380000
043900         WHEN SQLCODE = ZERO                                      04390000
044000              ADD +1                 TO PV-TABLE-ROW-SELECT-CNT   04400000
044100              SET PS-CURSOR-HAS-DATA TO TRUE                      04410000
044200         WHEN SQLCODE = +100                                      04420000
044300              SET PS-TABLE-LOOP-DONE TO TRUE                      04430000
044400         WHEN SQLWARN0 NOT EQUAL SPACE                            04440000
044500         WHEN SQLCODE NOT EQUAL ZERO                              04450000
044600              MOVE '9150-FETCH-TABLE-CURSOR'                      04460000
044700                                 TO PV-PARAGRAPH-NAME             04470000
044800              MOVE 'FETCH TABLE CURSOR'                           04480000
044900                                 TO PV-DB2-OPERATION-MSG-1        04490000
045000              MOVE 'TMESTDS'     TO PV-DB2-TABLE-NAME             04500000
045100              PERFORM 9999-DB2-ABEND                              04510000
045200     END-EVALUATE.                                                04520000
045300                                                                  04530000
