019100*---------------------------------------------------------------* 01910000
019200*  DB2 FORMATTED MESSAGE AREA                                     01920000
019300*---------------------------------------------------------------* 01930000
019400     COPY DPWS004.                                                01940000
019500                                                                  01950000
019600                                                                  01960000
019700*---------------------------------------------------------------* 01970000
019800*  AHRD002  - RECORD LAYOUT FOR PO DRIVER FILE, INPUT.          * 01980010
019900*---------------------------------------------------------------* 01990000
020000     COPY AHRD002.                                                02000010
020100                                                                  02010000
020200                                                                  02020000
020300*---------------------------------------------------------------* 02030000
020400*    DB2 AREA FOR TMESTDS                                       * 02040000
020500*---------------------------------------------------------------* 02050000
020600     EXEC SQL                                                     02060000
020700          INCLUDE TMESTDS                                         02070000
020800     END-EXEC.                                                    02080000
020900                                                                  02090000
021000*---------------------------------------------------------------* 02100000
021100*    DB2 AREA FOR COMMUNICATIONS                                * 02110000
021200*---------------------------------------------------------------* 02120000
021300     EXEC SQL                                                     02130000
021400          INCLUDE SQLCA                                           02140000
021500     END-EXEC.                                                    02150000
021600                                                                  02160000
021700*---------------------------------------------------------------* 02170000
021800*    DEFINE CURSOR FOR TMESTDS :                                * 02180000
021900*    PULL ALL AVAILABLE COLUMNS, SO EXTRACT FILE WILL CONTAIN   * 02190000
022000*    ALL DATA TO POPULATE "ARCHIVE HISTORY" COPY OF THIS TABLE. * 02200000
022100*---------------------------------------------------------------* 02210000
022200     EXEC SQL                                                     02220000
022300          DECLARE TABLE-CURSOR CURSOR FOR                         02230000
022400           SELECT PO_NBR                                          02240000
022500                 ,PO_LNE_NBR                                      02250000
022600                 ,PLAN_NBR                                        02260000
022700                 ,STR_NBR                                         02270000
022800                 ,STR_DISTRO_QTY                                  02280000
022900                 ,ALLOC_SEQ_CDE                                   02290000
023000                 ,RNK_SEQ_NBR                                     02300000
                       ,STR_CNSTR_CDE                                   02301009
                       ,STR_CNSTR_QTY                                   02302009
023200           FROM   TMESTDS                                         02320000
023300          WHERE  (PO_NBR = :PV-PO-NBR-COMP)                       02330000
023500            FOR  FETCH ONLY                                       02350000
023600           WITH  UR                                               02360000
023700     END-EXEC.                                                    02370000
023800                                                                  02380000
023900                                                                  02390000
024000 PROCEDURE DIVISION.                                              02400000
024100********************                                              02410000
024200                                                                  02420000
024300 0000-PROGRAM-MAINLINE.                                           02430000
024400                                                                  02440000
024500     PERFORM 1000-INITIALIZE.                                     02450000
024600                                                                  02460000
024700     SET PS-IN-DRIVER-NOT-EOF TO TRUE.                            02470000
024800     PERFORM 2000-PROCESS-DRIVER-FILE                             02480000
024900         UNTIL PS-IN-DRIVER-EOF.                                  02490000
025000                                                                  02500000
025100     PERFORM 6000-EOJ-ROUTINE.                                    02510000
025200     STOP RUN.                                                    02520000
025300                                                                  02530000
025400                                                                  02540000
025500* ************************************************************** *02550000
025600* * INITIALIZATIONS                                            * *02560000
025700* ************************************************************** *02570000
025800 1000-INITIALIZE.                                                 02580000
025900                                                                  02590000
026000     MOVE PC-CURRENT-PROGRAM-NAME TO DM-PROGRAM.                  02600000
026100     DISPLAY DM-DISPLAY-MESSAGE.                                  02610000
026200                                                                  02620000
026300     MOVE PC-BEGINNING              TO DP-STATUS-DESCRIPTION.     02630000
026400     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           02640000
026500                                                                  02650000
026600     PERFORM 9000-OPEN-IN-DRIVER-FILE.                            02660000
026700     PERFORM 9700-OPEN-OUT-EXTRACT-FILE.                          02670000
026800                                                                  02680000
026900                                                                  02690000
027000* ************************************************************** *02700000
027100* * 1) GET NEXT RECORD FROM DRIVER FILE, AND                   * *02710000
027200* *    1A- POPULATE KEY (WHERE CLAUSE) FOR TABLE CURSOR        * *02720000
027300* *    1B- OPEN TABLE CURSOR                                   * *02730000
027400* *    1C- LOOP THRU TABLE CURSOR TO EXTRACT ALL               * *02740000
027500* *        ROWS FOR THIS INPUT DRIVER RECORD                   * *02750000
027600* *    2) FOR EACH ROW,                                        * *02760000
027700* *       2A- INCREMENT COUNTS                                 * *02770000
027800* *       2B- WRITE EXTRACT RECORD                             * *02780000
027900* *    1D- CLOSE TABLE CURSOR                                  * *02790000
028000* ************************************************************** *02800000
028100 2000-PROCESS-DRIVER-FILE.                                        02810000
028200                                                                  02820000
028300     PERFORM 9050-READ-IN-DRIVER-FILE.                            02830000
028400     IF PS-IN-DRIVER-EOF                                          02840000
028500         CONTINUE                                                 02850000
028600     ELSE                                                         02860000
028700         MOVE AH002-PO-NBR TO         PV-PO-NBR-COMP              02870010
028800                                      PV-PO-NBR-NUM               02880000
028900         PERFORM 2100-CHECK-PGM-PROGRESS                          02890000
029000         SET PS-TABLE-LOOP-NOT-DONE   TO TRUE                     02900000
029100         SET PS-CURSOR-HAS-NO-DATA    TO TRUE                     02910000
029200         PERFORM 9100-OPEN-TABLE-CURSOR                           02920000
029300         PERFORM 3000-EXTRACT-ROWS-FROM-TABLE                     02930000
029400             UNTIL PS-TABLE-LOOP-DONE                             02940000
029500         PERFORM 9190-CLOSE-TABLE-CURSOR                          02950000
029600     END-IF.                                                      02960000
029700                                                                  02970000
029800                                                                  02980000
029900* ************************************************************** *02990000
030000* *    EVERY NTH RECORD (IE: INITIAL SETTING 1000), REPORT     * *03000000
030100* *    RECORD COUNT AND CURRENT DATE/TIME.  THIS WILL ASSIST   * *03010000
030200* *    WITH RUN-TIME ESTIMATE OF TIME REMAINING TO COMPLETE.   * *03020000
030300* ************************************************************** *03030000
030400 2100-CHECK-PGM-PROGRESS.                                         03040000
030500     DIVIDE    PV-IN-DRIVER-RECS-READ-CNT                         03050000
030600         BY        PC-HOW-OFTEN-TO-DISPLAY                        03060000
030700         GIVING    PV-DISPLAY-NTH-MULTIPLE                        03070000
030800         REMAINDER PV-DISPLAY-NTH-REMAINDER.                      03080000
030900                                                                  03090000
031000     IF PV-DISPLAY-NTH-REMAINDER = ZERO                           03100000
031100         MOVE PC-CONTINUING         TO DP-STATUS-DESCRIPTION      03110000
031200         PERFORM 6200-DISPLAY-PGM-PROGRESS                        03120000
031300     END-IF.                                                      03130000
031400                                                                  03140000
031500                                                                  03150000
031600* ************************************************************** *03160000
031700* * EXTRACT ROWS FROM TABLE, FOR THE CURRENT DRIVER RECORD     * *03170000
031800* ************************************************************** *03180000
031900 3000-EXTRACT-ROWS-FROM-TABLE.                                    03190000
032000     INITIALIZE DCLTMESTDS.                                       03200000
032100     PERFORM 9150-FETCH-TABLE-CURSOR.                             03210000
032200     IF PS-TABLE-LOOP-DONE                                        03220000
032300         CONTINUE                                                 03230000
032400     ELSE                                                         03240000
032500         PERFORM 9750-WRITE-EXTRACT-RECORD                        03250000
032600     END-IF.                                                      03260000
032700                                                                  03270000
032800                                                                  03280000
032900* ************************************************************** *03290000
033000* * DISPLAY END OF RUN MESSAGE, CLOSE THE OUTPUT FILES         * *03300002
033100* ************************************************************** *03310000
033200 6000-EOJ-ROUTINE.                                                03320000
033300                                                                  03330000
033400     PERFORM 9090-CLOSE-IN-DRIVER-FILE.                           03340000
033500     PERFORM 9790-CLOSE-OUT-EXTRACT-FILE.                         03350000
033600                                                                  03360000
033700     MOVE PC-ENDING                 TO DP-STATUS-DESCRIPTION.     03370000
033800     PERFORM 6200-DISPLAY-PGM-PROGRESS.                           03380000
033900                                                                  03390000
034000                                                                  03400000
034100* ************************************************************** *03410000
034200* * DISPLAY PROGRAM PROGRESS.                                  * *03420000
034300* ************************************************************** *03430000
034400 6200-DISPLAY-PGM-PROGRESS.                                       03440000
034500                                                                  03450000
034600     EXEC SQL                                                     03460000
034700          SET :PV-TMST = CURRENT TIMESTAMP                        03470000
034800     END-EXEC.                                                    03480000
034900                                                                  03490000
035000     MOVE PC-CURRENT-PROGRAM-NAME   TO DP-PROGRAM.                03500000
035100     MOVE PV-TIMESTAMP-19           TO DP-TIMESTAMP-19.           03510000
035200     MOVE PV-IN-DRIVER-RECS-READ-CNT                              03520000
035300                                    TO DP-IN-CNT.                 03530000
035400*    MOVE PV-RECS-WITH-MATCHES-CNT  TO DP-MATCHES-FOUND-CNT.      03540000
035500     MOVE PV-TABLE-ROW-SELECT-CNT   TO DP-ROWS-SELECTED-CNT.      03550000
035600     MOVE PV-EXTRACT-WRITTEN-CNT    TO DP-OUT-CNT.                03560000
035700                                                                  03570000
035800     DISPLAY DP-DISPLAY-PROGRESS-MSG.                             03580000
035900                                                                  03590000
036000                                                                  03600000
036100* ************************************************************** *03610000
036200* * FILE AND DB2 TABLE ACCESS PARAGRAPHS.                      * *03620000
036300* * 90XX- INPUT:  DRIVER FILE                                  * *03630000
036400* * 91XX- INPUT:  TABLE CURSOR                                 * *03640000
036500* * 97XX- OUTPUT: TABLE EXTRACT                                * *03650000
036600* ************************************************************** *03660000
036700                                                                  03670000
036800* ************************************************************** *03680000
036900* * OPEN  INPUT DRIVER FILE                                    * *03690000
037000* ************************************************************** *03700000
037100 9000-OPEN-IN-DRIVER-FILE.                                        03710000
037200     OPEN  INPUT  IN-DRIVER-FILE.                                 03720000
037300                                                                  03730000
037400                                                                  03740000
037500* ************************************************************** *03750000
037600* * READ  INPUT DRIVER FILE OF ELIGIBLE DATA TO EXTRACT        * *03760000
037700* ************************************************************** *03770000
037800 9050-READ-IN-DRIVER-FILE.                                        03780000
037900     READ IN-DRIVER-FILE                                          03790000
038000          INTO AH002-DRIVER-RECORD-LAYOUT                         03800010
038100          AT END                                                  03810000
038200              SET PS-IN-DRIVER-EOF TO TRUE.                       03820000
038300                                                                  03830000
038400     IF PS-IN-DRIVER-EOF                                          03840000
038500         CONTINUE                                                 03850000
038600     ELSE                                                         03860000
038700         ADD +1 TO PV-IN-DRIVER-RECS-READ-CNT                     03870000
038800     END-IF.                                                      03880000
038900                                                                  03890000
039000                                                                  03900000
039100* ************************************************************** *03910000
039200* * CLOSE INPUT DRIVER FILE                                    * *03920000
039300* ************************************************************** *03930000
039400 9090-CLOSE-IN-DRIVER-FILE.                                       03940000
039500     CLOSE IN-DRIVER-FILE.                                        03950000
039600                                                                  03960000
039700                                                                  03970000
039800* ************************************************************** *03980000
039900* * OPEN  TABLE CURSOR                                         * *03990000
040000* ************************************************************** *04000000
040100 9100-OPEN-TABLE-CURSOR.                                          04010000
040200     EXEC SQL                                                     04020000
040300          OPEN TABLE-CURSOR                                       04030000
040400     END-EXEC.                                                    04040000
040500                                                                  04050000
040600     EVALUATE TRUE                                                04060000
040700         WHEN SQLCODE = ZERO                                      04070000
040800              ADD +1 TO PV-RECS-WITH-MATCHES-CNT                  04080000
040900              SET PS-CURSOR-HAS-DATA TO TRUE                      04090000
041000         WHEN SQLCODE = +100                                      04100000
041100              SET PS-TABLE-LOOP-DONE TO TRUE                      04110000
041200         WHEN SQLWARN0 NOT EQUAL SPACE                            04120000
041300         WHEN SQLCODE  NOT EQUAL ZERO                             04130000
041400              MOVE '9100-OPEN-TABLE-CURSOR'                       04140000
041500                                 TO PV-PARAGRAPH-NAME             04150000
041600              MOVE 'OPEN TABLE CURSOR'                            04160000
041700                                 TO PV-DB2-OPERATION-MSG-1        04170000
041800              MOVE 'TMESTDS'     TO PV-DB2-TABLE-NAME             04180000
041900              PERFORM 9999-DB2-ABEND                              04190000
042000     END-EVALUATE.                                                04200000
042100                                                                  04210000
