048100*---------------------------------------------------------------* DPKCS036
048200*    CURSOR FOR VENDOR STYLES FOR LISTED PO'S IN THE CONTAINER  * DPKCS036
048300*---------------------------------------------------------------* DPKCS036
048400                                                                  DPKCS036
048421     EXEC SQL                                                     DPKCS036
048422          DECLARE VS-CSR CURSOR                                   DPKCS036
048430          FOR     SELECT DISTINCT                                 DPKCS036
048440                  A.PREPK_RATO_QTY                                        
048450              ,   C.CSTM_CLOR_DESC                                DPKCS036
048460              ,   C.SIZE_DESC                                     DPKCS036
048461              ,   C.SKU_NBR                                               
048462              ,   D.VS_NBR                                                
048463              ,   E.UPC_NBR                                               
048470          FROM    TPURITM A                                       DPKCS036
048490              ,   XLT_SKU C                                               
048491              ,   XLT_VND_STYL D                                          
048492              ,   XLT_UPC E                                               
048493          WHERE   A.PO_NBR         = :DK-PO-NO                            
048497            AND   A.PO_PREPK_ID    = :DK-PO-PREPK-ID                      
048498            AND   A.VER_STATUS_CDE = 'A'                          DPKCS036
048499            AND   A.ITM_NBR        = C.ITM_NBR                            
048500            AND   C.STYL_ID        = D.STYL_ID                            
048501            AND   E.UPC_NBR   >= 000400000000000                          
048502            AND   E.UPC_NBR   <= 000499999999999                          
048503            AND   C.INTR_UPC_ID = E.INTR_UPC_ID                           
048504            AND   E.UPC_TYP_CDE = 'I'                                     
048505          ORDER BY                                                DPKCS036
048506                D.VS_NBR                                          DPKCS036
048507              , E.UPC_NBR                                                 
048508     END-EXEC.                                                    DPKCS036
048520                                                                          
