175500*----------------------------------------------------------------*DPKCS036
175600*   OPEN THE VENDOR STYLE CURSOR.                                *DPKCS036
175700*----------------------------------------------------------------*DPKCS036
175800 4030-OPEN-VS-CURSOR.                                             DPKCS036
175900                                                                  DPKCS036
176000     PERFORM                                                      DPKCS036
176100         WITH TEST AFTER                                          DPKCS036
176200         VARYING PV-RETRY-COUNTER                                 DPKCS036
176300         FROM 1 BY 1                                              DPKCS036
176400         UNTIL ((PV-RETRY-COUNTER > PC-MAX-RETRIES)               DPKCS036
176500                   OR (SQLCODE = ZERO))                           DPKCS036
176600         MOVE 'CURSOR:  VS-CSR'    TO DP013-MESSAGE-TEXT (2)      DPKCS036
176700                                                                  DPKCS036
176800         EXEC SQL                                                 DPKCS036
176900              OPEN VS-CSR                                         DPKCS036
177000         END-EXEC                                                 DPKCS036
177100                                                                  DPKCS036
177200         EVALUATE TRUE                                            DPKCS036
177300             WHEN SQLCODE = ZERO                                  DPKCS036
177400             WHEN SQLCODE = -904                                  DPKCS036
177500             WHEN SQLCODE = -913                                  DPKCS036
177600                 CONTINUE                                         DPKCS036
177700             WHEN SQLWARN0 NOT = SPACES                           DPKCS036
177800             WHEN SQLCODE < ZERO                                  DPKCS036
177900             WHEN SQLCODE > ZERO                                  DPKCS036
178000                 MOVE '4030-OPEN-VS-CURSOR'                       DPKCS036
178100                                   TO DP013-PARAGRAPH             DPKCS036
178200                 MOVE 'OPEN VENDOR STYLE CURSOR - '               DPKCS036
178300                                   TO DP013-MESSAGE-TEXT (1)      DPKCS036
178400                 MOVE SQLCA        TO DP013-SQLCA                 DPKCS036
178500                 MOVE 'TSYMSG  '   TO DP013-DB2-TABLE-NAME (1)    DPKCS036
178600                 SET DP013-DB2-ABEND                              DPKCS036
178700                     DP013-XCTL-DISPLAY-RESTART TO TRUE           DPKCS036
178800                 PERFORM DP013-0000-PROCESS-ABEND                 DPKCS036
178900         END-EVALUATE                                             DPKCS036
179000     END-PERFORM.                                                 DPKCS036
179100*                                                                 DPKCS036
179200     IF  PV-RETRY-COUNTER > PC-MAX-RETRIES                        DPKCS036
179300         MOVE '4030-OPEN-VS-CURSOR'   TO DP013-PARAGRAPH          DPKCS036
179400         MOVE 'MAXIMUM RETRIES EXCEEDED ATTEMPTING TO OPEN CSR'   DPKCS036
179500                                   TO DP013-MESSAGE-TEXT (1)      DPKCS036
