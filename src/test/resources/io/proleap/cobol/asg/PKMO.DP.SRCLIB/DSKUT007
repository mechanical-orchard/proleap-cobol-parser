023000                                                                  02300005
023100         IF SQLCODE = ZERO                                        02310005
023200             SET DS009-SRCH-IDX TO +1                             02320005
023300             SEARCH DS009-CNTL-RECORDS                            02330005
023400                 VARYING DS009-SRCH-IDX                           02340005
023500                 AT END                                           02350005
023600                     CONTINUE                                     02360005
023700                 WHEN CNTL-TABLE (DS009-SRCH-IDX) =               02370005
023800                          DS009-PROCESS-TABLE                     02380005
023900*                    ADD SQLERRD(3)                               02390005
024000                     ADD +1                                       02400005
024100                        TO DS009-READ-COUNT (DS009-SRCH-IDX)      02410005
024200             END-SEARCH                                           02420005
024300         END-IF                                                   02430005
024400                                                                  02440005
024500                                                                  02450005
024600*        DISPLAY 'RETURN SQLCODE ON INSERT:' SQLCODE              02460005
024700*        DISPLAY 'NUMBER OF ROWS INSERTED:' SQLERRD (3)           02470005
024800         IF DS009-UPDATE-COUNT > DS009-COMMIT-COUNT               02480005
024900            EXEC SQL                                              02490005
025000                COMMIT                                            02500005
025100            END-EXEC                                              02510005
025200            IF SQLCODE NOT = ZERO                                 02520005
025300               PERFORM DS002-SQL-ABEND-COMMIT                     02530005
025400            END-IF                                                02540005
025500            MOVE +0  TO DS009-UPDATE-COUNT                        02550005
025600*           DISPLAY 'COMMIT TAKEN AFTER RECORD: '                 02560005
025700*                                     DS009-TUPDATE-COUNT         02570005
025800         END-IF                                                   02580005
025900         ADD SQLERRD(3) TO DS009-UPDATE-COUNT                     02590005
026000         ADD SQLERRD(3) TO DS009-TUPDATE-COUNT                    02600005
026100        END-IF                                                    02610005
026200     END-IF.                                                      02620005
