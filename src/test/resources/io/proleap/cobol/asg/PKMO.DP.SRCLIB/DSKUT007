007400     READ TBLE-FILE                                               00740005
007500         AT END SET TABLE-EOF TO TRUE.                            00750005
007600     IF NOT-TABLE-EOF                                             00760005
007700        MOVE TBLE-DATA-SIZE TO DS009-TBLE-DATA-SIZE               00770005
007800        MOVE TBLE-NAME  TO DS009-PROCESS-TABLE                    00780005
007900        MOVE TBLE-KEYS  TO DS009-PKYS                             00790005
008000        PERFORM DS002-DO-PROCESS                                  00800005
008100        IF DS009-DO-PROCESS                                       00810005
008200*       DISPLAY '**********************************'              00820005
008300*       DISPLAY DS009-PROCESS-TABLE                               00830005
008400        EVALUATE TRUE                                             00840005
008500         WHEN DS009-PROCESS-TABLE = 'TSHPTPO'                     00850005
008600             MOVE TBLE-DATA-ITEM TO WS-SHPTPO                     00860005
008700             MOVE WS-SHPTPO-PO-NBR TO HOLD-PO-NBR                 00870005
008800*            DISPLAY HOLD-PO-NBR                                  00880005
008900*            DISPLAY WS-SHPTPO                                    00890005
009000             PERFORM R100-INSERT                                  00900005
009100         WHEN DS009-PROCESS-TABLE = 'TPKSLIP'                     00910005
009200             MOVE TBLE-DATA-ITEM TO WS-PKSLIP                     00920005
009300             MOVE WS-PKSLIP-PO-NBR TO HOLD-PO-NBR                 00930005
009400*            DISPLAY HOLD-PO-NBR                                  00940005
