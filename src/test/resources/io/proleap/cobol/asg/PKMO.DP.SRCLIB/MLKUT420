      *** DATA INTEGRITY CONTROL CARD                                   00990000
           COPY MLRD110.                                                01000000
                                                                        01010000
                                                                        01020000
      *** DATA INTEGRITY TMNSCL EXTRACT FILE                            01030000
W26603     COPY MLRD111.                                                01040005
                                                                        01050000
       EJECT                                                            01060000
                                                                        01070000
      ******************************************************************01080000
      *  DB2 ERROR MESSAGES FORMAT AREA.                                01090000
      ******************************************************************01100000
           COPY DPWS004.                                                01110000
                                                                        01120000
      ******************************************************************01130000
      *  DB2 ERRORS                                                     01140000
      ******************************************************************01150000
           COPY SQLCA2.                                                 01160000
                                                                        01170000
      ******************************************************************01180000
      *  DB2 COMMUNICATION AREA                                         01190000
      ******************************************************************01200000
           EXEC SQL                                                     01210000
                INCLUDE SQLCA                                           01220000
           END-EXEC.                                                    01230000
                                                                        01240000
      ******************************************************************01250000
      *  DB2 TMNSCL TABLE LAYOUT                                        01260000
      ******************************************************************01270000
           EXEC SQL                                                     01280000
                INCLUDE TMNSCL                                          01290000
           END-EXEC.                                                    01300000
                                                                        01310000
      ******************************************************************01320000
      *  DB2 TMNSCL CURSOR                                              01330000
      ******************************************************************01340000
           EXEC SQL                                                     01350000
               DECLARE TMNSCL_CURSOR CURSOR FOR                         01360000
                SELECT FSCL_MN_END_DTE,                                 01370000
                       FSCL_MN_NBR,                                     01380000
                       DEPT_NBR,                                        01390000
                       SUB_CL_NBR,                                      01400000
                       SLS_RTL_AMT,                                     01410000
                       MKDN_RTL_AMT,                                    01420000
                       MKUP_RTL_AMT,                                    01430000
                       XFER_IN_RTL_AMT,                                 01440000
                       XFER_OUT_RTL_AMT,                                01450000
                       RCPT_RTL_AMT,                                    01460000
                       RCPT_NET_COST_AMT,                               01470000
                       PADJ_RTL_AMT,                                    01480000
                       PADJ_NET_COST_AMT,                               01490000
                       INV_ADJ_RTL_AMT,                                 01500000
                       SHNK_RTL_AMT,                                    01510004
                       END_INV_RTL_AMT                                  01511004
                  FROM TMNSCL                                           01520000
                 WHERE (FSCL_MN_END_DTE >= :ML110-STRT-DTE)             01530000
                   AND (FSCL_MN_END_DTE <= :ML110-END-DTE)              01540000
              ORDER BY FSCL_MN_END_DTE,                                 01550000
                       DEPT_NBR,                                        01560000
                       SUB_CL_NBR                                       01570002
W26603        WITH UR                                                   01571005
           END-EXEC.                                                    01580000
                                                                        01590000
       EJECT                                                            01600000
                                                                        01610000
      ******************************************************************01620000
       PROCEDURE DIVISION.                                              01630000
      ******************************************************************01640000
                                                                        01650000
      ******************************************************************01660000
       0000-MAINLINE-MLKUT420.                                          01670000
      ******************************************************************01680000
      *                                                                 01690000
           PERFORM 1000-INITIALIZATION.                                 01700000
                                                                        01710000
           PERFORM 2000-MAIN-PROCESS.                                   01720000
                                                                        01730000
           PERFORM 8000-EOJ.                                            01740000
                                                                        01750000
           STOP RUN.                                                    01760000
                                                                        01770000
       EJECT                                                            01780000
                                                                        01790000
      ******************************************************************01800000
       1000-INITIALIZATION.                                             01810000
      ******************************************************************01820000
      *                                                                 01830000
           OPEN INPUT  DATA-INTEGRITY-CTL-FILE.                         01840000
           OPEN OUTPUT DATA-INTEGRITY-EXTR-FILE.                        01850000
                                                                        01860000
           PERFORM 4000-READ-CONTROL-CARD.                              01870000
                                                                        01880000
           DISPLAY ' '.                                                 01890000
           DISPLAY '*** MLKUT420 INPUT CONTROL CARD VALUES ***'         01900000
           DISPLAY ML110-DATA-INTEGRITY-CC-REC.                         01910000
                                                                        01920000
       EJECT                                                            01930000
                                                                        01940000
      ******************************************************************01950000
       2000-MAIN-PROCESS.                                               01960000
      ******************************************************************01970000
      *                                                                 01980000
           PERFORM 7100-OPEN-CURSOR-TMNSCL.                             01990000
                                                                        02000000
           PERFORM 7200-FETCH-TMNSCL.                                   02010000
                                                                        02020000
           PERFORM                                                      02030000
             UNTIL PS-TMNSCL-IS-DONE                                    02040000
               PERFORM 5000-WRITE-EXTRACT                               02050000
               PERFORM 7200-FETCH-TMNSCL                                02060000
           END-PERFORM.                                                 02070000
                                                                        02080000
           PERFORM 7300-CLOSE-CURSOR-TMNSCL.                            02090000
                                                                        02100000
       EJECT                                                            02110000
                                                                        02120000
      ******************************************************************02130000
       4000-READ-CONTROL-CARD.                                          02140000
      ******************************************************************02150000
      *                                                                 02160000
           READ DATA-INTEGRITY-CTL-FILE                                 02170000
             INTO ML110-DATA-INTEGRITY-CC-REC                           02180000
               AT END                                                   02190000
                   MOVE '4000-READ-CONTROL-CARD'                        02200000
                                       TO AA-PARAGRAPH-NAME             02210000
                   MOVE 'CONTROL CARD IS MISSING'                       02220000
                                       TO AA-MESSAGE                    02230000
                   MOVE +4001          TO ABEND-CODE                    02240000
                   PERFORM Z990-COMMON-ABEND-ROUTINE                    02250000
           END-READ.                                                    02260000
                                                                        02270000
       EJECT                                                            02280000
                                                                        02290000
      ******************************************************************02300000
       5000-WRITE-EXTRACT.                                              02310000
      ******************************************************************02320000
      *                                                                 02330000
           MOVE MNSCL-FSCL-MN-END-DTE  TO ML111-FSCL-MN-END-DTE.        02340000
           MOVE MNSCL-FSCL-MN-NBR      TO ML111-FSCL-MN-NBR.            02350000
           MOVE MNSCL-DEPT-NBR         TO ML111-DEPT-NBR.               02360000
           MOVE MNSCL-SUB-CL-NBR       TO PV-SUB-CL-NBR.                02361000
           MOVE 0                      TO PV-2ND-BYTE-SUB-CL-NBR.       02362000
           MOVE PV-SUB-CL-NBR          TO ML111-MAJ-CL-NBR.             02370000
           MOVE MNSCL-SUB-CL-NBR       TO ML111-SUB-CL-NBR.             02380000
           MOVE 0                      TO ML111-STR-NBR.                02390002
           MOVE MNSCL-SLS-RTL-AMT      TO ML111-SLS-RTL-AMT.            02400000
           MOVE MNSCL-MKDN-RTL-AMT     TO ML111-MKDN-RTL-AMT.           02410000
           MOVE MNSCL-MKUP-RTL-AMT     TO ML111-MKUP-RTL-AMT.           02420000
           MOVE MNSCL-XFER-IN-RTL-AMT  TO ML111-XFER-IN-RTL-AMT.        02430000
           MOVE MNSCL-XFER-OUT-RTL-AMT TO ML111-XFER-OUT-RTL-AMT.       02440000
           MOVE MNSCL-RCPT-RTL-AMT     TO ML111-RCPT-RTL-AMT.           02450000
           MOVE MNSCL-RCPT-NET-COST-AMT                                 02460000
                                       TO ML111-RCPT-NET-COST-AMT.      02470000
           MOVE MNSCL-PADJ-RTL-AMT     TO ML111-PADJ-RTL-AMT.           02480000
           MOVE MNSCL-PADJ-NET-COST-AMT                                 02490000
                                       TO ML111-PADJ-NET-COST-AMT.      02500000
           MOVE MNSCL-INV-ADJ-RTL-AMT  TO ML111-INV-ADJ-RTL-AMT.        02510000
           MOVE MNSCL-SHNK-RTL-AMT     TO ML111-SHNK-RTL-AMT.           02520000
           MOVE MNSCL-END-INV-RTL-AMT  TO ML111-END-INV-RTL-AMT.        02521003
                                                                        02530000
           WRITE DATA-INTEGRITY-EXTR-REC                                02540000
               FROM ML111-DATA-INTEGRITY-EXTR-REC.                      02550000
           ADD 1                       TO PA-OUTPUT-RECS-WRITTEN.       02560000
                                                                        02570000
       EJECT                                                            02580000
                                                                        02590000
      ******************************************************************02600000
       7100-OPEN-CURSOR-TMNSCL.                                         02610000
      ******************************************************************02620000
      *                                                                 02630000
           EXEC SQL                                                     02640000
               OPEN TMNSCL_CURSOR                                       02650000
           END-EXEC.                                                    02660000
                                                                        02670000
           EVALUATE TRUE                                                02680000
               WHEN SQLCODE = 0                                         02690000
                   CONTINUE                                             02690100
               WHEN OTHER                                               02691000
                   MOVE '7100-OPEN-CURSOR-TMNSCL'                       02700000
                                       TO AA-PARAGRAPH-NAME             02710000
                   MOVE 'TMNSCL TABLE ERROR'                            02720000
                                       TO AA-MESSAGE                    02730000
                   MOVE 'OPEN CURSOR'  TO AA-DB2-OPERATION              02740000
                   PERFORM Z998-SQL-ERROR                               02750000
           END-EVALUATE.                                                02840000
                                                                        02850000
       EJECT                                                            02860000
                                                                        02870000
      ******************************************************************02880000
       7200-FETCH-TMNSCL.                                               02890000
      ******************************************************************02900000
      *                                                                 02910000
           EXEC SQL                                                     02920000
               FETCH TMNSCL_CURSOR                                      02930000
                INTO :MNSCL-FSCL-MN-END-DTE,                            02940000
                     :MNSCL-FSCL-MN-NBR,                                02950000
                     :MNSCL-DEPT-NBR,                                   02960000
                     :MNSCL-SUB-CL-NBR,                                 02970000
                     :MNSCL-SLS-RTL-AMT,                                02980000
                     :MNSCL-MKDN-RTL-AMT,                               02990000
                     :MNSCL-MKUP-RTL-AMT,                               03000000
                     :MNSCL-XFER-IN-RTL-AMT,                            03010000
                     :MNSCL-XFER-OUT-RTL-AMT,                           03020000
                     :MNSCL-RCPT-RTL-AMT,                               03030000
                     :MNSCL-RCPT-NET-COST-AMT,                          03040000
                     :MNSCL-PADJ-RTL-AMT,                               03050000
                     :MNSCL-PADJ-NET-COST-AMT,                          03060000
                     :MNSCL-INV-ADJ-RTL-AMT,                            03070000
                     :MNSCL-SHNK-RTL-AMT,                               03080004
                     :MNSCL-END-INV-RTL-AMT                             03081004
           END-EXEC.                                                    03090000
                                                                        03100000
           EVALUATE TRUE                                                03110000
               WHEN SQLCODE = 0                                         03120000
                   ADD +1              TO PA-TMNSCL-ROWS-FETCHED        03130000
               WHEN SQLCODE = +100                                      03140000
                   MOVE 'Y'            TO PS-TMNSCL-DONE                03150000
               WHEN OTHER                                               03160000
                   MOVE '7200-FETCH-TMNSCL'                             03170000
                                       TO AA-PARAGRAPH-NAME             03180000
                   MOVE 'TMNSCL TABLE ERROR'                            03190000
                                       TO AA-MESSAGE                    03200000
                   MOVE 'FETCH'        TO AA-DB2-OPERATION              03210000
                   PERFORM Z998-SQL-ERROR                               03220000
           END-EVALUATE.                                                03310000
                                                                        03320000
       EJECT                                                            03330000
                                                                        03340000
      ******************************************************************03350000
       7300-CLOSE-CURSOR-TMNSCL.                                        03360000
      ******************************************************************03370000
      *                                                                 03380000
           EXEC SQL                                                     03390000
               CLOSE TMNSCL_CURSOR                                      03400000
           END-EXEC.                                                    03410000
                                                                        03420000
           EVALUATE TRUE                                                03430000
               WHEN SQLCODE = 0                                         03431000
                   CONTINUE                                             03432000
               WHEN OTHER                                               03433000
                   MOVE '7300-CLOSE-CURSOR-TMNSCL'                      03450000
                                       TO AA-PARAGRAPH-NAME             03460000
                   MOVE 'TMNSCL TABLE ERROR'                            03470000
                                       TO AA-MESSAGE                    03480000
                   MOVE 'CLOSE CURSOR' TO AA-DB2-OPERATION              03490000
                   PERFORM Z998-SQL-ERROR                               03500000
           END-EVALUATE.                                                03590000
                                                                        03600000
       EJECT                                                            03610000
                                                                        03620000
      ******************************************************************03630000
       8000-EOJ.                                                        03640000
      ******************************************************************03650000
      *                                                                 03660000
           DISPLAY ' '.                                                 03670000
           DISPLAY '  PROGRAM MLKUT420 PROCESSING SUMMARY'.             03680000
           DISPLAY '  -----------------------------------'.             03690000
           DISPLAY ' '.                                                 03700000
      *                                                                 03710000
           MOVE PA-TMNSCL-ROWS-FETCHED TO CD-COUNT-HOLD1.               03720000
           DISPLAY 'TMNSCL ROWS FETCHED =       ' CD-COUNT-HOLD1.       03730000
      *                                                                 03740000
           MOVE PA-OUTPUT-RECS-WRITTEN TO CD-COUNT-HOLD1.               03750000
           DISPLAY 'EXTRACT RECORDS WRITTEN =   ' CD-COUNT-HOLD1.       03760000
      *                                                                 03770000
           DISPLAY ' '.                                                 03780000
                                                                        03790000
           CLOSE DATA-INTEGRITY-CTL-FILE.                               03800000
           CLOSE DATA-INTEGRITY-EXTR-FILE.                              03810000
                                                                        03820000
       EJECT                                                            03830000
                                                                        03840000
      ******************************************************************03850000
       Z990-COMMON-ABEND-ROUTINE.                                       03860000
      ******************************************************************03870000
      *                                                                 03880000
           DISPLAY AA-ABEND-LIT.                                        03890000
           DISPLAY AA-PROGRAM-LIT.                                      03900000
           DISPLAY AA-PARAGRAPH-LIT.                                    03910000
           DISPLAY AA-MESSAGE-LINE.                                     03920000
           DISPLAY ABEND-CODE.                                          03930000
                                                                        03940000
           CALL 'ILBOABN0' USING ABEND-CODE.                            03950000
                                                                        03960000
       EJECT                                                            03970000
                                                                        03980000
      ******************************************************************03990000
       Z998-SQL-ERROR.                                                  04000000
      ******************************************************************04010000
      *                                                                 04020000
           CLOSE DATA-INTEGRITY-CTL-FILE                                04030000
                 DATA-INTEGRITY-EXTR-FILE.                              04040000
                                                                        04050000
           DISPLAY AA-ABEND-LIT.                                        04060000
           DISPLAY AA-DB2-ERROR-LIT.                                    04070000
           DISPLAY AA-PROGRAM-LIT.                                      04080000
           DISPLAY AA-PARAGRAPH-LIT.                                    04090000
           DISPLAY AA-MESSAGE-LINE.                                     04100000
           DISPLAY AA-DB2-OPERATION-LIT.                                04110000
           SET AC-DB2-ERROR TO TRUE.                                    04120000
           MOVE +4013                  TO ABEND-CODE.                   04130000
                                                                        04140000
           COPY DPPD004.                                                04150000
           CALL 'ILBOABN0' USING ABEND-CODE.                            04160000
                                                                        04170000
